    "id": "177", 
    "name": "ಹಡಪದ ಅಪ್ಪಣ್ಣ", 
    "vachana": {
        "13976": "ಅಂಗಕ್ಕೆ ಲಿಂಗಕ್ಕೆ ಪೆÇಂಗೆ ಪರಿಮಳದ ಶೃಂಗಾರದ ಹಾರ.", 
        "13977": "ಅಂಗಗುಣಂಗಳನೆಲ್ಲ ಅನಲಂಗಿಕ್ಕಿ,", 
        "13978": "ಅಂಗದ ಮೇಲೆ ಲಿಂಗವಿದ್ದವರೆಲ್ಲರು ಸರಿಗಾಣಬೇಕೆಂಬರು,", 
        "13979": "ಅಂಗದ ಲಯ ಲಿಂಗದೊಳಗೆ, ಲಿಂಗದ ಲಯ ಅಂಗದೊಳಗೆ.", 
        "13980": "ಅಂಗ ಲಿಂಗವಾಯಿತ್ತೆಂಬಿರಿ, ಲಿಂಗ ಅಂಗವಾಯಿತ್ತೆಂಬಿರಿ.", 
        "13981": "ಅಂಗ ಲಿಂಗವೆಂದರಿದ ಬಳಿಕ, ಲಿಂಗ ಅಂಗವೆಂದರಿದ ಬಳಿಕ,", 
        "13982": "ಅಂಗಲಿಂಗವೆಂದರಿ[ದು]ದಕ್ಕೆ ಕುರುಹು ಎಂತಿಪ್ಪುದೆಂದರೆ,", 
        "13983": "ಅಂಗಲಿಂಗಸಂಬಂಧವಾದ ಬಳಿಕ ಮನ ಹಿಂಗದಿಪ್ಪುದು.", 
        "13984": "ಅಂಗವದಾರದು? ಲಿಂಗವದಾರದು? ಸಂಗವದಾರದು ?", 
        "13985": "ಅಂಗವ ಮರೆದು ಲಿಂಗವ ಕೂಡಿ,", 
        "13986": "ಅಂಗವ ಮಾರಿಕೊಂಡು ಉಂಬಾತನೊಬ್ಬ ಠಕ್ಕ.", 
        "13987": "ಅಂತರಂಗದಲ್ಲಿ ಆಸೆ, ಬಹಿರಂಗದಲ್ಲಿ ಕ್ರೋಧ,", 
        "13988": "ಅಖಂಡ ಗೋಳಾಕಾಕಾರವಾಗಿರ್ದ ಮಹಾಲಿಂಗವೆ ಅಂಗವಿಡಿದಲ್ಲಿ,", 
        "13989": "ಅತ್ತಿಗೆ ಸತ್ತಳು, ನಾದಿನಿ ಮೊರೆಯಲಿಲ್ಲ.", 
        "13990": "ಅನ್ನ ಉದಕವ ಕೊಂಡೆಹೆನೆಂದಡೆ ಭೂಮಿಯ ಹಂಗು,", 
        "13991": "ಅನ್ನವನ್ನಿಕ್ಕಿದರೇನು ? ಹೊನ್ನ ಕೊಟ್ಟರೇನು ?", 
        "13992": "ಅನುಭಾವ ಅನುಭಾವವೆಂದು, ನುಡಿದಾಡುತ್ತಿಪ್ಪಿರಿ.", 
        "13993": "ಅಯ್ಯಾ ಅರಗಿನ ಮರದ ಮೇಲೆ ಗಿರಿ ಹುಟ್ಟಿತ್ತಯ್ಯಾ.", 
        "13994": "ಅಯ್ಯಾ ಆರೂ ಇಲ್ಲದ ಅರಣ್ಯದಲ್ಲಿ, ನಾನಡಿಯಿಟ್ಟು ನಡವುತ್ತಿರ್ದೆನಯ್ಯಾ.", 
        "13995": "ಅಯ್ಯಾ ಎನೆಗೆ ಬಸವಪ್ರಿಯನೆಂದರೂ ನೀನೆ,", 
        "13996": "ಅಯ್ಯಾ ಎನ್ನಂಗದ ಮೇಲಿಪ್ಪ ಲಿಂಗವು ಕರ್ಪುರದಂತಾಯಿತ್ತು.", 
        "13997": "ಅಯ್ಯಾ ಎನ್ನಂಗದಲ್ಲಿಪ್ಪ ಅರುವೆಯ ಕಂಡು, ತೆಗೆದೆನ್ನ ಲಿಂಗಕ್ಕೆ ಹೊದ್ದಿಸಿ,", 
        "13998": "ಅಯ್ಯಾ ಎನ್ನ ಗುರು ಎನಗೆ ಉಪದೇಶವ ಮಾಡಿದ", 
        "13999": "ಅಯ್ಯಾ ಎನ್ನ ತಂದೆ ತಾಯಿಗಳು", 
        "14000": "ಅಯ್ಯಾ, ಎನ್ನ ಬಾಳುವೆಯಲ್ಲಿ ಹೇಳದೆ ಕೇಳದೆ,", 
        "14001": "ಅಯ್ಯಾ ಏನು ಏನೂ ಇಲ್ಲದಂದು, ಧರೆ ಬ್ರಹ್ಮಾಂಡಗಳು ಇಲ್ಲದಂದು,", 
        "14002": "ಅಯ್ಯಾ ಧರೆಯ ಮೇಲೆ ಹುಟ್ಟಿದವರೆಲ್ಲ", 
        "14003": "ಅಯ್ಯಾ ನಾ ಉತ್ತರವನೇರಿ ನೋಡಲಾಗಿ,", 
        "14004": "ಅಯ್ಯಾ ನಾನು ದಿಟ್ಟಿಸಿ ನೋಡಿ ನಿಂದು ಬಟ್ಟಬಯಲ ಕಂಡೆ.", 
        "14005": "ಅಯ್ಯಾ ನಾ ಹುಟ್ಟಿದಂದಿಂದ ಎನ್ನ ಹೊಟ್ಟೆಗೆ ಕಾಣದೆ,", 
        "14006": "ಅಯ್ಯಾ ನಿಮ್ಮ ಶರಣರು ವೇಷವ ತೋರಿ ಗ್ರಾಸವ ಬೇಡುವರಲ್ಲ.", 
        "14007": "ಅಯ್ಯಾ ನೀನಿಲ್ಲದಿದ್ದರೆ ಎನಗೆ ಮುನ್ನ ನಾಮ ರೂಪುಂಟೆ ?", 
        "14008": "ಅಯ್ಯಾ ಲಿಂಗವ ಪೂಜಿಸಿಹೆನೆಂದು ಅಂಗದ ಕುರುಹ ಮರೆದೆ.", 
        "14009": "ಅಯ್ಯಾ, ಶಿವಭಕ್ತರು ನುಡಿವಲ್ಲಿ ಜಾಣತನದಿಂದ ನುಡಿವರು.", 
        "14010": "ಅಯ್ಯಾ ಹಾಳೂರೊಳಗೊಂದು ಹಗವ ಕಂಡೆನಯ್ಯಾ.", 
        "14011": "ಅಯ್ಯಾ, ಹುಟ್ಟಿದ ಮನುಜರೆಲ್ಲ", 
        "14012": "ಅರಿವೆಂಬುದೆ ಆಚಾರ, ಆಚಾರವೆಂಬುದೆ ಅರಿವು.", 
        "14013": "ಅವರನಕ ಕೂಳ ಇಡಿದ ಒಡಲಿಗೆ,", 
        "14014": "ಅವಲೋಹವ ಪರುಷ ಮುಟ್ಟಲು ಸುವರ್ಣವಾಗುತ್ತಿದೆ.", 
        "14015": "ಅವಿರಳ ಲಿಂಗವ ಕಂಡಿಹೆನೆಂದು ಅವರಿವರಲ್ಲಿನ್ನೇನ ಕೇಳುವಿರಿ?", 
        "14016": "ಆಶನ ವಸನಕ್ಕಾಗಿ ಒಂದು ಪಶುವು ಹುಟ್ಟಿತ್ತು.", 
        "14017": "ಆಕಾಶವ ನೂಕುತ್ತಿಯ ಹಾಗೆ ಅಟಗೋಲ ಹಂಗುಂಟೆ ?", 
        "14018": "ಆಗುತಿವೆ ಉದಯ ಮಧ್ಯಾಹ್ನ ಅಸ್ತಮಾನ.", 
        "14019": "ಆಚಾರದ ಅರಿವು ಹೊರಗಾದ ಮೇಲೆ.", 
        "14020": "ಆಚಾರದರಿವು ಆಗಮವ ಕೂಡಿಕೊಂಡಿಹುದು.", 
        "14021": "ಆಚಾರಲಿಂಗ ಗುರುಲಿಂಗ ಶಿವಲಿಂಗ", 
        "14022": "ಆಟ ಕೋಟಲೆ ಎಂಬ ರಾಟಾಳವ ಮುರಿದು,", 
        "14023": "ಆಟದಲಿ ಕೆಲಹೊತ್ತುಗಳೆದು, ಕೂಟದಲಿ ಕೆಲಹೊತ್ತುಗಳೆದು,", 
        "14024": "ಆಡುವ ಹಾಡುವ ನಡೆವ ನುಡಿವ ಬೆಡಗ ಬಿಡದೆ,", 
        "14025": "ಆಣವಮಲ, ಮಾಯಾಮಲ, ಕಾರ್ಮಿಕಮಲದೊಳಗಣ", 
        "14026": "ಆದಿ ಅನಾದಿ ಅಂತ್ಯವೆಂದು ನುಡಿದಾಡುವರಲ್ಲದೆ,", 
        "14027": "ಆದಿ ಅನಾದಿ ಎಂದು ಗಾದೆಯ ಮಾತು ನುಡಿದಾಡುವರೆಲ್ಲ", 
        "14028": "ಆದಿ ಅನಾದಿ ಎಂದು ನೀವು ಗಾದೆಯಮಾತ ನುಡಿದಾಡುವಿರಿ.", 
        "14029": "ಆದಿ ನಿರಾಳ, ಮಧ್ಯ ನಿರಾಳ, ಊಧ್ರ್ವ ನಿರಾಳ.", 
        "14030": "ಆದಿಪ್ರಸಾದಿ, ಅನಾದಿಪ್ರಸಾದಿ, ಅಂತ್ಯಪ್ರಸಾದಿ,", 
        "14031": "ಆದಿಯ ತೋರಿದ, ಅನಾದಿಯ ತೋರಿದ, ಭಾವವ ತೋರಿದ.", 
        "14032": "ಆಧಾರದ ಕುಂಡಲಿ ಸರ್ಪನ ಮಂಡೆಯ ಮೆಟ್ಟಿ ನಿಲಲು,", 
        "14033": "ಆಧಾರ, ಸ್ವಾಧಿಷ್ಠನ, ಮಣಿಪೂರಕ, ಅನಾಹತ, ವಿಶುದ್ಧಿ,", 
        "14034": "ಆಯತಲಿಂಗದಲ್ಲಿ ನೀವೆನಗೆ ಆಚಾರವ ತೋರಿದಿರಾಗಿ,", 
        "14035": "ಆಸೆಯನುಳಿದು ನಿರಾಸೆಯಲ್ಲಿ ನಿಂದು,", 
        "14036": "ಆಸೆಯಳಿದು, ನಿರಾಸೆಯಲ್ಲಿ ನಿಂದು,", 
        "14037": "ಆಸೆ ರೋಷವೆಂಬ ದ್ವೇಷವ ಬಿಟ್ಟು,", 
        "14038": "ಇಂತಪ್ಪ ಘನವನಗಲಿಸಿದ ಮಹಾಪ್ರಸಾದಿಯ ನಿಲವೆಂತಿಪ್ಪುದೆಂದರೆ,", 
        "14039": "ಇಂದು ನಾಳೆ ಎಂಬ ಸಂದೇಹವ ಬಿಟ್ಟು,", 
        "14040": "ಇತ್ತಲು ಪೃಥ್ವಿಯಿಂದ, ಅತ್ತಲು ಉತ್ತರಿಯ ಬೆಟ್ಟಕ್ಕೆ ಹತ್ತದ", 
        "14041": "ಈ ಜಗದೊಳಗಣ ಆಟವ ನೋಡಿದರೆ, ಎನಗೆ ಹಗರಣವಾಗಿ ಕಾಣಿಸುತ್ತಿದೆ.", 
        "14042": "ಈಡಾ ಪಿಂಗಳ ಸುಷುಮ್ನನಾಳದ", 
        "14043": "ಈ ಬಚ್ಚಬರಿಯ ಬಯಲ ಕಂಡಿಹೆನೆಂದರೆ, ಕಾಣಬಾರದು.", 
        "14044": "ಈ ಮಹಾಘನ ನೆಲೆಗೊಂಡಿಪ್ಪ ಶರಣನ ನೆಲೆ ಎಂತಿಪ್ಪುದೆಂದಡೆ:", 
        "14045": "ಈಯನುವನರಿದು ಲಿಂಗವ ನೋಡಿ ಕೂಡಿಹೆನೆಂದರೆ,", 
        "14046": "ಉಟ್ಟರೆ ತೊಟ್ಟರೇನಯ್ಯ ? ನಟ್ಟುವರಂತೆ.", 
        "14047": "ಉತ್ತರ, ದಕ್ಷಿಣ , ಪೂರ್ವ, ಪಶ್ಚಿಮ", 
        "14048": "ಉದಯ, ಮಧ್ಯಾಹ್ನ, ಅಸ್ತಮಯ, ಕತ್ತಲೆ ಬೆಳಗು,", 
        "14049": "ಉದಯ, ಮಧ್ಯಾಹ್ನ  ಅಸ್ತಮಯವೆಂಬ  ತ್ರಿಕಾಲದಲ್ಲಿ", 
        "14050": "ಉರಗನ ಫಣಾಮಣಿಯ ಬೆಳಗಿನಲ್ಲಿ ನಿಂದ ಪ್ರಸದಿ", 
        "14051": "ಉರಿಯೊಳಗೆ ಉರಿ ಹುಟ್ಟಿ, ಶರಧಿಯಾಪೆÇೀಷಣವಾಯಿತ್ತು.", 
        "14052": "ಊರ ಮೇಲೆ ಊರ ಕಂಡೆ, ನೀರ ಮೇಲೆ ನೀರ ಕಂಡೆ.", 
        "14053": "ಎಡಬಲವೆಂದೇನೊ ನಿಮ್ಮ ಅಡಿಗಳನರಿದವಂಗೆ ?", 
        "14054": "ಎಮ್ಮ ಶರಣರು ಗುರುಸ್ಥಲ ನಾಸ್ತಿಯಾಯಿತ್ತೆಂಬರು,", 
        "14055": "ಒಂದು ಹುತ್ತಕ್ಕೆ ಒಂಬತ್ತು ಬಾಯಿ,", 
        "14056": "ಒಳಗೆ ತೊಳೆದು, ಹೊರಗೆ ಮೆರೆದ ಪ್ರಸಾದಿ.", 
        "14057": "ಒಳಹೊರಗೆಂಬೆರಡು ಬಟ್ಟೆಗಳೊಳು ಸುಳಿದು ಸೂಸುವ ಬೆಳಗನೆ ಕಂಡು,", 
        "14058": "ಓದಲೇಕೋ ಲಿಂಗದ ಭೇದಾದಿಭೇದವನರಿದವಂಗೆ?", 
        "14059": "ಒಂ ಏಕವ ನ ದ್ವಿತೀಯಾಃ ಸ್ವಯಂಭುವೇ", 
        "14060": "ಒಂಕಾರಂ ನಾದರೂಪಂ ಚ ಒಂಕಾರಂ ಮಂತ್ರರೂಪಕಂ", 
        "14061": "ಓದಲೇತಕ್ಕೆ ಪ್ರಾಣಲಿಂಗಿಗೆ ? ಹಾಡಲೇತಕ್ಕೆ ಶರಣಂಗೆ ?", 
        "14062": "ಓದಿದರೇನಯ್ಯಾ? ಗಾದೆಯ ಮಾತಾಯಿತ್ತು.", 
        "14063": "ಕಂಗಳ ಮಣಿಯ ಬೆಳಗಿನೊಳು, ಈರೇಳು ಭುವನದ ಶೃಂಗಾರವಡಗಿತ್ತು.", 
        "14064": "ಕಂಡುವ ಹೇಳಿಹೆನೆ ? ಸಮಯಕ್ಕೆ ದೂರ,", 
        "14065": "ಕಟ್ಟಬೇಕು ಮನವ, ಮೆಟ್ಟಬೇಕು ಮದವ,", 
        "14066": "ಕತ್ತಲೆ ಬೆಳಗೆನಬೇಡ, ಸತ್ಯ ತಾನೆನಬೇಡ,", 
        "14067": "ಕರಕನಿಷ್ಟ  ಕಬ್ಬಿಣವ ನೆರಹಿರೆ, ಪರುಷ ಮುಟ್ಟಲು ಚಿನ್ನವಾಗದೆ?", 
        "14068": "ಕರವನರಿದಂಗೆ ಕಮಲದ ಹಂಗೇಕೊ?", 
        "14069": "ಕರಿದೈದು ಬಿಳಿದೈದು ಭಾಸುರವೈದು ಹದಿನೈದು ಬಗೆಯ", 
        "14070": "ಕಲ್ಲುದೇವರ ನಂಬಿದವರೆಲ್ಲ", 
        "14071": "ಕಾಡಬೇಡ ಕಂಡವರ, ಬೇಡಬೇಡ ನರರುಗಳ,", 
        "14072": "ಕಾಣಬಾರದ ಘನವೆಂದು, ಜಗವೆಲ್ಲ  ಹೇಳುತ್ತಿದೆ.", 
        "14073": "ಕಾಯ ಕರಣಾದಿಗಳ ಏಕವ ಮಾಡಿದ ಪ್ರಸಾದಿ.", 
        "14074": "ಕಾಯಸ್ಥಲ, ಕರಸ್ಥಲ, ಉರಸ್ಥಲ,", 
        "14075": "ಕೀಡೆ ತುಂಬಿಯ ಬಿಡದೆ ನೆನೆಯೆ, ನೋಡಲಾಕ್ಷಣ ತುಂಬಿಯಾಗಿಹುದು.", 
        "14076": "ಕುದುರೆಯ ಕುಪ್ಪಟ ಘನವಾಯಿತ್ತು.  ಆನೆಯ ಹರಿದಾಟ ನಿಲಬಾರದು.", 
        "14077": "ಕೂಟವಿಲ್ಲದ ಮಾಟ, ಬೇಟವಿಲ್ಲದ ನೋಟ,", 
        "14078": "ಕೇಳು ಕೇಳಾ, ಭಕ್ತ, ಮಹೇಶ, ಪ್ರಸಾದಿ,", 
        "14079": "ಗುರು ಗುರು ಎಂದು ನುಡಿದಾಡುತಿಪ್ಪರು,", 
        "14080": "ಗುರು ಗುರು ಎಂದು ಪೂಜೆಯ ಮಾಡುವರು.", 
        "14081": "ಗುರುಪಾದೋದಕಕ್ಕೆ ಹರಿವ ನದಿಯೆಲ್ಲ ಸರಿಯೆಂದು ನುಡಿದರೆ,", 
        "14082": "ಗುರುಪ್ರಸಾದ, ಲಿಂಗಪ್ರಸಾದ, ಜಂಗಮಪ್ರಸಾದ,", 
        "14083": "ಗುರುಭಕ್ತಿಯ ಮಾಡಿಹೆವೆಂದು ಅಂಗಸೂತಕವ ಮಾಡಿದರು.", 
        "14084": "ಗುರುವ ಮುಟ್ಟಿ ಗುರುವಿನಂತಾಗಬೇಡವೆ?", 
        "14085": "ಗುರುವಿಂದಾದ ಲಿಂಗ, ಲಿಂಗದಿಂದಾದ ಜಂಗಮ,", 
        "14086": "ಗುರುವಿಂದಾದ ಬಯಲು, ಬಯಲಿಂದಾದ ಬ್ರಹ್ಮವೆ ಲಿಂಗ", 
        "14087": "ಗುರುವಿನಿಂದಾದ ಪರಂ ಗೂಢಂ ಶರೀರ ಸ್ಥಲಂ.", 
        "14088": "ಗುರುಶಿಷ್ಯಸಂಬಂಧವೆಂತಿಪ್ಪುದೆಂದಡೆ,   ಹೇಳಿಹೆ   ಕೇಳಿರೋ,", 
        "14089": "ಘನವೆಂದರೆ ತನುವಿನೊಳಗಾಯಿತ್ತು, ತನುವೆಂದರೆ ಮನದೊಳಗಾಯಿತ್ತು.", 
        "14090": "ಚೌದಳ ಷಡುದಳ ದಶದಳ ದ್ವಾದಶದಳ ಷೋಡಶದಳ ದ್ವಿದಳ,", 
        "14091": "ಜಂಗಮ ಜಂಗಮವೆಂದು ನುಡಿದು, ಜಗದ ಹಂಗಿಗರಾಗಿ ಇರಲಾಗದು.", 
        "14092": "ಜಂಗಮಲಿಂಗ ಎಂತಿಹನು ಎಂದರೆ,", 
        "14093": "ಜಂಗಮಲಿಂಗವೆಂತಾಹನೆಂದರೆ:", 
        "14094": "ಜಗದಗಲ ಮುಗಿಲಗಲ ಪಾದ ಪಾತಾಳದಿಂದತ್ತತ್ತ  ಪಾದ.", 
        "14095": "ಜಗದೊಳಗೆ ಹುಟ್ಟಿ ಜಗದ ಹಂಗಿಗರಾಗಿ,", 
        "14096": "ಜಗದೊಳಗೆ ಹುಟ್ಟಿದವರೆಲ್ಲ ಹಗರಣಿಗರಾಗಿ ಹುಟ್ಟಿದರಲ್ಲದೆ,", 
        "14097": "ಜಪ ತಪ ನೇಮ ನಿತ್ಯದಿಂದ ನಿಶ್ಚಂತರೂಪನ ಕಂಡೆ, ಏನೆಂಬಿರಿ?", 
        "14098": "ತನ್ನ ತಾನರಿದವರು ಎಂತಿಪ್ಪರೆಂದರೆ,", 
        "14099": "ತನ್ನ ತಾನರಿಯದೆ ತನ್ನ ತಾ ನೋಡದೆ, ತನ್ನ ತಾ ನುಡಿಯದೆ,", 
        "14100": "ತನ್ನ ಮನೆಯನರಿಯದೆ ತವರುಮನೆಗೆ ಹಾರುವ ಹೆಣ್ಣಿನಂತೆ,", 
        "14101": "ತನುವ ಕೊಟ್ಟು ಭಕ್ತರಾದೆವೆಂಬರು, ಮನವ ಕೊಟ್ಟು ಭಕ್ತರಾದೆವೆಂಬರು,", 
        "14102": "ತನುವಿನಿಚ್ಛೆಗೆ ಶೀಲವ ಕಟ್ಟಿಕೊಂಬವರು ಲಕ್ಷೋಪಲಕ್ಷ ಉಂಟು.", 
        "14103": "ತಲೆ ಇಲ್ಲದ ಮುಂಡ, ಮೊಲೆ ಇಲ್ಲದಾವು.", 
        "14104": "ತಾನೆನಬೇಡ, ಮನ ಮುಗ್ಧನೆನಬೇಡ,", 
        "14105": "ತೀರ್ಥ ಯಾತ್ರೆ ಲಿಂಗದರುಶನಕ್ಕೆ ಹೋಗಿ,", 
        "14106": "ದಾಸಿಯ ಸಂಗ ಎರಡನೆಯ ಪಾತಕ, ವೇಶಿಯ ಸಂಗ ಮೂರನೆಯ ಪಾತಕ.", 
        "14107": "ದಿನದಿನಕ್ಕೆ ದೀನಮಾನವನಂತೆ, ಹೀನಾಶ್ರಯದಲ್ಲಿ ಹುಟ್ಟಿ,", 
        "14108": "ದಿವ್ಯಾನಂದ ಗುರುವೆ, ದೇದೀಪ್ಯಮಯ ಗುರುವೆ,", 
        "14109": "ದೀP್ಷಗುರು, ಶಿP್ಷಗುರು, ಮೋಕ್ಷಗುರುವೆಂದು", 
        "14110": "ಧರೆಯ ಹೊತ್ತಿರ್ಪ ಸರ್ಪ ಹೆರಿಯಿತ್ತಾಕಾಶಕ್ಕೆ.", 
        "14111": "ಧರೆಯೊಳಗೆ ಹುಟ್ಟಿದವರೆಲ್ಲ ಬಲ್ಲೆನೆಂದು", 
        "14112": "ಧ್ಯಾನದಲ್ಲಿ ಕುಳ್ಳಿರ್ದು, ಜ್ಞಾನದಲ್ಲಿ ನೋಡಿ,", 
        "14113": "ನಂಬುವುದು, ಶರಣರನೆ ನಂಬುವುದು.", 
        "14114": "ನಮ್ಮಂತುವ ತಿಳಿದು, ನೋಡಿದರೆ ಹೇಳಿಹೆನು.", 
        "14115": "ನಾನೊಂದು ಸುಖವ ಕಂಡು, ಸುಯಿಧಾನಿಯಾಗಿ ನಿಂದೆ.", 
        "14116": "ನಾವು ಜಂಗಮವೆಂಬರು, ತಾವು ಭಕ್ತರೆಂಬರು.", 
        "14117": "ನಾವು ಭಕ್ತ, ಮಹೇಶ್ವರ, ಪ್ರಸಾದಿ,", 
        "14118": "ನಿತ್ಯ ಅನಿತ್ಯವೆಂಬುದ ತೋರಿದಿರಿ, ಸತ್ಯ ಅಸತ್ಯವೆಂಬುದ ತೋರಿದಿರಿ.", 
        "14119": "ನಿಶ್ಚಿಂತ ನಿರಾಕಾರ ಪರಂಜ್ಯೋತಿ ಪರಮಪ್ರಕಾಶ", 
        "14120": "ನಿಷ್ಠೆಯ ಮರೆದರೇನಯ್ಯಾ ?", 
        "14121": "ನೀವು ಹೊತ್ತಿಪ್ಪ ವೇಷದಲ್ಲಿ ತತ್ವವ ತಿಳಿದು ನೋಡಿರಣ್ಣಾ.", 
        "14122": "ನುಡಿದ ನುಡಿಗೆ ನಡೆ ಇಲ್ಲದಿದ್ದರೆ,", 
        "14123": "ನುಡಿದರೆ ಗುರುವಾಗಿ ನುಡಿಯಬೇಕು, ನಡೆದರೆ ಪರವಾಗಿ ನಡೆಯಬೇಕು.", 
        "14124": "ನುಡಿದರೇನಯ್ಯ ನಡೆ ಇಲ್ಲದನ್ನಕ್ಕ ?", 
        "14125": "ಪರಾಪರದಲ್ಲಿ ಹುಟ್ಟಿದ ಪಾದೋದಕ.", 
        "14126": "ಪಿಂಡವಾದುದ ಪಿಂಡಜ್ಞಾನ ಹುಟ್ಟಿ ಅರಿದನಯ್ಯಾ ಶರಣನು.", 
        "14127": "ಪುರಾತರು ಪುರಾತರು ಎಂದು,", 
        "14128": "ಪೃಥ್ವಿ, ಅಪ್ಪು, ಅಗ್ನಿ, ವಾಯು, ಆಕಾಶ, ಚಂದ್ರ,", 
        "14129": "ಪ್ರಣಮವೆ ಪ್ರಾಯ, ಪ್ರಾಣವೆ ಲಿಂಗ.", 
        "14130": "ಪ್ರಥಮದಲ್ಲಿ ಭಕ್ತಸ್ಥಲವೆಂದು ನುಡಿವಿರಿ.", 
        "14131": "ಪ್ರಸಾದವೆಂದು, ಅರ್ಪಿತಪ್ರಸಾದವೆಂದು, ಅವಧಾನಪ್ರಸಾದವೆಂದು,", 
        "14132": "ಪ್ರಾಣಲಿಂಗಿ ಪ್ರಾಣಲಿಂಗಿ ಎಂದು ನುಡಿದಾಡುತಿಪ್ಪಿರಿ.", 
        "14133": "ಬಟ್ಟಬಯಲ ತುಟ್ಟತುದಿಯ ನಟ್ಟನಡುವಣ,", 
        "14134": "ಬಯಲಬ್ರಹ್ಮವೆ ಪಿಂಡಬ್ರಹ್ಮವೆನಿಸಿ,", 
        "14135": "ಬಯಲಿಂದಲೆ ಹುಟ್ಟಿ, ಬಯಲಿಂದಲೆ ಬೆಳೆದು,", 
        "14136": "ಬಯಲೆ ರೂಪಾಯಿತ್ತು, ನಿರ್ವಯಲೆ ನಿರೂಪಾಯಿತ್ತು.", 
        "14137": "ಬೆಟ್ಟವ ಬೆಳ್ಳಕ್ಕಿ ನುಂಗಿ,", 
        "14138": "ಬೆಳಗ ನುಂಗಿದ ಕತ್ತಲೆಯಂತೆ, ಕತ್ತಲೆಯ ನುಂಗಿದ ಬೆಳಗಿನಂತೆ,", 
        "14139": "ಬ್ರಹ್ಮನ ಹುಟ್ಟು, ವಿಷ್ಣುವಿನ ಸ್ಥಿತಿ,", 
        "14140": "ಬ್ರಹ್ಮಾಂಡದ ಬಯಲ ಪಸರಿಸಿ, ಹಿಡಿವರೆ", 
        "14141": "ಭಕ್ತನಾದರು ಆಗಲಿ, ಗುರುವಾದರು ಆಗಲಿ,", 
        "14142": "ಭಕ್ತನಾದರೆ ಎಂತಿರಬೇಕೆಂದರೆ, ಉಲುಹಡಗಿದ ವೃಕ್ಷದಂತಿರಬೇಕು.", 
        "14143": "ಭಕ್ತನಾದರೆ ಪೃಥ್ವಿಸಾರದಲಾದ ಪದಾರ್ಥವ ಲಿಂಗಕ್ಕೆ ಕೊಡದ ಭಾಷೆ.", 
        "14144": "ಭಕ್ತನಾದರೆ ಮುಕ್ತಿಪಥಗತಿಗೆ ನಿಲುಕದಂತಿರಬೇಕು.", 
        "14145": "ಭಕ್ತನೊಂದು ಕುಲ, ಭವಿಯೊಂದು ಕುಲವೆಂಬರು.", 
        "14146": "ಭಕ್ತ, ಮಹೇಶ್ವರ, ಪ್ರಸಾದಿ ಪ್ರಾಣಲಿಂಗಿ,", 
        "14147": "ಭಕ್ತಿಯ ಮಾಡಿಹೆನೆಂಬವರೆಲ್ಲ ಭಾಗ್ಯವಂತರಾದರು.", 
        "14148": "ಮನವನರಿದಂಗೆ ಮತದ ಹಂಗೇಕೊ ?", 
        "14149": "ಮನವೆ ಮಾರುತನ ಒಡಗೂಡಿರ್ದು, ತನುವೆ ವಿಕಾರದೊಳಗಿರ್ದು,", 
        "14150": "ಮತ್ರ್ಯದಲ್ಲಿ ಹುಟ್ಟಿ, ಕಂಗಳ ಮುಂದಣ ಕತ್ತಲೆಯ ಕಳೆಯದೆ,", 
        "14151": "ಮರ್ತ್ಯಲೋಕದ ಮಹಾಗಣಂಗಳು ನೀವು ಕೇಳಿರಯ್ಯ,", 
        "14152": "ಮರದೊಳಗಣ ಕಿಚ್ಚು ಮರನ ಸುಡುತ್ತಿದ್ದಿತ್ತಯ್ಯ.", 
        "14153": "ಮರೆದರೆ ಮಾಯೆ, ಅರಿದರೆ ಮಾಯೆ ಇಲ್ಲ,", 
        "14154": "ಮಹಾಘನ ನೆಲೆಗೊಂಡಿಪ್ಪ ಶರಣನ ನೆಲೆ ಎಂತಿಪ್ಪುದೆಂದರೆ,", 
        "14155": "ಮಾತು ಮಾತಿಗೆ ಮಥನವ ಮಾಡುವಾತನೆ ಜಾತ.", 
        "14156": "ಮಿಥ್ಯವನರಿದವರೆಲ್ಲ ತತ್ವಕ್ಕೆ ಅಂದೇ ಹೊರಗು,", 
        "14157": "ವಿೂಸಲು ಸೂಸಲಾಗಿ, ದೋಸೆ ಕಡಬು ಹೊಯ್ದು,", 
        "14158": "ಮುಟ್ಟಿಯೂ ಮುಟ್ಟಬಾರದ ಠಾವಿನಲ್ಲಿ,", 
        "14159": "ಮುಳುಗುತ್ತ ತೆರಹಿಲ್ಲದಲ್ಲಿ ಅಂಗವ ಬೇರಿಟ್ಟರಸಿಹೆನೆಂಬುದೆ ಭಿನ್ನಭಾವ,", 
        "14160": "ಮೂಲಬ್ರಹ್ಮದಲ್ಲಿ  ಮೊನೆದೋರಿದ ಪ್ರಸಾದವೆ ಪರಮಪ್ರಣಮ.", 
        "14161": "ಯೋಗದಿಂದರಿದಿಹೆನೆಂಬಿರಿ, ಓದಿನಿಂದರಿದಿಹೆನೆಂಬಿರಿ.", 
        "14162": "ಯೋಗಾಂಗ ಭೋಗಾಂಗ ತ್ಯಾಗಾಂಗ ಈ ತ್ರಿವಿಧವನು ಮರೆದು,", 
        "14163": "ರಾಗ ರಚನೆಯ ಬಲ್ಲೆನೆಂಬ ವ್ಯಂಗಿಮನುಜರಿರಾ, ನೀವು ಕೇಳಿ :", 
        "14164": "ರಾಗವ ಹಾಡಿದವರೆಲ್ಲರು", 
        "14165": "ಲಿಂಗನಯನದಲ್ಲಿ ನೋಡುತ್ತ , ಲಿಂಗಜಿಹ್ವೆಯಲ್ಲಿ ನುಡಿವುತ್ತ ,", 
        "14166": "ಲಿಂಗಪೂಜೆಯ ಮಾಡುತ್ತ  ತಮ್ಮಂಗ ಸುಖದ ಸಲುವಾಗಿ", 
        "14167": "ಲಿಂಗಪ್ರಸಾದವ ಜಂಗಮಕ್ಕೆ ಕೊಡುವ ಕರ್ಮಿಗಳು ನೀವು ಕೇಳಿರೆ.", 
        "14168": "ವಂದನೆಗೆ ನಿಲ್ಲಬೇಡ, ನಿಂದೆಗಂಜಿ ಓಡಲಿಬೇಡ.", 
        "14169": "ವಜ್ರದ ಮನೆಯೊಳಗಿರ್ದು, ಗೊರಲೆ ಮುಟ್ಟಿತೆಂಬ ಸಂದೇಹವೇಕೆ ?", 
        "14170": "ವಾದಿಗೆ ಜೂಜನಾಡುವಾತನೊಬ್ಬ ಪಾತಕ.", 
        "14171": "ವೇದಪುರಾಣಾಗಮಶಾಸ್ತ್ರ ನಾದದ ಸೊಮ್ಮೆಂಬಿರಿ.", 
        "14172": "ವೇದವನೋದಿದವರೆಲ್ಲ ನಮ್ಮ ಶರಣರು ಹೋದ ಹಾದಿಯನರಿಯದೆ,", 
        "14173": "ವೇಷವ ಹೊತ್ತು , ಆಶೆ ರೋಷವ ಬಿಡದೆ, ದೇಶವ ತಿರುಗಿ,", 
        "14174": "ವ್ಯಸನವುಳ್ಳನ್ನಕ್ಕ  ಪ್ರಸಾದಿಯಲ್ಲ .", 
        "14175": "ಶರಣನ ಅಂಗವು ಎಂತಿಪ್ಪುದೆಂದರೆ,", 
        "14176": "ಶರಣರಿಗೆ ಭವವುಂಟೆಂದು ಮತ್ರ್ಯದಲ್ಲಿ ಹುಟ್ಟಿದ", 
        "14177": "ಶಿವಭಕ್ತರ ಹಾದಿಯ ಕಾಣದೆ,", 
        "14178": "ಶೀಲವಂತರು, ಶೀಲವಂತರು ಎಂದೇನೊ ?", 
        "14179": "ಶೀಲವಂತರು, ಶೀಲವಂತರು ಎಂಬರು", 
        "14180": "ಶುದ್ಧ , ಸಿದ್ಧ , ಪ್ರಸಿದ್ಧ , ಪ್ರಸಾದವೆಂದು ಹೆಸರಿಟ್ಟುಕೊಂಡು", 
        "14181": "ಶೃಂಗಾರದ ಊರಿಗೆ ಒಂಬತ್ತು ಹಾಗಿಲು,", 
        "14182": "ಶ್ರೀ ಗುರುವಿನ ಕೃಪಾದೃಷ್ಟಿ ತತ್‍ಶಿಷ್ಯನ ಹೃದಯದಲ್ಲಿ ಪ್ರವೇಶಿಸಿ,", 
        "14183": "ಶ್ರೀಜಂಗಮಲಿಂಗ ಎಂತಿಹನೆಂದಡೆ :", 
        "14184": "ಶ್ರೀಗುರುವೆ ಮದ್ಗುರುವೆ ಸದ್ಗುರುವೆ  ಸದಾಸನ್ನಿಹಿತ ಗುರುವೆ", 
        "14185": "ಶ್ರೋತ್ರ, ನೇತ್ರ, ಜಿಹ್ವೆ, ತ್ವಕ್ಕು, ಘ್ರಾಣವೆಂಬ ಬುದ್ಧೀಂದ್ರಿಯಂಗಳನರಿದು,", 
        "14186": "ಸಂಜೆ ಮಂಜಾನೆಯೆಂದೆನಬೇಡ, ಅಂಜಿಕೆ ಬೇಡ, ಅಳುಕು ಬೇಡ.", 
        "14187": "ಸತಿಸುತ ಮಾತಾಪಿತರಂದದಿ ಮೋಹದಲಿ ಮನಮಗ್ನವಾದರೆ,", 
        "14188": "ಸಾಕಾರ ನಿರಾಕಾರ ಏಕೀಕೃತಾನಂದ ಗುರುವೆ", 
        "14189": "ಸಾವಾಗ ದೇವನೆಂದರೆ, ಸಾವು ಬಿಡುವುದೇ ?", 
        "14190": "ಸೀಮೆಭೂಮಿಯೆಂದೇನೋ, ಹೇಮವನುರುಹಿದಂಗೆ.", 
        "14191": "ಸುಖಾನುಭಾವ, ಲಿಂಗಾನುಭಾವವೆಂದು ನುಡಿದಾಡುತಿಪ್ಪಿರಿ.", 
        "14192": "ಸುಪ್ರಭಾಕಳೆಯಿಂದ ಚಿತ್ಪ್ರಣಮ ಉದಯ.", 
        "14193": "ಸೋಹಂ ಹೊಕ್ಕು ದಾಸೋಹವೆಂಬ ಅಂಜನವ ಹಚ್ಚಿ,", 
        "14194": "ಸ್ಥಲವಿಟ್ಟು ನಡೆಯಬೇಕೆಂಬರು, ಸ್ಥಲವಿಟ್ಟು ನುಡಿಯಬೇಕೆಂಬರು.", 
        "14195": "ಸ್ಥಲವೆಂದರೆ ಒಂದು, ನೆಲೆಯೆಂದರೆ ಎರಡು,", 
        "14196": "ಹಂಚುಕಂಥೆ, ಅತೀತ, ವಿರಕ್ತರು, ಸ್ಥಲದವರು ಎಂದು ನುಡಿದಾಡುವರು.", 
        "14197": "ಹಗಲು ಗೂಗೆಗೆ ಇರುಳಾಗಿಪ್ಪುದು, ಇರುಳು ಕಪಟಗೆ ಹಗಲಾಗಿಪ್ಪುದು.", 
        "14198": "ಹರನಲ್ಲದೆ ದೈವವಿಲ್ಲೆಂದು ಶ್ರುತಿ ಸಾರುತಿರ್ದು,", 
        "14199": "ಹರಹರ ಎಂದು ಹತ್ತುಬಾರಿ ಎನಬಹುದಲ್ಲದೆ,", 
        "14200": "ಹರಿ ಬ್ರಹ್ಮ ಕಾಲ ಕಾಮ ದP್ಷದಿಗಳ ದೇವರೆಂದು", 
        "14201": "ಹರಿವ ಜಲಧಿಯಂತೆ, ಚರಿಸಿ ಬಹ ಮನವ ನಿಲ್ಲೆಂದು ನಿಲಿಸುವ ಪುರುಷರುಂಟೆ ?", 
        "14202": "ಹಲವಂದ ಚಂದದಲ್ಲಿ ಹಾಡಿದರೇನಯ್ಯ,", 
        "14203": "ಹಸನ ಮಾಡಿ ಹರಗಿ ಹೊಲದಲ್ಲಿ ಕಸವ ಬಿತ್ತುವ ಮರುಳರಂತೆ,", 
        "14204": "ಹಸಿವಿನಾಸೆಗೆ ಅಶನವ ಕೊಂಬರು, ವಿಷಯದಾಸೆಗೆ ಹುಸಿಯ ನುಡಿವರು.", 
        "14205": "ಹಾಕಿದ ಮುಂಡಿಗೆಯ ಎತ್ತುವರಿಲ್ಲ.", 
        "14206": "ಹಾಲ ಕಂಡ ಬೆಕ್ಕು ಮೆಲ್ಲುಲಿ ತೆಗೆದು ಕುಡಿವುತ್ತಿರಲು,", 
        "14207": "ಹಾಲಸಾಗರದೊಳು ತೇಲಾಡುತ್ತಿದ್ದು ,", 
        "14208": "ಹಾವ ಹಿಡಿವುದ ಹಾವಾಡಿಗ ಬಲ್ಲನಲ್ಲದೆ,", 
        "14209": "ಹಿಂದನರುಹಿ ಹಿಂದ ಹರಿದಿರಿ, ಮುಂದನರುಹಿ ಮುಕ್ತನ ಮಾಡಿದಿರಿ.", 
        "14210": "ಹುಟ್ಟಲೇಕೊ ನರರ ಜನ್ಮದಲ್ಲಿ ? ಕಟ್ಟಲೇಕೋ ಕೊರಳಲ್ಲಿ ಲಿಂಗವ ?", 
        "14211": "ಹುಟ್ಟಿದ ಮನುಜರೆಲ್ಲ ಹೊಟ್ಟೆಯ ಹೊರೆವುದೇನು ಸೋಜಿಗವೊ ?", 
        "14212": "ಹುಟ್ಟುವಾತ ಲಿಂಗಪಿಂಡದೊಳಡಗಿ ಬರಲು, ಪಿಂಡಗತಸ್ಥಲವೆನಿಸಿತ್ತು .", 
        "14213": "ಹುಡಿಯ ಹಾರಿಸಿ ಅಡಗಿಸುವ ವಾಯು ಅಡಗಿ ತೋರುವ ಬೆಡಗಿನಂತೆ,", 
        "14214": "ಹುತ್ತಕ್ಕೆ ಹಾಲು ತುಪ್ಪವನೆರೆದು,", 
        "14215": "ಹುತ್ತದ ಮೇಲಣ ಸರ್ಪ ಸತ್ತಿತ್ತೆಂದು ಮುಟ್ಟಿ ಬಾಳ್ದ ಮನುಜರುಂಟೆ ?", 
        "14216": "ಹುತ್ತದ ಮೇಲೊಂದು ಕಸ್ತೂರಿಯ ಮೃಗವ ಕಂಡೆ.", 
        "14217": "ಹುಸಿಯ ಹಟ್ಟಿಯಲಿಪ್ಪ ಸೊನಗ, ಹಾಕಿದ ಹಿಟ್ಟಿಗೆ ಬೊಗಳುವಂತೆ.", 
        "14218": "ಹೆತ್ತವರೇ ಹೆಸರಿಡಬೇಕಲ್ಲದೆ, ತೊತ್ತಿರು ಬಂಟರು ಎತ್ತಬಲ್ಲರೊ ?", 
        "14219": "ಹೆಸರಿಗೆ ಬಾರದ ಘನವ ಹೆಸರಿಗೆ ತಂದು,", 
        "14220": "ಹೊತ್ತಾರೆ ಎದ್ದು ಹೂವು ಪತ್ರೆ ಉದಕವ ತಂದು,", 
        "14221": "ಹೊತ್ತಾರೆಯಿಂದ ಅಸ್ತಮಯ ತನಕ", 
        "14222": "ಹೊತ್ತುಹೊತ್ತಿಗೆ  ನಿತ್ಯರ ಸಂಗವ ಮಾಡಿ,", 
        "14223": "ಹೊತ್ತುಹೊತ್ತಿಗೆ ಲಿಂಗಪೂಜೆಯ ಮಾಡಿಯೂ", 
        "14224": "ಹೊತ್ತುಹೋಗದ ಮುನ್ನ ನೀವು ಸತ್ತಂತೆ ಇರಿರೊ.", 
        "14225": "ಹೊನ್ನ  ಗಳಿಸಿದರೇನಯ್ಯಾ, ಹೊಲೆಯರ ಮನೆಯಲ್ಲಿದ್ದರೆ ?"
    }
}