    "name": "ನೀಲಮ್ಮ", 
    "vachana": {
        "20748": "ಭಾವವಿಲ್ಲದ ರೂಪೆ ರೂಪಿಲ್ಲದೆ ಅಡಗಿದೆ.", 
        "7451": "ಅಂಗದ ಸಂಗವ ಹಿಂಗಿ ಅಂಗವನಳಿದೆನಯ್ಯ.", 
        "7452": "ಅಂಗದ ಸಂಗಿಗನಲ್ಲ ನಮ್ಮ ಬಸವಯ್ಯನು.", 
        "7453": "ಅಂಗನೆಯ ಸಂಗವ ಮಾಡಿಹೆನು ನಾನು,", 
        "7454": "ಅಂಗವಡಗಿ ನಿರಂಗಿಯಾನಾದೆನು.", 
        "7455": "ಅಂಗವನರಿದು ಹಿಂಗಿದೆ ಪ್ರಾಣವ,", 
        "7456": "ಅಂಗವಿಲ್ಲವೆನಗೆ ಲಿಂಗವಿಲ್ಲವೆನಗೆ", 
        "7457": "ಅಂಡಜವ ಕಲ್ಪಿಸಲು ಆ ಅಂಡಜದ ರೂಪೆನ್ನಲಿಲ್ಲದ ಕಾರಣ", 
        "7458": "ಅಂಡಜವಳಿದ ಬಸವಾ; ಪಿಂಡಜವಳಿದ ಬಸವಾ", 
        "7459": "ಅಂಡದಲ್ಲಿಯೊಂದು ಆಕಾರದ ರೂಪು ಹುಟ್ಟಿತ್ತು.", 
        "7460": "ಅಫ್ಸೋರವಕ್ತ್ರ, ಅಜಾತವಕ್ತ್ರ, ಸಾಧ್ಯವಿಲ್ಲದ", 
        "7461": "ಅಟ್ಟಡವಿಯಲ್ಲಿ ಬಿಟ್ಟುಹೋದಿರಿ ಬಸವಯ್ಯಾ.", 
        "7462": "ಅಡಲಿಲ್ಲದ ಊಟವನುಣಹೋದಡೆ,", 
        "7463": "ಅಡವಿಯಲ್ಲಿ ಕಣ್ಣುಗೆಟ್ಟ ಪಶುವಿನಂತೆ ನಾನು ಪ್ರಳಾಪಿಸುತ್ತಿದ್ದೆನಯ್ಯಾ.", 
        "7464": "ಅಡವಿಯ ಹುಯ್ಯಲು ಎನ್ನ ಮೇಲುವರಿಯಿತ್ತು.", 
        "7465": "ಅಡಿಯಿಡಲಿಲ್ಲ, ನುಡಿಯನೇನೆನಲಿಲ್ಲ,ಸರಸವೆನಲಿಲ್ಲ,", 
        "7466": "ಅಣ್ಣೆವಾಲ ಕರೆದು, ಪುಣ್ಯದ ಕಡೆಗೋಲಿನಲ್ಲಿ ಕಡೆದು,", 
        "7467": "ಅತೀತವಡಗಿ, ನಿರಾಲಂಬದ ಮನದ ಮೂರ್ತಿಯಂ ತಿಳಿದು,", 
        "7468": "ಅಧಿಕ ತೇಜೋನ್ಮಯ ಬಸವಾ.", 
        "7469": "ಅನಾದಿಯ ಸ್ಥೂಲ, ಆದಿಯ ನಿಃಕಲ,", 
        "7470": "ಅನುಭಾವದ ಸಾರವೆ ನಿಜಸಾರವಾಗಿ ನಿಂದನೆಮ್ಮ ಬಸವಯ್ಯನು.", 
        "7471": "ಅಪ್ರಮಾಣದ ತಾಣದಲ್ಲಿ", 
        "7472": "ಅಮೃತಡೊವಿಗೆಯೊಳಗೆ ಅಮೃತಡೊವಿಗೆ,", 
        "7473": "ಅರಸರಸಲು ನಾನು", 
        "7474": "ಅರಿಯದ ಶಬ್ದವ ಕುರುಹಿಂಗೆ ತಂದ ಬಸವಯ್ಯನು.", 
        "7475": "ಅರಿಯೆನರಿಯೆ ನಾನು, ಏನುವನರಿಯೆನಯ್ಯ;", 
        "7476": "ಅರಿಯೆನರಿಯೆನಿಂತಹ ಮೂರ್ತಿಯ, ಅರಿಯದವನಲ್ಲ.", 
        "7477": "ಅರಿವಡೆ ನಾನು ಅರಿವುಳ್ಳ ಹೆಣ್ಣಲ್ಲ", 
        "7478": "ಅರಿವನರಸಿ, ಅರಿವ ಕುರುಹಿನಲ್ಲಿ ಕಂಡು,", 
        "7479": "ಅರುವೆರಳುದ್ದದೇಹವ ಮಾಡಿದೆ ಕರುಣಿ ಬಸವಾ.", 
        "7480": "ಅರುಹನರಿಯಲು ಕುರುಹ ಮರೆಯಲೇಬೇಕು.", 
        "7481": "ಅಲ್ಲಮನ ವಂಶದವಳು ನಾನು.", 
        "7482": "ಅಲ್ಲಮನ ಸಂಗ, ಅಜಗಣ್ಣನ ಸಂಗ,", 
        "7483": "ಅಲ್ಲಲ್ಲಿಯ ಶರಣರು ಅಲ್ಲಲ್ಲಿಯೇ ನಿಲಲು,", 
        "7484": "ಅಲ್ಲಲ್ಲಿಯ ಸುಖಂ ಭೋ ಎನ್ನಲಿಲ್ಲ.", 
        "7485": "ಆಗಿಂಗೆ ಮುಯ್ಯಾನದಿರು, ಚೇಗಿಂಗೆ ಬೆಂಬೀಳದಿರು,", 
        "7486": "ಆಟವಳಿದು ನಿರಾಕುಳವಾಯಿತ್ತು;", 
        "7487": "ಆಡದ ನುಡಿಯ ನುಡಿದೆನನ್ನ ಮನ ತುಂಬಿ.", 
        "7488": "ಆಡದ ಭಾಷೆಯ ನುಡಿವಳಲ್ಲ ನಾನು ಬಸವಾ,", 
        "7489": "ಆಡದ ಮುನ್ನವಚ್ಚನೆ bsಂದವಾಯಿತ್ತೆನಗಯ್ಯ.", 
        "7490": "ಆಡಲಿಲ್ಲವಯ್ಯಾ ನಾನು ಹೆಣ್ಣುರೂಪ ಧರಿಸಿ", 
        "7491": "ಆಡಲಿಲ್ಲ ಹಾಡಲಿಲ್ಲ ನುಡಿಯಲಿಲ್ಲ ನಡೆಯಲಿಲ್ಲ", 
        "7492": "ಆದಿ ಅನಾದಿ ತತ್ವವ ಭೇದಿಸಿಕೊಟ್ಟ ಗುರುವೆ,", 
        "7493": "ಆದಿ ನಾದದ ಬಿಂದುವನರಿದು ಭಕ್ತಿಸಂಭಾಷಣೆಯ ಮಾಡಿದ ಬಸವಾ,", 
        "7494": "ಆದಿಯಾಧಾರವಿಲ್ಲದಂದು, ಕಳೆಮೊಳೆದೋರದಂದು,", 
        "7495": "ಆನಳಿದೆನು ನೀನಳಿದೆನೆಂಬ ಶಬ್ದವಡಗಿ ನಿಃಶಬ್ದವಾಗಿ,", 
        "7496": "ಆನು ನಿಷ್ಠೆಯುಳ್ಳವಳೆ ಅಲ್ಲವಯ್ಯ.", 
        "7497": "ಆನು ಭಕ್ತೆಯಲ್ಲ, ಆನು ವಿರಕ್ತೆಯಲ್ಲ,", 
        "7498": "ಆನೆತ್ತಲೀ ಸಂಸಾರವೆತ್ತ ! ಆನೆತ್ತಲೀ ಕಾಯವೆತ್ತ !", 
        "7499": "ಆರಸಂಗವೂ ಸ್ವಯವಲ್ಲವೆನಗೆ,", 
        "7500": "ಆರಾಧ್ಯರಿಲ್ಲದಂದು ಹುಟ್ಟಿದ ಗಂಡನೆನ್ನ ಗಂಡ;", 
        "7501": "ಆರೆಸಳೆಂಬರು ಮೂರೆಸಳೆಂಬರು;", 
        "7502": "ಆವಾವ ಕಾಲದಲ್ಲಿಯೂ ಎನಗೆ ಎಮ್ಮವರೆ ಗತಿಮತಿಗಳಯ್ಯ.", 
        "7503": "ಇಪ್ಪೆಯ ಹೂವನನುಗೊಳಿಸಲು ಇಪ್ಪೆ ಹಿಪ್ಪೆಯಾಯಿತ್ತು.", 
        "7504": "ಇಬ್ಬರು ನಾವು ಒಂದೆಡೆಯನುಂಡೆವು.", 
        "7505": "ಇರವರಿದು ಪರವ ಮರದೆ.", 
        "7506": "ಇರವಿಲ್ಲದ ವಸ್ತುವ ಕಂಡು ಪರವಶದ ಮೂರ್ತಿಯನರಿದೆನಯ್ಯ.", 
        "7507": "ಇಷ್ಟದ ಹಂಗಿಲ್ಲವೆನಗೆ ಶಿವಬಸವ ಶಿವಬಸವ.", 
        "7508": "ಇಷ್ಟವನು ಅಷ್ಟಮದಲ್ಲಿ ಕಂಡು,", 
        "7509": "ಊಟಕ್ಕಿಕ್ಕದವರ ಕಂಡು ಎನಗೆ ತೃಪ್ತಿಯಾಯಿತ್ತು.", 
        "7510": "ಎಂಟೆಸಳ ಹೂವೆಂಬರು; ಆ ಹೂವಿಂಗೆ ರೂಪಿಲ್ಲ,", 
        "7511": "ಎಡದೆರಹಿಲ್ಲದ ಪ್ರತಿರೂಪ ಕಂಡೆ.", 
        "7512": "ಎಡಬಲನ ಕಾಯವ ತಿಳಿದು, ಎಡಬಲನ ಅರಿವನರಿದು", 
        "7513": "ಎಡೆಯ ಮಾಡಿದ ಎಡೆ ಏಕವಾರಕ್ಕೆ ಮುನ್ನವೇ ತೀದಿರಿತ್ತು.", 
        "7514": "ಎಡೆಯಲ್ಲಿ ಹುಟ್ಟಿದ ಭಕ್ತಿ ಎಡೆಯಲ್ಲಿಯೆ ಅಡಗಿತ್ತು.", 
        "7515": "ಎಡೆಯಿಲ್ಲ ಕಡೆಯಿಲ್ಲ ಎನಗೆ ಎಲೆ ಅಯ್ಯ.", 
        "7516": "ಎಡೆಯಿಲ್ಲದ ಭಕ್ತಿಯ ಮಾಡಹೋದರೆ", 
        "7517": "ಎಡೆಯಿಲ್ಲದೂಟವನುಂಡು ತಡವಳಿದು", 
        "7518": "ಎತ್ತಳ ಸುಖ ಬಂದು ಎತ್ತಲಡಗಿತ್ತು", 
        "7519": "ಎತ್ತಿದ ಪ್ರಸಾದ ನಿತ್ಯದ ಮುಖವ ಕಂಡು", 
        "7520": "ಎದೆಬಿರಿವನ್ನಕ್ಕರ, ಮನದಣಿವನ್ನಕ್ಕರ,", 
        "7521": "ಎನಗಿನ್ನಾರು ಗತಿಯಿಲ್ಲವಯ್ಯ", 
        "7522": "ಎನ್ನಗಿನ್ನೇನು ಎನಗಿನ್ನೇನು ಎನಗಿನ್ನೇನು", 
        "7523": "ಎನಗಿನ್ನೇನು ಎಮ್ಮಯ್ಯನೈಕ್ಯವನೈದಿದ ಬಳಿಕ,", 
        "7524": "ಎನಗೆ ಇಲ್ಲಿ ಏನು ಬಸವ ಬಸವಾ ?", 
        "7525": "ಎನಗೆ ಈ ಪ್ರಾಣದ ಕುರುಹಿಲ್ಲವಯ್ಯ.", 
        "7526": "ಎನಗೆ ಎಲ್ಲಿಯೂ ಕಾಣಿಸದು ಇರಪರದ ಸಿದ್ಧಿಯು.", 
        "7527": "ಎನಗೆ ಲಿಂಗವು ನೀವೆ ಬಸವಯ್ಯಾ,", 
        "7528": "ಎನಗೆ ಸಂಸಾರ ಬಂಧ ಕಾರಣವೇನೆಂದು ಕೇಳಲು", 
        "7529": "ಎನಗೆ ಹಾಲೂಟವನಿಕ್ಕುವ ತಾಯೆ,", 
        "7530": "ಎನಗೆ ಹಿತಕಾರಿಗಳಾಗಿ ಇದ್ದವರೀ ಶರಣರು,", 
        "7531": "ಎನಗೆ ಹುಟ್ಟುವ ಮುನ್ನವೆ ಇಲ್ಲಿ", 
        "7532": "ಎನಗೇನೂ ತೋರದಂದು ನಮ್ಮವ್ವೆಯ ಮಗನಾಗಿದ್ದ ನಮ್ಮ ಬಸವಯ್ಯನು.", 
        "7533": "ಎನಗೇನೆಂಬೆನೆಂಬ ಸಂದೇಹ ಹರಿಯಿತ್ತು.", 
        "7534": "ಎನ್ನ ಕೈಯಳ ಮಾತುವೆನ್ನಕೈಯಲಡಗಿತ್ತು.", 
        "7535": "ಎನ್ನಯ್ಯನೆನ್ನಲ್ಲಿಯಡಗಿದನೆಂದು ನಾ ನಂಬಿರಲು,", 
        "7536": "ಎಪ್ಪತ್ತೈದು ಸಾವಿರದಲ್ಲಿ ಇಪ್ಪತ್ತೊಂದು ಪ್ರಾಣ", 
        "7537": "ಎಯ್ದದು ಎಯ್ದದು ಈ ಮನ ಬಸವನಲ್ಲಿ.", 
        "7538": "ಎರಡಿಲ್ಲದ ಅಂಗಕ್ಕೆ ಒಂದೆ ಕುರುಹಿಲ್ಲದ ಸ್ಥಲವಾಯಿತ್ತು.", 
        "7539": "ಎರಡಿಲ್ಲದ ವಸ್ತುವೆ ನೀನೆರಡಾದೆಯಲ್ಲ ಬಸವಯ್ಯ.", 
        "7540": "ಎಲ್ಲರ ಸಂಗವಲ್ಲಲ್ಲಿಯೆ;", 
        "7541": "ಎಲ್ಲವನರಿಯಬಹುದೆ ಎಂದು ನಾನು ತಿಳಿಯಲು", 
        "7542": "ಎಲ್ಲವನರಿಯೆನೆಂದರೆ ಎನಗೆ", 
        "7543": "ಎಲೆ ಅಯ್ಯಗಳಿರಾ, ಎಲೆಗಳೆದ ವೃಕ್ಷವ ಕಂಡಿರೆ ಬಸವನ ?", 
        "7544": "ಎಲೆ ಅಯ್ಯಾ ಎಲೆ ಅಯ್ಯಾ ಏಕಾಕ್ಷರ ರೂಪ ಬಸವಾ,", 
        "7545": "ಎಲೆ ಅಯ್ಯಾ ಬಸವಾ", 
        "7546": "ಎಲೆ ಅಯ್ಯಾ ಬಸವಾ, ಕರಸ್ಥಲ ಬಯಲಾಯಿತ್ತೆನಗೆ,", 
        "7547": "ಎಲೆಗಳೆದ ಮರದಲ್ಲಿ ನೆಳಲನರಸಲಿಲ್ಲ.", 
        "7548": "ಎಲೆ ಲಿಂಗವೆ,", 
        "7549": "ಎಲೆಯಿಲ್ಲದೆ ಮರ ಕಾಯಾಯಿತ್ತು,", 
        "7550": "ಎಲೆ ಶರಣರಿರಾ, ಎಲೆ ಭಕ್ತರಿರಾ,", 
        "7551": "ಎಸಳ ಕಂಡು ಹೂವಿನ ಮೂಲವ ನೋಡಲು", 
        "7552": "ಎಸಳ ಕರಣವ ಕಂಡು,", 
        "7553": "ಎಸಳಕ್ಷರವ ಕಂಡು ಎಸಳ ಬಗೆಯ ತಿಳಿದು", 
        "7554": "ಎಸಳದಳವನಳಿದು ನಿಂದ ಬಸವಾ,", 
        "7555": "ಎಸಳ ಪಂಜರದ ಪಕ್ಷಿಯೆ ನೀನೆಲ್ಲಿ ತೋರಿ ಎಲ್ಲಿಯಡಗಿದೆ ?", 
        "7556": "ಎಸಳು ಬಿಳಿದು ಆ ಎಸಳ ಕಂಪಿನ ವರ್ಣದ ಮುಂದೆ", 
        "7557": "ಎಸಳೆಸಳ ಮಾಡಿಸಲು", 
        "7558": "ಎಸಳೆಸಳಹೊಸದು ನೋಡುವ ಯೋಗಿಗಳು", 
        "7559": "ಎಸೆವ ಅನಂಗನ ಸಂಗವ ಹರಿದು,", 
        "7560": "ಎಸೆವಕ್ಷರಕ್ಕೆ ಹೆಸರಿಲ್ಲ, ಆ ಹೆಸರಿಂಗೆ ರೂಹಿಲ್ಲ;", 
        "7561": "ಎಸೆವಾಕ್ಷರದ ಕುರುಹ ಕಂಡು", 
        "7562": "ಏಕಮೂರ್ತಿ ತ್ರಿಮೂರ್ತಿ ದ್ವಿಮೂರ್ತಿಯಾಗಿ ತೋರಿ", 
        "7563": "ಏಕಯೇವ ದೇವನೊಬ್ಬನೆ ಶರಣ ಬಸವಯ್ಯ.", 
        "7564": "ಏಕಯೇವದೇವ ಬಸವಾ,", 
        "7565": "ಏಕಲಿಂಗದಲ್ಲಿ ಏಕತ್ವವಲ್ಲ ಮೂರ್ತಿಗಳಲ್ಲ", 
        "7566": "ಏಕಲಿಂಗನಿಷ್ಠಾ ಪಾರಿಗಳೆಂದೆಂಬರಯ್ಯ;", 
        "7567": "ಏಕ ಸಂಗ ನಿಸ್ಸಂಗವಾಗಿ ಪ್ರಸಾದದ ಹಂಗಿಗಳಲ್ಲ.", 
        "7568": "ಏಕಾಕಾರ ನಿರಾಕಾರವಾಯಿತ್ತಯ್ಯ,", 
        "7569": "ಏಕಾಂಗವೆನಗೆ ಅನೇಕ ಬಸವಾ,", 
        "7570": "ಏಕೆನ್ನ ಪುಟ್ಟಿಸಿದೆಯಯ್ಯಾ ಹೆಣ್ಣು ಜನ್ಮದಲ್ಲಿ", 
        "7571": "ಏಣನಗರ, ಎಸಳಗಂಗಳ ಸಾರಂಗ, ಪ್ರಭೆಯನೊಳಕೊಂಡ ಮೊಲ,", 
        "7572": "ಏತರಮಾರ್ಗವಡಗದ ಸಂಗ,", 
        "7573": "ಏತರಲ್ಲಿಯೂ ತೆರಹಿಲ್ಲವೆನಗೆ", 
        "7574": "ಏತರಲ್ಲಿಯೂ ತೆರಹಿಲ್ಲವೆನಗೆ", 
        "7575": "ಏತರಲ್ಲಿಯೂ ಪರಿಣಾಮವಿಲ್ಲವೇತರಲ್ಲಿಯೂ ಗಮನವಿಲ್ಲ", 
        "7576": "ಏತರಲ್ಲಿಯೂ ಹೆಸರಿಲ್ಲದ ಕುರುಹು ಈ ವಸ್ತು ಬಸವಯ್ಯನು.", 
        "7577": "ಏನೂ ಏನೂ ಎನಲಿಲ್ಲ", 
        "7578": "ಏನೆಂದೆನ್ನಬಹುದಯ್ಯ ?ಎಂತೆಂದೆನ್ನಬಹುದಯ್ಯ ?", 
        "7579": "ಏಹೆ ಎಲೆ ಅಭವ ಬಸವಾ,", 
        "7580": "ಐಕ್ಯವ ತೋರಿ ಅಜಾತನಲ್ಲಡಗಿದ ಬಸವಾ.", 
        "7581": "ಐದದು ಎನಗೆ ಕಡೆಮುಟ್ಟದ ಪ್ರಸಾದ", 
        "7582": "ಒಡಲಗುಣವ ಕಳದ ಬಳಿಕ ಹೆಣ್ಣಿಂಗೆ,", 
        "7583": "ಒಡಲಳಿದ ಕಾರಣ ಒಡಲಿಲ್ಲದ ಹೆಣ್ಣು ನಾನಾದೆನಯ್ಯಾ ಬಸವಾ,", 
        "7584": "ಕರಣಂಗಳ ಹಂಗಹರಿದು, ಕರಣಂಗಳ ಮುಖವನಳಿದು,", 
        "7585": "ಕರಣಂಗಳ ಹಂಗ ಹರಿದು", 
        "7586": "ಕಲ್ಲಮಾಲೆಯ ಕಡಿದಾತ ಬಸವಯ್ಯನು.", 
        "7587": "ಕಲ್ಯಾಣವಿಲ್ಲ ಕೈಲಾಸವಿಲ್ಲ, ಬಸವಾ.", 
        "7588": "ಕಲಿಯುಗದಲ್ಲಿ ಹುಟ್ಟಿ ಆ ಕಲಿಯುಗದಲ್ಲಿ ಬೆಳೆದೆನಯ್ಯ.", 
        "7589": "ಕಾಮದ ಹಂಗಿಗನಲ್ಲ ಶರಣ", 
        "7590": "ಕಾಮವನಳಿದ ಹೆಣ್ಣಲ್ಲ ನಾನು, ಕಾಮ ಉಂಟೆನಗೆ.", 
        "7591": "ಕಾಮಿತಸುಖವ ಕಂಗೊಳಿಸಿದ ಗುರುವೆ,", 
        "7592": "ಕಾಮಿಯಾನಾಗಿ ಕಾಮದ ಹಂಗಹರಿದೆನು ಬಸವಾ.", 
        "7593": "ಕಾಯದ ಹಂಗ ಹರಿದು, ಕಲ್ಪಿತದ ಗುಣವ ನಷ್ಟವಮಾಡಿ,", 
        "7594": "ಕಾಯವಿಲ್ಲದೆ ಕಾಯಕ್ಕೆ ಕಲ್ಪಿತದ ಸಯದಾನವ ಕೂಡಲಿಕ್ಕಲು", 
        "7595": "ಕಾಯವಿಲ್ಲದೆ ಪ್ರಾಣವಿರದು, ಪ್ರಾಣವಿಲ್ಲದೆ ಕಾಯವಿರದು.", 
        "7596": "ಕಾಯವಿಲ್ಲದೆ ಪುಷ್ಪಕವ ಕಂಡ ನಮ್ಮ ಬಸವಯ್ಯನು.", 
        "7597": "ಕಾಲವ ಕಂಡ ಬಸವಾ, ಕಲ್ಪಿತವ ಕಂಡ ಬಸವಾ,", 
        "7598": "ಕಾವಲಕಾದಿದ್ದವರು ಕಾವಲಮೀರಿ", 
        "7599": "ಕುಲವಳಿದ ಹೆಣ್ಣ ಕಣ್ಣ ಬಯಲ ಕಂಡು", 
        "7600": "ಕೋಪದ ತಾಪದ ಸಂಗವ ಕಳೆದು", 
        "7601": "ಕ್ರಮವನರಿಯದೆ ಪೂಜೆಯ ಮಾಡಹೋದರೆ,", 
        "7602": "ಗಮನದ ಸುಖವಡಗಿ ನಿರ್ಗಮನವಾಯಿತ್ತು ಬಸವಾ.", 
        "7603": "ಜನನ ಮರಣವಳಿದು, ಜಲ್ಮದೊಪ್ಪವ ಕಳೆದು,", 
        "7604": "ಜಯ ಸುಖ ವಿಸುಖವಿಲ್ಲ.", 
        "7605": "ಜ್ಞಾನವಿಲ್ಲದ ಕ್ರೀಯ ಮಾಡಿದಲ್ಲಿ ಫಲವೇನಯ್ಯ ?", 
        "7606": "ಠಾವಿಲ್ಲ, ಆ ಠಾವಿಂಗೆ ಆ ಠಾವೆ ಮೂಲವಾಯಿತ್ತು.", 
        "7607": "ತತ್ವದ ಮನ ತಾಯಿಗಳ ಬಿಡಾರವೆಂದೆನಿಸುವುದು ಬಸವಾ.", 
        "7608": "ತತ್ವದ ಹಂಗೇನೋ ಶರಣ ಬಸವಂಗೆ ?", 
        "7609": "ತನುವಾವುದಯ್ಯಾ ಬಸವಾ ?", 
        "7610": "ತನುವಿಲ್ಲ ಬಸವಯ್ಯಂಗೆ, ಮನವಿಲ್ಲ ಬಸವಯ್ಯಂಗೆ,", 
        "7611": "ತಾಯಿಲ್ಲವೆನಗೆ ಬಸವ ಬಸವಾ.", 
        "7612": "ತಿಳುವಿನ ಸಂಗವಿನ್ನೇತಕಯ್ಯ ಎನಗೆ.", 
        "7613": "ತ್ರಿವಿಧದ ಮಾಟವಿಲ್ಲವೆನಗೆ ಬಸವಾ.", 
        "7614": "ತ್ರಿವಿಧ ಪ್ರಸಾದವಿಲ್ಲ, ತ್ರಿವಿಧಾಕಾರವಿಲ್ಲ,", 
        "7615": "ದಾಯದಲ್ಲಿ ಹುಟ್ಟಿದ ಧ್ವನಿ ಆ ದಾಯದಲ್ಲಿ ಹೋಯಿತ್ತು.", 
        "7616": "ದೃಢವಿಡಿದ ಭಕ್ತಿಗೆ ಸಮಯಾಚಾರವೇತಕ್ಕಯ್ಯಾ ?", 
        "7617": "ದ್ವಯಲಿಂಗವೆಂಬರುದ", 
        "7618": "ಧ್ವನಿಯ ತೋರಲು, ಆ ಧ್ವನಿಯ ಮರೆಯಲ್ಲಿ ಹುಟ್ಟಿದ", 
        "7619": "ನಡವ ಕಾಲಿಂಗೆ ಶಕ್ತಿ ನಿಃಶಕ್ತಿಯಾಯಿತ್ತು.", 
        "7620": "ನಡೆನೋಟವಿಲ್ಲವೆ ತೃಪ್ತಿಯ ಕೂಡಲು ?", 
        "7621": "ನಡೆಯಲಿಲ್ಲ ನುಡಿಯಲಿಲ್ಲ", 
        "7622": "ನನಗೊಂದು ತಾಣವಾಗಿಯದೆ ನಾನತ್ತಲಡಗಲೇಬೇಕು.", 
        "7623": "ನನ್ನನಾರೂವರಿಯರು,", 
        "7624": "ನಮಗಾರ ಸಂಗವಿಲ್ಲ, ನಮಗಾರ ಸಂಗವಿಲ್ಲ", 
        "7625": "ನಮ್ಮ ಹಂಗಿಗನಲ್ಲ ಬಸವಯ್ಯನು.", 
        "7626": "ನವಕಲ್ಪಿತದ ರೂಪನರಿದು,", 
        "7627": "ನಾಡನಾಳಹೋದರೆ,", 
        "7628": "ನಾಡಿನ ಹೆಣ್ಣುಗಳೆಲ್ಲಾ ಬನ್ನಿರೆ ಅಕ್ಕಗಳಿರಾ,", 
        "7629": "ನಾನಾರ ಸಾರುವೆನೆಂದು ಚಿಂತಿಸಲೇತಕ್ಕಯ್ಯಾ ಬಸವಾ ?", 
        "7630": "ನಾನಾರ ಹೆಸರ ಕುರುಹಿಡಲಯ್ಯಾ ಬಸವಾ ?", 
        "7631": "ನಾನಾವ ಗಮನವ ಕಂಡೆನಯ್ಯ ?", 
        "7632": "ನಾನು ನಿಮ್ಮವಳಲ್ಲವಯ್ಯಾ, ನಾನು ಅನಿಮಿಷನವರವಳು.", 
        "7633": "ನಾವು ನಮ್ಮ ವಶವಲ್ಲದವರ ಸಂಗವ ಮಾಡಲಿಲ್ಲ", 
        "7634": "ನಿರ್ಮೂಲವಾಯಿತ್ತಾಹಾ ನಿರಾಲಂಬವಾಯಿತ್ತಾಹಾ !", 
        "7635": "ನಿರೂಪ ರೂಪಿನಲ್ಲಿ ಅಡಗಿ,", 
        "7636": "ನಿರೋಧವಳಿದು ನಿರಾಕಾರವಾಯಿತ್ತು ಬಸವಾ,", 
        "7637": "ನಿಷ್ಠೆಯೆಂಬುದನೊಂದ ತೋರಿ", 
        "7638": "ನೀರುಂಡ ಸಾರ ನಿಸ್ಸಾರವಾಯಿತ್ತಯ್ಯ.", 
        "7639": "ನೆನಹು ನಷ್ಟವಾಯಿತ್ತು ಬಸವಾ, ಲೀಯವಾಗಲು.", 
        "7640": "ನೆನೆಯಲಾಗದು ಎನ್ನ ಹೆಣ್ಣೆಂದು ನೀವು ಭಕ್ತರು.", 
        "7641": "ನೆಲೆಯಿಲ್ಲದ ಜಲವ ಹೊಕ್ಕಡೆ", 
        "7642": "ನೋಡುವಡೆ ಎನ್ನ ಕಣ್ಣಿಂಗೆ ಗೋಚರವಲ್ಲ", 
        "7643": "ಪಯಣವಿಲ್ಲದೆ ಗಮನವ ಕಂಡವರುಂಟೆ ?", 
        "7644": "ಪರಮನ ಹಂಗು, ಪ್ರಾಣದ ಸಂಗ ಉಂಟೆಂದೆನಲಿಲ್ಲ ಬಸವಾ.", 
        "7645": "ಪಾರ್ವತಿಯ ರೂಪಕಂಡು ಪರಶಿವನಸಂಗ ನಿಸ್ಸಂಗವಾಗಿ,", 
        "7646": "ಪ್ರಣವದ ಹೆಸರಿಲ್ಲ ಬಸವಂಗೆ.", 
        "7647": "ಪ್ರಣವಾಕ್ಷರವ ಕಂಡು ಪ್ರಣವರೂಪನರಿದೆನಯ್ಯ", 
        "7648": "ಪ್ರಸಾದಿಗಳು ಪ್ರಸಾದಿಗಳೆಂದೆಂಬರಯ್ಯ,", 
        "7649": "ಪ್ರಾಣದ ಹಂಗೆಮಗಿಲ್ಲ,", 
        "7650": "ಪ್ರಾಣಯೋಗವ ಕಂಡು ಮನಯೋಗ ಬಳಲಿತ್ತು.", 
        "7651": "ಪ್ರಾಣವಿಲ್ಲದ ಹೆಣ್ಣು ನಾನಾಗಿರಲು,", 
        "7652": "ಪ್ರಾಣವಿಲ್ಲ ಪ್ರಸಾದವಿಲ್ಲ", 
        "7653": "ಬಣ್ಣದ ಪುತ್ಥಳಿಯ ಮಾಡಿ ಸಲಹಿದರೆನ್ನ ನಮ್ಮಯ್ಯನವರು.", 
        "7654": "ಬಸವಣ್ಣನೆ ಗುರುವೆಂದು ಭಾವಿಸಲಾಗಿ,", 
        "7655": "ಬಸವನರಿವು ನಿರಾಧಾರವಾಯಿತ್ತು.", 
        "7656": "ಬಸವನ ಹೆಸರಳಿಯಿತ್ತು, ಬಸವನ ಕುರುಹಳಿಯಿತ್ತು,", 
        "7657": "ಬಸವಯ್ಯಾ ಬಸವಯ್ಯಾ ಹುಯ್ಯಲಿಲ್ಲದ ಹುಲ್ಲೆಯಾದೆಯಾ ?", 
        "7658": "ಬಸವಾ ಬಸವಾ ಎಂಬ ಶಬ್ದವಡಗಿತ್ತು.", 
        "7659": "ಬಸವಾ, ಹಂಗನಳಿದೆ ನಾ ನಿಮ್ಮ ಬಸವಾ.", 
        "7660": "ಬಹಿರಂಗದಾರೋಗಣೆಯ ರುಚಿಯನರಿಯಬಾರದು", 
        "7661": "ಬುದ್ಧಿಯನಳಿದು ನಿರ್ಬುದ್ಧಿವಂತಳಸಂಗದಿಂದ", 
        "7662": "ಬೆಳಗಿನಪ್ರಭೆ ಥಳಥಳಿಸಿ ಹೊಳೆಯಲು,", 
        "7663": "ಬ್ರಹ್ಮದ ನೆಮ್ಮುಗೆಯನಳಿದೆ", 
        "7664": "ಬ್ರಹ್ಮದ ಮುಂದೆ ಒಂದು ಬ್ರಹ್ಮದ ಕುರುಹ ಕಂಡೆ", 
        "7665": "ಬ್ರಹ್ಮವ ಕೂಡಲು ಆ ಬ್ರಹ್ಮವನರಿದು", 
        "7666": "ಭಕ್ತಿಪ್ರಸಾದ, ಮುಕ್ತಿಪ್ರಸಾದ,", 
        "7667": "ಭಕ್ತಿಯಿಲ್ಲದ ಕಿಂಕುರ್ವಾಣವ ಮಾಡಹೋದರೆ", 
        "7668": "ಭೃತ್ಯಾಚಾರವಳಿಯ ಭೃತ್ಯವನನುಭವಿಸಿದೆನು.", 
        "7669": "ಮಂಗಳಸೂತ್ರವ ಕಟ್ಟಲು", 
        "7670": "ಮಂಡೆಯಿಲ್ಲದೆ ಪುಷ್ಪವ ಮುಡಿಯಲು", 
        "7671": "ಮಂತ್ರಾಕ್ಷತೆಯನರಿದು ಮಂತ್ರಸರವ ಪರಿಗೊಳಿಸಲು", 
        "7672": "ಮಡದಿ ಎಂಬ ಶಬ್ದ ನಿಶ್ಶಬ್ದವಾದಡೆ ನಾನೀಗ ನಿಜಸುಖಿ ಬಸವಾ.", 
        "7673": "ಮಡದಿ ಎನಲಾಗದು ಬಸವಂಗೆ ಎನ್ನನು.", 
        "7674": "ಮಧ್ಯಕಲ್ಪ ನಾಸ್ತಿಯಾಯಿತ್ತೆ ಬಸವಾ ?", 
        "7675": "ಮನದ ಮಧ್ಯದಮೂರ್ತಿಯನರಿದು", 
        "7676": "ಮನದ ಹಂದೆ ಏತಕ್ಕೆ ? ನೀ ದಿsೀರನೆಂಬೆ.", 
        "7677": "ಮನವಿಲ್ಲದ ಮಾತನಾಡಹೋದರೆ", 
        "7678": "ಮನವಿಲ್ಲದೆ ತನುವ ಕುಡಹೋದರೆ", 
        "7679": "ಮನವೊಂದು ರೂಪಾಗಿ ಧನವೊಂದು ರೂಪಾಗಿ", 
        "7680": "ಮಲ ಮೂತ್ರ ವಿಸರ್ಜನೆಯಿಲ್ಲದೆ", 
        "7681": "ಮನವಿಲ್ಲ ಬಸವಯ್ಯಂಗೆ, ತನುವಿಲ್ಲ ಬಸವಯ್ಯಂಗೆ,", 
        "7682": "ಮಾಟಕೂಟ ಸಮಯಾಚಾರ ಸದ್ಭಕ್ತಿಯ ನೆಲೆಯ", 
        "7683": "ಮಾಟವಿಲ್ಲದ ಸಮಯಾಚಾರವ ಮಾಡಹೋದೆ ಬಸವಾ.", 
        "7684": "ಮಾತನಳಿದು ಮನವಳಿದು", 
        "7685": "ಮಾತಿನ ಹಂಗಿಲ್ಲದವಳಾದೆ ನಾನು.", 
        "7686": "ಮಾತಿನ ಹಂಗೇತಕ್ಕೆ, ಮನವೇಕಾಂತದಲ್ಲಿ ನಿಂದ ಬಳಿಕ, ಬಸವಯ್ಯಾ ?", 
        "7687": "ಮಾತಿಲ್ಲದವನ ಕೂಡೆ ಮಾತನಾಡಹೋದಡೆ,", 
        "7688": "ಮಾತಿಲ್ಲದ ಮಥನವ ಮಾಡಿ ಮೆರದೆ ಬಸವಾ.", 
        "7689": "ಮಾತಿಲ್ಲವೆನಗೆz ಆ ಮಾತು ನುಡಿಯಲಿಲ್ಲವೆನಗೆ.", 
        "7690": "ಮಾಯದ ಮನದ ಕರ್ಮದ ಹಂಗಹರಿದು", 
        "7691": "ಮಾಹೇಶ್ವರರ ಸಂಗವಳಿದು ಮಹಾಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "7692": "ಮುಕ್ತಿಯನಳಿದು ನಿರ್ಮುಕ್ತಳಾದ ಕಾರಣ", 
        "7693": "ಮುಖದಂತರದ ಬಾಗಿಲ ಮುಂದೆ", 
        "7694": "ಮುಖವಿಲ್ಲದ ಕನ್ನಡಿಯ ನೋಡಲು,", 
        "7695": "ಮುಗಿಸಿದೆ ಮುಗಿಸಿದೆ ಮನದಲ್ಲಿ ನಾನು ಬಸವಾ.", 
        "7696": "ಮುನ್ನಲೊಂದು ಕಾಯವಿಡಿದು ನಾನು ಹುಟ್ಟಿ", 
        "7697": "ಮುನ್ನಲೊಂದು ಶಿಶು ಹುಟ್ಟಿತ್ತು.", 
        "7698": "ಮುನ್ನವೆ ಮುನ್ನವೆ ಹುಟ್ಟಿದೆ ನಾನು.", 
        "7699": "ಮುನ್ನಳ ದೋಷವೆನ್ನ ಬೆನ್ನಬಿಡದಯ್ಯ,", 
        "7700": "ಮುಯ್ಯೂರ ಮನೆಯೊಳಗೆ ನಾನು ಸಂಸಾರವ ಮಾಡುತ್ತಿರಲು,", 
        "7701": "ಮೂನೂಲನಳಿದೆ ಮುಖವ ಕಳದೆ", 
        "7702": "ಮೂರ್ತಿಯ ಸಂಗ ಮೂರಡಿಗೊಂಡಿತ್ತು.", 
        "7703": "ಮೂರ್ತಿಯನರಿದು ಮುಖ ವಿಕಸಿತವಾಯಿತ್ತಯ್ಯಾ.", 
        "7704": "ಮೂಲ ಪ್ರಣವ", 
        "7705": "ಮೂಲಾಧಾರದ ಬಾಗಿಲ ಕಾಯಲು", 
        "7706": "ಮೂಲಾಧಾರದ ಮಂಟಪದ ಮನೆಯಮೇಲೆ", 
        "7707": "ಮೂವರಳಿದು, ಮೂರ್ತಿಯ ಕಳದು,", 
        "7708": "ಮೌಕ್ತಿಕದ ಮಂಟಪ ಕಟ್ಟಿ", 
        "7709": "ಮೃತವಳಿದು ಕಾಯವುಳ್ಳವಳಾದೆ.", 
        "7710": "ಯತಿಯರ ಮನವನತಿ ವಿರತಾಕಾರವ ಮಾಡಿ ಕಂಡೆನಯ್ಯ.", 
        "7711": "ಯುಗವಿಲ್ಲ ಜುಗವಿಲ್ಲ ಕಾಲವಿಲ್ಲ ಕಲ್ಪಿತವಿಲ್ಲ", 
        "7712": "ರೂಪಿಲ್ಲದ ವಸ್ತುವೆ ರೂಪಾದೆಯಾ ಬಸವಯ್ಯಾ ?", 
        "7713": "ಲಕ್ಷದ ಮೇಲೆ ತೊಂಬತ್ತಾರುಸಾವಿರ ಜಂಗಮಕ್ಕೆ", 
        "7714": "ಲಿಂಗದ ಹಂಗಿಗಳಲ್ಲ ನಾನು ಬಸವಾ.", 
        "7715": "ಲಿಂಗವಿದ್ದರೇನು ?", 
        "7716": "ಲಿಂಗ ಸಂಗದಲ್ಲಿ ಹುಟ್ಟಿ", 
        "7717": "ಶಿರ ಭಾಳ ಕರ್ಣಂಗಳ ಗುಣವಿಲ್ಲ.", 
        "7718": "ಶಿವತತ್ವವ ಕಾಣದ ಮುನ್ನ", 
        "7719": "ಸಂಗನಕೂಟಕ್ಕೆ ತೆರಹಿಲ್ಲ.", 
        "7720": "ಸಂಗ ನಿಸ್ಸಂಗವಾಯಿತ್ತೆನಗೆ", 
        "7721": "ಸಂಗಯ್ಯ ಸಂಗಯ್ಯ ನಿರಾಕಾರವೇನಯ್ಯ ?", 
        "7722": "ಸಂಗವಪ್ಪ ಬಸವಾ,", 
        "7723": "ಸಮಯಾಚಾರವಡಗಿದ ಬಸವಾ,", 
        "7724": "ಸರ್ವಾಂಗಶುದ್ಭವಾಗಿ ಲಿಂಗದೇಹಿಯಾನಾದೆನು.", 
        "7725": "ಸಯದಾನವರತ ಬಸವಾ.", 
        "7726": "ಸುವೀರವಾದಡಾಗಲಿ ಸಮಯಾಚಾರವಾದಡಾಗಲಿ ಸಂಗಪ್ರಸಾದ", 
        "7727": "ಸ್ವಯಸಮರಸದ ಇರವನಂಗವಿಸಿ", 
        "7728": "ಸ್ಥಾನಮಾನವಿಲ್ಲದೆ ಶರಣರ ಹಂಗ ಹರಿದೆ.", 
        "7729": "ಹಂದೆಯಲ್ಲ ನಾನು,", 
        "7730": "ಹಿರಿಯತನಕ್ಕೆ ಹೆಣ್ಣೆಂದು ಕರೆದರೆ", 
        "7731": "ಹುಟ್ಟಿಲ್ಲದ ಭೂಮಿಯಲ್ಲಿ ಹುಟ್ಟಿದೆ ನಾನು.", 
        "7732": "ಹುಟ್ಟುಗೆಟ್ಟೆ ನಾನು, ತೊಟ್ಟ ಬಿಟ್ಟ ನಾನು,", 
        "7733": "ಹೆಚ್ಚನರಿದು ಪ್ರಣವವ ಕೂಡಲು", 
        "7734": "ಹೆಸರಿಲ್ಲದ ರೂಪ ಕಂಡು", 
        "7735": "ಹೃದಯಮಧ್ಯದಲೊಂದು ಜ್ಯೋತಿಯಮನೆ ಹುಟ್ಟಿತ್ತು.", 
        "7736": "ಹೆಸರಳಿಯಿತ್ತು ಬಸವಾ, ಇಂದಿಂಗೆ ಭಕ್ತಿಯಿಲ್ಲದ ಕಾರಣ.", 
        "7737": "ಹೊಲಬರಿಯೆ ನಾನು,"
    }
}