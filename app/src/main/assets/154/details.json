    "id": "154", 
    "name": "ಶಿವಲೆಂಕ ಮಂಚಣ್ಣ", 
    "vachana": {
        "13284": "ಏಕಮಯ ವಸ್ತು ತನ್ಮಯ ರೂಪಾದಲ್ಲಿ ,", 
        "13285": "ಕಾರುಕನ ಕೈ  ಮುಟ್ಟುವುದಕ್ಕೆ ಮುನ್ನವೆ,", 
        "13286": "ಪೃಥ್ವಿ ಸಾಕಾರವ ಮಾಡಿದಲ್ಲಿ, ಅಪ್ಪು ಆಸ್ತಿಕವಾದಲ್ಲಿ,", 
        "13287": "ಗುರು ಮೂರು ಕೂಡಿ ಗುರುವಾದಲ್ಲಿ,", 
        "13288": "ಇಚ್ಛಾಶಕ್ತಿ ಮರ್ಕಟರೂಪಾದಲ್ಲಿ, ಕ್ರಿಯಾಶಕ್ತಿ ವಿಹಂಗರೂಪಾದಲ್ಲಿ,", 
        "13289": "ನಡೆದು ನಡೆಯದವನ ಕಂಡು, ನುಡಿದು ನುಡಿಯದವನ ಕಂಡು,", 
        "13290": "ಕಂಡು ತವಕ ಸಂಗದಿಂದಳಿವಂತೆ,", 
        "13291": "ಗುರುಲಿಂಗ ಬ್ರಹ್ಮರೂಪವಾಯಿತ್ತು. ಚರಲಿಂಗ ವಿಷ್ಣುರೂಪಾಯಿತ್ತು.", 
        "13292": "ಪೂಜಿಸಿ ಕಾಬುದು ಗುರುವಿನ ಭೇದ,  ಧ್ಯಾನಿಸಿ ಕಾಬುದು ಲಿಂಗದ ಭೇದ,", 
        "13293": "ಸ್ತ್ರೀಲಿಂಗ ನಾಸ್ತಿಯಾಗಿರಬೇಕು ಗುರುವಿನಿರವು.", 
        "13294": "ಅಕಾರತತ್ವ ಆಕಾಶದೊಡಲಾಯಿತ್ತು.", 
        "13295": "ಮರೀಚಿಕಾಜಲದಂತೆ ತೋರುತ್ತಿಹ ವಸ್ತು", 
        "13296": "ವೇದ ಅಪರವನರಸಿತ್ತು, ಪುರಾಣ ಪುಣ್ಯವ ಬಯಸಿತ್ತು.", 
        "13297": "ಐವತ್ತೊಂದು ಶಾಖೆಯಲ್ಲಿ, ಮೂವತ್ತೊಂದು ಬಿಂದುವಿನಲ್ಲಿ,", 
        "13298": "ಪೃಥ್ವಿ ರೂಪಾದಲ್ಲಿ ಸದ್ಯೋಜಾತನಾದ,", 
        "13299": "ವಟವೃಕ್ಷದ ಘಟದ ಮಧ್ಯದಲ್ಲಿ ಒಂದು ಮಠವಿಪ್ಪುದು.", 
        "13300": "ಧ್ಯಾನ ಧಾರಣ ಸಮಾಧಿ ಯೋಗಂಗಳಿಂದ ಕಾಬುದು ತನುಪ್ರಾಪ್ತಿ ಐಸೆ ?", 
        "13301": "ಆದಿವಸ್ತು, ಅನಾದಿವಸ್ತುವೆಂದು ಭೇದವ ಮಾಡಿದಲ್ಲಿ,", 
        "13302": "ಅಂಗಕ್ಕೆ ಲಿಂಗವ ಕೊಟ್ಟವ ಆಚಾರ್ಯನಾದ.", 
        "13303": "ಕಾರುಕ ಮುಟ್ಟಿ ಗುರುವಿನ ಕೈಗೆ ಬಂದುದಲ್ಲ.", 
        "13304": "ಆದಿಶಕ್ತಿ ರೂಪಾಗಿ, ಅನಾದಿಶಕ್ತಿ ಪ್ರಾಣವಾಗಿ,", 
        "13305": "ರಸವನರಿವುದಕ್ಕೆ ಜಿಹ್ವೆಯಾಗಿ ಬಂದು,", 
        "13306": "ವಿಶ್ವಮಯ ರೂಪು ನೀನಾಗಿ, ಅರಿವ ಆತ್ಮ ಒಬ್ಬನಲ್ಲಿಯೇ ಅಡಗಿದೆಯಲ್ಲಾ!", 
        "13307": "ಈಶ್ವರರೂಪ ತಾಳಿ ಜಗ ಜೀವಾಳರುಗಳ ಬಾಗಿಲಲ್ಲಿ", 
        "13308": "ಬಹುರೂಪಕ್ಕೆ ಭಾಷಾಂಗವಿಲ್ಲದಿರೆ ಮೆಚ್ಚದು ಜಗ.", 
        "13309": "ಅಂಗದೇಹಿ ಲಿಂಗೋದಕವ ಕೊಳಲಾಗದು.", 
        "13310": "ಜಂಗಮದ ಪಾದತೀರ್ಥವ ಲಿಂಗಕ್ಕೆ ಮಜ್ಜನವ ಮಾಡಲಾಗಿ,", 
        "13311": "ಗುರುಪ್ರಸಾದವ ಲಿಂಗಕ್ಕೆ ಕೊಡಲಿಲ್ಲ,", 
        "13312": "ನಾದವಿಲ್ಲದೆ ಬಿಂದುವಿಲ್ಲ, ಬಿಂದುವಿಲ್ಲೆದ ಕಳೆಯಿಲ್ಲ,", 
        "13313": "ಉತ್ತರಕ್ಷೆಯನರಿತು, ಪೂರ್ವಕಕ್ಷೆಯನರಿತು,", 
        "13314": "ಮೇಲು ಹೇಮದಲ್ಲಿ ಕೀಳುಕೊಡೆ,", 
        "13315": "ತನುವಿಗೆ  ರುಜೆಯಡಸಿದಲ್ಲಿ ಆತ್ಮಕ್ಕೆ  ಅವಗಡೆ ಬಂದಿತ್ತು.", 
        "13316": "ತುಷವಿದ್ದಲ್ಲಿ ಭತ್ತವಾಯಿತ್ತು, ತುಷ ಹೆರಹಿಂಗೆ ತಂಡುಲವಾಯಿತ್ತು,", 
        "13317": "ಗುರುವಿರೆ ಲಿಂಗಪ್ರಸಾದವ ಕೊಳಲಾಗದು.", 
        "13318": "ಅಪ್ಪು ಮೂಲಾಧಾರವಾಗಿ ಬೀಜದ ಮೇಲೆ ಬೀಳೆ,", 
        "13319": "ಮಡಿಕೆಗೆ ಮುಸುಕು, ತಮಗೆ ಗುಹ್ಯಕ್ಕೆ ದಶಾವಸ್ಥೆ.", 
        "13320": "ಹೊರಗೆ ಚಿಲಮೆ,  ಒಳಗೆ ಮೂತ್ರದ ಕುಳಿ.", 
        "13321": "ಪರಧನವನೊಲ್ಲದಿಪ್ಪುದೆ ವ್ರತ, ಪರಸ್ತ್ರೀಯರ ಕೂಡದಿಪ್ಪುದೆ ಶೀಲ.", 
        "13322": "ನಿತ್ಯ ಚಿಲಮೆಯ ನೋಡುವಾತಂಗೆ ಉಚ್ಚೆಯ ಬಚ್ಚಲ ತೋಡಲಾಗದು.", 
        "13323": "ಅಪ್ಪು ಲವಣವೆಲ್ಲವು ಸರಿ, ಪರಿಪಾಪಕವೆಲ್ಲವು ಸರಿ.", 
        "13324": "ಮನ ಅರೋಚಕವಾದಲ್ಲಿ,", 
        "13325": "ಜಲ ಕೆಯ್ಯಿ ಭಾಜನ ವಾಸ ಇವು ಮುಂತಾಗಿ", 
        "13326": "ಅರಿವನರಿತೆನೆಂಬಲ್ಲಿ ಮರೆದುದೇನು ಅರಿವೋ ಮರವೆಯೋ ?", 
        "13327": "ತರ್ವಾಯಕ್ಕೆ ಸಿಕ್ಕಿದಲ್ಲಿ ವೇದಾಂತಿಯಾದ.", 
        "13328": "ವಾಸದ ದೀಪಕ್ಕೆ ವಾಯು ವಿರೋಧವಲ್ಲದೆ", 
        "13329": "ಸಕಲ ಬಹುಕೃತವೆಂಬ ಗಹನದಲ್ಲಿ,", 
        "13330": "ಜಗಕ್ಕೆ ಹೊರಗಾಗಿ ಅರಿವ ಠಾವಿಲ್ಲ, ಜಗಕ್ಕೆ ಒಳಗಾಗಿ ಮರೆವ ಠಾವಿಲ್ಲ ,", 
        "13331": "ಜಲದೊಳಗಣ ಮತ್ಸ್ಯ, ವನದೊಳಗಣ ಮರ್ಕಟ, ಆಕಾಶದ ಪಕ್ಷಿ,", 
        "13332": "ಪಿಪೀಲಿಕ ಮಧುರವ ಕಾಂಬಂತೆ, ಮರ್ಕಟ ಲಂಘನವ ಕಾಂಬಂತೆ,", 
        "13333": "ಕಾಯದಲ್ಲಿ ಪರಿಪೂರ್ಣನಾಗಿ ಜೀವನಿಪ್ಪ ಭೇದವ,", 
        "13334": "ಕಾಯವ ಮರೆದು ಜೀವವನರಿಯಬೇಕು.", 
        "13335": "ಕಾಯವಿದಲ್ಲದೆ ಜೀವಕ್ಕೆ ಬೆಲೆಯಿಲ್ಲ, ಜೀವವಿದ್ದಲ್ಲದೆ e್ಞನಕ್ಕೆ ಕುರುಹಿಲ್ಲ.", 
        "13336": "ಬಂಗಾರದ ರೂಪಿದ್ದಲ್ಲದೆ ಬಣ್ಣ ವನವಗವಿಸದು.", 
        "13337": "ಕ್ರೀಯ ಮರೆದಲ್ಲಿ, ಅರಿವು ಹೀನವಾಗಿಪ್ಪುದು.", 
        "13338": "ಧರೆಯ ಕೃಷಿ ಶುದ್ಧವಾಗಿಯಲ್ಲದೆ, ಸಸಿ ಶುದ್ಧವಿಲ್ಲ.", 
        "13339": "ಮನಸಿಜನ ಇರವುಳ್ಳನ್ನಕ್ಕ ಶೃಂಗಾರವ ಹಾರಬೇಕು.", 
        "13340": "ಅಂಗ ಶಿರದ ಮಲಿನವ ಕಳೆದಲ್ಲಿ, ಮನಕ್ಕೆ ಲಂಘನೆಯಾದಂತೆ,", 
        "13341": "ಅರ್ಚನೆ ಪೂಜೆಯ ಕುರಿತು ಮಾಡಿದಲ್ಲಿ,", 
        "13342": "ಧರೆ ಆಕಾಶವಿಲ್ಲದಿರೆ, ಆಡುವ ಘಟಪಟ, ಚರಸ್ಥಾವರ,", 
        "13343": "ಅಂಗವನಳಿವನ್ನಕ್ಕ ಶಿವಲಿಂಗಪೂಜೆಯ ಮಾಡಬೇಕು.", 
        "13344": "ಕಾಯ ಜೀವವನರಿದಲ್ಲಿ , ಇಷ್ಟ ಪೂಜೆಯ ಮರೆದಿರಬೇಕು.", 
        "13345": "ಪೂಜೆ ಅರತಲ್ಲಿ, ಎಲೆ ಉದುರಿದ ವೃಕ್ಷ ಉಲುಹಡಗಿದಂತಿರಬೇಕು.", 
        "13346": "e್ಞನವುಳ್ಳನ್ನಕ್ಕ ಅರಿವು, ಅರಿವುಳ್ಳನ್ನಕ್ಕ ಆತ್ಮ ,", 
        "13347": "e್ಞನಾದ್ವೈತದ ಹೆಚ್ಚುಗೆ, ಭಾವಾದ್ವೈತಕ್ಕೆ ಸಂಬಂಧವಾಗಿಹುದು.", 
        "13348": "ಶಿಲೆ ಕಾಷ್ಠದಲ್ಲಿ ಪಾವಕ ಜನಿಸುವ ಭೇದದಂತೆ,", 
        "13349": "ವಾಗದ್ವೈತದಲ್ಲಿ ನಿಂದು, ಸ್ವಯಾದ್ವೆ ೈತವನರಿಯಬೇಕು.", 
        "13350": "ಕಾಲಿನಲ್ಲಿ ನಡೆವುದು, ಕೈಯಲ್ಲಿ ಮುಟ್ಟುವುದು,", 
        "13351": "ಸಕಲ ಗಂಧಕ್ಕೆ ವಾಯುವೆ ಬೀಜ, ಸಕಲ ಉತ್ಪತ್ಯಕ್ಕೆ ಅಪ್ಪುವೆ ಬೀಜ.", 
        "13352": "ಕಾಯವಿದ್ದು ಕಾಬುದು ಗುರುಸ್ಥಲ, ಜೀವವಿದು ಕಾಬುದು ಲಿಂಗಸ್ಥಲ,", 
        "13353": "ಆರುಸ್ಥಲವಿಟ್ಟು ಬೇರೆ ಕರೆದ ಭಾವವಾವುದಯ್ಯಾ ?", 
        "13354": "ಗುರು ವೈಭವಕ್ಕೆ ಸಿಕ್ಕಿದಾಗಲೆ ಶಿಷ್ಯಂಗೆ ನರಕ ಪ್ರಾಪ್ತಿ .", 
        "13355": "ಗುರುವೆಂದಡೂ ಕಾಯವುಳ್ಳನ್ನಕ್ಕ, ದೋಷಕ್ಕೆ ಹೊರಗಾಗಬೇಕು.", 
        "13356": "ಅಂಗ ಲಿಂಗವ ಪೂಜಿಸಿ, ಪ್ರಾಣ e್ಞನವ ಕಂಡು,", 
        "13357": "ಮಧುರ ದಂಡ ಒರಳಿಗೆ ಬಂದು,", 
        "13358": "ಕಂಡೆಹೆನೆಂಬನ್ನಕ್ಕ ಆತುರ ಕಂಡು ಮನ ನಿಂದಲ್ಲಿ,", 
        "13359": "ಗುರುವೆಂಬ ಅಂಗವ ಧರಿಸಿದಡೇನು?", 
        "13360": "ಲಿಂಗವಸ್ತುವಾದಲ್ಲಿ, ನಂಬುವ ಮನ ವಿಶ್ವಾಸವಾಗಬೇಕು.", 
        "13361": "ಕಿರಾತಂಗೆ ಪಿಸಿತದ ಅರೋಚಕವುಂಟೆ?", 
        "13362": "ಕಾಯ ಲಿಂಗದರುಶನವನರಿದಲ್ಲಿ, ಮನವನರಿದು ತನುವೊಪ್ಪುವಂತೆ,", 
        "13363": "ತನುವಿಂಗೆ ಗುರುಲಿಂಗ, ಮನಕ್ಕೆ ಆಚಾರಲಿಂಗ,", 
        "13364": "ಕಾಮವೆಂಬುದು ಕಂಗಳ ಮುಂದೆ ಸುಳಿದಾಡುತ್ತಿದೆ.", 
        "13365": "ಪಾದತೀರ್ಥವ ಕೊಂಡಲ್ಲಿ ತೃಷೆಯರತು.", 
        "13366": "ಚಚ್ಚೆಗೊಟ್ಟಿಯಿಂದ ಒಡೆಯತನ ಹುಚ್ಚಾಗಿ ಕೆಟ್ಟಿತ್ತು.", 
        "13367": "ಪರಸ್ತ್ರೀಯರ ಬಿಟ್ಟಾಗಲೆ ಗುರುವಿನ ಇರವು ಸಾಧ್ಯವಾಯಿತ್ತು.", 
        "13368": "ಅರ್ತಿಯಿಂದ ಮಾಡುವ ಭಕ್ತಿ , ಕರ್ತಾರನ ಕಮ್ಮಟಕ್ಕೊಳಗಾಯಿತ್ತು.", 
        "13369": "ಕಾಮದಿಂದ ಕಾಬುದೆಲ್ಲವು ಗುರುವಿಂಗೊಳಗಾಗಿ.", 
        "13370": "ತನುವಿಂಗೆ ಕುರುಹಾದಲ್ಲಿ, ಅರ್ಚನೆ ಆವರಿಸಬೇಕು.", 
        "13371": "ಬರಿಹುಂಡನ ಗಡಿವಾಡದಲ್ಲಿರಿಸಿದಂತೆ,", 
        "13372": "ಬಾಹ್ಯವ್ರತ, ಭ್ರಮೆವ್ರತ, ಸೀಮೋಲ್ಲಂಘನವ್ರತ,", 
        "13373": "ಕಂಗಳ ಮುಂದೆ ಬಂದು ನಿಂದು, ಕಂಡುದೆಲ್ಲವು ಲಿಂಗಾರ್ಪಿತವೆ ?", 
        "13374": "ಅರ್ಥಪ್ರಾಣವ ಕೊಟ್ಟಲ್ಲಿ,", 
        "13375": "ಕಾಯವೆ ದೇಗುಲವಾಗಿ, ಕುರುಹಿಟ್ಟು ಕೊಟ್ಟ ಕುರುಹೆ ದೇವರಾಗಿ,", 
        "13376": "ಅಂಗ ಮುಟ್ಟಿ ಅಪ್ಪಿ, ಕಂಗಳು ತುಂಬಿ ನೋಡಿ,", 
        "13377": "ತ್ರಿವಿಧಕ್ಕೆ ಮನ ಎಷ್ಟರಾಸೆ ಉಂಟು", 
        "13378": "ವ್ಯಾಧನ ಚಿತ್ತದಂತೆ, ಸಾಧನೆಯಯ್ಯನ ಮೈ ಲಾಗಿನಂತೆ,", 
        "13379": "ಅರ್ಪಿತ ಅವಧಾನವನರಿವ ಸತ್ಯನ ನಿತ್ಯನ ಇರವು,", 
        "13380": "ಉಂಡಿಗೆಯನೆತ್ತಿದವನ ಮನದಂಗದಂತೆ, ಮತ್ತುಂಟೆ ಹಸಿಗೆಯ ತೋಟಿ ?", 
        "13381": "ಮಾಟ ಕೂಟವ ಮಾಡುವನ್ನಬರ", 
        "13382": "ಅರ್ತಿ ಅಭ್ಯಾಸ ಮಚ್ಚು ಕಾರಣದಿಂದ ಮಾಡುವ ಭಕ್ತಿ ,", 
        "13383": "ಕಾಯಭ್ರಮೆಯಿಂದ ಮಾಡುವುದು ದೇವಪೂಜೆಯಲ್ಲ.", 
        "13384": "ರೂಪು ರುಚಿ ಚಿತ್ತ, ಈ ಮೂರು ಏಕವಾಗಿ ನಿಂದಲ್ಲಿ, ಲಿಂಗಾರ್ಪಿತ.", 
        "13385": "ಕಲ್ಲ ತಾಗಿದ ಕಠಿಣಸರದಂತೆ, ವಲ್ಲಭನೊಲ್ಲದ ಸತಿಯಂತೆ,", 
        "13386": "ಭಕ್ತನ ಭಾಷೆ ಗುರುಲಿಂಗಜಂಗಮವಲ್ಲ.", 
        "13387": "ಹೇರನೊಪ್ಪಿಸಿದವಂಗೆ ಸುಂಕವ ಬೇಡಿ ಗಾರುಮಾಡಲೇತಕ್ಕೆ ?", 
        "13388": "ನೀ ಹಾಕಿದ ಮುಂಡಿಗೆ ಎನಗೂ ಸರಿ, ನಿನಗೂ ಸರಿ.", 
        "13389": "ಉಭಯವನರಿವ ಚಿತ್ತಕ್ಕೆ ಅರಿವೆ ಇಷ್ಟಲಿಂಗಕ್ಕೆ ಗೊತ್ತು.", 
        "13390": "ಜಲದಲ್ಲಿ ಹುಟ್ಟಿದ ಚೇತನ, ಜಲವುಳಿದು ಜೀವಿಸಬಲ್ಲುದೆ ?", 
        "13391": "ರೂಪಿನಲ್ಲಿ ಉಂಟೆಂದು ಹೋರುವಾಗ, ಅದು ಅಚೇನಮೂರ್ತಿ.", 
        "13392": "ಭಕ್ತ ನಾನೆ ಎಂದು ತನ್ನ ತಾನೆ ನಿಶ್ಚೆ ೈಸುವನ್ನಬರ,", 
        "13393": "ಭಕ್ತನಾಗಿದ್ದಲ್ಲಿ, ಗುರುಲಿಂಗಜಂಗಮ", 
        "13394": "ಪರಶಿವಮೂರ್ತಿಯ ರೂಪತಾಳಿ,", 
        "13395": "ತನುವಿನಲ್ಲಿ  ಮನೋಮೂರ್ತಿಯಾಗಿ,", 
        "13396": "ಸುಖದುಃಖಾದಿಗಳ ಮೀರುವ ಎನ್ನವರ ಕಾಣೆ.", 
        "13397": "ಅಂಗದ ಮೇಲಣ ಲಿಂಗ,", 
        "13398": "ತಮಕ್ಕೂ ದಿವಕ್ಕೂ ಸೂರ್ಯನಾದ ಮತ್ತೆ,", 
        "13399": "ಶಿವಭಕ್ತಂಗೆ ಭೃತ್ತಾಚಾರವಳವಟ್ಟಲ್ಲಿ, ಸದಾಚಾರವಿಪ್ಪುದು.", 
        "13400": "ಪ್ರಸಾದವ ನೆಮ್ಮಿದ ಭಕ್ತನಲ್ಲಿ, ಪರಶಿವನು ಪ್ರಸನ್ನನಾಗಿರ್ಪನು.", 
        "13401": "ಅಂದಳ ಸತ್ತಿಗೆ ಕರಿ ತುರಗಂಗಳಿಂದ ಮನಗುಂದದೆ,", 
        "13402": "ಗುರುಸೇವೆಯಲ್ಲಿ ತನು ಕರಗಿ, ಲಿಂಗಸೇವೆಯಲ್ಲಿ ಮನ ಕರಗಿ,", 
        "13403": "ಅಂಗದಲ್ಲಿ ಸೋಂಕಿದ ಸುಳುಹ ಮನವರಿದು,", 
        "13404": "ಘಟದಲ್ಲಿದ ಗಂಧ ಅಡಗುವುದಲ್ಲದೆ, ಸಂಚಾರವ ನೆಮ್ಮಿದ ಗಂಧ ಅಡಗುವುದೆ?", 
        "13405": "ಗಂಗೆವಾಳುಕ ರುದ್ರರೆಲ್ಲರು ಅಂಗವರತು ಲಿಂಗವನರಿದಲ್ಲದೆ,", 
        "13406": "ಸಾರಿದುದ ಕಂಡು, ಕೇಳಿ ಮೀರಿ ನಡೆದಡೆ,", 
        "13407": "ವ್ರತಾಚಾರವ ಹಿಡಿದು ಬಿಟ್ಟಲ್ಲಿ, ಗುರುವಾದಡೂ ಪರಿಹರಿಸಬಾರದು.", 
        "13408": "ಪಾತಕ ಹೊಲೆಯೆಂದರಿದು ಬಿಟ್ಟಲ್ಲಿ, ಮತ್ತಾ ಗುಣ ಸ್ವೀಕರಿಸಬಹುದೆ ?", 
        "13409": "ಮಹಾರ್ಣವವುರಿದು ಬೇವಲ್ಲಿ, ಕರಗದ ಜಲಕ್ಕೆ ಹೊಡೆಗೆಡೆವುದೆ ?", 
        "13410": "ಅಂಗದಲ್ಲಿ ಅರಿದು ನಡೆವ ಆಚರಣೆ,", 
        "13411": "ಹಲವಾಡುವಂತೆ ಕೆಲೆದಾಡಲಿಲ್ಲ.", 
        "13412": "ಸ್ವಯಂಪಾಕದಲ್ಲಿ ಆದ ದ್ರವ್ಯವ,", 
        "13413": "ವ್ರತಸ್ಥನಾದಲ್ಲಿ,", 
        "13414": "ಕಾಯಕವೆಂದು ಕಾಯವ ಬಳಲಿಸದೆ, ತನು ಕರಗದೆ, ಮನ ನೋಯದೆ,", 
        "13415": "ಮಣ್ಣು ಮೂರು, ಹೆಣ್ಣು  ಆರು, ಹೊನ್ನು ಒಂಬತ್ತು ,"
    }
}