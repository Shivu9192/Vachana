        "20501": "ಕುಲದಲ್ಲಿ ಶಿವಕುಲವೆಂಬೆ, ಛಲದಲ್ಲಿ ಶಿವಛಲವೆಂಬೆ,", 
        "20502": "ಕರಿಗಳ ಹೊಯಿದಾಟದಾ ನಡುವಿನ ಮರ ಮುರಿ[ವಂತೆ]", 
        "20503": "ಮದ ಎಂಟರೊಳು ಸಿಲ್ಕಿ ಮೈಮರೆದು ಅಲ್ಲಮನ", 
        "20504": "ಅರಿಷಡುವರ್ಗವೆಂದೆಂಬ ಕರ್ಮಿಗಳ ಬಲಿಯೊಳಿಟ್ಟೆನ್ನನಗಲಿದೆ.", 
        "20505": "ಅರಿಗಳಾರುಮಂದಿ ಬರಸಿಡಿಲಂತೆ", 
        "20506": "ಕಾಮ ಕ್ರೋಧ ಲೋಭ ಮೋಹ ಮದ ಮತ್ಸರದ ಸೀಮೆಗಳೆನಿಸಿ", 
        "20507": "ಕಾಮವೆಂಬ ಆನೆಯ ಕಾಲಿಂಗೆ ಹಾಕಿ ತುಳಿಸಿ,", 
        "20508": "ನೆನೆವ ಜಿಹ್ವೆ[ಗೆ] ಕಾಮನ ನೆನಸಿದೆ,", 
        "20509": "ನುಡಿವೆಡೆಯಲ್ಲಿ ಕ್ರೋಧವನೆ ನುಡಿವರು,", 
        "20510": "ಒಡವೆ ವಸ್ತು ಧನ ಧಾನ್ಯವ ಲೋಭದಿಂದ ಗಳಿಸಿ,", 
        "20511": "ಹೊನ್ನು ಹೆಣ್ಣು ಮಣ್ಣಿನ ಮೇಲೆ ಮೋಹವಿಪ್ಪಂತೆ", 
        "20512": "ವೃಥಾ ಸತಿ ಸುತ ಪಿತ ಮಾತೆಯರ ಮದ ತಲೆಗೆ ಹತ್ತಿ,", 
        "20513": "ಕಾಮವುಳ್ಳವ ಭಕ್ತನಲ್ಲ , ಕ್ರೋಧವುಳ್ಳವ ಮಹೇಶ್ವರನಲ್ಲ ,", 
        "20514": "ಕಾಮವಿಲ್ಲದಾತ ಭಕ್ತ , ಕ್ರೋಧವಿಲ್ಲದಾತ ಮಹೇಶ್ವರ,", 
        "20515": "ಕಾಮವು ಲಿಂಗಮುಖವಾಗಿ, ಕ್ರೋಧವು ಲಿಂಗಮುಖವಾಗಿ,", 
        "20516": "ಆನೆಯ ನುಂಗಿದ ಹುಲಿ, ಹುಲಿಯ ನುಂಗಿದ ಸರ್ಪ,", 
        "20517": "ಸೊಂಡಿಲ್ಲದಾನೆ ಹೊಯಿವುದ ಕಂಡೆ,", 
        "20518": "ಅರಿಷಡುವರ್ಗವೆಂಬ ಕರ್ಮಿಯ ಬಲಿಯೊಳಿಂಬು", 
        "20519": "ವ್ಯಸನವೇಳೆಂಬ, ರಸವ್ಯಸನವೇಳೆಂಬ", 
        "20520": "ಏಳುವ್ಯಸನಂಗಳೆಂಬ ಕಾಳುವಿಷಯಕ್ಕೆನ್ನ", 
        "20521": "ಅಹೋ ನಿಲ್ಲಿ ನಿಲ್ಲಿ ವ್ಯಸನಗಳಿರಾ !", 
        "20522": "ತನುವ್ಯಸನವೆತ್ತಿದಲ್ಲಿ ತರಣಿಕೋಟಿತೇಜಪ್ರಕಾಶನ ನೆನೆವೆನು.", 
        "20523": "ತನುವಿನ ಹಂಬಲವ ಮರೆದಂಗೆ ತನುವ್ಯಸನವೆಲ್ಲಿಯದೋ ?", 
        "20524": "ಅನ್ನದ ಗೊಡವಿಲ್ಲದಾತಂಗೆ ಆರಂಭದ ಗೊಡವಿಯುಂಟೇ ?", 
        "20525": "ಏಳು ಸಮುದ್ರ ಉಕ್ಕಿ ಜಂಬೂದ್ವೀಪವ ಮುಸುಕದ ಮುನ್ನ", 
        "20526": "ಉರು ಬಡತನಕ್ಕೆ ವಾಳಿ (?) ಧನಿಕನಾಗುವುದ ಕಂಡೆ.", 
        "20527": "ಶೂಲದ ಮೇಲಣ ಕುಣಪಂಗಳು", 
        "20528": "ಸಪ್ತವ್ಯಸನವೆಂಬ ಪಾಪಿಯ ಕೂಸಿಂಗೆ", 
        "20529": "ಪಂಚೇಂದ್ರಿಯೆಂಬ ಕರ್ಮಿಗಳ ಉರಿಯ ಪ್ರಪಂಚಿಗೆ ಗುರಿಯಾಗಿ", 
        "20530": "ಶ್ರೋತ್ರ ತ್ವಕ್ಕು ನೇತ್ರ ಜಿಹ್ವೆ ಘ್ರಾಣವೆಂಬ ಪಂಚೇಂದ್ರಿಯಂಗಳು.", 
        "20531": "ಮೇಷನ ದನಿಗೇಳಿ ಹುಲಿ ಬಲಿಯ ಬೀಳ್ವಂತೆ,", 
        "20532": "ಮೊಲನ ಕಂಡರೆ ಶ್ವಾನಂಗಳು ತುಡುಕುವಂತೆ.", 
        "20533": "ಹರನಿಂದೆ ಗುರುನಿಂದೆ ಪರನಿಂದ್ಯವ ತೊರೆದು", 
        "20534": "ಸರ್ಪಕಡಿದು ಸತ್ತ ಹೆಣನೆದ್ದು ಸುಳಿದಾಡುವುದ ಕಂಡೆ.", 
        "20535": "ಅನ್ಯವಿಷಯ ಪಂಚೇಂದ್ರಿಯವೆಂದೆಂಬ", 
        "20536": "ದ್ಯುಮಣಿ ಇಲ್ಲದ ನಭಕೆ ತಮದ ಹಾವಳಿ ನೋಡಾ.", 
        "20537": "ವಿಕಾರದೊಳು ವಿಕಾರ ಕಪಿವಿಕಾರ.", 
        "20538": "ರಾಜಂಗೆ ಮಂತ್ರಿ ಮುಖ್ಯವಾದಂತೆ", 
        "20539": "ನೊಂದೆನೀ ಮನದಿಂದಲಿ, ಬೆಂದೆನೀ ಮನದಿಂದಲಿ,", 
        "20540": "ಎಲ್ಲಿ ನೋಡಿದರೆನ್ನಯ  ಮನವಿಪ್ಪುದು.", 
        "20541": "ಸ್ಥೂಲದಲ್ಲಿಪ್ಪುದೆನ್ನಯ ಮನ, ಸೂಕ್ಷ್ಮದಲ್ಲಿಪ್ಪುದೆನ್ನಯ ಮನ,", 
        "20542": "ಹಳ್ಳ ಮೇರೆದಪ್ಪಿದರೆ ಇಳಿವುದು, ಹೊಳೆ ಮೇರೆದಪ್ಪಿದರೆ ಇಳಿವುದು,", 
        "20543": "ಉಳಿ ಬಾಚಿಂಗೆ ಹರಿಯದ ಕೊರಡು,", 
        "20544": "ವನದೊಳಿಪ್ಪ ಕಪಿ ವನವನುಂಗಿ, ವನದ ತಿಳಿಗೊಳನನುಂಗಿ,", 
        "20545": "ಪಕ್ಕವಿಲ್ಲದ ಹಕ್ಕಿ ಅಕ್ಕಜನ ಪಂಜರವ ಗೂಡುಮಾಡಿಕೊಂಡು", 
        "20546": "ನೀರ ನೆಳಲ ಮಧ್ಯದಲ್ಲಿ ಹಾರುವ ಭ್ರಮರನ ಗರಿಯ ಗಾಳಿಯಲ್ಲಿ", 
        "20547": "ಗೂಡಿನೊಳಗಿಪ್ಪ ಪಕ್ಷಿಗೆ ಗೂಡಿನ ಹೊರಗೆ ಪಕ್ಕ ಬಂದಿವೆ.", 
        "20548": "ಮನವಿಕಾರ ಸುಟ್ಟದನು ಮನವಿಕಾರದ ಭ್ರಮೆಯ ಹೋರಟೆಯಾಗದೆಂ", 
        "20549": "ಆಶೆಯಾಮಿಷವೆಂಬ ಮಾಯಾಪಾಶಕೆನ್ನ ಗುರಿಮಾಡಿ ನೀ ಸಿಕ್ಕದೆ", 
        "20550": "ಮಾನಸ ವಾಚಿಕ ಕಾಯಿಕದಲ್ಲಿ", 
        "20551": "ಆಸೆಯೆಂಬುದು ಆರಾರನು ಕೆಡಿಸದಯ್ಯ ?", 
        "20552": "ಯತಿಗಳು ಆಸೆರೋಷವ ಕಳೆಯಲರಿಯದೆ ಮತಿಭ್ರಷ್ಟರಾದರು.", 
        "20553": "ಸಹಸ್ರಲಕ್ಷ ದ್ರವ್ಯದೊರೆದರೂ ಅದು ಸಾಲದೆಂಬುದೀ", 
        "20554": "ತನುವೆಂಬ ಹೊಲದೊಳು ಜೀವವೆಂಬ ಒಕ್ಕಲಿಗ,", 
        "20555": "ಕಾಳರಕ್ಕಸಿಯ ಕೈಯೊಳಿಪ್ಪ ಶಿಶುವಿನ ಭ್ರಮೆ", 
        "20556": "ಮೂರುಮುಖದ ಪಕ್ಷಿಯ ಹೆಸರಕ್ಷರವೆಂಟು.", 
        "20557": "ಲಿಂಗಾಸಕ್ತಂಗೆ ಅಂಗಾಸಕ್ತಿವುಂಟೇನಯ್ಯಾ ?", 
        "20558": "ಆಸೆಬದ್ಧವು ಬಿಡದು ಈ ಮನದ", 
        "20559": "ಆಸೆ ಮಾಡದಿರು ಅಧಮ ಚಾಂಡಾಲಿ ನರರಿಂಗೆ.", 
        "20560": "ಕೊಡುವವ ಮಾನವನೆಂದು ಒಡಲಾಸೆಯ ತೋರದಿರು,", 
        "20561": "ವಾರಿಯಿಲ್ಲದ ಕೆರೆಗೆ ಹಾರೈಸಿ ಬಂದ ತುರುವಿನತೆ,", 
        "20562": "ಕಾಮಧೇನುವಲ್ಲದೆ ಸಾದಾ ಧರೆಯ ಗೋವುಗಳು", 
        "20563": "ಒಡಲಾಸೆಗೆ ಅನ್ಯರ ಸೇವೆಯ ಮಾಡುವ", 
        "20564": "ಆಸೆ ಮಾಡದಿರು ಪರರಿಗೆ ಕೇಳು ಮನವೆ", 
        "20565": "ಏನ ಹೇಳುವೆನಯ್ಯಾ ? ಸಂಸಾರಬಂಧನದಲ್ಲಿ ಕಂದಿಕುಂದಿದೆನಯ್ಯಾ.", 
        "20566": "ಇನ್ನೇವೆ ಇನ್ನೇವೆನಯ್ಯಾ ?", 
        "20567": "ಕ್ಷೀರವನೊಲ್ಲದೆ ಹಂಸ ನೀದಂಗೆ ಹರಿವಂತೆ,", 
        "20568": "ನೆಲೆಯನರಿಯದ ಮನುಜರು ಜಲವ ಹೊಕ್ಕರೆ ಮುಳುಗುವರಲ್ಲದೆ", 
        "20569": "ಅಕಟಾ, ರಾಟಾಳದ ಘಟದಂತೆ ಭವಾರಣ್ಯದೊಳು ತಿರುಗಿತಿರುಗಿ", 
        "20570": "ಹಿಂದಣಜನ್ಮದಲ್ಲಿ ಗುರುವ ಮರೆದ ಕಾರಣ,", 
        "20571": "ಪಂಕದೊಳು ಬಿದ್ದ ಪಶುವಿನಂತೆ", 
        "20572": "ಆಸತ್ತೆ ಸಂಸಾರಸಂಗಕ್ಕೆ, ಬೇಸತ್ತೆ ಸಂಸಾರಸಂಗಕ್ಕೆ.", 
        "20573": "ಕೆಚ್ಚಲ ಕಚ್ಚಿದ ಉಣ್ಣೆ", 
        "20574": "ಮಾಯದ ಸಂಸಾರಬಲೆಯೊಳು ಸಿಲ್ಕಿ ಆಯಾಸಗೊಳುತ", 
        "20575": "ಎಂದಾದರೂ ಸಂಸಾರದಂದುಗ ಬಿಡುದು ನೋಡಯ್ಯಾ !", 
        "20576": "ಹೆಳವರ ನಡುವೆ ಅಂಧಕರ ನಡುವೆ ಮನೆಬೆಂದಂತೆ ಸಂಸಾರ.", 
        "20577": "ಯೋನಿಯಲ್ಲಿ ಹುಟ್ಟಿ, ಯೋನಿಯನೆ ಬಯಸಿ,", 
        "20578": "ಹರಿಣಪಾದಮಾತ್ರದಷ್ಟು ಉಪಾಸ್ತಕೆ (?)", 
        "20579": "ಹೆಣ್ಣಿನ ಮುದ್ದುಮುಖ ಮೊಲೆ ಮೋಹನ ಮುಗುಳುನಗೆಯ", 
        "20580": "ಹೊನ್ನಿನಾಸೆ ತನುವಿನೊಳಿಂಬುಗೊಂಡು ಪನ್ನಗಧರ ನಿಮ್ಮ ಮರೆಸಿತಯ್ಯ.", 
        "20581": "ಸಂಸಾರಸುಖಕ್ಕೆ ಕಟ್ಟಿದ ಮನೆ, ಸಂಸಾರಸುಖಕ್ಕೆ ಕೊಂಡ ಹೆಣ್ಣು,", 
        "20582": "ಮನೆ ಮಡದಿ ಮಕ್ಕಳು ದ್ರವ್ಯ ಬಂಧುಬಳಗವೆಂಬುದು", 
        "20583": "ಬಲ್ಲೆನೆಂಬುವರ ಭ್ರಾಂತಿಗೊಳಗುಮಾಡಿತ್ತು ಮಾಯೆ,", 
        "20584": "ತೆರಣಿಯ ಹುಳು ತನ್ನ ಮನೆಯೊಳು ತಾನೆ", 
        "20585": "ಕುಣಪ ಕೂಗದಕಿಂದ ಮುನ್ನ,", 
        "20586": "ಹೊತ್ತು ಹೋಗಿ ಕತ್ತಲೆಯಾಗದಕಿಂದ ಮೊದಲೆ,", 
        "20587": "ಮುಂಡೆಗೆ ಮುತ್ತೈದೆತನ ಬಂದಂದಿಗೆ,", 
        "20588": "ನಡೆಯಲಾರದ ಹೆಳವಂಗೆ ಅಂಧ ಹೆಂಡತಿ ದೊರೆತರೆ", 
        "20589": "ಆನೆಯ ಹೆಣಕ್ಕೆ ಕೋಡಗ ಶೋಕಂಗೈವುದ ಕಂಡೆ.", 
        "20590": "ಹಳ್ಳದೊಳು ತೇಲಿಹೋಗುವ ಕಂಬಕೆ ತೊರೆನೆರೆಗಳು ಮುಸುಕಿ", 
        "20591": "ಇ[ರಿ]ವಾಕಳದೆರಡು ಕೋಡು ಧರೆಯಾಕಾಶಕ್ಕೆ ಬೆಳೆದಿಪ್ಪುದ ಕಂಡೆನು", 
        "20592": "ಸಾಸುವೆಯ ಮೇಲೆ ಸಾಗರ ಹರಿವುದ ಕಂಡೆನು.", 
        "20593": "ಅತ್ತಿಯಹಣ್ಣಿಗಾಗಿ ಬಂದು ಸಿಕ್ಕಿದ ಪಿಕಳಿಯಂತೆ", 
        "20594": "ಅಂಗಗುಣಸಂಸಾರಿಗೆ ಲಿಂಗಗುಣಸಂಸಾರವುಂಟೇನಯ್ಯಾ ?", 
        "20595": "ಅರ್ಥ ಪ್ರಾಣ ಅಭಿಮಾನವ ಗುರುಲಿಂಗಜಂಗಮಕ್ಕೆ ಸವೆದು,", 
        "20596": "ಬಂಧನ ಸಂಸಾರದಂದುಗದ ದಾಳಿಯಲ್ಲಿ", 
        "20597": "ಗಗನದ ತಮವ ಕಳೆವರೆ ರವಿ ಅಧಿಕ ನೋಡಾ.", 
        "20598": "ಹಡಗ ಹರಿಗೋಲ ನಂಬಿದವರು", 
        "20599": "ಗುರುವೆ ಸುರತರುವೆ ಸಕಲಾಗಮದಿರವೆ", 
        "20600": "ಏನು ಸುಕೃತದ ಫಲವೊ, ಅದೇನು ಪುಣ್ಯವೋ,", 
        "20601": "ಹಿಂದೇನು ಸುಕೃತವ ಮಾಡಿದ ಕಾರಣ", 
        "20602": "ಅಂದು ಸಹಸ್ರಜನ್ಮಾಂತರದಲ್ಲಿ ತಪೆÇೀಧ್ಯಾನವ ಮಾಡಿ", 
        "20603": "ಗುರುವೆ ಬೇರೆ ಹರ ಬೇರೆಯೆಂಬ", 
        "20604": "ಹಲವು ತರುಗಿರಿ ತೃಣಕಾಷ್ಠವೆಲ್ಲಕೆಯೂ ಒಂದೇ ಕಿಡಿ ಸಾಲದೇನಯ್ಯಾ !", 
        "20605": "ಗುರುವ ನರನೆಂದು ನುಡಿವ ಕುರಿಮಾನವರ", 
        "20606": "ಗುರುವೆಂಬೆರಡಕ್ಷರವು ಹರನಾಮವಲ್ಲದೆ ನರನಾಮವೇನಯ್ಯಾ ?", 
        "20607": "ಸದ್ಯೋಜಾತ ವಾಮದೇವ ಅಘೋರ ತತ್ಪುರುಷ ಈಶಾನ್ಯವೆಂಬ", 
        "20608": "ಹರಸ್ರಾವದಲ್ಲಿ ಹುಟ್ಟಿ ನರಸ್ರಾವವ ನೆನೆವ", 
        "20609": "ಗುರುದೀಕ್ಷೆಯ ಪಡೆದು, ಗುರುಲಿಂಗವ ಶಿರದಲ್ಲಿ ಧರಿಸಿ,", 
        "20610": "ಗುರುಹಸ್ತದಿಂದ ಹುಟ್ಟಿದ ಮೇಲೆ", 
        "20611": "ಗುರು ನರನೆಂದು, ಸತ್ತನುಕೆಟ್ಟನುಯೆಂಬ ನರಕಜೀವಿ ನೀ ಕೇಳಾ.", 
        "20612": "ಗುರುದೀಕ್ಷೆಯಿಲ್ಲದ ಶಿಲೆಯ ಕೊರಳಲ್ಲಿ ಕಟ್ಟಿಕೊಂಡು", 
        "20613": "ಗುರು ಸಂಸಾರಿ, ಶಿಷ್ಯ ಯತಿಯಾದರೇನಯ್ಯಾ !", 
        "20614": "ಗುರು ಸಹಜವಾಗಿರೆ,", 
        "20615": "ಪುರುಷನಿಂದ ಸತಿ ಹಿರಿಯಳಾದರೂ ಆಗಲಿ,", 
        "20616": "ಗುರುವಿನಲ್ಲಿ ಗುಣ ವಿದ್ಯೆ ಕುಲ ಬಾಲ್ಯ ಯೌವನ", 
        "20617": "ಗುರುಲಿಂಗವಿದ್ದ ಊರು ಸೀಮೆಯ ಕಂಡಾಕ್ಷಣವೆ", 
        "20618": "ಗುರುವರಿಯದವನೆಂದುದಾಸೀನವ ಮಾಡಿ", 
        "20619": "ಸರ್ವರಲ್ಲಿ ಹುಸಿಯನಾಡಿದರೆ ಆಡಲಿ,", 
        "20620": "ಧ್ಯಾನಕೆಲ್ಲ ಮೂಲ ಗುರುವಿನ ಮೂರ್ತಿ ಕಾಣಿರೊ !", 
        "20621": "ಗುರುಶಿಷ್ಯ ಸಂಬಂಧಕೆ ಹೋರಾಡಿ, ಧರೆಯೆಲ್ಲ ಬಂಡಾದರು ನೋಡಾ.", 
        "20622": "ಅಂಧಕ ಅಂಧಕರು ಕರವಿಡಿದರಣ್ಯದೊಳು", 
        "20623": "ಬೀರ ಜಟ್ಟಿ ಮೈಲಾರ ಮಾರಿ ಮಸಣೆಯೆಂಬ", 
        "20624": "ಪಟ್ಟವ ಕಟ್ಟಿದ ಮೇಲೆ  ಲಕ್ಷಣವನರಸಲುಂಟೇನಯ್ಯಾ ?", 
        "20625": "ಜ್ಞಾನ ಗುರುವಿಗೆ ಜ್ಞಾನ ಶಿಷ್ಯನಾದರೆ", 
        "20626": "ಗುರುದೈವದ ಮುಂದೆ ಪರದೈವವ ಹೊಗಳುವ", 
        "20627": "ಗುರು ಕರುಣಾಕರ ಪರಶಿವ ಪರಮ", 
        "20628": "ಶರಧಿಯನಾರಾಧಿಸಿದರೆ ಹವಳ ಮೌಕ್ತಿಕಗಳನೀವುದು ನೋಡಾ !", 
        "20629": "ತ್ರಿವಿಧ ತನುವಿಗೆ ತ್ರಿವಿಧ ದೀಕ್ಷೆ.", 
        "20630": "ಬಡವನೆಡವಿ ಧನಕಂಡಂತೆ, ಭವಪಡುವನ ಕರದೊಳು", 
        "20631": "ಹೆಳವನಿದ್ದೆಸೆಗೆ ಜಲನಿಧಿ ನಡೆತಂದಂತೆ,", 
        "20632": "ಕಂಡೆನೆನ್ನ ಕರದೊಳು ಕರುಣವರಮೂರ್ತಿಲಿಂಗವ.", 
        "20633": "ಗುರು ಕರುಣಿಸೆ ಇಷ್ಟಲಿಂಗವೆನ್ನ ಕರವ ಸೇರಲು", 
        "20634": "ಅಂತರಂಗದಲ್ಲಿ ಜ್ಞಾನಪರಿಪೂರ್ಣನಾಗಿ ಸರ್ವಾಂಗವೆಲ್ಲ", 
        "20635": "ಅಂಗದ ಮೇಲೆ ಲಿಂಗವಿಪ್ಪ ಶಿವಭಕ್ತನ ಕಂಡರೆ", 
        "20636": "ತನುಲಿಂಗವೆಂದೆಂಬ ಅದ್ವೈತಿಯ ಮಾತ ಕೇಳಲಾಗದು.", 
        "20637": "ಬರುತ ಬರುತ ಅರಣ್ಯದೊಳಗೊಂದು ಬಟ್ಟೆಯ ಕಂಡೆನು.", 
        "20638": "ಹುಟ್ಟದ ಮುನ್ನ ಬೆಳೆವ ಶಿಶುವ ಹೆಸರಿಟ್ಟು ಕರೆವವರಾರೂಯಿಲ್ಲ.", 
        "20639": "ಹುಟ್ಟಿದ ಶಿಶುವಿಗೆ ಮೊಲೆ ಮುಡಿ ಜವ್ವನ ಬಂದು,", 
        "20640": "ಮಣಿಮಾಡದೊಳಡಗಿಹ ಮಾಣಿಕಕ್ಕೆ", 
        "20641": "ನಿರಾಕಾರ ಪರಬ್ರಹ್ಮವಸ್ತು ಎನ್ನ ಕರಸ್ಥಲಕೆ ಆಕಾರವಾಗಿ ಬಂದರೆ", 
        "20642": "ಮುಂಡದ ಮೇಲಣ ತಲೆಯ ಮೂರ್ತವ ಅಂಗೈ ಮೇಲೆಗೂಡಿ,", 
        "20643": "ಲಿಂಗವ ಧ್ಯಾನಿಸುವನ ಅಂಗ ಕೈಲಾಸದ ರಾಜ್ಯಾಂಗಣ ಕಾಣಿರೊ.", 
        "20644": "ಕೈಲಾಸವೆಂಬುದು ಬೇರಿಲ್ಲಾ ಕಾಣಿರೋ !", 
        "20645": "ಜ್ಯೋತಿ ಇದ್ದ ಗೃಹಕ್ಕೆ ತಮವುಂಟೇನಯ್ಯಾ ?", 
        "20646": "ಕರಸ್ಥಲದ ಲಿಂಗ ಕೈಸೇರಿ ಇರು", 
        "20647": "ಪರುಷ ಮನೆಯೊಳಿರುತಿರೆ ಪಾಷಾಣಕೆ ಚಾಲಿವರಿವಂತೆ,", 
        "20648": "ಹಸಿಹಂದರದಿ ಪಡೆದ ಗಂಡನ ವಂಚಿಸಿ,", 
        "20649": "ಇಷ್ಟಲಿಂಗವ ಮರೆದು ಬರಿಯ ಕಲ್ಲಿಗೆರಗುವ", 
        "20650": "ಶಿವಾಚಾರಸಂಪನ್ನನಾಗಿ ಶಿವಭಕ್ತನೆನಿಸಿಕೊಂಡು", 
        "20651": "ಭವಿ ಭಕ್ತರೆಂಬೆರಡು ವಿವರವಾಯಿತು ನೋಡಾ ಅಯ್ಯಾ.", 
        "20652": "ಲಿಂಗಕ್ಕೆರಗದೆ ಅನ್ಯದೈವಂಗಳಿಗೆರಗುವಾತನ ತಲೆ", 
        "20653": "ಹುಟ್ಟಿದಂದು ತನುವ ಸೋಂಕಿದ ಲಿಂಗವ,", 
        "20654": "ಶೈವರು ಕಟ್ಟಿದ ಗುಡಿಯ ಹೊಗದ ಭಾಷೆ,", 
        "20655": "ಕೊಡುವಾತ ಶಿವ, ಕೊಂಬಾತ ಶಿವ, ಹುಟ್ಟಿಸುವಾತ ಶಿವ,", 
        "20656": "ಮುಂದೆ ಮಾಡಿದ ಕರ್ಮ ಬೆನ್ನಟ್ಟಿ ಬರುವಾಗ,", 
        "20657": "ಹರಲಿಂಗಕೆ ಹರಿ ಅಜಾಸುರರು ಸರಿಯೆಂಬ", 
        "20658": "ಸರ್ವದೇವಪಿತ ಶಂಭುವೆಂಬ ನಾಮ ಹರಗೆ ಸಲ್ಲುವುದಲ್ಲದೆ", 
        "20659": "ಹರಿಯ ನಯನ ಹರನ ಶ್ರೀಪಾದದೊಳು ಅಡಕವಾದುದ", 
        "20660": "ಗುರುಕರುಣದ ಇಷ್ಟಲಿಂಗವ", 
        "20661": "ಕಾಮನ ಸುಟ್ಟು ವಿಭೂತಿಯಿಲ್ಲದಂದಿನ,", 
        "20662": "ಜನನಭಯದ ಮೂಲದ ಬೇರ ಕಿತ್ತು ಸುಟ್ಟುರುಹುವ ಭಸ್ಮ,", 
        "20663": "ಹಲವು ಜನ್ಮದಲೊದಗಿದ ಪಾಪವ ತೊಲಗಿಸುವುದಕ್ಕೆ ಶ್ರೀವಿಭೂತಿ.", 
        "20664": "ಬೆಟ್ಟದಷ್ಟು ಕರ್ಮವುಳ್ಳರೆ ಬೊಟ್ಟಿನಷ್ಟು ಶ್ರೀವಿಭೂತಿಯ ಧರಿಸಲು", 
        "20665": "ವೇದಕ್ಕೆಯಾ [ಮುಖ್ಯ] ಶ್ರೀವಿಭೂತಿ,", 
        "20666": "ತ್ರಿಪುಂಡ್ರ ತ್ರಿದೇವರುತ್ಪತ್ಯಸ್ಥಿತಿಲಯವ ಕೆಡಿಸುವುದೆಂದು", 
        "20667": "ನೆತ್ತಿಯಲ್ಲಿ ಶ್ರೀ ವಿಭೂತಿಯ ಧರಿಸಿ", 
        "20668": "ಶ್ರೀ ವಿಭೂತಿಯಿಲ್ಲದವನ ಹಣೆಯ ಸುಡುಯೆಂದಿತ್ತು ಪೌರಾಣ.", 
        "20669": "ಭಸಿತದ ಮೇಲೆ ಅನ್ಯದೇವರ ಬಂಡಾರವನಿಡುವ", 
        "20670": "ಭಕಾರ ಭವಬಂಧನ ಕರ್ಮವ ಕಳೆವುದೆಂದು,", 
        "20671": "ವಿಕಾರ ಈ ಜನ್ಮದ ಹೊರೆಯ ಕಳೆವುದೆಂದು,", 
        "20672": "ಭಸಿತವಿದು ಶಿವರೂಪೆಂದು", 
        "20673": "ಧರಿಸಿರಣ್ಣ ಶ್ರೀ ವಿಭೂತಿಯ, ಧರಿಸಿರಣ್ಣ ಶ್ರೀ ರುದ್ರಾಕ್ಷಿಯ,", 
        "20674": "ಹರ ತ್ರಿಪುರದಹನದಲ್ಲಿ  [ಈಕ್ಷಿಸಿ] ನೋಡಲಾಕ್ಷಣ", 
        "20675": "ಶಿರದೊಳು ಧರಿಸಲು ಕೋಟಿ ಫಲ", 
        "20676": "ಶಿಲೆ ಮೌಕ್ತಿಕ ಪದ್ಮಾಕ್ಷಿ ಸುವರ್ಣ ಸಲೆ ಪುತ್ರಂಜಿ ಮೊದಲಾದ", 
        "20677": "ಧರಿಸಿರೊ ಶ್ರೀ ರುದ್ರಾಕ್ಷಿಯ ಬಾಹು", 
        "20678": "ಏನೆಂದು ಉಪಮಿಸುವೆನಯ್ಯಾ ಶ್ರೀಪಂಚಾಕ್ಷರಿಯ !", 
        "20679": "ಪುಣ್ಯಫಲಕೆ ಬೀಜ ಶ್ರೀಪಂಚಾಕ್ಷರಿ,", 
        "20680": "ಶ್ರೀಪಂಚಾಕ್ಷರಿಯ ಮೂಲದ ಹೊಲಬ ತಿಳಿಯಬಲ್ಲಾತನೆ", 
        "20681": "ಶ್ರೀ ಪಂಚಾಕ್ಷರಿಯುಳ್ಳ ಅಗ್ರಜ ವಿಪ್ರನಿಂದಧಿಕ ನೋಡಾ !", 
        "20682": "ನಕಾರ ನರಜನ್ಮದ ಹೊಲೆಯ ಕಳೆದು,", 
        "20683": "ಪಂಚಾಕ್ಷರವೆ ಶಿವನ ಪಂಚಮುಖದಿಂದಲುದಯವಾಗಿ", 
        "20684": "ಗುದದಲ್ಲಿ ಆಧಾರಚಕ್ರ, ಪೃಥ್ವಿಯೆಂಬ ಮಹಾಭೂತ,", 
        "20685": "ಮಂತ್ರವ ಬಲ್ಲೆವೆಂದು ಪರಮನುಜರಿಗೆ ಭೂತ ಪ್ರೇತ ಪಿಶಾಚಿಗಳು", 
        "20686": "ಜಪಿಸಿರೊ ಜಪಿಸಿರೊ ಶ್ರೀ ಪಂಚಾಕ್ಷರಿಯ.", 
        "20687": "ಕಂದ :      ಬಣ್ಣಿಪರಳವೆ ಶ್ರೀ ಪಂಚಾಕ್ಷರಿ", 
        "20688": "ಅಷ್ಟಾವರಣದಲ್ಲಿ ನೈಷ್ಠೆವಾರಿಯಾಗದನ್ನಕ್ಕರ", 
        "20689": "ಗುರು ಲಿಂಗಜಂಗಮ ವಿಭೂತಿ ರುದ್ರಾಕ್ಷಿ ಪಂಚಾಕ್ಷರಿ", 
        "20690": "ಗುರುವಾದಾತನು ಪರಶಿವ ತಾನೆ ನೋಡಾ !", 
        "20691": "ಗುರುವಾಗಿ ಎನ್ನ ಅನಂತಜನ್ಮದ ಹೊಲೆಯ ಕಳದೆಯಲ್ಲಾ ದೇವಾ !", 
        "20692": "ಗುರುಕೃಪೆಯಿಲ್ಲದವನ ಕರ್ಮಹರಿಯದು.", 
        "20693": "ಅಜ್ಞಾನ ಕುಕರ್ಮದಿಂದ ಮುಸುಕಿದ ಕತ್ತಲೆಗೆ", 
        "20694": "ಹಲವು ಹಡಗವಾದರೇನಯ್ಯಾ ? ಸಮುದ್ರ ಒಂದೇ ನೋಡಾ !", 
        "20695": "ಹಿಂಗದೆನ್ನಯ ಮನಸು ಗುರುಪಾ", 
        "20696": "ಅಂದು ಆದಿಯಲ್ಲಿ ಶಿವ ಬೀಜವಾಗಿ ಬಂದ ಕಾರಣ", 
        "20697": "ಶಿವಂಗೂ ಶಿವಭಕ್ತಂಗೂ ಭಿನ್ನವುಂಟೇ ? ಇಲ್ಲ.", 
        "20698": "ಗುರುವಿನಲ್ಲಿ ಲಿಂಗವುಂಟು, ಜಂಗಮವುಂಟೆಂದು", 
        "20699": "ಬಸವಣ್ಣ ಶ್ರುತಿಸಿದ ವಾಕ್ಯದಿಂದ", 
        "20700": "ಶಿವ ಗುರುವೆಂದು ನಂಬಿಪ್ಪಾತನೆ ಶಿವಭಕ್ತ.", 
        "20701": "ಗುರುಲಿಂಗಜಂಗಮವ ನೆರೆ ನಂಬಿಪ್ಪಾತನೆ ಶಿವಭಕ್ತ.", 
        "20702": "ಭಕ್ತಿಯಿಲ್ಲದೆ ಗುರುಪೂಜೆಯ ಅನಂತಕಾಲ ಮಾಡಿದರೂ", 
        "20703": "ಶಿವಪೂಜೆಯ ಮಾಡುತಿದ್ದೆನೆಂಬ ಅಣ್ಣಗಳು ನೀವು ಕೇಳಿರೊ.", 
        "20704": "ಇಷ್ಟಲಿಂಗದ ವೀರಶೈವ, ಪ್ರಾಣಲಿಂಗದ ವೀರಶೈವ,"
    }
}