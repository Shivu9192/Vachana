        "19780": "ಓಂಕಾರವೆಂಬ ಮರಕ್ಕೆ ಷಟ್‍ಕೃತಿಯೆಂಬ ಶಾಖೆ ಪಸರಿಸಿ,", 
        "19781": "ಎನ್ನ ಷಡ್‍ಧಾತುಗಳೆಲ್ಲ ಷಡಕ್ಷರಮಂತ್ರಸ್ವರೂಪವಾಗಿ", 
        "19782": "ಎನ್ನ ಸ್ಥೂಲತನುವೆಂಬ ಮನೆಯಲ್ಲಿ", 
        "19783": "ಶಿವಭಕ್ತಿ  ಶಿವಜ್ಞಾನ ಶಿವನಲ್ಲಿ ವಿಶ್ವಾಸವಮಾಡಿದ", 
        "19784": "ಶಿವಶಿವಾ ಎನ್ನಿರೋ ! ಶಿವನ ಧ್ಯಾನಕ್ಕೆ ತನ್ನಿರೊ !", 
        "19785": "ಅಕುಲಜ ಅಧಮ ಮೂರ್ಖನಾದಡಾಗಲಿ", 
        "19786": "ಶಿವಶಿವಾ ! ಶಿವಭಕ್ತನಿರ್ದ ಹಳ್ಳಿಯಾದಡಾಗಲಿ,", 
        "19787": "ಪಂಚಮಹಾಪಾತಕಂಗಳ ಮಾಡಿದವನಾದಡಾಗಲಿ,", 
        "19788": "ಸದ್‍ಭಕ್ತನಾದಾತನು ಸತ್‍ಪಾತ್ರದಾನಯುಕ್ತನಾಗಿರಬೇಕಲ್ಲದೆ", 
        "19789": "ಬ್ರಾಹ್ಮಣನ ದರ್ಶನ ಪಾಪದ ಪುಂಜ ನೋಡಾ.", 
        "19790": "ಜಂಗಮವೇ ಜಗದ್ಭರಿತನು ನೋಡಾ.", 
        "19791": "ಚತುರ್ವೇದಿಗಳಾದ ಶತಕೋಟಿ ಬ್ರಾಹ್ಮಣರಿಗೆ", 
        "19792": "ಮತ್ತೆ ಕೇಳಿರೋ, ಸತ್ಪಾತ್ರಜಂಗಮಕ್ಕೆ ಇತ್ತ ಪುಣ್ಯವು", 
        "19793": "ತೆಂಗಿಗೆ ನೀರನೆರೆದರೆ ಅಂಗೈಯಲ್ಲಿ ಫಲವು ಕಾಣುವಂತೆ", 
        "19794": "ಜಂಗಮಕ್ಕೆ ನೀಡಿದ ತೃಪ್ತಿ ಜಗಕೆಲ್ಲ ತೃಪ್ತಿಯಾಯಿತ್ತು ಕಾಣಿರೋ !", 
        "19795": "ಆಲಸ್ಯವೇತಕೋ ಲಿಂಗಪೂಜೆಮಾಡುವುದಕ್ಕೆ ?", 
        "19796": "ಇಂದು ನಾಳೆಯೆಂಬ ಮಂದಬುದ್ಧಿಗೆ ಸಂದುಗೊಡದಿರಣ್ಣಾ.", 
        "19797": "ಸಿರಿ ಬಂದೊದಗಿತ್ತೆಂದು ಹಿರಿದಾಗಿ ಹಿಗ್ಗಬೇಡ.", 
        "19798": "ಮಡದಿ ಮಕ್ಕಳು, ಪಡೆದ ದ್ರವ್ಯವು ಎನ್ನೊಡವೆ ಎಂದು", 
        "19799": "ಮಹೇಂದ್ರಜಾಲದಂತೆ ಕಣ್ಣಮುಂದೆ ಒಡ್ಡಿದ", 
        "19800": "ಸುರಚಾಪದಂತೆ ತೋರಿ ಅಡಗುವ ತನುವ ನೆಚ್ಚಬೇಡರೋ !", 
        "19801": "ಇಮ್ಮನ ಭಕ್ತಂಗೆ ಕರ್ಮದ ವಿಧಿ ಕಾಡುತ್ತಿರ್ಪುದು ನೋಡಾ !", 
        "19802": "ಅಂಜನಸಿದ್ಧಿಯ ಸಾಧಿಸುವ ಅಣ್ಣಗಳ ಕಣ್ಮನಕ್ಕೆ", 
        "19803": "ವಿದ್ಯೆಯ ಬಹಳ ಕಲಿತಡೇನು ಭಕ್ತಿಯಲ್ಲಿ ಶುದ್ಧನಲ್ಲದವನು.", 
        "19804": "ಗಾಳಿ ಬೀಸುವ ವೇಳೆಯಲ್ಲಿ ತೂರಿಕೊಳ್ಳಿರೋ ಬೇಗ ಬೇಗ !", 
        "19805": "ಮುನ್ನ ಪಡೆಯದೆ ಇನ್ನು ಬೇಡಿದರೆ ಬರ್ಪುದೆ", 
        "19806": "ಹಿಂದೆ ಗುರುಭಕ್ತಿ ಲಿಂಗಪೂಜೆ", 
        "19807": "ಹಿಂದಣ ಜನ್ಮದಲ್ಲಿ ಗುರುಲಿಂಗಜಂಗಮವ", 
        "19808": "ನುಡಿಯಲ್ಲಿ ಕರ್ಕಶವಿಲ್ಲದೆ ನಡೆಯಲ್ಲಿ ಬೀಸರವಾಗದೆ,", 
        "19809": "ಎನ್ನ ತನು ಎನ್ನ ಧನ ಎನ್ನ ಮನೆ", 
        "19810": "ಇದಿರಿನಲ್ಲಿ ಜಂಗಮವು ಬರುವುದು ಕಂಡು", 
        "19811": "ಸರ್ವಲೋಕೋಪಕಾರವಾಗಿ ಶಿವನೇ ಜಂಗಮವಾಗಿ", 
        "19812": "ಮನೆಗೆ ಬಂದ ಜಂಗಮವ ಕಂಡು", 
        "19813": "ಸದ್ಭಕ್ತನ ಸಹಜಸ್ವಭಾವದನಡೆ ಹೇಗಿರಬೇಕೆಂದಡೆ,", 
        "19814": "ಕೋಳಿ ಕೂಗುವುದು ಬೆಳಗಿನ ವೇಳೆಯನರಿದು.", 
        "19815": "ಲಿಂಗ ಜಂಗಮಕ್ಕೆ ಮಾಡಿದ ಭಕ್ತಿ", 
        "19816": "ಧರ್ಮಾರ್ಥವಾಗಿ ದೀಕ್ಷೆಯ ಮಾಡಬೇಕಲ್ಲದೆ,", 
        "19817": "ಲಿಂಗಾರ್ಚನೆಯ ಮಾಡಿದ ಬಳಿಕ", 
        "19818": "ಒಳಗೆ ಲಿಂಗದ ಕೂಟದಲ್ಲಿ ಮನವಡಗಿರಬೇಕು.", 
        "19819": "ಆದಿಯಲ್ಲಿಯು ನೀನೇ ದೇವನು ನಾನೇ ಭಕ್ತನಯ್ಯ ;", 
        "19820": "ನಿಮ್ಮ ತೊತ್ತಿನ ತೊತ್ತು ಪಡಿದೊತ್ತೆಂದು", 
        "19821": "ಕಾಮವುಳ್ಳಂಗೆ ಲಿಂಗದಪ್ರೇಮವಿನ್ನೆಲ್ಲಿಯದೊ ?", 
        "19822": "ಲಿಂಗಪ್ರೇಮವುಳ್ಳವಂಗೆ ಭವಬಂಧನವಿಲ್ಲ ;", 
        "19823": "ಲಿಂಗಪ್ರೇಮಿಗಳನಂತರುಂಟು ಜಗದೊಳಗೆ,", 
        "19824": "ಲಿಂಗಾರ್ಚನೆಯಿಂದ ಜಂಗಮಾರ್ಚನೆಯಧಿಕ ನೋಡಾ.", 
        "19825": "ಜಂಗಮವ ಕಂಡು ವಂದನೆಯ ಮಾಡಿದರೆ", 
        "19826": "ಜಂಗಮವೇ ಜಗದೀಶನೆಂದು ನಂಬದವನ", 
        "19827": "ಸಂಗನಬಸವಣ್ಣನಂತೆ ಸದ್‍ಭಕ್ತನೆಂದೆನಿಸಯ್ಯ ಎನ್ನ.", 
        "19828": "ಸ್ವರ್ಗಕ್ಕೆ ಏಳು ಸೋಪಾನವಿರ್ಪುದನು ಆರೂ ಅರಿಯರಲ್ಲ !", 
        "19829": "ಲಿಂಗಪೂಜಕನಾದಡೆ ಜಂಗಮವನರ್ಚಿಸಬೇಕು.", 
        "19830": "ಭಕ್ತಿಯ ಸ್ಥಳಕುಳವನರಿಯದೆ ಬರಿದೆ ಭಕ್ತರೆನಿಸಿಕೊಂಬ", 
        "19831": "ತನುವ ಗುರುವಿಂಗೆ ಕೊಟ್ಟು ಗುರುಭಕ್ತನಾಗಬೇಕು.", 
        "19832": "ಗುರುವಿನಲ್ಲಿ ಗುಣವನರಸಿದಡೆ  ಒಂದನೆಯ ಪಾತಕ.", 
        "19833": "ಗುರುವಿನಲ್ಲಿ ಭಕ್ತಿಯಿಲ್ಲ, ಲಿಂಗದಲ್ಲಿ ನಿಷ್ಠೆಯಿಲ್ಲ,", 
        "19834": "ಭಕ್ತನಾಗಬೇಕು ಭವವ ಹರಿದು.", 
        "19835": "ಸಟೆಯನಾಡದಾತ ಭಕ್ತ.", 
        "19836": "ಶಿವಭಕ್ತನೆನಿಸುವಾತಂಗೆ ಆವುದು ಚಿಹ್ನವೆಂದೊಡೆ :", 
        "19837": "ಗುರುವಿಂಗೆ ತನುವನರ್ಪಿಸಿದಲ್ಲದೆ", 
        "19838": "ಹರಹರಾ, ಎನ್ನ ಬಡಮನದ ಅಳಿಯಾಸೆಯ ನೋಡಾ !", 
        "19839": "ಶಿವಶಿವಾ, ನೀವೆನ್ನ ನಡೆಯೊಳಗೆ ನಡೆರೂಪವಾಗಿರ್ದು", 
        "19840": "ನೀನೊಲಿದಡೆ ಕಲ್ಲೆಲ್ಲ ಕನಕವಯ್ಯ.", 
        "19841": "ನೀನೊಲಿದಡೆ ಜಗವೆಲ್ಲ ಕೊಂಡಾಡುತಿರ್ಪುದು ನೋಡಾ ;", 
        "19842": "ಸತ್ಯನೆಂದೆನಿಸಯ್ಯ ಎನ್ನ ; ನಿತ್ಯನೆಂದೆನಿಸಯ್ಯ ಎನ್ನ ;", 
        "19843": "ಎನ್ನ ಕಂಗಳ ಮುಂದಣ ಕಾಮವ ಕಳೆದು", 
        "19844": "ಗುರುವ ನೋಡಿರೋ, ಲಿಂಗವ ಹಾಡಿರೋ,", 
        "19845": "ಭಕ್ತಿಗೆ ಮುಖವಾದಾತನೇ ಸದ್‍ಭಕ್ತನು.", 
        "19846": "ಗುರುಲಿಂಗಜಂಗಮದಲ್ಲಿ ಭಯ ಭಕ್ತಿ", 
        "19847": "ಸತ್ಕಾಯಕದಿಂದ ತಂದ ದ್ರವ್ಯಂಗಳ", 
        "19848": "ಸತ್ಯವಚನವ ನುಡಿವಾತನೇ ಸದ್‍ಭಕ್ತನು.", 
        "19849": "ನಾನಾದೇಶವ ತಿರುಗಿದಡಿಲ್ಲ.", 
        "19850": "ಭಕ್ತನಾದಡೆ ನಿರ್ವಂಚಕಭಾವದಿಂದೆ", 
        "19851": "ಭಕ್ತಿಯ ಮರ್ಮವನರಿಯೆ, ಜ್ಞಾನದ ಕುರುಹನರಿಯೆ,", 
        "19852": "ಶಿವಶಿವಾ, ಎನ್ನ ಮನದಾಳಾಪವನವಧರಿಸಯ್ಯ ಸ್ವಾಮಿ.", 
        "19853": "ಸೋಹಂ ಎಂದಡೆ ಅಂತರಂಗದ ಗರ್ವ;", 
        "19854": "ಸದ್‍ಭಕ್ತರೇ ಎನ್ನ ತಂದೆ ತಾಯಿಗಳಯ್ಯ.", 
        "19855": "ಸತ್ಯದ ಮನೆಯಲ್ಲಿ ಶಿವನಿರ್ಪನಲ್ಲದೆ", 
        "19856": "ಶ್ರೀಗುರುವೇ ತಂದೆ ತಾಯಿಗಳಲ್ಲದೆ,", 
        "19857": "ಗುರುಕರಜಾತನಾದೆನಾಗಿ,", 
        "19858": "ಬ್ರಾಹ್ಮಣನಾಗಲಿ ಕ್ಷತ್ರಿಯನಾಗಲಿ ವೈಶ್ಯನಾಗಲಿ ಶೂದ್ರನಾಗಲಿ", 
        "19859": "ಭಕ್ತನ ನಡೆ ಶುದ್ಧ , ಭಕ್ತನ ನುಡಿ ಶುದ್ಧ ,", 
        "19860": "ಭಕ್ತನಾದಡೆ ಲಿಂಗನಿಷ್ಠಾಪರನಾಗಿರಬೇಕು.", 
        "19861": "ಅರ್ಥ ಗುರುವಿನಲ್ಲಿ ಸವೆದು, ಪ್ರಾಣ ಲಿಂಗದಲ್ಲಿ ಸವೆದು,", 
        "19862": "ಮನದಲ್ಲಿ ನಂಬಿಗೆ, ಮಾತಿನಲ್ಲಿ ಕಿಂಕುರ್ವಾಣ ಬಿಡದೆ", 
        "19863": "ತನುವಂಚನೆಯಿಲ್ಲದೆ ಮಾಡುವಾತನೇ ಭಕ್ತ ;", 
        "19864": "ಮುಕ್ತಿಯ ಪಡೆವೆನೆಂದು ಯುಕ್ತಿಗೆಟ್ಟು", 
        "19865": "ಸ್ನೇಹ ಸಮರಸ ಮೋಹವಿರಬೇಕು ಗುರುವಿನಲ್ಲಿ.", 
        "19866": "ಹೊನ್ನೆನ್ನದು ಹೆಣ್ಣೆನ್ನದು ಮಣ್ಣೆನ್ನದು", 
        "19867": "ಹೊನ್ನಿನಾಶೆಯ ಮಾಡುವವನಲ್ಲ ಭಕ್ತ.", 
        "19868": "ಅಂಗ ಆಪ್ತ ಸ್ಥಾನ ಸದ್ಭಾವ ಎಂಬ", 
        "19869": "ಗುರುಸೇವೆಯಲ್ಲಿ ತನು ಸವೆದು,", 
        "19870": "ತನುವೆ ಗುರುವೆಂದರಿದ ಬಳಿಕ", 
        "19871": "ಹಿಂದಣಜನ್ಮದ ಸಂಸಾರವ ಮರೆದು,", 
        "19872": "ಉಪಾಧಿಯನಳಿದು, ನಿರುಪಾಧಿಯ ತಿಳಿದು,", 
        "19873": "ತನುವಿನ ಪ್ರಕೃತಿಯಳಿದು, ಮನದ ಮಾಯವಡಗಿ,", 
        "19874": "ತಾನೇ ಗುರುವಾಗಿ ಗುರುಭಕ್ತಿಯ ಮಾಡುತಿರ್ಪನು.", 
        "19875": "ಹಾಡಿದರೆ ಹಾಡುವೆನಯ್ಯ ಶಿವಶರಣರ ಮನವೊಲಿದು.", 
        "19876": "ಎನ್ನ ತನುವೆ ಶ್ರೀಗುರುಸ್ಥಾನವಯ್ಯ,", 
        "19877": "ಆದಿಯ ಭಕ್ತ  ಅನಾದಿಯ ಜಂಗಮ", 
        "19878": "ತನುವಿಲ್ಲದೆ ಮಾಡಿ,", 
        "19879": "ಮಾಡುವ ಭಕ್ತ  ನೀನೆಯೆಂದರಿದೆ ;", 
        "19880": "ತನುವ ನಿಮಗೆ ಸಮರ್ಪಿಸಿಹೆನೆಂದಡೆ ತನುವಿಲ್ಲವಯ್ಯ ಎನಗೆ.", 
        "19881": "ಭಾವವಿಲ್ಲದ ಬಯಲಮೂರ್ತಿಯಾದವ ಭಕ್ತ.", 
        "19882": "ಕಾಯವಿಲ್ಲದ ಭಕ್ತ, ಜೀವವಿಲ್ಲದ ಭಕ್ತ ,", 
        "19883": "ಮಾಡುವ ತನುವು ನೀವೇ ಆದಿರಿ.", 
        "19884": "ಪರಸ್ತ್ರೀಯರ ಮುಟ್ಟದಿರಬೇಕು.", 
        "19885": "ಹುಸಿ ಕಳವು ಪರದಾರ ಪರಹಿಂಸೆಯ ಬಿಟ್ಟು", 
        "19886": "ನೈಷ್ಠಿಕಭಾವ ನಂಬುಗೆ ಇಲ್ಲದ ಬಳಿಕ,", 
        "19887": "ನೈಷ್ಠೆಯೆಂಬುದು ತನುವಿನ ಪ್ರಕೃತಿಯ ಕೆಡಿಸುವುದು.", 
        "19888": "ನೈಷ್ಠೆನೆಲೆಗೊಳ್ಳಬೇಕು ಗುರುಭಕ್ತಿಯ ಮಾಡುವಲ್ಲಿ.", 
        "19889": "ಛಲವಿರಬೇಕು ಶಿವಭಕ್ತಿಯ ಮಾಡುವಲ್ಲಿ ಹಿಡಿದುಬಿಡೆನೆಂಬ.", 
        "19890": "ಗುರುಭಕ್ತಿಯ ಮಾಡಿದ ಬಳಿಕ", 
        "19891": "ಫಲಪದವಿಯ ಬಯಸಿ ಮಾಡಲಾಗದು ಗುರುಭಕ್ತಿಯ.", 
        "19892": "ನಿತ್ಯ ಗುರುಲಿಂಗಜಂಗಮಕ್ಕೆ ಪೂಜೆಯ ಮಾಡಿ", 
        "19893": "ಮೊಟ್ಟೆ ಮೊಟ್ಟೆ ಪತ್ರಿ ಪುಷ್ಪವ ತಂದು", 
        "19894": "ಶಿವಶಿವಾ, ಎನ್ನ ಮನವು ನಿಮ್ಮ ನೆನೆಯಲೊಲ್ಲದೆ", 
        "19895": "ಹೊರಗೆ ಹೊನ್ನ ಬಿಟ್ಟೆನೆಂದು ನುಡಿವುತಿರ್ಪೆನಲ್ಲದೆ", 
        "19896": "ಬಾಳೆಯ ಎಲೆಯ ಮೇಲೆ ತುಪ್ಪವ ತೊಡೆದಂತೆ", 
        "19897": "ದುರ್ಗುಣಿ ದುರಾಚಾರಿಯಯ್ಯ ನಾನು.", 
        "19898": "ಆಸೆವಿಡಿದು ಹಲವು ದೇಶಕ್ಕೆ ಹರಿದಾಡಿದೆನಲ್ಲದೆ", 
        "19899": "ಮನದಲ್ಲಿ ಆಸೆ ಮೊಳೆದೋರಿ ಮತ್ತೆ ಮತ್ತೆ ಬೇಕೆಂಬಲ್ಲಿ", 
        "19900": "ಎನ್ನ ತನುವು ನಿಮ್ಮ ಚರಣವ ಪೂಜಿಸಲೊಲ್ಲದೆ", 
        "19901": "ತನುವ ನಿಮಗೊಪ್ಪಿಸಿ ತನುಶುದ್ಧನಾಗಲರಿಯೆನಯ್ಯ ನಾನು.", 
        "19902": "ತನುವ ಕೊಟ್ಟು ನಿಮ್ಮನೊಲಿಸಿಹೆನೆಂದಡೆ,", 
        "19903": "ಸತ್ತು  ಹುಟ್ಟುವನಲ್ಲ ನೋಡಾ ಮಹೇಶ್ವರನು.", 
        "19904": "ನುಡಿಯಲ್ಲಿ ಎರಡು ನುಡಿಯನು; ನಡೆಯಲ್ಲಿ ಎರಡು ನಡೆಯನು;", 
        "19905": "ಹಿಡಿದುದ ಬಿಡುವವನಲ್ಲ ; ಬಿಟ್ಟುದ ಹಿಡಿವವನಲ್ಲ.", 
        "19906": "ಪರಧನ ಪರಸ್ತ್ರೀಯರ ಬಿಟ್ಟಡೆ", 
        "19907": "ಮಾತಿನಲ್ಲಿ ಕರ್ಕಶ, ಮನದಲ್ಲಿ ಘಾತಕತನವುಳ್ಳವನ", 
        "19908": "ತಾ ಒಳ್ಳೆಯವನಾದಡೆ ಸರ್ವರೂ ತನಗೆ", 
        "19909": "ತೊತ್ತಿಂಗೆ ಒಡತಿಯ ಬಲವಯ್ಯ.", 
        "19910": "ಕಾಯವ ದಂಡಿಸಿ, ಕಂಡ ಕಂಡ ಕ್ಷೇತ್ರಂಗಳಿಗೆ ಹೋಗಿ", 
        "19911": "ಕನಕ ಕಾಮಿನಿ ಭೂಮಿಗಾಗಿ ಹೊಡೆದಾಡಿ ಕೆಟ್ಟಿತೀ ಜಗವೆಲ್ಲ.", 
        "19912": "ಅಂದೊಬ್ಬ ದೇವ, ಇಂದೊಬ್ಬ ದೇವನೆಂದು", 
        "19913": "ನೋಡಬಾರದು ಪರಸತಿಯರ.", 
        "19914": "ನೋಟ ಭಂಗ ಪರಸತಿಯರ.", 
        "19915": "ಇಂದ್ರಿಯಂಗಳಿಚ್ಛೆಯಲ್ಲಿ ಇರಿಸದಿರಯ್ಯ ಎನ್ನ.", 
        "19916": "ಗುರುಹರ ವಚನ ಪ್ರಮಾಣದಿಂದೆ ಹಿಡಿದ", 
        "19917": "ಎನ್ನ ಜನನಮರಣಂಗಳೆಲ್ಲ ಜಾರಿಹೋದವಯ್ಯ", 
        "19918": "ಎನ್ನ ಕರಕಮಲಮಧ್ಯದಲ್ಲಿ ಪರಮ ಶಿವಲಿಂಗವ ತುಂಬಿ,", 
        "19919": "ಪ್ರಾಣವು ಲಿಂಗವ ನುಂಗಿತ್ತೋ,", 
        "19920": "ಘನತರದಿಷ್ಟಲಿಂಗದಲ್ಲಿ ಅನಿಮಿಷದೃಷ್ಟಿ ಬಲಿದು,", 
        "19921": "ಬೇಕೆಂಬನಲ್ಲವಯ್ಯ ನಿಮ್ಮ ಶರಣ.", 
        "19922": "ನಿಮ್ಮ ನೋಡಿ ನೋಡಿ ಎನ್ನ ಕಂಗಳು ದಣಿಯವಯ್ಯ.", 
        "19923": "ಎನ್ನ ಕಂಗಳು ನಿಮ್ಮ ನೋಡಲಿಚ್ಛಿಸುತಿರ್ಪುವು.", 
        "19924": "ನಾನಾ ವರ್ಣದ ಕಾಷ್ಠವ ಸುಟ್ಟಲ್ಲಿ", 
        "19925": "ನೀರು ಗಟ್ಟಿಗೊಂಡು ಮುತ್ತಪ್ಪುದಲ್ಲದೆ ಮುತ್ತು ನೀರಪ್ಪುದೇ ಅಯ್ಯ ?", 
        "19926": "ಜಾತಿಪೂರ್ವಾಶ್ರಯವ ಕಳೆದು, ಸೂತಕ ಪಾತಕಂಗಳನಳಿದು,", 
        "19927": "ತನು ಲಿಂಗವಾದ ಶರಣಂಗೆ ತಾಪತ್ರಯಂಗಳಿಲ್ಲ.", 
        "19928": "ಎನ್ನ ಜನನ ಸೂತಕ ಹೋಯಿತ್ತು", 
        "19929": "ಆಚಾರ ವಿಚಾರವೆಂದರಿಯರು.", 
        "19930": "ನುಡಿಯಲಾಗದು ನುಡಿಯಲಾಗದು", 
        "19931": "ನುಡಿಯಬೇಕು ಸತ್ಯಸದಾಚಾರವುಳ್ಳವರೊಡನೆ", 
        "19932": "ಇಷ್ಟಲಿಂಗದಲ್ಲಿ ನೈಷ್ಠೆ ಬಲಿಯರು.", 
        "19933": "ಇಷ್ಟಲಿಂಗದ ನಿಜವನರಿಯದೆ ಪ್ರಾಣಲಿಂಗದ ಮಾತನುಡಿವ", 
        "19934": "ಇಷ್ಟಲಿಂಗ ಪ್ರಾಣಲಿಂಗ ಒಂದೆಯೆಂದರಿಯದೆ", 
        "19935": "ಕಾಷ್ಠದಲ್ಲಿ ಅಗ್ನಿ ಉಂಟೆಂದಡೆ,", 
        "19936": "ಪರಾತ್ಪರವಾದ ಪರಶಿವಬ್ರಹ್ಮವನೊಡಗೂಡುವ", 
        "19937": "ಕ್ರಿಯವೇ ಅಧಿಕವೆಂಬ", 
        "19938": "ಶೈವಸಿದ್ಧಾಂತಿಗಳೆಂಬ ಅಬದ್ಧ ಭವಿಗಳ", 
        "19939": "ಷಡೀಂದ್ರಿಯ ಸಪ್ತಧಾತುಗಳಲ್ಲಿ", 
        "19940": "ಜಗದೊಳಹೊರಗೆಲ್ಲ ತೆರಹಿಲ್ಲದೆ", 
        "19941": "ಕುರುಹು ಉಂಟೇ ಮರುಳೆ ಲಿಂಗಕ್ಕೆ ?", 
        "19942": "ಅಷ್ಟಮೂರ್ತಿಗಳು ದೇವರೆಂಬ", 
        "19943": "ಪೃಥ್ವಿ ದೇವರೆಂಬೆನೆ ಪೃಥ್ವಿ ದೇವರಲ್ಲ.", 
        "19944": "ಶರಣಭರಿತ ಶಿವನು ಶಿವಭರಿತ ಶರಣನೆಂಬುದು ನಿಜವಲ್ಲದೆ,", 
        "19945": "ಸರ್ವಗತ ಶಿವನೆಂದು ಹೇಳುವ", 
        "19946": "ಪೃಥ್ವಿ ಆಕಾಶಮಧ್ಯದಲ್ಲಿ ಉತ್ಪತ್ತಿಯಾದ", 
        "19947": "ಸರ್ವಜಗದಲ್ಲಿ ಶಿವನಿಲ್ಲವೆಂದಡೆ", 
        "19948": "ಜಗವಾಗಬಲ್ಲ ನೋಡಿರೊ ನಮ್ಮ ಶಿವನು.", 
        "19949": "ಬ್ರಹ್ಮ ದೇವನಾದಡೆ", 
        "19950": "ಗುರುಭಕ್ತರಾದವರು", 
        "19951": "ಹರಿ ಹರಗೆ ಸರಿಯೆಂಬ", 
        "19952": "ಇಕ್ಕಿದೆನು ಮುಂಡಿಗೆಯ", 
        "19953": "ಇಕ್ಕಿದೆನು ಮುಂಡಿಗೆಯ ದೇವರದೇವನೆಂದು.", 
        "19954": "ಅಜಹರಿಸುರರೆಲ್ಲ ಆವ ದೇವನ ಶ್ರೀಚರಣವನರ್ಚಿಸಿ", 
        "19955": "ಕಟ್ಟಿದೆನು ಬಿರಿದು ಶಿವನಲ್ಲದೆ ಬೇರೆದೇವರಿಲ್ಲವೆಂದು.", 
        "19956": "ವೇದಂಗಳು ನಿಮ್ಮ ಭೇದಿಸಲರಿಯವು ನೋಡಾ !", 
        "19957": "ಎನ್ನ ನಡೆನುಡಿ ಚೈತನ್ಯವೇ ನೀವೆಂದರಿದೆ.", 
        "19958": "ಒಮ್ಮೆ ಜ್ಞಾನಿಯೆನಿಸಿ, ಒಮ್ಮೆ ಅಜ್ಞಾನಿಯೆನಿಸಿ,", 
        "19959": "ಸುಖ ಬಂದಲ್ಲಿ ನಿಮ್ಮ ಹಾಡಿಹರಸುವೆನಯ್ಯ.", 
        "19960": "ತಂದೆ ನೀನೆ ಅಯ್ಯ ಎನಗೆ,", 
        "19961": "ದರ್ಪಣದೊಳಗಣ ರೂಹಿಗೆ ಚೇಷ್ಟಾಭಾವ ಉಂಟೆಂದಡೆ", 
        "19962": "ಆಚಾರಲಿಂಗವಾಗಿ ಬಂದೆನ್ನ", 
        "19963": "ಅರಿದರಿದು ಗುರುಭಕ್ತಿಯು ನೆಲೆಗೊಂಬುದು.", 
        "19964": "ಗುರುಭಕ್ತನಾದ ಬಳಿಕ ಗುರುವಿಂಗೆ", 
        "19965": "ಜಂಗಮದಲ್ಲಿ ರೂಪು ಕುರೂಪವ ನೋಡಲಾಗದು.", 
        "19966": "ಜಂಗಮದ ಘನವನು, ಜಂಗಮದ ನಿಲವನು", 
        "19967": "ಜಂಗಮಕ್ಕೆ ಮಾತಾಪಿತರಿಲ್ಲ.", 
        "19968": "ಸ್ಥಾವರವು ಜಂಗಮವು ಒಂದೆ ಎಂಬಿರಿ,", 
        "19969": "ಲಿಂಗಕ್ಕೆ ಅರ್ಪಿಸಿದ ಪದಾರ್ಥವು ಕರ್ತವ್ಯವಲ್ಲ,", 
        "19970": "ಲಿಂಗಭಕ್ತನಾದ ಬಳಿಕ,", 
        "19971": "ಶ್ರೀಗುರುಸ್ವಾಮಿ ಕರುಣಿಸಿಕೊಟ್ಟ ಇಷ್ಟಲಿಂಗವನು", 
        "19972": "ಸದ್ಭಕ್ತರಾದವರು ಸ್ಥಾವರಲಿಂಗದ ಭಜನೆಯ ಮಾಡಲಾಗದು.", 
        "19973": "ಪರದೈವವ ಪೂಜಿಸುವಾತ ಸದ್‍ಭಕ್ತನೆ ?", 
        "19974": "ಅನಂತಕೋಟಿ ಹೀನ ಭವಿಜನ್ಮವ ನೀಗಿ ಶಿವಜನ್ಮಕ್ಕೆ ಬಂದು,", 
        "19975": "ಮಾಡಲಾಗದು ಭವಿಯ ಸಂಪರ್ಕವ.", 
        "19976": "ನೆರೆಹೊಲ್ಲ ದುರಾಚಾರಿಗಳ, ನೆರೆಹೊಲ್ಲ ಜಾರಚೋರರ,", 
        "19977": "ಮಾಡಬಾರದು ಮಾಡಬಾರದು", 
        "19978": "ಒಬ್ಬರೊಬ್ಬರ ಸಂಗವ ಬಯಸುತಿರ್ಪವರು ನೀವು ಕೇಳಿರೋ !", 
        "19979": "ಸಂಗ ಲೇಸು ಸಾಧುಮಹಾಪುರುಷರ.", 
        "19980": "ತನ್ನ ತಾನರಿಯದೆ ಅನ್ಯರ ಗುಣಾವಗುಣಗಳ", 
        "19981": "ತನ್ನ ಎಡೆಯಲ್ಲಿ ಕತ್ತೆ ಸತ್ತುಬಿದ್ದುದನರಿಯದೆ", 
        "19982": "ತಮ್ಮೊಳಗೆ ತಾವು ತಿಳಿಯಲರಿಯರು,", 
        "19983": "ಸಲ್ಲರು ಸಲ್ಲರಯ್ಯ ಶಿವಪಥಕ್ಕೆ", 
        "19984": "ದಯವಿರಬೇಕು ಸಕಲಪ್ರಾಣಿಗಳಲ್ಲಿ,", 
        "19985": "ಬಲ್ಲವರು ಬೆಸಗೊಂಡಡೆ ಸೊಲ್ಲನಾರೈದು", 
        "19986": "ಬಲ್ಲರೆ ಬಲ್ಲರೆಂಬೆನು, ಲಿಂಗಜಂಗಮ", 
        "19987": "ಅಲ್ಲದುದ ಹಿಡಿವನಲ್ಲ ಶರಣ.", 
        "19988": "ಸತ್ಯವಚನವ ನುಡಿಯಬಲ್ಲರೆ ಶರಣನೆಂಬೆನು.", 
        "19989": "ಹಾಲ ಹರವಿಯ ಮೇಲೆ ಮಜ್ಜಿಗೆಯ ತುಂಬಿರಿಸಿದಡೇನು", 
        "19990": "ಕ್ರಿಯೆಯಲ್ಲಿ ಬಲ್ಲಿದರು, ಜ್ಞಾನದಲ್ಲಿ ಸಂಪನ್ನರು", 
        "19991": "ಕಟ್ಟಿದ ಬುತ್ತಿ ಎಷ್ಟು ದಿನ ಈಡೇರಲಾಪುದು.", 
        "19992": "ಒಲ್ಲನು ಒಲ್ಲನಯ್ಯ ಶಿವಭಕ್ತಿ ಇಲ್ಲದವರ.", 
        "19993": "ಎಚ್ಚರವಿರಬೇಕು ನಡೆನುಡಿಯಲ್ಲಿ.", 
        "19994": "ಇಂದು ಎನ್ನ ಮನೆಯಲ್ಲಿ ದೇವರಹಬ್ಬ", 
        "19995": "ಹೊನ್ನಿನಾಸೆ ಉಳ್ಳನ್ನಕ್ಕರ ಸಂಚಿತಕರ್ಮ ಬಿಡದು.", 
        "19996": "ಹೊನ್ನು ಬ್ರಹ್ಮನ ಹಂಗು, ಹೆಣ್ಣು ವಿಷ್ಣುವಿನ ಹಂಗು,", 
        "19997": "ಶಿವ ಶಿವಾ ಮಹಾಪ್ರಸಾದ", 
        "19998": "ಕುಚಿತ್ತವಳಿದು ಸುಚಿತ್ತ ನೆಲೆಗೊಂಡು", 
        "19999": "ಗುರುವಿಂಗಾದಡೂ ಅರುಹು ಆಚಾರ ಸತ್ಕ್ರಿಯವೇ ಬೇಕು.", 
        "20000": "ಆಣವಮಲ ಮಾಯಾಮಲ ಕಾರ್ಮಿಕಮಲವೆಂಬ", 
        "20001": "ಇಷ್ಟಲಿಂಗದಲ್ಲಿ ತನುವ ಸವೆಯದೆ,", 
        "20002": "ಜಡೆ ಬೋಳು ಲೋಚು ದಿಗಂಬರವನು", 
        "20003": "ಷಡುವರ್ಗಂಗಳ ಸಡಲಿಸಿ ಒಡಲುಪಾಧಿಯನರುಹಿ,", 
        "20004": "ಒಳಗೆ ಬೋಳಾಗಲರಿಯದೆ ಹೊರವೇಷದ ಬೋಳಿನಲ್ಲಿ", 
        "20005": "ವಿರಕ್ತ ವಿರಕ್ತರೆಂದು ನುಡಿದುಕೊಂಬ ಅಣ್ಣಗಳಿರಾ.", 
        "20006": "ನಡೆವ ಗತಿಯಲ್ಲಿ ಲಿಂಗದ ನಡೆಯ ತುಂಬಿ", 
        "20007": "ಈಶನ ಮರೆಯಲ್ಲಿ ವೇಷವ ತೊಟ್ಟಾಡುವರೆಲ್ಲ", 
        "20008": "ಆದಿಯಾಧಾರವಿಲ್ಲದ ಮುನ್ನ,", 
        "20009": "ಜಂಗಮಲಿಂಗವೇ ನಮೋ ನಮೋ.", 
        "20010": "ಎನ್ನ ಗರ್ವ ಅಹಂಕಾರವ ಕೆಡಿಸಯ್ಯ", 
        "20011": "ಶಿವ ಶಿವಾ, ನೀ ನಡೆಸಿದಂತೆ ನಡೆವೆ.", 
        "20012": "ಮಡುವಿನಗ್ಗಣಿ, ಗಿಡದ ಹೂವು, ಒಡಲಿಗೆ ಅನ್ನ,", 
        "20013": "ನಿತ್ಯಲಿಂಗಾರ್ಚನೆಯ ಮಾಡುತ್ತ ಸದ್‍ಭಕ್ತರನರಸುತ್ತ", 
        "20014": "ಒಲ್ಲೆನಯ್ಯ ಜಗದ ಹಂಗಿನ ಭೋಗೋಪಭೋಗವನು.", 
        "20015": "ಹಾಡುವೆನಯ್ಯ ನಿಮ್ಮ ,", 
        "20016": "ನಿತ್ಯ ಲಿಂಗಾರ್ಚನೆಯ ಮಾಡುವೆ.", 
        "20017": "ಅಂಜುವೆನಂಜುವೆನಯ್ಯ ಅಡಿಗಡಿಗೆ", 
        "20018": "ಜಗವೆಲ್ಲ ಮುನಿದಡಂಜೆ.", 
        "20019": "ಮನದೊಡೆಯ ಮನವ ನೋಡುವೆನೆಂದು", 
        "20020": "ವೀರಶೈವ ಭಕ್ತಮಾಹೇಶ್ವರರು", 
        "20021": "ಗಂಡಸತ್ತನೆಂದು ಗಂಡನೊಡನೆ ಕೆಂಡವ ಬೀಳುವೆನೆಂದು", 
        "20022": "ನಡೆಯೊಳಗೆ ನುಡಿತುಂಬಿ,", 
        "20023": "ನಡೆದು ತಪ್ಪುವರಲ್ಲ ಶರಣರು.", 
        "20024": "ನುಡಿದಂತೆ ನಡೆ ಇಲ್ಲವಯ್ಯ ಎನ್ನಲ್ಲಿ.", 
        "20025": "ಎನ್ನ ಮಾನಾಪಮಾನವೆಲ್ಲ ನಿಮ್ಮದಯ್ಯ.", 
        "20026": "ಶಿವಶಿವಾ ಮಹಾಪ್ರಸಾದ", 
        "20027": "ಹರಹರಾ ಎಂದು ಹರನೊಳಗೈಕ್ಯರಾದ", 
        "20028": "ಅಂತರಂಗದಲ್ಲಿ ಅಖಂಡ ಪರಿಪೂರ್ಣ", 
        "20029": "ಸೃಷ್ಟಿ ಸ್ಥಿತಿ ಪ್ರಳಯಂಗಳ ನೇಮವ ವಿೂರಿ,", 
        "20030": "ಎಲೆ ಶಿವನೆ, ನೀವು ಭಕ್ತದೇಹಿಕದೇವನೆಂಬುದು", 
        "20031": "ಹತ್ತು ದಿಕ್ಕಿನ ಒಳಹೊರಗೆ ಸುತ್ತಿ ಸುಳಿದಾಡುವ", 
        "20032": "ಮನವು-ಘನವ ನುಂಗಿ, ಘನವು ಮನವ ನುಂಗಿ,", 
        "20033": "ಮಾಡಿದ ಮಾಟವ ಮರೆದೆನಯ್ಯ ನಿಮ್ಮೊಳಗೆ.", 
        "20034": "ಆಳವಿಲ್ಲದ ನಿರಾಳನಾಟವನೇನೆಂಬೆ,", 
        "20035": "ಲಿಂಗಕ್ಕೆ ಸಮರ್ಪಿಸಿದ ಸಕಲದ್ರವ್ಯಂಗಳನು", 
        "20036": "ಶಿವಶಿವಾ ಮಹಾಪ್ರಸಾದದ ಮಹಿಮೆಯನೇನೆಂಬೆನಯ್ಯ ?", 
        "20037": "ನಂಬಿರೊ ನಂಬಿರೊ ಶಿವನ ಪಾದತೀರ್ಥದ", 
        "20038": "ಸಪ್ತಸಾಗರದಷ್ಟು ಪಾದೋದಕವ", 
        "20039": "ಶಿವಪಾದೋದಕವ ನಂಬಿ ಕೊಂಡಡೆ", 
        "20040": "ಫಲಪದ ಮುಕ್ತಿಯ ಬಯಸಿ ಯುಕ್ತಿಗೆಟ್ಟು", 
        "20041": "ತ್ರಿಕರಣಂಗಳು ಶುದ್ಧನಾಗಿ", 
        "20042": "ಗುರುಪ್ರಸಾದಿಯಾದಡೆ,", 
        "20043": "ಗುರುಪ್ರಸಾದಿಯಾದಡೆ,", 
        "20044": "ಗುರುಪ್ರಸಾದವ ಕೊಂಬ ನೇಮವಿರ್ದ ಬಳಿಕ,", 
        "20045": "ನೇಮವಿರಬೇಕು ಗುರುಪ್ರಸಾದವಲ್ಲದೆ", 
        "20046": "ಕಾಯದ ಕೈಮುಟ್ಟಿ ಇಷ್ಟಲಿಂಗಕ್ಕೆ ರೂಪವನರ್ಪಿಸಿದಲ್ಲಿ", 
        "20047": "ಒಂದು ವೇಳೆ ಲಿಂಗಕ್ಕೆ ಕೊಟ್ಟು", 
        "20048": "ಗುರುಪ್ರಸಾದವನರಿಯದವಂಗೆ ಲಿಂಗಪ್ರಸಾದವಿಲ್ಲ.", 
        "20049": "ಒಳಗೆ ತಿಳಿಯದೆ ಹೊರಗೆ ಮಾಡುವ ಮಾಟವೆಲ್ಲ", 
        "20050": "ಆಧಾರದಲ್ಲಿ ಗುರುಸಂಬಂಧವು.", 
        "20051": "ಶಿವನೇ ಗುರುವೆಂದು ತಿಳಿದು", 
        "20052": "ವಿಶ್ವಾಸನೋಟ ಜಾರಿದಡೆ ಜಾರುವನಯ್ಯಾ ಶ್ರೀಗುರುವು.", 
        "20053": "ಶುದ್ಧ ಸುಯಿಧಾನ ಭಯ ವಿಶ್ವಾಸವಿರಬೇಕು ಗುರುವಿನಲ್ಲಿ.", 
        "20054": "ಶಿವಶಿವಾ ಪ್ರಸಾದಭಕ್ತರ ತೋರಿಸಯ್ಯಾ.", 
        "20055": "ಭಕ್ತನಾದಡೆ ಚಿತ್ತ ನಿಶ್ಚಲವಾಗಿ ಸದ್ಭಕ್ತಿ ನೆಲೆಗೊಂಡಿರಬೇಕು.", 
        "20056": "ಧರ್ಮ ಅರ್ಥ ಕಾಮ ಮೋಕ್ಷವೆಂಬ", 
        "20057": "ಜೀವಭಾವದಿಂದೆ ಜೀವನ ತಿಂದು", 
        "20058": "ಪರಿಪಾಕವಾದ ಸಕಲಪದಾರ್ಥಂಗಳ ಹಸ್ತಪರುಷದಿಂದೆ", 
        "20059": "ಇಷ್ಟಲಿಂಗಕ್ಕೆ ರೂಪುಪದಾರ್ಥವನರ್ಪಿಸಿ,", 
        "20060": "ಪ್ರಸಾದಿ ಪ್ರಸಾದಿಗಳೆಂದು ನುಡಿದುಕೊಂಬ", 
        "20061": "ಸಿಂಹದ ಮೊಲೆವಾಲು ಸಿಂಹದ ಮರಿಗಲ್ಲದೆ,", 
        "20062": "ಗುರುಪ್ರಸಾದಿಯಾದಡೆ", 
        "20063": "ಶಿವಪ್ರಸಾದವನಾರೋಗಣೆಯ ಮಾಡುವಲ್ಲಿ", 
        "20064": "ದೇವರದೇವ ಮಹಾಪ್ರಸಾದ ತ್ರಿಕರಣಶುದ್ಧವಾಗಿ", 
        "20065": "ಲಿಂಗವನರಿಯದ ಅಂಗವಿಕಾರಿಗೇತಕೋ ಶಿವಪ್ರಸಾದ ?", 
        "20066": "ಜಂಗಮದ ದರ್ಶನ ಸ್ಪರ್ಶನದಿಂದೆ", 
        "20067": "ಜಂಗಮ ಜಂಗಮವೆಂದು ನುಡಿಯುತಿರ್ಪರೆಲ್ಲರು,", 
        "20068": "ಗುರುಪ್ರಸಾದವ ಕೊಂಡು ಎನ್ನ ತನು", 
        "20069": "ಹರಿವಿರಂಚಿಗಳಿಗೆ ನಿಲುಕದ ಪರಬ್ರಹ್ಮಪ್ರಸಾದ.", 
        "20070": "ಗುರುವೆ ಲಿಂಗವೆಂದರಿದೆನಾಗಿ, ಲಿಂಗದಲ್ಲಿ ನಿಲವ ಕಂಡೆ.", 
        "20071": "ಶಿವಂಗೆ ಐದುಮುಖವಿರ್ಪುದ ಸಕಲರು ಬಲ್ಲರು.", 
        "20072": "ಒಮ್ಮೆ ನೀ ದೇವನಾದಲ್ಲಿ ನಾ ಭಕ್ತನಾಗಿರ್ಪೆನು.", 
        "20073": "ಗುರುಭಕ್ತಿಯ ಮಾಡಿದರೆ ಮಾಡಬಹುದು ;", 
        "20074": "ಪ್ರಸಾದಿಯೊಳಗಣ ಭಕ್ತಸ್ಥಲವನಿಂಬುಗೊಂಡರು", 
        "20075": "ಪ್ರಸಾದಿಯಾದಡೆ ಚೇಳಿಯಕ್ಕನಂತಿರಬೇಕು.", 
        "20076": "ಮಾದಾರ ಚನ್ನಯ್ಯನ ಬಾಯ ತಾಂಬೂಲವ ಮೆಲುವೆ.", 
        "20077": "ತನುವಿನಲ್ಲಿ ಗುರುಭಕ್ತಿಯಿಂಬುಗೊಂಡು,", 
        "20078": "ಕ್ರಿಯಾದೀಕ್ಷೆಯಿಂದೆ ಇಷ್ಟಲಿಂಗದಲ್ಲಿ", 
        "20079": "ಶ್ರೀಗುರುವಿನಿಂದಧಿಕರು ಆವ ಲೋಕದೊಳಗಿಲ್ಲವಯ್ಯಾ.", 
        "20080": "ತಾರಕಾಕೃತಿ, ನಕಾರಪ್ರಣಮ, ಆಧಾರಚಕ್ರ,", 
        "20081": "ಕೇಳುವ ಸಂಗೀತ, ನೋಡುವ ಸುರೂಪುಗಳ", 
        "20082": "ಪೃಥ್ವಿಯೆ ಅಂಗವಾದ ಭಕ್ತಂಗೆ ಚಿತ್ತವೆ ಹಸ್ತ.", 
        "20083": "ಅಪ್ಪುವೆ ಅಂಗವಾದ ಮಹೇಶ್ವರಂಗೆ ಬುದ್ಧಿಯೆ ಹಸ್ತ.", 
        "20084": "ಅಗ್ನಿಯೆ ಅಂಗವಾದ ಪ್ರಸಾದಿಗೆ ನಿರಹಂಕಾರವೆ ಹಸ್ತ.", 
        "20085": "ಪವನವೆ ಅಂಗವಾದ ಪ್ರಾಣಲಿಂಗಿಗೆ ಸುಮನವೆ ಹಸ್ತ.", 
        "20086": "ಗಗನವೆ ಅಂಗವಾದ ಶರಣಂಗೆ ಸುಜ್ಞಾನವೆ ಹಸ್ತ.", 
        "20087": "ಆತ್ಮವೆ ಅಂಗವಾದ ಐಕ್ಯಂಗೆ ಸದ್‍ಭಾವನೆ ಹಸ್ತ.", 
        "20088": "ಪೃಥ್ವಿಯೆಂಬ ಭೂತಾಂಗದ ಪ್ರಕೃತಿವಿಕಾರದ ಗುಣವಳಿದು,", 
        "20089": "ಎನ್ನ ತನುವಿನೊಳಗೆ ನಿಮ್ಮ ತನುವಡಗಿರ್ಪುದು ;", 
        "20090": "ಎನ್ನ ಚಿತ್ತ ಸುಚಿತ್ತವಾಯಿತ್ತಯ್ಯಾ.", 
        "20091": "ಹಾಡಿದರೆ ಹಾಡುವೆನಯ್ಯಾ ನಿಮ್ಮ ಪರಮಪ್ರಸಾದವನೆ.", 
        "20092": "ಆದಿಪ್ರಸಾದ, ಅನಾದಿಪ್ರಸಾದ,", 
        "20093": "ನಿಮ್ಮ ಪ್ರಸಾದವೆನಗೆ ತನುವಾಯಿತ್ತು.", 
        "20094": "ಎನ್ನ ನೇತ್ರವು ನಿಮ್ಮ ಪ್ರಸಾದವನೆ ನೋಡುತಿರ್ಪುದು.", 
        "20095": "ಕರಸ್ಥಲದ ಲಿಂಗವನು ಮನಸ್ಥಲದಲ್ಲಿ ಕುಳ್ಳಿರಿಸಿ,", 
        "20096": "ಲಿಂಗಘ್ರಾಣದಲ್ಲಿ ಲಿಂಗಕ್ಕೆ ಲಿಂಗಗಂಧವನರ್ಪಿಸಿ,", 
        "20097": "ಎನ್ನ ಘ್ರಾಣದ ಪರಿಣಾಮ ನಿಮಗರ್ಪಿತವಯ್ಯಾ.", 
        "20098": "ಎನ್ನ ಘ್ರಾಣದ ಕೊನೆಯಲ್ಲಿ ಕುಳ್ಳಿರ್ದು", 
        "20099": "ಎನ್ನ ಘ್ರಾಣ ಗಂಧವ ಗ್ರಹಿಸುವುದಕ್ಕೆ ಮೊದಲೆ", 
        "20100": "ಎನ್ನ ಘ್ರಾಣ ಸಮರಸವಾಯಿತ್ತಯ್ಯಾ", 
        "20101": "ಎನ್ನ ತನು ನಿಮ್ಮ ಪ್ರಸಾದವನೆ ಸೋಂಕುತಿರ್ಪುದಯ್ಯಾ.", 
        "20102": "ಎನ್ನ ನೇತ್ರ ಪ್ರಸಾದವಾಯಿತ್ತಯ್ಯಾ.", 
        "20103": "ಎನ್ನ ಆಧಿವ್ಯಾಧಿಗಳೆಲ್ಲ ಪ್ರಸಾದವಯ್ಯಾ.", 
        "20104": "ಪೃಥ್ವಿಯಾಕಾಶದೊಳಗೆಲ್ಲ", 
        "20105": "ಕಾಯವಿಲ್ಲದ ಪುರುಷನು ಕಸವಿಲ್ಲದ ಭೂಮಿಯಲ್ಲಿ", 
        "20106": "ಎನ್ನ ತನು ಅಡಗುವುದಕ್ಕೆ", 
        "20107": "ಕಾರ್ಯವಿಲ್ಲದ ಪ್ರಸಾದ, ಕಾರಣವಿಲ್ಲದ ಪ್ರಸಾದ,", 
        "20108": "ಭಯವಿಲ್ಲದ ಪ್ರಸಾದ, ನಿರ್ಭಯವಿಲ್ಲದ ಪ್ರಸಾದ,", 
        "20109": "ಅಡಿಮುಡಿಯಿಲ್ಲದ ಪ್ರಸಾದ, ನಡುಕಡೆಯಿಲ್ಲದ ಪ್ರಸಾದ,", 
        "20110": "ಪಂಚಪ್ರಾಣವಾಯುಗಳ ಸಂಚಲಗುಣವನಳಿದಿರಬೇಕು.", 
        "20111": "ಪ್ರಾಣಲಿಂಗ ಪ್ರಾಣಲಿಂಗವೆಂದು", 
        "20112": "ಕ್ಷೀರದ ರುಚಿಯ ಹಂಸಬಲ್ಲುದಲ್ಲದೆ,", 
        "20113": "ಪಿಂಡದೊಳಗೆ ಪ್ರಾಣವಿರ್ಪುದ ಎಲ್ಲರೂ ಬಲ್ಲರಲ್ಲದೆ,", 
        "20114": "ವೇದ ಶಾಸ್ತ್ರ ಆಗಮ ಪುರಾಣ ತರ್ಕ", 
        "20115": "ವೇದವನೋದಿದ ವೇದಜ್ಞಾನಿಗಳು ಸರಿಯಲ್ಲ.", 
        "20116": "ವೇದಂಗಳು ನಿಮ್ಮ ಭೇದಿಸಿ ಕಂಡಿಹೆವೆಂದು", 
        "20117": "ವೇದಂಗಳಿಗಭೇದ್ಯವಾದ ಶಿವನ", 
        "20118": "ಇದಿರ ಹಳಿದು ತನ್ನ ಬಣ್ಣಿಸುವನ್ನಕ್ಕರ", 
        "20119": "ಅನುಭಾವ ಅನುಭಾವವೆಂದು ನುಡಿವುತಿರ್ಪರೆಲ್ಲರು.", 
        "20120": "ತನುವಿನ ಮಧ್ಯದಲ್ಲಿ ಹೂಳಿರ್ದ ಇಷ್ಟಲಿಂಗದಲ್ಲಿ", 
        "20121": "ಅನುಭಾವಿಯಾದಡೆ ತಿರುಳುಕರಗಿದ", 
        "20122": "ಲಿಂಗದ ನಡೆ, ಲಿಂಗದ ನುಡಿ,", 
        "20123": "ಕಾಯಜೀವದ ಕೀಲವನರಿದು", 
        "20124": "ಪರಾತ್ಪರವಾದ ವಸ್ತುವನೊಡಗೂಡಿದ ಪ್ರಾಣಲಿಂಗಿಯ", 
        "20125": "ಚಿತ್ತ ನಿರ್ಮಲವಾದಾತ್ಮನು ಭಕ್ತಿಯಿಂದೆ", 
        "20126": "ಸರ್ವಜ್ಞನು ನೀನೇ ಅಯ್ಯಾ.", 
        "20127": "ಕುಂಭಸಹಸ್ರ ಉದಕದೊಳಗೆ", 
        "20128": "ವಿಶ್ವದೊಳಗೆಲ್ಲ ನೀನೇ ದೇವ", 
        "20129": "ಸಕಲ ವಿಸ್ತಾರದ ರೂಪು ನಿಮ್ಮೊಳಗಿರ್ಪುದು :", 
        "20130": "ಪಶ್ಚಿಮದ ಗಿರಿಯಲ್ಲಿ ಚಿತ್‍ಸೂರ್ಯನುದಯವಾದುದ ಕಂಡೆ.", 
        "20131": "ಅಂಬರದೊಳಗಣ ಅಮೃತದ ಕೊಣನುಕ್ಕಿ", 
        "20132": "ಕಪ್ಪೆಯ ಶಿರದ ಮೇಲೆ ಎಪ್ಪತ್ತೆರಡು ಪುರವಿರ್ಪುವು.", 
        "20133": "ನವಖಂಡ ಮಂಡಲದೊಳಗೊಂದು", 
        "20134": "ಅಂಜನಗಿರಿಯಲ್ಲಿ ಅರ್ಕನ ಉದಯವ ಕಂಡೆ.", 
        "20135": "ನಂಜು ಅಮೃತವಾದುದ ಕಂಡೆ.", 
        "20136": "ಆಕಾಶಮಂಡಲದೊಳಗೆ ಲೋಕೇಶನ", 
        "20137": "ಕುಂಡಲಿಯ ಬಾಗಿಲಲ್ಲಿ ಕೆಂಡವ ಪುಟಮಾಡಿ,", 
        "20138": "ಆನೆಗಳೆಂಟನು ಒಂದು ಮೌನದ ಚಿಕ್ಕಾಡು ನುಂಗಿತ್ತು.", 
        "20139": "ಕಡಲಮಧ್ಯದಲ್ಲಿ ವಡವಾಗ್ನಿ ಹುಟ್ಟಿ,", 
        "20140": "ಬಣ್ಣವಿಲ್ಲದ ಪಕ್ಷಿ", 
        "20141": "ನೀರ ಮಂಟಪದೊಳಗೊಂದು", 
        "20142": "ನಳಿನಾಸನದಲ್ಲಿ ಕುಳ್ಳಿರ್ದು ಅತ್ತಿತ್ತ ಕಳವಳಿಸದೆ", 
        "20143": "ದೇಹದ ವಾಸನೆ ಹರಿದು", 
        "20144": "ಶುದ್ಧಪದ್ಮಾಸನದಲ್ಲಿ ಕುಳ್ಳಿರ್ದು ಊಧ್ರ್ವಲೋಚನನಾಗಿ,", 
        "20145": "ಕಮಲದ ಸಾಲುಗಳೊಳಗೆ", 
        "20146": "ಗಗನಮಂಟಪದಲ್ಲಿ", 
        "20147": "ಉತ್ತರಗಿರಿಯ ಚಿತ್ರಮಂಟಪದೊಳಗೆ", 
        "20148": "ತ್ರಿಕೂಟಗಿರಿಯಲ್ಲಿ ತ್ರಿಣಯನ ಕುಳ್ಳಿರಿಸಿ", 
        "20149": "ಚಂದ್ರಶಿಲೆಯ ಮಂಟಪದೊಳಗೆ", 
        "20150": "ಸಾಸಿರ ಕಂಬದ ಮಂಟಪದೊಳಗೆ", 
        "20151": "ಅಂಬರದೇಶದ ಕುಂಭ ಕೋಣೆಯೊಳಗೆ", 
        "20152": "ದೇಹವೆಂಬ ದೇಗುಲದೊಳಗೆ", 
        "20153": "ತನುವೆಂಬ ಗುಡಿಯೊಳಗೆ, ಮನವೆಂಬ ಸಿಂಹಾಸನವನಿಕ್ಕಿ,", 
        "20154": "ಮೇರುವಿನ ಮಂದಿರದಲ್ಲಿ ಆರೈದು ನೋಡಿದರೆ", 
        "20155": "ಹೊರಗಣ ಒಂಬತ್ತು ಬಾಗಿಲವ ಮುಚ್ಚಿ", 
        "20156": "ಎನ್ನಂತರಂಗದ ಆರು ಭುವನದ ಮೇಲೆ ತೋರುತಿರ್ಪ", 
        "20157": "ಮುಕ್ತಿಗೆ ಸದಾ ಸಂಧಾನವಾದ", 
        "20158": "ಉನ್ಮನಿಯ ಮಂಟಪದಲ್ಲಿ ಉಮೆಯಾಣ್ಮನ ಉಗ್ಗಡಣೆಯ ನೋಡಾ !", 
        "20159": "ಸೂರ್ಯಮಂಡಲದಲ್ಲಿ ವೀರಗಣಂಗಳ ವಿಪರೀತವಿದೇನೋ ?", 
        "20160": "ಸಹಸ್ರದಳ ಕಮಲಮಧ್ಯದಲ್ಲಿ ಪಂಚಪತ್ರ.", 
        "20161": "ಯೋಗ ಯೋಗವೆಂದು ನುಡಿಯುತಿರ್ಪರೆಲ್ಲರು ;", 
        "20162": "ಈಡಾ ಪಿಂಗಳೆಯಲ್ಲಿ ತುಂಬಿ ಸೂಸುವ", 
        "20163": "ಇನ್ನು ಹಠಯೋಗಕ್ಕೆ ಸಾಧನಮಾದ", 
        "20164": "ಈ ಹಠಯೋಗಕ್ಕೆ ನಿಜದಂಗವಾಗಿರ್ಪ", 
        "20165": "ಬಳಿಕ ನಿಯಮದ ಲಕ್ಷಣವೆಂತೆಂದೊಡೆ :", 
        "20166": "ಇನ್ನು ಆಸನದ ಭೇದವೆಂತೆಂದೊಡೆ:", 
        "20167": "ಇನ್ನು ಪ್ರಾಣಾಯಾಮದ ಲಕ್ಷಣವೆಂತೆಂದೊಡೆ :", 
        "20168": "ಇನ್ನು ಪ್ರತ್ಯಾಹಾರದ ಭೇದವೆಂತೆನೆ :", 
        "20169": "ಇನ್ನು ಯೋಗೀಶ್ವರರ ಧ್ಯಾನಯೋಗಕ್ಕೆ ಸ್ಥಾನಂಗಳಾವುವೆನೆ :", 
        "20170": "ಇನ್ನು ಧಾರಣಯೋಗದ ಲಕ್ಷಣವೆಂತೆನೆ :", 
        "20171": "ಇನ್ನು ಸಮಾಧಿಯೋಗವೆಂತೆಂದೊಡೆ :", 
        "20172": "ಬಳಿಕೀ ಪ್ರಕಾರಮಾದ ಹಠಯೋಗರೂಪಮಪ್ಪ", 
        "20173": "ಇನ್ನು ತಾರಕಯೋಗದ ಲಕ್ಷಣಮಂ ಪೇಳ್ವೆನೆಂತೆನೆ :", 
        "20174": "ಇನ್ನು ತಾರಕಯೋಗದ ಲಕ್ಷಣವೆಂತೆನೆ:", 
        "20175": "ಸದ್ಗುರುಪ್ರಸಾದದಿಂದೊಗೆದ ತಾರಕಬ್ರಹ್ಮದ ಅಭ್ಯಾಸವೆಂತೆನೆ;", 
        "20176": "ಚಂದ್ರಸೂರ್ಯರೆನಿಸುವ", 
        "20177": "ದಕ್ಷಿಣಜ್ಯೋತಿಮಂಡಲದ  ಮಧ್ಯದಲ್ಲಿ", 
        "20178": "ಪರತರ ಪರಮ ಸಮರಸಸ್ವರೂಪವಾದ ತಾರಕಬ್ರಹ್ಮವು", 
        "20179": "ನಾಸಿಕಾಗ್ರದಿಂ ಮುಂದೆ ನಾಲ್ಕಂಗುಲಪ್ರಮಾಣದಲ್ಲಿ", 
        "20180": "ಮತ್ತಂ, ಶಿರಸ್ಸಿನ ಮೇಲುಭಾಗದ ಹನ್ನೆರಡಂಗುಲಪ್ರಮಾಣಿನಲ್ಲಿ", 
        "20181": "ಚತುರ್ಭೂತಂಗಳನೊಳಕೊಂಡು", 
        "20182": "ಈ ಲಕ್ಷ್ಯತ್ರಯಂಗಳನರಿದ ಯೋಗೀಶ್ವರನು", 
        "20183": "ರಾಜಯೋಗಾನುಸಂಧಾನದಿಂದೆ ಕಾಣಿಸುವ ವಿಚಿತ್ರ ಪ್ರಭಾಮಂಡಲಕ್ಕೆ", 
        "20184": "ಸಕಲ ಸಾಧನಂಗಳಿಲ್ಲದೆ ಬಹುಪ್ರಯಾಸವಿಲ್ಲದೆ", 
        "20185": "ತ್ರಿಕೂಟವೆಂಬ ಭ್ರೂಮಧ್ಯಸ್ಥಾನದಲ್ಲಿ", 
        "20186": "ರಾಜಯೋಗದಲ್ಲಿ ನಿಶ್ಚಿಂತನಾದ ಯೋಗೀಶ್ವರನ", 
        "20187": "ಕಾಯದೊಳಗಣ ಮನಸ್ಸು", 
        "20188": "ಎನ್ನ ಆಧಾರಚಕ್ರವೆ ಶ್ರೀಶೈಲಕ್ಷೇತ್ರ :", 
        "20189": "ಎನ್ನ ಆರುಚಕ್ರಂಗಳಲ್ಲಿ ಪೂರೈಸಿ ತುಂಬಿರ್ಪ", 
        "20190": "ಪ್ರಾಣಲಿಂಗಸಂಬಂಧಿಗಳೆಂದು ನುಡಿಯುವವರು ಅನೇಕರುಂಟು:", 
        "20191": "ಎನ್ನ ಆಧಾರದಲ್ಲಿ", 
        "20192": "ನವನಾಳಂಗಳ ಬಲಿದು ಶಿವಧ್ಯಾನದಲ್ಲಿ ಕುಳ್ಳಿರ್ದು", 
        "20193": "ಅಯ್ಯಾ, ನಿಮ್ಮನೆನ್ನ ಕರಸ್ಥಲದಲ್ಲಿ ನೋಡಿದಡೆ", 
        "20194": "ಎನ್ನೊಡಲೊಳಗೆ ತೋರಿ ತೋರಿ ಅಡಗುವ", 
        "20195": "ಎನ್ನ ಸ್ಥೂಲತನುವೆಂಬ ಕೈಲಾಸದ ಮೇಲೆ", 
        "20196": "ಕೇಳು ಕೇಳಯ್ಯ ಪ್ರಾಣನಾಥನೆ,", 
        "20197": "ಒಳಗೆ ನೋಡಿದಡೆ ನಿಮ್ಮನೆ ನೋಡುವೆನಯ್ಯಾ.", 
        "20198": "ಒಳಗೆ ಕೂಡಿ ಹೊರಗೆ ಮರೆದಿರ್ಪೆನಯ್ಯಾ.", 
        "20199": "ಒಳಗೆಂಬುದನರಿಯೆ, ಹೊರಗೆಂಬುದನರಿಯೆ,", 
        "20200": "ನೇತ್ರದ ಸೂತ್ರದಲ್ಲಿ", 
        "20201": "ತನುವಿನ ಕೊನೆಯಲ್ಲಿ ನೇತ್ರದ  ಅನುವ ಕಂಡೆನಯ್ಯಾ", 
        "20202": "ನೇತ್ರವೆಂಬ ಸುವರ್ಣದ ಕೊಡದಲ್ಲಿ", 
        "20203": "ಭಾವವೇ ಬ್ರಹ್ಮವಾದ ಬಳಿಕ", 
        "20204": "ಕಾಲಲ್ಲಿ ಕಣ್ಣು ಮೂಡಿತ್ತ ಕಂಡೆ.", 
        "20205": "ಕಾಲಿಲ್ಲದೆ ನಡೆಯಬಲ್ಲಡೆ ಶಿವಯೋಗಿಯೆಂಬೆನಯ್ಯಾ.", 
        "20206": "ತನುವಿನವಗುಣಂಗಳ ತರಿದೊಟ್ಟಿ,", 
        "20207": "ಕರ್ಮಸಾದಾಖ್ಯಸ್ವರೂಪವಾದ ಆಚಾರಲಿಂಗದಲ್ಲಿ", 
        "20208": "ಎನ್ನ ಘ್ರಾಣದ ಕೊನೆಯಲ್ಲಿ", 
        "20209": "ಗುರುಪಾದೋದಕವ ಕೊಂಡು", 
        "20210": "ಪಾದತೀರ್ಥವೆಂದಡೆ ಪರಾತ್ಪರವು ತಾನೆ ನೋಡಾ.", 
        "20211": "ಜಂಗಮದ ಪಾದತೀರ್ಥವು ಭವರೋಗವೈದ್ಯವಯ್ಯಾ.", 
        "20212": "ನಿತ್ಯನಿರಂಜನ ಜಂಗಮವ ಭಕ್ತಿಯಿಂ ಬಿಜಯಂಗೈಸಿ", 
        "20213": "ಕೇಳಿ, ಕೇಳಿರಯ್ಯಾ ಶಿವಭಕ್ತಶರಣಜನಂಗಳು ನೀವೆಲ್ಲ.", 
        "20214": "ಆದಿಯಾಧಾರವಿಲ್ಲದ ಮುನ್ನ,", 
        "20215": "ನಿರುಪಮ ಬಸವಣ್ಣನ ನಿರಾಳ ಬೆಳಗಿನೊಳಗೆ", 
        "20216": "ಬೀಜದೊಳಗೆ ಅಂಕುರವಿರ್ಪುದು.", 
        "20217": "ಎನ್ನ ಪೃಥ್ವಿತತ್ತ್ವದಲ್ಲಿ ಆಧಾರಚಕ್ರವಿರ್ಪುದು.", 
        "20218": "ಮನವ ನಿಲಿಸಿಹೆನೆಂದು ನುಡಿವ ಅಣ್ಣಗಳ", 
        "20219": "ನಿಮ್ಮ ಸ್ವಲೀಲೆಯಿಂದೆ", 
        "20220": "ಬಚ್ಚಬರಿಯ ಬಯಲೊಳಗೊಂದು", 
        "20221": "ಪಿಂಡದೊಳಗೊಂದು ಅಖಂಡಜ್ಯೋತಿ", 
        "20222": "ದ್ವೀಪ ಏಳರೊಳಗೆ ವ್ಯಾಪಿಸಿಕೊಂಡಿರ್ಪುದು", 
        "20223": "ಒಂಬತ್ತುನಾಳದೊಳಗೆ ತುಂಬಿಸೂಸುವ ಮನಪವನಂಗಳ", 
        "20224": "ನೀರಿಲ್ಲದ ಭೂಮಿಯಲ್ಲಿ ಬೇರಿಲ್ಲದ ವೃಕ್ಷಹುಟ್ಟಿತ್ತ ಕಂಡೆ.", 
        "20225": "ಸತ್ತು ಚಿತ್ತು ಆನಂದ ನಿತ್ಯಪರಿಪೂರ್ಣ ಅಖಂಡವೆಂಬ", 
        "20226": "ಕಾಮದ ಕಳವಳದಲ್ಲಿ ಕಂಗೆಡುವನಲ್ಲ ಶರಣ.", 
        "20227": "ಗೋಸುಂಬೆ ಹುಳದಂತೆ ಬಹುವೇಷಧಾರಿಯಲ್ಲ ಶರಣ.", 
        "20228": "ಮನದಲ್ಲಿ ಒಂದು, ಮಾತಿನಲ್ಲಿ ಎರಡಾಗಿ", 
        "20229": "ಸತ್ಯಸದಾಚಾರಿಯಯ್ಯಾ ನಿಮ್ಮ ಶರಣ .", 
        "20230": "ಲಿಂಗದ ನಡೆಯಂತೆ ನಡೆವನಲ್ಲದೆ,", 
        "20231": "ಅಂದಿನ ಶರಣರಿಗೆ ಇಂದಿನವರು ಸರಿಯಲ್ಲವೆಂದು ನುಡಿವ", 
        "20232": "ಶರಣಸ್ಥಲದ ಕುರುಹಿನ ಮಾರ್ಗವನರಿಯದೆ,", 
        "20233": "ಮಾತಿನಲ್ಲಿ ಶುದ್ಧವಿಲ್ಲದವರು ಶರಣರೆ ?", 
        "20234": "ಸತ್ತು ಹೋಗುವರೆಲ್ಲ ಸ್ವರ್ಗಪದಸ್ಥರೆ ? ಅಲ್ಲಲ್ಲ.", 
        "20235": "ಪುರುಷನಿಲ್ಲದ ಬಳಿಕ ಸ್ತ್ರೀಗೆ ಗರ್ಭವಿನ್ನೆಲ್ಲಿಯದೊ ?", 
        "20236": "ಗುರು ಕರುಣಿಸಿಕೊಟ್ಟ ಮಂತ್ರವೆ", 
        "20237": "ಗುರುದೀಕ್ಷೆಯಿಲ್ಲದ ಲಿಂಗವು ಧರೆಯ ಕಲ್ಲೆನಿಸಿತ್ತು.", 
        "20238": "ಗುರುಶಿಷ್ಯ ಸಂಬಂಧವೆಂದು ನುಡಿಯುತಿರ್ಪರೆಲ್ಲರು;", 
        "20239": "ನೋಡಿರೆ ನೋಡಿರೆ ಒಂದು ವಿಚಿತ್ರವ.", 
        "20240": "ಶ್ರೇಷ್ಠ ಶ್ರೀಗುರುಸ್ವಾಮಿ", 
        "20241": "ಶ್ರೀಗುರು ಕರುಣಿಸಿ ಕರಸ್ಥಲಕ್ಕೆ ಕೊಟ್ಟ ಇಷ್ಟಲಿಂಗವನು", 
        "20242": "ಸದ್ಗುರುಕಾರುಣ್ಯವ ಪಡೆದು ಲಿಂಗಾಂಗಸಮರಸವುಳ್ಳ", 
        "20243": "ಸಕಲ ಗಣಂಗಳು ಸಾಕ್ಷಿಯಾಗಿ", 
        "20244": "ಲಿಂಗ ಛಿನ್ನ ಭಿನ್ನವಾದಡೆ ಸ್ಥೂಲಸೂಕ್ಷ್ಮವನರಿಯಬೇಕು.", 
        "20245": "ಎಂಬತ್ತುನಾಲ್ಕುಲಕ್ಷ ಮಂಡಲದೊಳಗೆ", 
        "20246": "ಶ್ರೀಗುರುವಿನ ಕರಗರ್ಭದಲ್ಲಿ ಉದಯವಾದ", 
        "20247": "ಎಲೆ ಶಿವನೆ, ನೀನು ಎನ್ನ ಮೆಚ್ಚಿ ಕೈವಿಡಿದ ಕಾರಣ", 
        "20248": "ಪುರುಷನೆಂದು ಕರೆವುತಿರ್ಪುದು ಜಗದವರೆಲ್ಲ ಎನ್ನ ;", 
        "20249": "ಗಂಡನುಳ್ಳ ಗರತಿಯರೆಲ್ಲರು", 
        "20250": "ಎಲ್ಲರಂತಲ್ಲ ನೋಡಿರೆ ನನ್ನ ನಲ್ಲ.", 
        "20251": "ನಲ್ಲನ ಕಾಣದೆ", 
        "20252": "ಚಿತ್ತದೊಲ್ಲಭನ ಕಾಣದೆ", 
        "20253": "ಬುದ್ಧಿಗೂಡದು ನಿದ್ರೆಬಾರದು ಎನಗೆ, ಸುದ್ದಿ ಹೇಳಿರೆ ಆತಗೆ.", 
        "20254": "ಅರಳಿಯ ಮರದೊಳಗಿರುವ ಅರಗಿಳಿಗಳಿರಾ,", 
        "20255": "ಆತನ ಆಳಾಪದಿಂದೆ ಹಗಲಾದುದನರಿಯೆನವ್ವಾ !", 
        "20256": "ನಾನೇನೆಂದೆನೆ ? ತಾನೇತಕೆ ಮುನಿದನೆ ?", 
        "20257": "ತನ್ನನಲ್ಲದೆ ಅನ್ಯವ ನೋಡೆನವ್ವಾ.", 
        "20258": "ಜೀವದೊಡೆಯನನಗಲಿ ಜೀವಿಸಲಾರೆನವ್ವಾ.", 
        "20259": "ಪ್ರಾಣನಾಯಕನ ಕಾಣದೆ", 
        "20260": "ಅರಸನ ಕಾಣದಬಳಿಕ ಹರುಷವಿಲ್ಲವ್ವಾ ಎನಗೆ.", 
        "20261": "ಕಂಡರೆ ಸಂತೋಷ, ಕಾಣದಿರ್ದರೆ ಕಡುದುಃಖವೆನಗೆ", 
        "20262": "ಬಾರನೇತಕವ್ವಾ ನಮ್ಮನೆಯಾತ ?", 
        "20263": "ಘುಲ್ಲುಘುಲ್ಲೆಂದಡೆ ನಲ್ಲ ಬರುತಾನೆಂದು", 
        "20264": "ಹಗಲು ಹನ್ನೆರಡುತಾಸು ತನ್ನ ಹಂಬಲದಲ್ಲಿ", 
        "20265": "ಬಾರಯ್ಯ ಬಾರಯ್ಯ ಗಂಡನೆ,", 
        "20266": "ನೋಡು ನೋಡಯ್ಯಾ ಗಂಡನೆ,", 
        "20267": "ಸೊಕ್ಕಿದ ಜವ್ವನದ, ಕಕ್ಕಸಕುಚದ,", 
        "20268": "ಇಂದಿನ ಇರುಳಿನಲ್ಲಿ", 
        "20269": "ಚಂದ್ರಸಾಲೆಯಲ್ಲಿ ಬಂದು ನಿಂದವನಾರೆಂದು", 
        "20270": "ಆತನ ದಿವ್ಯರೂಪು ನೋಡಿ ಎನ್ನ ಕಂಗಳು ದಣಿಯವು.", 
        "20271": "ನಲ್ಲನ ಕೂಡಿದ ಸುಖವೆಲ್ಲವ", 
        "20272": "ಬಾಯೊಳಗೆ ಬಾಯನಿಕ್ಕಿ", 
        "20273": "ಕೇಳಿರೇ ಕೇಳಿರವ್ವಾ ಕೆಳದಿಯರೆಲ್ಲ.", 
        "20274": "ನಿಚ್ಚ ನಿಚ್ಚ ಶರಣಂಗೆ ಹಬ್ಬ ಆವಾಸಗಿರಿ ತಾನೆ ಕಾಣಿರೊ.", 
        "20275": "ಶರಣನ ಚರಣದಲ್ಲಿ ನಡೆಪರುಷ.", 
        "20276": "ಶರಣನ ಕಂಗಳೆ ಕಾಮಧೇನುವಿನ ಹಿಂಡು.", 
        "20277": "ಎಲೆ ಶಿವನೆ ನಿಮ್ಮಲ್ಲಿ ನಾನೊಂದ ಬೇಡಿಕೊಂಬೆನು,", 
        "20278": "ಅಂಗಕ್ಕೆ ಆಚಾರವೆ ಚೆಲುವು.", 
        "20279": "ಶರಣರ ಸಂಗದಿಂದೆ ತನು ಶುದ್ಧವಪ್ಪುದು ನೋಡರೆ.", 
        "20280": "ಸಕಲ ಗಣಂಗಳು", 
        "20281": "ಅನುಭಾವಿಗಳ ಸಂಗ ಕೀಟಕ ಭ್ರಮರವಾದಂತೆ ಕಾಣಿರೊ.", 
        "20282": "ಅಗ್ನಿಯ ಸಂಗದಿಂದೆ ಕಾನನ ಕೆಟ್ಟಂತೆ,", 
        "20283": "ಕಾಯವೇ ಕೈಲಾಸವಾಗಿ,", 
        "20284": "ಒಬ್ಬರು ನಡೆದಾಚರಣೆಯಲ್ಲಿ ನಡೆಯರು.", 
        "20285": "ಶೀಲವಂತ ಶೀಲವಂತರೆಂದು", 
        "20286": "ಶಿವನೆ ಗುರುವೆಂದು ಗುರುವಿಂಗೆ ತನುವನರ್ಪಿಸಿ,", 
        "20287": "ಶೀಲ ಶೀಲವೆಂದು ನುಡಿವುತಿರ್ಪರೆಲ್ಲರು.", 
        "20288": "ಇಷ್ಟಲಿಂಗದಲ್ಲಿ ತನುವನಡಗಿಸಿ,", 
        "20289": "ತನು ಗುರುವಾದುವೆ ಶೀಲ.", 
        "20290": "ಶಿವನ ಕೂಡಿರ್ಪ ಶರಣರ ಶೀಲವನೇನೆಂಬೆನಯ್ಯಾ !", 
        "20291": "ಶರಣನಾದಡೆ ಮುರಿದ ಬಂಗಾರವ", 
        "20292": "ತಾಮಸಗುಣಂಗಳಲ್ಲಿ ಬಿದ್ದು,", 
        "20293": "ಗುರುಸ್ಥಲದ ಗಂಭೀರವಸ್ತುವ ಕೂಡಬಲ್ಲಡೆ", 
        "20294": "ಘನಲಿಂಗದೇವರು ಘನಲಿಂಗದೇವರೆಂದು", 
        "20295": "ಸತ್ಯಾಸತ್ಯವೆಂದು ವಿವರಿಸಿ ತಿಳಿದು", 
        "20296": "ಪರಧನವ ಹಿಡಿಯದೆ,", 
        "20297": "ಮಹಾಂತನ ಕೂಡಿದ ದೇವರುಗಳೆಂಬ", 
        "20298": "ಅಂತರಂಗದಲ್ಲಿ ಅರುಹಿನ ಶುದ್ಧಿಯನರಿಯದೆ,", 
        "20299": "ಅನುಪಮಲಿಂಗದಲ್ಲಿ ಅಂಗ ಮನ ಪ್ರಾಣಂಗಳನಡಗಿಸಿ", 
        "20300": "ನಾದಬಿಂದುಕಳಾತೀತವಾದ ಪರವಸ್ತುವೆ", 
        "20301": "ವಿರಕ್ತನೆನಿಸುವಂಗಾವುದು ಚಿಹ್ನೆವೆಂದೊಡೆ :", 
        "20302": "ವಿರಕ್ತಂಗೆ ಕಾಮಕ್ರೋಧಂಗಳುಂಟೆ ?", 
        "20303": "ಹಲವು ವೇಷವ ಧರಿಸಿ", 
        "20304": "ಬಹುಕ್ರಿಯೆಯ ನಟಿಸದೆ, ಬಹುಶಾಸ್ತ್ರಕ್ಕೆ ಮುಖವಾಗದೆ,", 
        "20305": "ಚರಣದೊಳಗೆ ಚರಣವಿಟ್ಟು ನಡೆವ ಭೇದವು", 
        "20306": "ಎನ್ನ ತನುವೆ ಬಸವಣ್ಣನು.", 
        "20307": "ಬಸವಣ್ಣನೆ ಗುರುವೆನಗೆ,", 
        "20308": "ಬಸವನ ನಾಮವು ಕಾಮಧೇನು ಕಾಣಿರೊ.", 
        "20309": "ಇಷ್ಟಲಿಂಗವಾಗಿ ಎನ್ನ ಸ್ಥೂಲತನುವಿಗೆ", 
        "20310": "ಜಗದ ಮಧ್ಯದಲ್ಲಿ ಶರಣ ಜನಿಸಿದಡೇನು", 
        "20311": "ಜನನವಿಲ್ಲದ ಶರಣ, ಮರಣವಿಲ್ಲದ ಶರಣ,", 
        "20312": "ಶರಣನ ತನುವೆ ಕೈಲಾಸ ತಾನೆ ನೋಡಾ.", 
        "20313": "ಶರಣ ಗಮನಿಯಾದಡೆ ಕಿರಿದೆಂಬರು,", 
        "20314": "ಸಂದೇಹಿಸೂತಕಿಯಲ್ಲ ಶರಣ,", 
        "20315": "ಕಾಷ್ಠದಲ್ಲಿ ಬೊಂಬೆಯ ಮಾಡಿ,", 
        "20316": "ಎನ್ನ ನಡೆವ ನಡೆಗಳೆಲ್ಲ ನಿಮ್ಮ ನಡೆಗಳಯ್ಯಾ.", 
        "20317": "ಎನ್ನ ಸ್ಥೂಲತನುವಿನ ಜಾಗ್ರಾವಸ್ಥೆಯಲ್ಲಿ", 
        "20318": "ಲಿಂಗಾಂಗಸಾಮರಸ್ಯವನರಿಯದೆ", 
        "20319": "ತನುವಿನ ಕೈಯಲ್ಲಿರ್ದ ಘನಲಿಂಗವನು", 
        "20320": "ಅನಾದಿ ಪರವಸ್ತುವು ತನ್ನ", 
        "20321": "ಅನಾದಿ ಶಿವಾಂಶಿಕರಾದ ಶಿವಶರಣರಿಗಲ್ಲದೆ", 
        "20322": "ಶರಣಲಿಂಗಕ್ಕೆ ನೋಡುವ ಕಣ್ಣು", 
        "20323": "ಅರಿದಲ್ಲಿ ಶರಣ ಮರೆದಲ್ಲಿ ಮಾನವನೆಂದು ನುಡಿವ", 
        "20324": "ಅನಂತಕೋಟಿ ಬ್ರಹ್ಮಾಂಡಗಳನೊಳಕೊಂಡ", 
        "20325": "ಎನ್ನ ತನುವಿನೊಳಗೆ ತನುರೂಪಾಗಿರ್ದಿರಯ್ಯಾ ನೀವು.", 
        "20326": "ಅಂಗಕ್ಕೆ ಇಷ್ಟಲಿಂಗದ ಸತ್ಕ್ರಿಯವನಳವಡಿಸಿಕೊಂಡು", 
        "20327": "ಘನಮಹಾಲಿಂಗಕ್ಕೆ", 
        "20328": "ಶಿವಶಿವಾ, ಏನೆಂಬೆನಯ್ಯಾ ಶಿವಶರಣರ ಘನವನು !", 
        "20329": "ಮನ ಮಹಾಘನವಾಯಿತ್ತು.", 
        "20330": "ಕ್ಷುತ್ ಪಿಪಾಸೆ ಶೋಕ ಮೋಹ ಜನನ ಮರಣವೆಂಬ", 
        "20331": "ವೇದ ಶಾಸ್ತ್ರಾಗಮ ಪುರಾಣಂಗಳನೋದಿ", 
        "20332": "ಸಗುಣನಲ್ಲ ನಿರ್ಗುಣನಲ್ಲ ನೋಡಾ ಲಿಂಗೈಕ್ಯನು.", 
        "20333": "ಸಾವಯವ ನಿರವಯವನಲ್ಲ ನೋಡಾ ಲಿಂಗೈಕ್ಯನು.", 
        "20334": "ನಿರುಪಮ ನಿರಾಳನು ನೋಡಾ ಲಿಂಗೈಕ್ಯನು.", 
        "20335": "ಕಾಯಕಲ್ಪಿತಕ್ಕೆ ದೂರನು ನೋಡಾ ಲಿಂಗೈಕ್ಯನು.", 
        "20336": "ಸರ್ವಾಚಾರಸಂಪತ್ತನರಿದಲ್ಲದೆ", 
        "20337": "ಗರ್ವಾಹಂಕಾರವಳಿದು ಸರ್ವಕರಣಂಗಳು ತರಹರವಾಗಿ", 
        "20338": "ಅಂಗದ ಗುಣವಳಿಯದೆ, ಪ್ರಾಣದ ಪ್ರಪಂಚು ಹಿಂಗದೆ,", 
        "20339": "ಅಂಗವಿಕಾರಿಗೇಕೊ ಲಿಂಗದೊಡನೆ ಏಕಭಾಜನ ?", 
        "20340": "ಇಷ್ಟಲಿಂಗಕ್ಕೆ ತನುವೆ ಭಾಜನ.", 
        "20341": "ಮನವೆಂಬ ಒರಳಿಗೆ", 
        "20342": "ಬಾರಯ್ಯ ಬಾರಯ್ಯ ಗಂಡನೆ,", 
        "20343": "ಕೇಳಿ ಕೇಳಿರವ್ವಾ ನಮ್ಮ ಮನೆಯಾತನ", 
        "20344": "ಲಿಂಗದೊಡನೆ ಸಹಭೋಜನ ಮಾಡುವ", 
        "20345": "ಭಾಷೆಗಳ್ಳಗೇಕೊ ಸಹಭೋಜನ ?", 
        "20346": "ಪ್ರಾಣನ ಹಸಿವೆದ್ದು ದೇಹವನಂಡಲೆವಾಗ", 
        "20347": "ಗುರುವಿನೊಡನೆ ಸಹಭೋಜನ ಮಾಡಬೇಕಾದಡೆ,", 
        "20348": "ಕಾಯದ ಕಳವಳದಲ್ಲಿ ಕಂಗೆಟ್ಟು,", 
        "20349": "ಹೊನ್ನೆನ್ನದು ಹೆಣ್ಣೆನ್ನದು ಮಣ್ಣೆನ್ನದು", 
        "20350": "ಸಂಸಾರದೊಳಗಿರ್ದ ಸದ್ಭಕ್ತನು", 
        "20351": "ನಡೆವ ಕಾಲದಲ್ಲಿ ನಿಮ್ಮ ಕೂಡೆ ನಡೆವೆನಯ್ಯಾ.", 
        "20352": "ಎನಗೆ ನೀನೇ ಚಿದ್‍ಭಾಂಡವಯ್ಯಾ;", 
        "20353": "ಕೇಳು ಕೇಳಯ್ಯಾ ಕರುಣಿ,", 
        "20354": "ತಂದೆ ಕೇಳಯ್ಯಾ ಲಿಂಗವೆ.", 
        "20355": "ಒಮ್ಮೆ ಜ್ಞಾನಿಯೆನಿಸಿ ಮತ್ತೊಮ್ಮೆ ಅಜ್ಞಾನಿಯೆನಿಸಿ", 
        "20356": "ಸಕಲ ವ್ಯಾಪಾರವ ಬಿಟ್ಟು ನಿಮಗೆ ನಾನು ಮರುಳಾಗಿರ್ಪೆನಯ್ಯಾ.", 
        "20357": "ಪರಮನಪ್ಪಣೆಯಿಂದೆ ಧರೆಗಿಳಿದು ಬಂದು", 
        "20358": "ಸಕಲಮೂರ್ತಿಯಾಗಿ ಇಷ್ಟಲಿಂಗವಾಯಿತ್ತು.", 
        "20359": "ಏನೆಂಬೆನೇನೆಂಬೆನಯ್ಯ ಒಂದು ಎರಡಾದುದ.", 
        "20360": "ತನುವಿನ ಕೈಯಲ್ಲಿ ಮೂರ್ತಿಗೊಂಡಿರ್ದ", 
        "20361": "ನಾನಿಹ ಪರಿಯಂತರ ನೀನುಂಟು ;", 
        "20362": "ತನುವಿಹ ಪರಿಯಂತರ ನಿಮ್ಮ ಪೂಜಿಸಿದೆನಯ್ಯಾ.", 
        "20363": "ತನು ನಿಮ್ಮದಾದ ಬಳಿಕ", 
        "20364": "ನಿಮ್ಮ ಪೂಜಿಸಿಹೆನೆಂದಡೆ ತನುವಿಲ್ಲವಯ್ಯಾ ಎನಗೆ.", 
        "20365": "ಎನ್ನ ಕಾಲಕಲ್ಪಿತಂಗಳು ಹೊರಗಾದುವಯ್ಯಾ.", 
        "20366": "ಅಹುದೆನಲಮ್ಮೆ, ಅಲ್ಲೆನಲಮ್ಮೆ,", 
        "20367": "ಕಾಲಿಲ್ಲದ ನಡೆ, ಒಡಲಿಲ್ಲದ ರೂಪು,", 
        "20368": "ಕನ್ನಡಿಯ ನೋಡುವಲ್ಲಿ ಪ್ರತಿರೂಪು ಕಾಣುತಿರ್ಪುದು.", 
        "20369": "ನಾದ ನಿಜವೆಂಬೆನೆ ? ನಾದ ನಿಜವಲ್ಲ.", 
        "20370": "ಘನಗಂಭೀರ ಮಹಾಘನ ಬೆಳಗಿನೊಳಗೆ", 
        "20371": "ಖಂಡಿತಭಾವವಳಿದು ಅಖಂಡಬ್ರಹ್ಮದಲ್ಲಿ", 
        "20372": "ಅನಂತಸಾಧಕಂಗಳ ಕಲಿತ ಆಯಗಾರನು,", 
        "20373": "ಶರಣನಿರ್ದಲ್ಲಿ ಸಕಲರ್ತಿರ್ಥಕ್ಷೇತ್ರಂಗಳಿರ್ಪವು.", 
        "20374": "ಹಿಂದಣ ಶಂಕೆಯ ಹರಿದು, ಮುಂದಣ ಭವವ ಮರೆದು,", 
        "20375": "ಇಂದ್ರಚಂದ್ರರು ಪ್ರಳಯವಾಗಿ ಹೋದಡು", 
        "20376": "ಅಣುವಿಂಗೆ ಅಣು ಮಹತ್ತಿಂಗೆ ಮಹತ್ತಾದ", 
        "20377": "ಘನಕ್ಕೆ ಘನವಾದ ಮಹಾಘನವಸ್ತುವಿನಲ್ಲಿ", 
        "20378": "ಎಲ್ಲಿ ನೋಡಿದಡಲ್ಲಿ ನೀನೇ ದೇವ.", 
        "20379": "ತೆರಹಿಲ್ಲ ತೆರಹಿಲ್ಲವಯ್ಯಾ ನಡೆನೋಟಕ್ಕೆ.", 
        "20380": "ಅಂಡಾಭರಣರು ಘನವೆಂಬೆನೆ ? ಅಂಡಾಭರಣರು ಘನವಲ್ಲ.", 
        "20381": "ಅಖಂಡಜ್ಞಾನಭರಿತ ಶರಣಂಗೆ ಪೃಥ್ವಿಯೆ ಖಟ್ವಾಂಗ,", 
        "20382": "ಪೃಥ್ವಿ ಅಡಗುವುದಕ್ಕೆ ಅಪ್ಪುವೆ ಆಶ್ರಯವಾಗಿರ್ಪುದು.", 
        "20383": "ಅನುಪಮ ಶರಣನ ನೆನಹಿನ ಕೊನೆಯಲ್ಲಿ ಘನಲಿಂಗವು.", 
        "20384": "ಆಕಾಶದಲ್ಲಿ ತೋರಿದ ಇಂದ್ರಧನು ಅಡಗುವುದಕ್ಕೆ", 
        "20385": "ಬಯಲು ಬಯಲು ಬೆರೆದಲ್ಲಿ ಮೇರೆಯುಂಟೆ ಅಯ್ಯಾ ?", 
        "20386": "ನದಿ ನದಿಯ ಕೂಡಿದಂತೆ, ಬಯಲು ಬಯಲ ಬೆರೆದಂತೆ,", 
        "20387": "ಕೇಳಿ ಕೇಳಿರಯ್ಯಾ ಮತ್ರ್ಯಲೋಕದ ಮಹಾಗಣಂಗಳು ನೀವೆಲ್ಲ.", 
        "20388": "ಬಯಲ ಸ್ತ್ರೀಯಳ ನಿರವಯಲ ಪುರುಷ ಬಂದು ಕೂಡಲು", 
        "20389": "ನೋಡಲಿಲ್ಲದ ಬಯಲು, ಸೂಡಲಿಲ್ಲದ ಬಯಲು,", 
        "20390": "ಮೂರ್ತಿಯಿಲ್ಲದ ಬಯಲು, ಅಮೂರ್ತಿಯಿಲ್ಲದ ಬಯಲು,", 
        "20391": "ಸಾಕಾರವಿಲ್ಲದ ಬಯಲು, ನಿರಾಕಾರವಿಲ್ಲದ ಬಯಲು,", 
        "20392": "ಭಾವವಿಲ್ಲದ ಬಯಲು, ಜೀವವಿಲ್ಲದ ಬಯಲು,", 
        "20393": "ಆದಿಯಿಲ್ಲದ ಬಯಲು, ಅನಾದಿಯಿಲ್ಲದ ಬಯಲು,"
    }
}