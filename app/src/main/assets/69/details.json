        "10167": "ಜಂಗಮಲಿಂಗವನೊಂದೆಂದು ಕಾಬಡೆ ಭಂಗವಿಲ್ಲದೆ", 
        "10168": "ಜಂಗಮವಾಗಿ ಜಂಗಮನ ಕೊಂದಾಗಲೆ ತನ್ನ ದೈವ ತನಗಿಲ್ಲ.", 
        "10169": "ಜಂಗಮವಾಗಿ ಹುಟ್ಟಿದ ಮತ್ತೆ", 
        "10170": "ಜಲದೊಳಗಣ ಬೊಬ್ಬುಳಿಕೆ ಜಲ ಘಟ್ಟಿಗೊಂಡಲ್ಲಿ", 
        "10171": "ಜೀವವೆಂಬುದಿಲ್ಲ, ಭಾವವೆಂಬುದಿಲ್ಲ, ಬಯಕೆಯೆಂಬುದಿಲ್ಲ.", 
        "10172": "ತನ್ನಿಂದ ಬಿಟ್ಟು ಗುರುವಿಲ್ಲ,", 
        "10173": "ತನು ತನ್ನ ಕಾಣದು, ಮನ ತನ್ನನರಿಯದು.", 
        "10174": "ತನುವ ಮರೆಯಬೇಕೆಂದು ಗುರುವ ತೋರಿ,", 
        "10175": "ತಪವೆಂಬುದು ಬಂಧನ.", 
        "10176": "ತಮ್ಮ ಮಲವ ತಾವು ಮುಟ್ಟವು ಆವ ಜಾತಿಯು.", 
        "10177": "ತಾನೆಂದಲ್ಲಿ ಓಯೆಂದುದದೇನೊ!", 
        "10178": "ತೋರದೆ ತೋರಿದ ನೆಳಲಿನ ಬಿಂದುವ ನುಂಗಿ", 
        "10179": "ದಗ್ಧವಾದ ಮರ ಇದ್ದಿಲಲ್ಲದೆ ಕಾಷ್ಠಕ್ಕೆ ಹೊದ್ದಿಗೆ ಉಂಟೆ?", 
        "10180": "ದೃಷ್ಟಿ ಕಷ್ಟವ ಮುಟ್ಟುವಲ್ಲಿ ಮೂವರ ಸಂಚವಿಲ್ಲಾಗಿ", 
        "10181": "ನಮ್ಮವರು ಭಕ್ತರಲ್ಲ", 
        "10182": "ನಾದದೊಳಗಿಲ್ಲ, ಸುನಾದದೊಳಗಿಲ್ಲ.", 
        "10183": "ನಾನಾ ವೃಕ್ಷಂಗಳೊಳಗೆ ವಸ್ತು ತಾನೇ.", 
        "10184": "ನಾ ನೀನೆಂಬುದಳಿದ ಪರಮಲಿಂಗೈಕ್ಯಂಗೆ", 
        "10185": "ನಾನೆಂದೆನಲಿಲ್ಲ, ನುಡಿದು ಹೇಳಲಿಕ್ಕಿಲ್ಲ.", 
        "10186": "ನಾಮಾನಾಮವೆಂದು ನೀನು ನಾಮಕ್ಕೆ ಬಳಲುವೆ.", 
        "10187": "ನಿಜವನರಿದವಂಗೆ ಸತ್ಯವೂ ಇಲ್ಲ, ಅಸತ್ಯವೂ ಇಲ್ಲ;", 
        "10188": "ನಿನ್ನ ಪ್ರಾಣವಾದಡೆ ನಿನ್ನ ಹೇಳಿತ ಕೇಳದೆ?", 
        "10189": "ನಿರಾಳಲಿಂಗಕ್ಕೆ ನಿರೋಧ ಬಂದಿತ್ತಲ್ಲಾ!", 
        "10190": "ನೀರ ಕೊಳಗವ ಮಾಡಿ ನೆಳಲನಳೆದೆನೆಂಬ", 
        "10191": "ನೀರೊಳಗೆ ಕಿಚ್ಚಿರ್ದು ನೆಳಲ ಸುಟ್ಟಿತ್ತು!", 
        "10192": "ನೀರೊಳಗೊಂದು ನೆಳಲು ಸುಳಿಯಿತ್ತು", 
        "10193": "ಪರಮಂಗೆ ನಾನು ಮದವಳಿಗೆ.", 
        "10194": "ಪವನದ ಬಳಿಯಲಿ ಹರಿಯಲಿ ಹರಿಯಲಿ", 
        "10195": "ಪ್ರಾಣಲಿಂಗವ ಪರಲಿಂಗವ ಮಾಡಿ", 
        "10196": "ಪ್ರಾಣಲಿಂಗವೆಂದಡೆ ಹೇಳದೆ ಹೋಯಿತ್ತು.", 
        "10197": "ಪುರಹರ ಕ್ಷೇತ್ರದಲ್ಲಿ ಪುರಪತಿಯೊಡನೆ ಸಂಪಾದನೆ.", 
        "10198": "ಪೂಜಕರೆಲ್ಲರೂ ಪೂಜಿಸುತ್ತಿದ್ದರು.", 
        "10199": "ಪೃಥ್ವಿ ಅಪ್ಪು ತೇಜ ವಾಯುವಾಕಾಶವೆಂಬ", 
        "10200": "ಬಂಧವಿಲ್ಲದ ಭಾವವಿಲ್ಲದ ಸಂದಿಲ್ಲದ ಸಮರಸವಿಲ್ಲದ,", 
        "10201": "ಬಯಲೊಳಗುಣ ಬಯಲು ನಿರ್ವಯಲ ನುಂಗಿತ್ತು.", 
        "10202": "ಬಸವ ಮಾಡಿಹನೆಂದು ಬಾಯಲ್ಲಿ ಬಸಿವ", 
        "10203": "ಬಾಯಕ್ಕಿಯ ಬೇಡುವರೆಲ್ಲರು", 
        "10204": "ಭಕ್ತಿಯನರಿಯ ಭಾವವನರಿಯ,", 
        "10205": "ಭೂಮಿ ಘನವೆಂಬೆನೆ ಪಾದಕ್ಕೊಳಗಾಯಿತ್ತು", 
        "10206": "ಮಜ್ಜನದ ಮಾರಿಯೆ ಪತ್ರೆಯ ತಾಪತ್ರಯ", 
        "10207": "ಮನ್ಯ ಓಡಿ ಆಹಾ! ಮನ್ಯ ನಿಂದು", 
        "10208": "ಮಹಾನಾದ ಕಳೆಯವೆ ಲಿಂಗಕ್ಕಾಶ್ರಯವಾಗಿ,", 
        "10209": "ಮಹಾದೇವ ಹತಕ್ಕೆಡೆಯಾಡುವನೆ?", 
        "10210": "ಮಾಯೆ ಉಂಡವರೆಲ್ಲ ಮಹಂತ ಬೋದರು.", 
        "10211": "ಮುಂಡವ ಹೊತ್ತುಕೊಂಡು ಮಂಡಲಕ್ಕೆ ಬಂದಡೆ", 
        "10212": "ಮುಟ್ಟಿ ಭಕ್ತನು ಮುನ್ನಲ್ಲ, ಬಿಟ್ಟ ಸೂತಕಿ ಬಳಿಕಲ್ಲ.", 
        "10213": "ಮುನ್ನವೆ ಆಚಾರಕ್ಕೆ ಹೊರಗು, ನಾನನಾಚರಿಯಾದ ಕಾರಣ,", 
        "10214": "ಮೂರ ಬಿಟ್ಟವಂಗಲ್ಲದೆ ಸತ್ಯವ ಸಾಧಿಸಬಾರದು.", 
        "10215": "ಮೇಲನರಿತಲ್ಲಿ ಕೀಳಿಲ್ಲ,", 
        "10216": "ಯೋಗದ ಹೊಲಬ ಸಾಧಿಸಬಾರದು.", 
        "10217": "ರುದ್ರದೇವ ಮಹಾದೇವ ಇವರಿಬ್ಬರೂ ಬೀಗರಯ್ಯಾ.", 
        "10218": "ರೋಗಿಗೆ ಹಾಲು ಸಿಹಿಯಪ್ಪುದೆ?", 
        "10219": "ಲಿಂಗ ಜಂಗಮ, ಜಂಗಮ ಲಿಂಗವೆಂಬ ನಿಂದೆಯ ಮಾತು", 
        "10220": "ಲಿಂಗದಿಂದಲಾದ ಕಾಯವ ಹಿಂಗಿ ಹೋಹ ತೆರನಿನ್ನೆಂತೊ!", 
        "10221": "ಲಿಂಗವೆನ್ನೆ ಲಿಂಗೈಕ್ಯವೆನ್ನೆ.", 
        "10222": "ಲೀಲೆಗೆ ಹೊರಗಾದ ಲಿಂಗವೆ ಬಾರಯ್ಯಾ,", 
        "10223": "ವಾಙ್ಮನಕ್ಕೆ ಬಾರದ ಅಪ್ರತಿಮಲಿಂಗವ ಕೂಡಿ", 
        "10224": "ವಾಯದ ರಾಸಿಗೆ ಮಾಯದ ಕೊಳಗ", 
        "10225": "ವೇದ ಶಾಸ್ತ್ರ ಪುರಾಣಗಳಿಂದ ಅರಿದೆವೆಂಬ", 
        "10226": "ವೇಷವೆಂಬ ರುದ್ರನ ಪಾಶವ ಹೊತ್ತು", 
        "10227": "ಶಿವ ಶಕ್ತಿಯಲ್ಲಿ ತೃಪ್ತರಾದವರು ನೀವು ಕೇಳಿರೆ.", 
        "10228": "ಸತ್ತವಂಗೆ ಸಾಹಿತ್ಯ; ಇದ್ದವಂಗೆ ದೇವರು.", 
        "10229": "ಸತ್ತ ಶವ ಕಿಚ್ಚ ಬಲ್ಲುದೆ?", 
        "10230": "ಸರ್ಪಂಗೆ ಪಿಕಂಗೆ ಒಪ್ಪದ ಮನೆ ಉಂಟೆ?", 
        "10231": "ಸಬಳದ ತುದಿಯಲ್ಲಿ ಬಿರುದ ಕಟ್ಟಿ", 
        "10232": "ಸರದ ಸಾರದ ನಿಸ್ಸಾರಾಯವ ನಿರುತವಲ್ಲೆಂದು", 
        "10233": "ಸರಿವಿಡಿಯೆ ಗುರುವಿಡಿಯೆ.", 
        "10234": "ಸರ್ವ ಲಯವು ತಾನೆ.", 
        "10235": "ಸ್ಥಲನಲ್ಲ ಕುಳನಲ್ಲ ನಿಃಸ್ಥಲನಲ್ಲ!", 
        "10236": "ಸಾಧಕನವನೊಬ್ಬ ನೇತ್ರವಟ್ಟೆಯನುಟ್ಟುಕೊಂಡು", 
        "10237": "ಸಾರಂಗದ ತುದಿಯಲ್ಲಿ ಧಾರೋದ್ರಿಜವ ತುಂಬಿದರು.", 
        "10238": "ಸಾವಿರ ಪರಿಯಲಿ ಉದರವ ಹೊರೆವರೆ", 
        "10239": "ಸ್ಥಾವರವಾಗಿ ಭೂಮಿಯಲ್ಲಿ ಪೂಜಿಸಿಕೊಂಬುದು ಶೈವಲಿಂಗ.", 
        "10240": "ಸುಳಿದಡೆ ಒಡಲಿಲ್ಲ, ನಿಂದಡೆ ನೆಳಲಿಲ್ಲ,", 
        "10241": "ಸುಳುಹೆಂಬ ಸೂತಕವಡಗಿ,", 
        "10242": "ಹಲವು ಬಣ್ಣದ ಹಕ್ಕಿ ಹಣ್ಣ ಮೆಲಬಂದಡೆ,", 
        "10243": "ಹಸಿದು ತಾರಕೆ ಊಟವ ಕೇಳಹೋದಡೆ", 
        "10244": "ಹಸೆಯಿಲ್ಲದ ಹಂದರದಲ್ಲಿ ನಿಬ್ಬಣವಿಲ್ಲದ ಮದುವೆಯ ಮಾಡಿ", 
        "10245": "ಹಾದಿಯ ಬೆಳ್ಳನ ಕಳ್ಳರು ತಾಕಿದಡೆ", 
        "10246": "ಹಿಂದೆ ಕೆಟ್ಟುದ ಮುಂದೆ ನೀನರಸುವೆ.", 
        "10247": "ಹುಟ್ಟಿದ ಮರ ನೆಟ್ಟ ಕಲ್ಲು", 
        "10248": "ಹೆಣ್ಣು ಹೊನ್ನು ಮಣ್ಣು ಕುರಿತು ಚರಿಸುವನ್ನಬರ ಉಭಯದ ಕೇಡು.", 
        "10249": "ಹೆಣ್ಣು ಹೊನ್ನು ಮಣ್ಣಿಗೆ ಹೊಡೆದಾಡುವನ್ನಬರ", 
        "10250": "ಹೊರಗಣ ಮಲತ್ರಯಕ್ಕೆ ಒಳಗಣ ಅರಿವ ನೀಗಾಡಲೇತಕ್ಕೆ?"
    }
}