        "10783": "ಎಂಟು ಯೋಗ ಆರು ಭೇದ ಮೂರು ಬಟ್ಟೆ ಐದು ಮುಟ್ಟು", 
        "10784": "ಏತರ ದೊಣ್ಣೆಯಲ್ಲಿ ಹೊಯ್ದಡೂ ಪೂಸನಪ್ಪನು.", 
        "10785": "ಕಂಡಡೇನು ನುಡಿ ಅಡಗಿದ ಮತ್ತೆ", 
        "10786": "ಕಂಡ ಮತ್ತೆ ಕಂಡುದ ಕಾಣುವುದಕ್ಕೆ ಮುನ್ನವೇ ಕಾಣದುದ", 
        "10787": "ಕಂಡುದ ಬಿಟ್ಟು ಕಾಣದುದ ಕಂಡೆಹೆನೆಂದಡೆ", 
        "10788": "ಕಕ್ಷೆ ಭಕ್ತನ ಸೋಂಕು.", 
        "10789": "ಕಣ್ಣಿನಿಂದ ನಡೆದು, ಕಾಲಲ್ಲಿ ಮುಟ್ಟಿ ಕಂಡು", 
        "10790": "ಕರಣಂಗಳ ನಿವೃತ್ತಿಯ ಮಾಡಿ ಲಿಂಗವ ಕಂಡೆಹೆನೆಂಬಲ್ಲಿ", 
        "10791": "ಕಾಯದ ಗುಣವನರಿಯದೆ ಕರ್ಮಯೋಗವ ಮಾಡಬಹುದೆ?", 
        "10792": "ಕಾಲಿಗೆ ಕಣ್ಣು ಹುಟ್ಟಿ ಬಾಯಿ ಬಸುರಾಗಿ ಕಿವಿಯಲ್ಲಿ ಹುಟ್ಟಿ", 
        "10793": "ಕಾಲು ಮೂರು, ಬಸುರು ನಾಲ್ಕು, ಕೈ ಐದು,", 
        "10794": "ಕ್ರಿಯಾಶಕ್ತಿಗೆ ಫಟ ಕ್ರೀಪತಿಯಾಗಿ", 
        "10795": "ಕುಂಭದಲ್ಲಿ ಬೆಂದ ಅಶನಕ್ಕೆ ಒಂದಗುಳಲ್ಲದೆ", 
        "10796": "ಕುಸುಮ ಕ್ರೀ ಗಂಧ ನಿಃಕ್ರೀಯೆಂದಲ್ಲಿ", 
        "10797": "ಗಂಧವ ಬಂಧಿಸಿ ಹಿಡಿದೆಹೆನೆಂದಡೆ ಆ ಗಂಧದ", 
        "10798": "ಗುರುವನರಿದಲ್ಲಿ ಮೂರನರಿದು ಐದ ಬಿಡಬೇಕು.", 
        "10799": "ಗುರುವಿಂಗೊಂದು ಲಿಂಗಕ್ಕೆರಡು ಜಂಗಮಕ್ಕೆ ಮೂರು ಸತಿಸಂದಲ್ಲಿ,", 
        "10800": "ಗುರುವೈದು ಬಂದು ಮೂರು ಲಘುವಾರರಿಂದ ಹುಟ್ಟಿದ ಅಂಡ ಪಿಂಡ.", 
        "10801": "ಗುರುಸ್ವರೂಪನಾಗಿ ಬಂದು ಬ್ರಹ್ಮನ ಉತ್ಪತ್ಯವ ಕೆಡಿಸಿ", 
        "10802": "ಜಗಭಗದ ಊಧ್ರ್ವದಲ್ಲಿ ಮೋಹನದ ಗೊಟ್ಟಿ ಹುಟ್ಟಿತ್ತು.", 
        "10803": "ಜಲವ ಕಾಣದ ಬೋದನಂತೆ", 
        "10814": "ತಮದ ಗುರಿಯ ಜ್ಯೋತಿಯ ಕುಡಿವೆಳಗಿನ ಸರದಲ್ಲಿ ಎಸಲಿಕ್ಕಾಗಿ,", 
        "12921": "ತನಗೆ ಉಂಟಾಗಿ ಶಿಷ್ಯನ ಮನೆಗೆ ಹೋಗಿ", 
        "12979": "ತನು ಮೂರು, ಆತ್ಮವಾರು ಜೀವವೆರಡು", 
        "12980": "ತನುಧರ್ಮವ ಕಂಡು ಮನಧರ್ಮವನರಿದು", 
        "12981": "ತಾನಿರಿಸಿದ ಕಡವರವೆಂದಡೆ, ಸಾಕ್ಷಿಯಿಲ್ಲದೆ", 
        "12982": "ತ್ರಿವಿಧ ಕ್ರೀಯಿಂದ ಭೇದವನರಿತು ಮಾಡುವುದು ಭಕ್ತಿಸ್ಥಲ", 
        "12983": "ತ್ರಿವಿಧ ಪ್ರಸಾದವ ಸ್ವೀಕರಿಸುವಲ್ಲಿ,", 
        "12984": "ತುಂಬಿ ಕುಸುಮದ ಬಂಡುಂಬಂತೆ !", 
        "12985": "ತೊಟ್ಟುಬಿಡುವ ವೇಳೆಯನರಿದ ಮತ್ತೆ,", 
        "12986": "ದೀಕ್ಷಾಗುರುವಾಗಿ ಬಂದು ಬ್ರಹ್ಮನ ಉತ್ಪತ್ಯವ ತೊಡೆದೆ", 
        "12987": "ದೃಕ್ಕು ಸಕಲವನವಗವಿಸುವನ್ನಕ್ಕ", 
        "12988": "ದೃಷ್ಟದ ಜ್ಯೋತಿಯ ನಂದಿಸಬೇಕಲ್ಲದೆ", 
        "12989": "ಧ್ಯಾನವಿಲ್ಲದೆ ಲಿಂಗವ ನಿಧಾನಿಸಬಹುದೆ?", 
        "12990": "ನಸುಗಾಯವಡೆದವನಂತೆ ನೋವುಣ್ಣದೆ,", 
        "12992": "ನಾ ನೀನೆಂಬನ್ನಕ್ಕ ಕ್ರೀಯ ಮಾಡುತ್ತಿರಬೇಕು.", 
        "12993": "ನೋಡುವರೆಲ್ಲರು ಆಡಬಲ್ಲರೆ?", 
        "12994": "ಪಂಚಭೌತಿಕದಿಂದತ್ತ ಪಂಚಭೌತಿಕ ಒಳಗಾದುದರಿಂದಿತ್ತ,", 
        "12995": "ಪೃಥ್ವಿಯ ಅಂಶಿಕ ಶರೀರ ದರ್ಪಗೆಡುವುದ ಕಂಡು,", 
        "12997": "ಪೃಥ್ವೀತತ್ವದಿಂದ ಭಕ್ತಿರೂಪು,", 
        "12999": "ಪೃಥ್ವೀತತ್ವದೊಳಗಾದವೈದು", 
        "13001": "ಪ್ರಥಮ ಮೂರು, ಆಶ್ರಯ ನಾಲ್ಕು, ಸ್ಥಲವಾರು,", 
        "13002": "ಪ್ರಸಾದಕ್ಕೆಂದು ಮುಯ್ಯಾಂತು ಕೈವೊಡ್ಡಿ ಬಾಯಿದೆರೆವಲ್ಲಿ", 
        "13004": "ಬರಡು ಕರೆವಾಗ ಈಯಿತ್ತೆ?", 
        "13005": "ಬೇರಿನ ಬಣ್ಣ ನೋಡ ನೋಡಲಿಕ್ಕೆ ಹಬೆಗೆ ಹಾರುವ ತೆರದಂತೆ", 
        "13007": "ಬೇವಿನ ಮರನನೇರಿ ಬೆಲ್ಲವ ಮೆದ್ದಡೆ ಕಹಿಯಾದುದುಂಟೆ?", 
        "13008": "ಭಕ್ತಂಗೆ ಮೂರು ಗುಣ, ಮಾಹೇಶ್ವರಂಗೆ ಆರು ಗುಣ,", 
        "13009": "ಭಕ್ತನ ಕ್ರೀ, ಮಾಹೇಶ್ವರನ ನಿಶ್ಚಯ, ಪ್ರಸಾದಿಯ ನಿಷ್ಠೆ,", 
        "13010": "ಭಕ್ತ ಮಾಹೇಶ್ವರ ಪ್ರಸಾದಿ ಈ ತ್ರಿವಿಧ ಒಂದೆ ಬೀಜ", 
        "13011": "ಭಕ್ತಿಸ್ಥಲ ಮೂರು, ಮಾಹೇಶ್ವರಸ್ಥಲ ನಾಲ್ಕು,", 
        "13012": "ಭಕ್ತಸ್ಥಲವ ತಾಳ್ದ ಮತ್ತೆ ತ್ರಿವಿಧದಲ್ಲಿ ಆವ ಸೋಂಕು ಬಂದಡೂ", 
        "13013": "ಮರ್ಕಟನ ಉಚಿತ, ವಿಹಂಗನ ಪವನ, ಪಿಪೀಲಿಕನ ಧ್ಯಾನ,", 
        "13014": "ಮರ್ಕಟನ ಲಾಗು ಶಾಕೆಯುಳ್ಳನ್ನಕ್ಕ,", 
        "13015": "ಮಂಜರಿ ನುಂಗಿದ ಚೂಡ ತನ್ನ ಕಾಲವೇಳೆಗೆ", 
        "13016": "ಮಣ್ಣಿನಲ್ಲಿ ಹುಟ್ಟಿದ ಹೊನ್ನು, ಕಲ್ಪಿನಲ್ಲಿ ಹುಟ್ಟಿದ ರನ್ನ,", 
        "13017": "ಮರಾಳನ ಗದಕ ತೇಜಿಯ ಚಿತ್ತ ಪನ್ನಗನ ವಳಿ ಹೊಳಹಿನಂತೆ", 
        "13019": "ಮಾಂಸ ಪಿಂಡತ್ರಯ, ಸೂಕ್ಷ ್ಮ ಪಿಂಡತ್ರಯ, ಕಾರಣ ಪಿಂಡತ್ರಯ", 
        "13020": "ಮಾರುತನ ಮನೆಯಂತೆ ಉದಕ ಮುಕುರದ ಬಿಂಬದಂತೆ", 
        "13022": "ಮುತ್ತಿನ ನೀರಿನಂತೆ, ರತ್ನದ ಬೆಂಕಿಯಂತೆ,", 
        "13023": "ಮೂಲಾಧಾರ ಮುಂತಾದ ಷಡಾಧಾರಂಗಳಲ್ಲಿ", 
        "13025": "ಮೆಟ್ಟಿದ ಪಂಕವ ಅಪ್ಪುವಿನಿಂದಲ್ಲದೆ ನೀಗಬಾರದು.", 
        "13026": "ಯೋಗವೆಂಬುದೇನು ಲಕ್ಷ ್ಯದೊಳಗೆ ಅಲಕ್ಷ ್ಯವಡಗಿಪ್ಪುದು;", 
        "13028": "ಲಿಂಗಕ್ಕೆ ನೈವೇದ್ಯ ಸಕಲ ಪದಾರ್ಥ ಬಂದಿರಲಿಕ್ಕಾಗಿ", 
        "13029": "ಲಿಂಗ ಸಹಿತವಾಗಿ ಸರ್ವಗುಣಂಗಳ ಭೋಗಿಸಬೇಕೆಂಬಲ್ಲಿ", 
        "13030": "ಲಿಂಗಸ್ವರೂಪಕ್ಕೆ ಪಂಚಸೂತ್ರ ಲಕ್ಷಣವನರಿತು", 
        "13032": "ಲಿಂಗಾಂಗವಾದ ಭೋಗ ಭೋಜ್ಯಂಗಳ ತೆರನೆಂತುಟೆಂದಡೆ:", 
        "13033": "ವಾದ್ಯಕ್ಕೆ ಬಂಧವಲ್ಲದೆ ನಾದಕ್ಕೆ ಬಂಧವುಂಟೆ?", 
        "13035": "ವಿಷ ಚರದಲ್ಲಿ ಉಂಟು, ವಿಷ ಸ್ಥಾವರದಲ್ಲಿ ಉಂಟು.", 
        "13036": "ವಿಷ ತನ್ನೊಳಗಾದಡೂ ತನ್ನ ಕೇಡು,", 
        "13037": "ವಿರಕ್ತಂಗೆ ವಿಷಯವುಂಟೆ?", 
        "13039": "ಶಿಲೆಯೊಳಗೆ ಪಾವಕನಿಪ್ಪ ತೆರನಂತೆ", 
        "13040": "ಶ್ರೀಗುರು ಕುರುಹು ಕೊಟ್ಟು, ಕುರುಹಿನ ಬೆಂಬಳಿಯಲ್ಲಿ", 
        "13042": "ಷಡ್ಭಾವಲಿಂಗ ಷಡುವರ್ಣಭೇದ,", 
        "13043": "ಸಂಚದ ಸರಾಗದಂತೆ, ಮಿಂಚಿನ ಕುಡಿವೆಳಗಿನ ಸಂಚಾರದಂತೆ,", 
        "13045": "ಸತ್ತು ಚಿತ್ತುವಿನಿಂದ, ಚಿತ್ತು ಆನಂದದಿಂದ,", 
        "13046": "ಸರ್ಪ ಮುಟ್ಟಿದಡೇನು ವಿಷ ತಪ್ಪಿದ ಮತ್ತೆ?", 
        "13048": "ಸರ್ವಾಂಗಲಿಂಗ ವ್ಯವಧಾನಿ ಎಂದು ಆತ್ಮಂಗೆ ಕಟ್ಟಮಾಡಿ,", 
        "13049": "ಸಹಪಂತಿಯಲ್ಲಿ ಗುರುಚರವಿರುತಿರಲಿಕ್ಕಾಗಿ", 
        "13051": "ಸುಗಂಧದ ಅಂಗವಿದ್ದು ಕರಂಡದಲ್ಲಿ ಬಂಧಿಸಲಿಕ್ಕಾಗಿ,", 
        "13052": "ಸ್ಥೂಲತನುವ ಬಿಟ್ಟು ನಿನ್ನ ಕಂಡೆಹೆನೆಂದಡೆ", 
        "13054": "ಸ್ಥೂಲ ತನುವಿಂಗೆ ಶಿಲೆಯ ಮರೆಯಲ್ಲಿ", 
        "13055": "ಹರಿದ ನೀರಿನ ಅಡಿಯ ಕಾಣಬಹುದಲ್ಲದೆ", 
        "13057": "ಹುತ್ತಕ್ಕೆ ಸರ್ಪನಾಗಿ, ಉದಕಕ್ಕೆ ಒಳ್ಳೆಯಾಗಿ,", 
        "13058": "ಪ್ರಮಾಣವ ಪ್ರಮಾಣಿಸಿದಲ್ಲಿ ತಪ್ಪದಿಪ್ಪೆ."
    }
}