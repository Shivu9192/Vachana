        "16840": "ಗುರುವೆಂದರಿಯರು, ಲಿಂಗವೆಂದರಿಯರು, ಜಂಗಮವೆಂದರಿಯರು.", 
        "16841": "ಆವ ವೇಷವ ಧರಿಸಿದಡೇನು?", 
        "16842": "ತಪವೆತ್ತ? ಹೆರರ ಕೇಡಿಂಗೆ ಬಗೆವುದೆತ್ತ?", 
        "16843": "ಆಚಾರವನನಾಚಾರವ ಮಾಡಿ ನುಡಿವರು.", 
        "16844": "ಹಲವು ಮಾತಕಲಿತ ಉಲಿಗಿತಿ ಸೂಳೆಯ ಹಾಂಗೆ,", 
        "16845": "ನಿಚ್ಚ ನಿಚ್ಚ ಲಿಂಗಪೂಜೆಯ ಮಾಡಿ,", 
        "16846": "ನಿಚ್ಚ ಸಾವಿರ ನೋಂಪಿಯ ನೋಂತು,", 
        "16847": "ಆರಾದರೆ ಆಗಲಿ,", 
        "16848": "ಕಾಡೊಳಗಣ ಹುಲುಗಿಣಿಯ ಹಿಡಿತಂದು,", 
        "16849": "ಜಗದ ಪ್ರಾಣಿಗಳಂತೆ ಉಳಿವವರಿಗೆ,", 
        "16850": "ಜಾಳು ಮಾತೆಂದಡೆ ನಲಿದು ನಲಿದು ನುಡಿವರು.", 
        "16851": "ನಾನೆಲ್ಲಿಂದ ಬಂದೆ?", 
        "16852": "ಕಾಯದ ಕಳವಳವ ಗೆಲಲರಿಯದವರು,", 
        "16853": "ಹಾವಾಡಿಗ ಹಾವನಾಡಿಸುವಲ್ಲಿ, ತನ್ನ ಕಾಯ್ದುಕೊಂಡು,", 
        "16854": "ಇಂದಿಗೆಂತು ನಾಳಿಂಗೆಂತೆಂದು, ಬೆಂದ ಒಡಲಿಗೆ ಚಿಂತಿಸಿ,", 
        "16855": "ಸುಖ ದುಃಖ ಮೋಹದೊಡಲುಗೊಂಡು ಹುಟ್ಟಿತ್ತೀ ಜಗವೆಲ್ಲ.", 
        "16856": "ಕ್ಷುತ್ತು ಪಿಪಾಸೆ ಶೋಕ ಮೋಹ ಜರೆ ಮರಣ", 
        "16857": "ಶಿವಕಥನಂಗಳ ಕೇಳಿ ಕೇಳಿ  ಸಂತೋಷಿಸಿ,", 
        "16858": "ಅಂಗೈಯ ಲಿಂಗದಲ್ಲಿ ಕಂಗಳು ನಟ್ಟು,", 
        "16859": "ಹೊತ್ತುಳ್ಳಲ್ಲಿ ಅಗ್ಛವಣಿ ಪತ್ರೆ ಪುಷ್ಪವ ತಂದು,", 
        "16860": "ಜಾವ ಘಳಿಗೆಯ ನೇಮದ ಪೂಜೆಯ ನೇಮಕರೆಲ್ಲ,", 
        "16861": "ಶಿವಭಕ್ತರಾದವರು, ಅಂಗದ ಮೇಲಣ ಲಿಂಗಕ್ಕೆ,", 
        "16862": "ಶಿವಪ್ರಸಾದವನಲ್ಲದೆ ಕೊಳ್ಳನಾ ಭಕ್ತನು.", 
        "16863": "ಆಸ್ತಿ ಜಾಯತೇ ವಿಪರಿಣಮತೇ ವಿವರ್ಧತೇ", 
        "16864": "ಷಡಧ್ವಾಶ್ರಯವಾವ ಪರಬಿಂದುಸ್ಥಾನದ ಮೇಲಿರ್ದ", 
        "16865": "ಊದ್ರ್ವಮುಖವಾದ, ಅನಂತೇಶನೆಂಬ ವಾಸುಕಿಯ", 
        "16866": "ಅನ್ನಮಯ ಪ್ರಾಣಮಯ ಮನೋಮಯ", 
        "16867": "ಶ್ರೀ ವಿಭೂತಿಯ, ರುದ್ರಾಕ್ಷಿಯ ಧರಿಸಿ,", 
        "16868": "ನಾನು ಭಕ್ತನಪ್ಪೆನಯ್ಯ ನಿಮ್ಮ ಶ್ರೀ ಚರಣಸೇವಕನಾಗಿ,", 
        "16869": "ಎಲೆ ಶಿವನೆ, ನಾ ನಿಮ್ಮನೊಂದೆ ಬೇಡಿಕೊಂಬೆನು:", 
        "16870": "ಭಕ್ತನ ಕಾಯವೆ ಶಿವನ ಕಾಯ, ಶಿವನ ಕಾಯವೆ ಭಕ್ತನ ಕಾಯ.", 
        "16871": "ಶಿವಭಕ್ತರ ನಡೆ ಶುದ್ಧ, ನುಡಿ ಶುದ್ಧ.", 
        "16872": "ಗುರುವೆನೆ ಲಿಂಗವೆಂದರಿದ ಕಾರಣ, ಗುರುವ ಲಿಂಗದಲ್ಲಿ ಕಂಡೆ.", 
        "16873": "ಶ್ರೀಗುರುಕರುಣ ಕಟಾಕ್ಷಮಾತ್ರದಿಂದ ಶಿವಲಿಂಗದ ಒಲುಮೆ.", 
        "16874": "ಗುರುವ ನರಭಾವದಲ್ಲಿ ಕಂಡಡೆ, ನರಕ ತಪ್ಪದು ನೋಡಯ್ಯಾ.", 
        "16875": "ಗುರುದೇವನೆ ಮಹಾದೇವನು, ಗುರುದೇವನೇ ಪರಶಿವನು.", 
        "16876": "ತನು ಮುಂತಾದ ಕ್ರೀಯಿಂದ ಗುರುಭಕ್ತನಹುದಯ್ಯ.", 
        "16877": "ಪ್ರಸಾದವ ಬಯಸಿ ಪರವನರಿದೆನೆಂಬವಂಗೆ,", 
        "16878": "ಕೊಡುವಾತ ನಾನಲ್ಲ.", 
        "16879": "ಶಿವಭಕ್ತಿಯುಳ್ಳವರಿಗೆ, ಪುಣ್ಯವಿಲ್ಲ. ಪಾಪವಿಲ್ಲ.", 
        "16880": "ಬಿಸಜತಂತುವಿನ ಶೃಂಖಲದಿಂದ,", 
        "16881": "ಅಗ್ನಿ ರಜ್ಜುವಿನಿಂದ ಕಟ್ಟುವಡೆವುದೆ?", 
        "16882": "ಆದಿಬಿಂದು ಬೀಜವನರಿದು,", 
        "16883": "ತಪ ಸತ್ಕಿ ್ರೀ ಜಪ ಧ್ಯಾನ ಜ್ಞಾನಂಗಳಿಂದ ಮಾಡುವ ಭಕ್ತಿಗೆ", 
        "16884": "ಧ್ಯಾತೃ, ಧ್ಯಾನ ಧ್ಯೇಯವೆಂಬ ತ್ರಿವಿಧದ ಭೇದವನರಿದು,", 
        "16885": "ಅಂತರಂಗ ಶುದ್ಧವಿಲ್ಲದವರು,", 
        "16886": "ಏನೆಂದರಿಯರು, ಎಂತೆಂದರಿಯರು,", 
        "16887": "ಶಮೆ ದಮೆ ವಿವೇಕ ವೈರಾಗ್ಯ ಪರಿಪೂರ್ಣಭಾವ ಶಾಂತಿ", 
        "16888": "ಚಂದ್ರನಿಲ್ಲದ ರಾತ್ರಿ, ಸೂರ್ಯನಿಲ್ಲದ ದಿವಸವೇತಕ್ಕೆ ಬಾತೆ?", 
        "16889": "ಸಂಸಾರವೆಂಬ ಘೋರಾರಣ್ಯದೊಳಗೊಬ್ಬಳು,", 
        "16890": "ಭಕ್ತಸ್ಥಲ ಬಸವಣ್ಣಂಗಾಯಿತ್ತು.", 
        "16891": "ಶಿವಭಕ್ತಿತತ್ಪರನಾದ ಮಹಿಮನು", 
        "16892": "ನಿಜವನರಿದು ನಿರ್ಮಲಜ್ಞಾನಿಯಾದ ಕರ್ಮರಹಿತ ಶಿವಯೋಗಿ", 
        "16893": "ಜಪ ತಪ ನಿತ್ಯ ನೇಮ ವ್ರತ ಶೀಲಂಗಳೆಂಬ", 
        "16894": "ಶಿವಲಿಂಗ ಪೂಜೆಯಿಂದ ಭಕ್ತಿ ದೊರಕೊಂಬುದು,", 
        "16895": "ಗುರು ಲಿಂಗ ಜಂಗಮ ಪ್ರಸಾದವೆಂಬ", 
        "16896": "ಅತ್ತಲಿತ್ತ ಹರಿವ ಮನವ ನಿಲಿಸಿ ಸ್ವಸ್ಥಾನಂಗೊಳಿಸಿ,", 
        "16897": "ಏನನೋದಿ ಏನ ಕೇಳಿ ಏನ ಹೇಳಿದಡೆ ಏನು ಫಲ", 
        "16898": "ಲಿಂಗವ ನೆರೆಯರಿದು ನಿಷ್ಠೆ ನಿಬ್ಬೆರಸಿದ ಬಳಿಕ", 
        "16899": "ಲಿಂಗದ ಕಲೆಯನರಿದಲ್ಲದೆ ಭಕ್ತಿ ವಿರಕ್ತಿಯೆಂಬವು", 
        "16900": "ಕುರುಡ ಕನ್ನಡಿಯ ಹಿಡಿದಡೇನು?", 
        "16901": "ಗಿರಿ ತರು ಗುಹೆ ಮಹಾವಿಪಿನ ಸರೋವರದಲ್ಲಿ", 
        "16902": "ಇಬ್ಬರು ಮೂವರು ದೇವರೆಂದು ತಬ್ಬಿಬ್ಬುಗೊಂಡು ನುಡಿಯಬೇಡ.", 
        "16903": "ಅಂಧಕನ ಮುಂದೆ ನೃತ್ಯ ಬಹುರೂಪವನಾಡಿದಡೇನು", 
        "16904": "ಹಲವು ದೇವರುಗಳ ಭಜಿಸಿ ಹೊಲಬುದಪ್ಪಿ ಹೋದರೆಲ್ಲ.", 
        "16905": "ಬ್ರಹ್ಮಾದಿ ದೇವತೆಗಳೇನು ಮುಕ್ತಿ ದಾನಶೀಲರೇ? ಅಲ್ಲ.", 
        "16906": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ಇಂದ್ರಾದಿ ದಿಕ್ಪಾಲಕರ ಪದವೆಲ್ಲ", 
        "16907": "ಲಿಂಗವ ಸ್ಥೂಲವೆಂಬರು ಕೆಲವರು, ಲಿಂಗ ಸ್ಥೂಲವಲ್ಲ.", 
        "16908": "ಲಿಂಗವೆಂಬುದು ಸರ್ವಕಾರಣ ಪರಮ ನಿರ್ಮಲ.", 
        "16909": "ಅನಲ ಸಂಗದಿಂದ ಕಾಷ್ಠ ಅನಲವಾದಂತೆ,", 
        "16910": "ಹುರಿದ ಬೀಜದಂತೆ, ಬೆಂದ ನುಲಿಯಂತೆ,", 
        "16911": "ಕರ್ಮರಹಿತನಾದ ನಿರ್ಮಲ ನಿತ್ಯಂಗೆ ಮಾಯಾ ಭ್ರಮೆಯಿಲ್ಲ.", 
        "16912": "ಶರಣ ಸಕಾಯನೆಂಬೆನೆ ಸಕಾಯನಲ್ಲ.", 
        "16913": "ಸೂರ್ಯೋದಯವಾಗೆ ತಿಮಿರ ಉಂಟೆ ಹೇಳಾ?", 
        "16914": "ತಾನೆಂಬುದನರಿಯ, ಪರವೆಂಬುದನರಿಯ,", 
        "16915": "ಅರುಣೋದಯಕ್ಕೆ ಕತ್ತಲೆ ಹರಿದು ಬೆಳಗು ಪಸರಿಸಿದಂತೆ,", 
        "16916": "ಅನುಭಾವವಿಲ್ಲದ[ವನ] ಭಕ್ತಿ ಆಯುಧವಿಲ್ಲದ ವೀರನಂತೆ.", 
        "16917": "ಆವಾವ ಜಾತಿ ಯೋನಿಗಳಲ್ಲಿ ಹುಟ್ಟಿದ ಪ್ರಾಣಿಗಳು", 
        "16918": "ಎತ್ತನೇರಿ ನಡೆಸುವ ಅಣ್ಣಗಳಿರ,", 
        "16919": "ಅಂಡಜ ಸ್ವೇದಜ ಉದ್ಭಿಜ ಜರಾಯುಜವೆಂಬ,", 
        "16920": "ಮೂರುಮುಖದಗ್ನಿಯೆದ್ದು ಮೂರುಲೋಕವ ಸುಡುತ್ತಿರಲು", 
        "16921": "ನಿತ್ಯಾನಿತ್ಯವಿಚಾರವಿಡಿದು ನೋಡಿದಡೆ,", 
        "16922": "ತನ್ನಲ್ಲಿ ತಾನು ಪ್ರತ್ಯಕ್ಷಾನುಭಾವದಿಂದ ತಿಳಿದುನೋಡಿ,", 
        "16923": "ಅದ್ವೆ ೈತವ ಸಾಧಿಸಿ ಸರ್ವವೂ ಶಿವನೆಂಬರು ಎನಲಾಗದು.", 
        "16924": "ಶಿವನು ಸರ್ವಗತನಾಗಿ ಎಲ್ಲಿಯು ಇಹನೆಂದಡೆ", 
        "16925": "ಶ್ರೀಗುರುಸದ್ಭಾವಜಾತಲಿಂಗವು", 
        "16926": "ತನುವಿಂಗೆ ತನುವಾಗಿ, ಮನಕ್ಕೆ ಮನವಾಗಿ,", 
        "16927": "ಪೃಥ್ವಿ ಅಪ್ಪು ತೇಜ ವಾಯು ಆಕಾಶ ಚಂದ್ರ ಸೂರ್ಯ ಆತ್ಮನೆಂಬ", 
        "16928": "ಆದಿಮುಕ್ತನ, ಅನಾದಿಮುಕ್ತನ, ನಾದ, ಬಿಂದು, ಕಳಾತೀತನ,", 
        "16929": "ಕಾದಬಲ್ಲೆವೆಂಬವರೆಲ್ಲ ಕಲಿವೀರಭಟರಹರೆ?", 
        "16930": "ಹಣೆಗಣ್ಣು ಕೊರಳಕಪ್ಪು ಫಣಿಕುಂಡಲ ದಶಭುಜ ಉಡುಪತಿಯ", 
        "16931": "ಆವಾವ ಘಟವ ಧರಿಸಿ ಜೀವಿಸುವ ಜೀವರಿಗೆ,", 
        "16932": "ಬೀಜದಿಂದ ಅಂಕುರ ತೋರಿದ ಬಳಿಕ", 
        "16933": "ಇಂದು ಭಾನುವನೊಂದುಗೂಡಿಸಿ,", 
        "16934": "ಸುವಿಚಾರದರಿವು ಕಣ್ದೆರೆದು,", 
        "16935": "ಶಿವಜ್ಞಾನೋದಯವಾದ ಭಕ್ತನ ಪೂರ್ವಾಶ್ರಯವಳಿದು", 
        "16936": "ಪರಿಶುದ್ಧ ಮನದಿಂದ ಲಿಂಗವ ಧರಿಸಿ,", 
        "16937": "ನಾ ಬಲ್ಲೆ ತಾಬಲ್ಲೆನೆಂಬುವರೆಲ್ಲ", 
        "16938": "ಲಿಂಗನಿಷ್ಠಾಭರಿತನಾದ ಕಾರಣ ಅಂಗಗುಣವಳಿದು", 
        "16939": "ಸ್ವರ್ಗ ಮತ್ರ್ಯ ಪಾತಾಳವೆಂಬ ಮೂರುಲೋಕದ", 
        "16940": "ಶಿವ ತನ್ನ ಶಕ್ತಿಯಿಂದ ಜಗವನಧಿಷ್ಠಿಸಿ ತೊಲಗದೆ,", 
        "16941": "ಹಲವು ತೃಣ ಕಾಷ್ಠಗಳಲ್ಲಿ ಅಗ್ನಿ ಬೆರದಿರ್ದಂತೆ,", 
        "16942": "ಸುಜ್ಞಾನವೆಂಬ ಹಡಗನೇರಿದ ಗುರು", 
        "16943": "ವೇಷವ ಧರಿಸಿ, ಭಾಷೆಯ ಕಲಿತು, ದೇಶವ ಸುತ್ತಿ ಬಳಲಬೇಡ.", 
        "16944": "ಶಿರ ಮುಖ ಹೃದಯ ಪಾದ ಬಾಹುಗಳೆಲ್ಲ", 
        "16945": "ಸರ್ವೇಂದ್ರಿಯಂಗಳಲ್ಲಿ ಸರ್ವಮುಖವಾಗಿ ಬಂದ ಪದಾರ್ಥದ", 
        "16946": "ಘ್ರಾಣ ಮುಟ್ಟದ ಮುನ್ನ ನಿಮಗರ್ಪಿತವಾಯಿತ್ತು.", 
        "16947": "ಅಯ್ಯಾ ಘ್ರಾಣದಲ್ಲಿ ನಿಂದು ಗಂಧವ ಗ್ರಹಿಸಿ", 
        "16948": "ಅಯ್ಯಾ, ನಿಮಗೆ ಪತ್ರೆ ಪುಷ್ಪದಿಂದ ಪೂಜೆಯ ಮಾಡುತಿರ್ದಡೇನು", 
        "16949": "ಕಾಯದ ಕರದಲ್ಲಿ ಲಿಂಗವ ಧರಿಸಿ,", 
        "16950": "ಮನ ಮನವ ಬೆರೆಸಿ, ಸ್ನೇಹ ಬಲಿದ ಬಳಿಕ,", 
        "16951": "ಪ್ರಕಾಶದ್ವಾರ ಗಂಧದ್ವಾರ ಶಬ್ದದ್ವಾರವೆಂಬ", 
        "16952": "ನಡೆವ ಗತಿಗಳಲ್ಲಿ ನಿಮ್ಮ ಕೂಡಿ ನಡೆವೆನಯ್ಯ.", 
        "16953": "ಊಧ್ರ್ವಮುಖಮೂಲ ಅಧೋಶಾಖೆಯಾದ", 
        "16954": "ಸಿತ ಕೆಂಪು ಕೃಷ್ಣವೆಂಬ ಮೂರರ ಮೇಲೆ,", 
        "16955": "ಅರಳಿದ ಪುಷ್ಪ ಪರಿಮಳಿಸಿದಲ್ಲದೆ ಮಾಣದು.", 
        "16956": "ಶರಣನ ಶರೀರವ ಸೋಂಕಿದ", 
        "16957": "ಪದಾರ್ಥವನರ್ಪಿಸಿ ಪ್ರಸಾದವ ಪಡೆದೆವೆಂದೆಂಬರು", 
        "16958": "ಶ್ರೋತ್ರೇಂದ್ರಿಯವನು ಆ ಇಂದ್ರಿಯದೊಡನೆ", 
        "16959": "ಶುದ್ಧಾನ್ನಪಾನಾದಿಗಳನು ಮಂತ್ರಭಸ್ಮದಿಂದ ಪವಿತ್ರವ ಮಾಡಿ,", 
        "16960": "ಪದಾರ್ಥದ ಪೂರ್ವವ ಕಳೆದಲ್ಲದೆ ಹಿಡಿಯಬಾರದು.", 
        "16961": "ಕರ್ಣಂಗಳು ಕೇಳಿದ ಶಬ್ದದಿಂದಾದ ಸುಖವನು,", 
        "16962": "ದೇವಾ, ನಿನ್ನ ಭಕ್ತನು ಶ್ರೋತ್ರಮುಖದಲ್ಲಿ", 
        "16963": "ಅಯ್ಯಾ ನಿನ್ನ ಭಕ್ತನು ನಿನ್ನನಲ್ಲದೆ ಕೇಳನಾಗಿ,", 
        "16964": "ದೇವಂಗೂ ಭಕ್ತಂಗೂ ದೇಹವೊಂದೇ ಪ್ರಾಣವೊಂದೇ", 
        "16965": "ಅಂಗದಲ್ಲಿ ಲಿಂಗ ಬೆರೆದು, ಲಿಂಗದಲ್ಲಿ ಅಂಗ ಬೆರೆದು", 
        "16966": "ಶಿವನ ಶಿವಪ್ರಸಾದಿಯ ಉಭಯಸಂಬಂಧ", 
        "16967": "ಶಬ್ದವನು ಕಿವಿಗಳು ಬಲ್ಲವೆ? ಅರಿಯವು.", 
        "16968": "ಲಿಂಗಭಕ್ತನ ಇಂದ್ರಿಯಂಗಳು, ಲಿಂಗ ಸನ್ನಿಹಿತವಾಗಿ", 
        "16969": "ರೂಪಾರ್ಪಣಕ್ಕೆ ಅಂಗವೇ ಆದಿ.", 
        "16970": "ಗುರುಲಿಂಗ ಪ್ರಸಾದವನು ಸೇವಿಸುವಲ್ಲಿ ಏಕರತಿಯಾಗಿರಬೇಕು.", 
        "16971": "ಕಾಲೋಚಿತವಾಗಿ ಅರ್ಪಿತಕ್ಕೆ ಬಂದ ಭೂತಜಾತಪದಾರ್ಥಂಗಳನು,", 
        "16972": "ಪರಮಾತ್ಮಲಿಂಗದಲ್ಲಿ ಅರ್ಪಿತವಾಗಿ", 
        "16973": "ತಾನು ಲಿಂಗಾವಧಾನಿಯಾಗಿ,", 
        "16974": "ಭಕ್ತನಾದಡೆ ಇಂದ್ರಿಯಂಗಳ ಭಕ್ತರ ಮಾಡಿ,", 
        "16975": "ಶ್ರೀ ಗುರುವಿನ ಮಂತ್ರೋಪದೇಶವು", 
        "16976": "ತ್ರಾಸಿನ ತೂಕದಂತೆ, ಅಂಗ ಲಿಂಗ ಸಮವಾಗಿ,", 
        "16977": "ರೂಪು ಕುರೂಪುಗಳನು,", 
        "16978": "ಲಿಂಗಕ್ಷೇತ್ರವೆಂಬ ಪವಿತ್ರಸ್ಥಲದಲ್ಲಿ,", 
        "16979": "ಪ್ರಸಾದವನು ಪ್ರಸಾದಿಯನು ಪದಾರ್ಥದ ಭೇದವನು", 
        "16980": "ಅನುಭಾವ ನೆಲೆಗೊಂಡಲ್ಲದೆ,", 
        "16981": "ಅನುಭಾವವ ನುಡಿವ ಅಣ್ಣಗಳಿರಾ,", 
        "16982": "ನಡೆವ ಬಟ್ಟೆಯ ಬಿಡಿಸಿ,", 
        "16983": "ಆರಾಧಾರದ ಆರು ಕಮಲಂಗಳಲ್ಲಿ,", 
        "16984": "ನಾಲ್ಕು ಬಾಗಿಲು ಕೂಡಿದ ಠಾವಿನಲ್ಲಿ", 
        "16985": "ಅಂತರಂಗದಲ್ಲಿದ್ದ ನಿರ್ಮಲ ಪರಂಜ್ಯೋತಿ", 
        "16986": "ಮೂರಾಧಾರ ಮಧ್ಯದಲ್ಲಿ ಹುಟ್ಟಿದ ಪ್ರಾಣಾಪಾನಂಗಳು", 
        "16987": "ಲಿಂಗದಲ್ಲಿ ಪ್ರಾಣವನಿರಿಸಿ, ಪ್ರಾಣದಲ್ಲಿ ಲಿಂಗವನಿರಿಸಿ", 
        "16988": "ತಿರುಳು ಕರಗಿದ ಬೀಜ ಮರಳಿ ಹುಟ್ಟಬಲ್ಲುದೆ ಅಯ್ಯಾ?", 
        "16989": "ತನುವಿಂಗೆ ತನುರೂಪಾಗಿ ತನುವಿಂಗಾಧಾರವಾದೆ.", 
        "16990": "ಖಂಡಿತವಿಲ್ಲದ ಅಖಂಡಿತರೂಪ ನೀನು ಕಂಡಾ ಎಲೆ ಅಯ್ಯಾ.", 
        "16991": "ಕಾಯದ ಸ್ಥಿತಿಗತಿಯನರಿದು, ಕಾಯವ ಶೋಧಿಸಿ", 
        "16992": "ವೇದ ಶಾಸ್ತ್ರ ಆಗಮ ವಿಜ್ಞಾನ ತರ್ಕ ವ್ಯಾಕರಣಾದಿಗಳ ಕಲಿತು", 
        "16993": "ಅಂತರಂಗ ಸನ್ನಿಹಿತ ಪ್ರಾಣಲಿಂಗಕ್ಕೆ", 
        "16994": "ಪೂರ್ವದ್ವಾರಮಂ ಬಂಧಿಸಿ ಅಧೋದ್ವಾರವ ಬಲಿದು", 
        "16995": "ಬಿಂದು ನಾದವನೊಂದು ರೂಪು ಮಾಡಿ", 
        "16996": "ಒಳಗೆ ಹೊರಗಾಯಿತ್ತು, ಹೊರಗೆ ಒಳಗಾಯಿತ್ತು.", 
        "16997": "ಅರ್ಕನ ಉದಯಕ್ಕೆ ಕತ್ತಲೆ ಹರಿದು, ಚಕ್ಕನೆ ಬೆಳಗಾಯಿತ್ತು.", 
        "16998": "ಒಡಲೆಂಬ ಗುಡಿಯೊಳಗೆ", 
        "16999": "ಏಳುನೆಲೆಯಲ್ಲಿ ಹೂಳಿದ ನಿಧಾನವ ಸಾಧಿಸಹೋದರೆ", 
        "17000": "ಆಧಾರ ಸ್ವಾಧಿಷ್ಠಾನ ಮಣಿಪೂರಕ ಅನಾಹತ", 
        "17001": "ಬ್ರಹ್ಮಸ್ಥಾನದ ಬಳಿಯ ಸಹಸ್ರದಳಕಮಲ ಮಧ್ಯದಲ್ಲಿ", 
        "17002": "ತನುವೆಂಬ ಭೂಮಿಯ ಮೇಲೆ,", 
        "17003": "ಹುಲ್ಲಹೊರೆಯೊಳಗೊಂದು ಕಿಚ್ಚು ಹುಟ್ಟಿ ಸುಡುವುದ ಕಂಡೆ.", 
        "17004": "ಆಸನ ಬಂಧವ ಮಾಡಿ,", 
        "17005": "ಯೋಗದ ನೆಲೆಯನರಿದೆನೆಂಬವರಿಗೆ ಹೇಳಿಹೆವು ಕೇಳಿರೇ.", 
        "17006": "ಇಂಬಾದ ಬ್ರಹ್ಮದಲ್ಲಿ ತುಂಬಿದ ಜಗವೆಲ್ಲಾ ಸಂಭ್ರಮಿಸುತ್ತದೆ", 
        "17008": "ಪ್ರಾಣಾಪಾನಸಂಘಟದಿಂದ ಪ್ರಾಣನ ಗುಣವಳಿದು", 
        "17009": "ಸ್ವಸ್ಥ ಸಿದ್ಧಾಸನದಲ್ಲಿ ಕುಳ್ಳಿರ್ದು ಅತ್ತಿತ್ತ ಕಂಪಿಸದೆ", 
        "17010": "ಊರ ಮೇಗಡೆಯಲೊಂದು ನರಿ ಕೂಗಿಡಲು", 
        "17011": "ಮಿಂಚಿನ ರವೆಯಂತೆ ಎನ್ನಂತರಂಗದಲ್ಲಿ ತೋರುವೆ, ಅಡಗುವೆ,", 
        "17012": "ಕೋಳಿ ಕೂಗಿದಡೆ ಹಾವು ಹೆಡೆ ಎತ್ತಿತ್ತು.", 
        "17013": "ಭೂಮಿಯ ಮೇಲಣ ಅಗ್ನಿ, ಆಕಾಶವನಡರಿದಡೆ", 
        "17014": "ಗರುಡನ ಗರಿಯ ಮುರಿದು, ಉರಗನ ಸಪ್ತ ಡೊಂಕ ತಿದ್ದಿ,", 
        "17015": "ಸ್ವಯಂ ಜ್ಯೋತಿರ್ಲಿಂಗ ಸಂಗದಿಂದ", 
        "17016": "ರೂಪು ಲಿಂಗವೋ ನಿರೂಪು ಲಿಂಗವೋ ಎಂಬುದ", 
        "17017": "ಶಿರದೊಳಗೆ ಶಿರ, ಕರದೊಳಗೆ ಕರ,", 
        "17018": "ಸತ್ವ ರಜ ತಮದ ಮೇಲೆ ನಿಂದು ಬೆಳಗುವ ಜ್ಯೋತಿಯ ಬೆಳಗು", 
        "17019": "ಮೂರು ಮಂಡಲದಲ್ಲಿ ತೋರುವ ಜ್ಯೋತಿಯದು", 
        "17020": "ಹರಿವ ಮನ ವಾಯುವನೊಂದು ಹುರಿಯ ಮಾಡಿ", 
        "17021": "ಮರ ಮೊದಲಿಗೆ ಬೀಳಲು", 
        "17022": "ಯೋಗತಾಣವನರಿದು ಯೋಗಿಸಿಹೆನೆಂಬ ಯೋಗಿಗಳಿಗೆ", 
        "17023": "ಶುದ್ಧ ಸಿದ್ಧಾಸನದಲ್ಲಿ ಕುಳ್ಳಿರ್ದು ಅರ್ಧಾವಲೋಕನದಿಂದ", 
        "17024": "ಯೋಗದ ಲಾಗನರಿದು ಯೋಗಿಸಿಹೆನೆಂಬ", 
        "17025": "ಅಂತರಂಗದಲ್ಲಿ ಬೆಳಗುವ ಜ್ಯೋತಿರ್ಲಿಂಗವು,", 
        "17026": "ನುಡಿಯೊಳಗೆ ಸತ್ಯ, ಹೃದಯದಲ್ಲಿ ಯುಕ್ತಿ.", 
        "17027": "ತಾಲು ಮೂಲ ದ್ವಾದಶಾಂತದ ಮೇಲಣ ಚಿತ್ಕಲಾ ಸೂರ್ಯನು", 
        "17028": "ಆಜ್ಞಾಚಕ್ರದ ಮಧ್ಯಹೃದಯದಲ್ಲಿ ಭರಿತವಾಗಿ", 
        "17029": "ಆತ್ಮ ಪರಮಾತ್ಮ ಯೋಗವನರಿದೆನೆಂಬ", 
        "17030": "ಅರೆದೆರೆದೆವೆ ದೃಷ್ಟಿಯ ನಾಸಿಕಾಗ್ರದಲ್ಲಿರಿಸಿ ನೋಡುತ್ತ,", 
        "17031": "ಜಂಬೂದ್ವೀಪದ ಮಧ್ಯದಲ್ಲಿ ಹುಟ್ಟಿ ಬೆಳೆದ ವಂಶದ", 
        "17032": "ಪ್ರಾಣಲಿಂಗೈಕ್ಯವಾದ ಬಳಿಕ ವಿಧಿ ನಿಷೇಧ, ಪುಣ್ಯ ಪಾಪ", 
        "17033": "ಲಿಂಗವ ನೋಡಲೆಂದು ಅಂಗವಿಸಹೋದರೆ", 
        "17034": "ಸ್ಥೂಲ ಸೂಕ್ಷ ್ಮ ಕಾರಣವೆಂಬ ತನುತ್ರಯವ ವಿಭಾಗಿಸಿ ಕಳೆದು", 
        "17035": "ಭೂತಪಂಚಕಕಾಯವ ಕಳೆದು", 
        "17036": "ಆಚಾರದನುಭಾವವಿಡಿದು, ವಿಚಾರವಂತರಾಗಿ,", 
        "17037": "ಮೂಲಮಂತ್ರಾತ್ಮಸ್ವರೂಪದಿಂದ", 
        "17038": "ಕಾಷ್ಠದ ಮೇಲೊಂದು ಕಾಷ್ಠವನಿರಿಸಿ", 
        "17039": "ಕರ್ಮವ ನುಂಗಿತ್ತು ಹಾಹೆ, ಹಾಹೆಯ ನುಂಗಿತ್ತು ರಜ್ಜು,", 
        "17040": "ಚಂದ್ರಸೂರ್ಯರ ಹಿಡಿದೆಳೆತಂದು", 
        "17041": "ಕಾಲ ಕೈಯೊಳಗಿರಿಸಿ ನಡೆವಾತನ ನಡೆ ಶುದ್ಧ.", 
        "17042": "ಸ್ಫಟಿಕದ ಘಟದಂತೆ ಒಳಹೊರಗೆ ಒಂದೆ ಪರಿ ನೋಡಾ.", 
        "17043": "ಅಂತರಂಗದಲ್ಲಿ ಆಡುವ ಪಕ್ಷಿಯ", 
        "17044": "ನಿಮ್ಮ ಪೂಜೆಯ ಮಾಡುವೆನೆಂತಯ್ಯ", 
        "17045": "ಗುರುವ ತಲೆಯಲ್ಲಿ ಹೊತ್ತು ನಡೆವ ಶಿಷ್ಯನಲ್ಲಿ ಗುರುವಡಗಿ", 
        "17046": "ಗುರು ಶಿಷ್ಯನ ಕೊಂದು ಶಿಷ್ಯನಾದನು.", 
        "17047": "ಆಚಾರವೆಂಬ ಭಿತ್ತಿಯ ಮೇಲೆ, ವಿಚಾರವೆಂಬ ಚಿತ್ರವ ಚಿತ್ರಿಸಿ,", 
        "17048": "ಶಿವಯೋಗಿ ಸುಳಿದ ಸುಳುಹು ಜಗತ್ಪಾವನ.", 
        "17049": "ಲಿಂಗದಲ್ಲಿ ಲೀಯವಾದ ಪರಮಾನಂದ ಸ್ವರೂಪಂಗೆ ಕಾಯವಿಲ್ಲ,", 
        "17050": "ನರ ಸುರ ಮುನಿಗಳೊಳಗಲ್ಲ ಶರಣ.", 
        "17051": "ಲಿಂಗ ಬೇರೆ, ಶರಣ ಬೇರೆಂದು ಹಂಗಿಸಿ ನುಡಿಯಲಾಗದು.", 
        "17052": "ಜ್ಞಾನಿಯ ನಡೆ ನುಡಿ ಅಜ್ಞಾನಿಗೆ ಸೊಗಸದು.", 
        "17053": "ಊರೊಂದೆಸೆ, ಕಾಡೊಂದೆಸೆ.", 
        "17054": "ಅಂತರಂಗ ಶುದ್ಧವಿಲ್ಲದವರೊಳಗೆ", 
        "17055": "ಕೆಲರೊಳ್ಳಿದನೆಂಬರು, ಕೆಲರು ಹೊಲ್ಲನೆಂಬರು", 
        "17056": "ಸರ್ವ ಕ್ರೀಯ ಲಯಸ್ಥಾನವಾದ ಭಾವಭರಿತ ಲಿಂಗವ", 
        "17057": "ಮೃತ್ ಕಾಷ್ಠ ಷಾಷಾಣಂಗಳಿಂದಲಾದವೆಲ್ಲ ಲಿಂಗವೇ?", 
        "17058": "ಜ್ಞಾನಾನಂದ ಪರಬ್ರಹ್ಮವೆ ಲಿಂಗವೆಂದು ಅರಿದ ಅರಿವು", 
        "17059": "ನಡುರಂಗದ ಜ್ಯೋತಿ ಕವಲುವಟ್ಟೆಯಲ್ಲಿ ಕುಡಿವರಿದು", 
        "17060": "ತುದಿ ಮೊದಲಾಯಿತ್ತು. ಮೊದಲೇ ತುದಿಯಾಯಿತ್ತು.", 
        "17061": "ನೆಲವಿಲ್ಲದಲ್ಲಿಯ ಉದಕವ ತಂದು,", 
        "17062": "ಆಶೆಯೆಂಬ ಶೃಂಖಲೆಯಿಂದ", 
        "17063": "ಕಾಶಿಯಲ್ಲಿ ಗೋವಧೆಯ ಮಾಡಿ ಗುರುಕರುಣವ ಪಡೆದು", 
        "17064": "ಆನೆಯು ಕೋಣನೂ ಕೂಡಿ ಅಡವಿಯಲ್ಲಿ ಆಡುತ್ತಿರಲು", 
        "17065": "ಹರಿವ ಹರಿಯ ನಿಲಸಿ, ಉರಗನ ನಿದ್ರೆಯ ಕೆಡಿಸಿ,", 
        "17066": "ಉರಗನ ಫಣಾಮಣಿಯ ಬೆಳಗನರಿ ಕಂಡಾ.", 
        "17067": "ಸಪ್ತದ್ರವ್ಯಂಗಳು ಎಡೆಯಿಲ್ಲದೆ ಹೋದುವು.", 
        "17068": "ಅಂಬರದಲಾಡುವ ಪಕ್ಷಿ ಕೊಂಬಿನ ಮೇಲಣ ಕೋಡಗವ ನುಂಗಿ", 
        "17069": "ತಾಳಮರದ ಮೇಲಣ ಕೋಡಗ, ತಾಳರಸವ ಕೊಂಡು", 
        "17070": "ಕಿಚ್ಚಿನೊಳಗೆ ಕಿಚ್ಚು ಹುಟ್ಟಿ ಉರಿವುತ್ತಿದ್ದಿತ್ತು.", 
        "17071": "ಆವಾವ ಭಾಷಾಂಗವ ತೊಟ್ಟರೂ", 
        "17072": "ನಾನು ಹಿರಿಯ, ತಾನು ಹಿರಿಯರೆಂಬವರೆಲ್ಲ ಹಿರಿಯರೇ?", 
        "17073": "ಈಶವೇಷವ ಧರಿಸಿದಡೇನು ವೇಷಕ್ಕೆ ಈಶನಂಜಿ ಒಲಿವನೇ?", 
        "17074": "ಅಶನಕ್ಕಾಗಿ ವನಸಕ್ಕಾಗಿ ದೆಸೆ ದೆಸೆಯಲ್ಲಿರ್ದವರೆಲ್ಲ", 
        "17075": "ಹರಗಣಂಗಳೆಲ್ಲ ನರಗಣಂಗಳಾಗಿ", 
        "17076": "ಅಂಗದ ಮೇಲೆ ಲಿಂಗ ಬರಲಾಗಿ, ಕಾಯದ ಗುಣವಳಿದು", 
        "17077": "ಪ್ರಸಾದ ಪ್ರಸಾದವೆಂದೆಂಬರು,", 
        "17078": "ಬ್ರಹ್ಮ ವಿಷ್ಣುಗಳಿಗಗೋಚರ ಪ್ರಸಾದವು.", 
        "17079": "ಅಂಗದೊಳಗೆ ಲಿಂಗವಿದೆ, ಲಿಂಗದೊಳಗೆ ಅಂಗವಿದೆ.", 
        "17080": "ಸಮತೆಯೆಂಬ ಕಂಥೆಯ ಧರಿಸಿ", 
        "17081": "ಹರ ಹರ ಶಿವ ಶಿವ ಪ್ರಸಾದದ ಘನವ,", 
        "17082": "ಭಾವಿಸಬಾರದ ಪ್ರಸಾದವ ರೂಹಿಸಬಾರದು.", 
        "17083": "ಆದಿಯ ಪ್ರಸಾದವ ಸಾಧಿಸಬಾರದು, ಬೇಧಿಸಬಾರದು.", 
        "17084": "ಅಣುವಿಂಗೆ ಅಣುವಾಗಿ, ಮಹತ್ತಿಂಗೆ ಮಹತ್ತಾಗಿ", 
        "17085": "ಆರಾಧಾರದ ನಿಚ್ಚಣಿಗೆಯನೇರಿ ನೋಡಲು", 
        "17086": "ಹಲವು ಬಣ್ಣದ ಮೃಗದ ನೆಲೆಯನರಿದೆವೆಂದು", 
        "17087": "ಆಯುಧವೈನೂರಿದ್ದರೇನು,", 
        "17088": "ಕಾಯವೆಂಬ ಪಟ್ಟಣಕ್ಕೆ ಅಕಾಯನಿತ್ಯನೆಂಬರಸು", 
        "17089": "ಆಡಿನ ಕೋಡಗದ ಕೂಟದ ಸಂಚದ ಕೀಲ ಕಳೆದು", 
        "17090": "ಕಮಲನಾಳದ ಸೂತ್ರ ಸಮವಾಗಿ ನಿಂದಲ್ಲಿ", 
        "17091": "ಭಾವಿಸಿಹೆನೆಂಬ ಭಾವಕರಿಗೆ ಭಾವಭೇದವುಂಟು.", 
        "17092": "ಮೇಘ ಮರೆಯಾಗಿ ಚಂದ್ರನಿದ್ದನೆಂದಡೆ", 
        "17093": "ಬ್ರಹ್ಮನಾಳಾಗ್ರದ ಸಹಸ್ರದಳಕಮಲ ಕರ್ಣಿಕಾಮಧ್ಯದೊಳು,", 
        "17094": "ಜಗಭರಿತ ಶಿವನೆಂದು ನೆಗಳಿ ಹೊಗಳಿತಿವೆ ವೇದಂಗಳೆಲ್ಲ.", 
        "17095": "ಆದಿ ಅನಾದಿಯೆಂಬವಕ್ಕೆ  ತಾನಾದಿಯಾದ ಕಾರಣ,", 
        "17096": "ಶಿವಪ್ರಸಾದಸಂಪತ್ತು ದೊರಕೊಂಡಾತಂಗೆ", 
        "17097": "ಸತ್ತು ಚಿತ್ತು ಆನಂದ ನಿತ್ಯಪರಿಪೂರ್ಣ ಪರಂಜ್ಯೋತಿಯೊಡಗೂಡಿ", 
        "17098": "ಸೇವ್ಯಗುರುವಿನ ಮಹಾಪ್ರಸಾದವನನುಭವಿಸಿ,", 
        "17099": "ಗುರುತತ್ತ್ವವನರಿದು ಆ ಶ್ರೀಗುರುವಿನಿಂದ ಶಿವದೀಕ್ಷೆಯಾದ", 
        "17100": "ಅರಿವಿನಿಂದ ಅರಿದೆನೆಂಬ ಶಿಷ್ಯನಿಲ್ಲ,", 
        "17101": "ಆವ ಕ್ರೀಯ ಮಾಡಿದಡೇನು?", 
        "17102": "ಶರಣನ ಸರ್ವಾಂಗವು ಲಿಂಗದಂಗವು.", 
        "17103": "ತಾನೇ ಲಿಂಗವೆಂದರಿದು, ಮತ್ತೆ ಬೇರೆ ಲಿಂಗವಿದೆಂದು", 
        "17104": "ಕಾಷ್ಠದಲ್ಲಿ ಹುಟ್ಟಿದ ಅಗ್ನಿ, ಕಾಷ್ಠವ ದಹಿಸಿ,", 
        "17105": "ನಿತ್ಯಾನಂದ ನಿಜದಾಕಾರದ", 
        "17106": "ಕಾಬೂದೊಂದು ಜ್ಞಾನ, ಕಾಣಿಸಿಕೊಂಬುದೊಂದು ಜ್ಞೇಯವೆಂದು", 
        "17107": "ಘಟಮಠೋಪಾಧಿಯೊಡನೆ ಕೂಡಿ, ಆಕಾಶವಿದ್ದಿತ್ತೆಂದೊಡೆ", 
        "17108": "ಅಣುವಿಂಗೆ ಅಣು, ಮಹತ್ತಿಂಗೆ ಮಹತ್ತಾಗಿ", 
        "17109": "ಎಲೆಯುದುರಿದ ವೃಕ್ಷ ಉಲಿಯಬಲ್ಲುದೆ?", 
        "17110": "ಎಲ್ಲೆಲ್ಲಿ ನೋಡಿದಡಲ್ಲಲ್ಲಿ ನೀನೆ ದೇವ.", 
        "17111": "ಖೇಚರದ ಗಮನವ ಖೇಚರರಲ್ಲದೆ", 
        "17112": "ತನು ಮನ ಪ್ರಾಣವ ಲಿಂಗಕ್ಕರ್ಪಿಸಿ ತಾನೆ", 
        "17113": "ತತ್ತ್ವ ಮೂವತ್ತಾರರಿಂದತ್ತತ್ತ ಪರಕ್ಕೆ ಪರವಾದ", 
        "17114": "ಸರ್ವಾಂಗವು ಲಿಂಗಸಂಗವಾಗಿ, ಲಿಂಗ ಸರ್ವಾಂಗಸಂಗವಾಗಿ,", 
        "17115": "ಇದ ಮಾಡಬಹುದೆಂಬ, ಇದ ಮಾಡಬಾರದೆಂಬ", 
        "17116": "ಜಾಗ್ರತ್ ಸ್ವಪ್ನ ಸುಷುಪ್ತಿ ಎಂಬ ತ್ರಿವಿಧಾವಸ್ಥೆಯೊಳಗೆ,", 
        "17117": "ಜ್ಞಾತೃ ಜ್ಞಾನ ಜ್ಞೇಯ ಮೂರೊಂದಾದುದೇ", 
        "17118": "ಲಿಂಗವೇದಿಯಾದ ಶರಣಂಗೆ,", 
        "17119": "ಶಿವಶರಣನ ಶಿರ ಮೊದಲು ಪಾದ ಕಡೆಯಾದವೆಲ್ಲವು", 
        "17120": "ಶರಣನ ಸುಜ್ಞಾನಾಗ್ನಿಯಿಂದ, ಸಂಸಾರಬೀಜ ರೂಪವಾದ", 
        "17121": "ಅನಂತಕಾಲ ಅಗಲಿದ  ನಲ್ಲನ ಕೂಡಿಹೆನೆಂಬ ಸತಿಗೆ", 
        "17122": "ಶಿವಜ್ಞಾನಸಂಪನ್ನನಾದ ಶರಣಂಗೆ ಹಿರಿದೊಂದಾಶ್ಚರ್ಯವು", 
        "17123": "ಹದಿನಾಲ್ಕು ಭುವನದಲ್ಲಿ ಮನೋವೇದ್ಯವಾಗಿ,", 
        "17124": "ಅನುವನರಿದು ಅನುಭಾವಿಯಾದ ಕಾರಣ", 
        "17125": "ಅಂತರಂಗ ಸನ್ನಿಹಿತ ಪರಂಜ್ಯೋತಿರ್ಲಿಂಗದಲ್ಲಿ", 
        "17126": "ಸರ್ವ ಶಾಸ್ತ್ರೋಪಾಧಿಯಿಂದ ಬೇರೊಂದನಾಶ್ರೈಸಿ", 
        "17127": "ನಿತ್ಯಾನಂದ ಸಂವಿದಾಕಾರ", 
        "17128": "ಸ್ಥಾವರ ಜಂಗಮಾತ್ಮಕವಾದ ಸಮಸ್ತ ಲೋಕವನು", 
        "17129": "ಶಾಂತ ಸೂಕ್ಷ ್ಮ ಸರ್ವಜೀವ ಮನಃಪ್ರೇರಕ[ಸ್ವ]ನಾಥ", 
        "17130": "ಆವಾವ ಕಾಲದಲ್ಲಿ ಆವಾವ ಪರಿಯಲ್ಲಿ ಒಂದೊಂದು ವಸ್ತುವನು", 
        "17131": "ಶಿವಧ್ಯಾನದ ಕಡೆಯಲ್ಲಿ ಒಪ್ಪಿ ತೋರುವ ಸಂವಿತ್ತಿನ ತೃಪ್ತಿಯಂದ", 
        "17132": "ಖಂಡಿತಜ್ಞಾನವಳಿದು, ಅಖಂಡಿತಜ್ಞಾನಸ್ವರೂಪವಾದುದೇ ತೃಪ್ತಿ.", 
        "17133": "ಉದಯಾಸ್ತಮಯವೆಂಬೆರಡಿಲ್ಲದ ಚಿದಾದಿತ್ಯನ ಬೆಳಗು", 
        "17134": "ಹಗಲಿರುಳ ನುಂಗಿದರೆ ಉದಯಾಸ್ತಮಾನ ನಿಂದಿತ್ತು.", 
        "17135": "ಅಂಗದಲ್ಲಿ ಆಯತವಾಯಿತ್ತು.", 
        "17136": "ಆರನರಿದು, ಮೂರತಿಳಿದು, ಎರಡರೊಳು ನಿಲಿಸಿ", 
        "17137": "ಬೆಳಗು ಬರಲು ಕತ್ತಲೆ ಹರಿಯಿತ್ತು.", 
        "17138": "ಜಗದಗಲದಲ್ಲಿ ಮುಸುಕಿದ ಕತ್ತಲೆ, ದೀವಿಗೆಯ ಬೆಳಗಿಂಗೆ", 
        "17139": "ತುಂಬಿ, ಇಂಬಿನ ತುಂಬಿ, ಕಾಯ ತುಂಬಿ, ಕರಣ ತುಂಬಿ,", 
        "17140": "ಆವಾವ ಪರಿಯಲ್ಲಿ ಭಾವಿಸುತ್ತಿಹ ಕರಣದ ವ್ಯಾಕುಲವಡಗಿ", 
        "17141": "ಅಂಗ ಲಿಂಗ ಸಂಗವರಿದ ಲಿಂಗದೇಹಿ, ತನ್ನಿಂದ", 
        "17142": "ಅಂಡ ಪಿಂಡೋಪಾಧಿಗಳಿಲ್ಲದ ಅಖಂಡ ಬಯಲು,", 
        "17143": "ಮನದಲ್ಲಿ ತೋರುವ ನಾನಾ ತೋರಿಕೆಯ ಚಿಂತೆಯಳಿದು", 
        "17144": "ಶರಣನ ಸರ್ವಭಾವಂಗಳು", 
        "17145": "ಸೂರ್ಯನ ಕರಜಾಲಂಗಳು ಸೂರ್ಯೋದಯವಾದಲ್ಲಿ ಹುಟ್ಟಿ,", 
        "17146": "ಅಜ ಹರಿಗಳರಿತಕ್ಕೆ ಅಗೋಚರವಾದ ನಿಜವನರಿದು", 
        "17147": "ಅವನಿಯ ಪಿಂಡವ ನುಂಗಿ, ಉದಕವ ಪಾನವ ಮಾಡಿ,", 
        "17148": "ಅಂಗ ಲಿಂಗವೆಂಬ ಸಂದು ಸಂಶಯವಳಿದು", 
        "17149": "ಹಿಂದು ಮುಂದಣ ಮನದ ಸಂಕಲ್ಪ ವಿಕಲ್ಪವಳಿದು,", 
        "17150": "ಭಾವಲಯವಾದ ಮತ್ತೆ, ಇನ್ನು ಭಾವಿಸಲೇನುಂಟು ಹೇಳಾ,", 
        "17151": "ಕಾರ್ಯಕಾರಣವಾದ ತತ್ತ್ವವಿತತ್ತ್ವಂಗಳೆಲ್ಲ", 
        "17152": "ದಶದಿಕ್ಕುಗಳಿಂದ ರೂಹಿಸಬಾರದಾಗಿ,", 
        "17153": "ಭಾವಿಸುವ ಭಾವದ ವಿಕಾರವಳಿದು, ನಿರ್ಭಾವ ನೆಲೆಗೊಂಡು", 
        "17154": "ಬಯಲಲ್ಲಿ ಹುಟ್ಟಿದ ಶಿಶುವಿಂಗೆ,", 
        "17155": "ಪೃಥ್ವಿಯ ಬೀಜ ಅಪ್ಪುವ ಕೂಡಿ, ಅಗ್ನಿಯಲ್ಲಿ ಮೊಳೆದೋರಿತ್ತು.", 
        "17156": "ಕುಂಭಿನಿಯ ಘಟಪುರುಷನು ಅಂಬರದ ರಂಭೆಗೆ", 
        "17157": "ಶ್ರೀಗುರುವಿನ ಉಪದೇಶದಿಂದ ಲಿಂಗವನರಿದು,", 
        "17158": "ತೆರಹಿಲ್ಲದ ಕುರುಹಿಲ್ಲದ ಅರಿವಿಗಗೋಚರವಾದ", 
        "17159": "ಲಿಂಗವೇ ತಾನಾಗಿ, ತಾನೇ ಲಿಂಗವಾಗಿ", 
        "17160": "ಶಿವ ತಾನೆಂಬ ವಿವೇಕವಿಲ್ಲದೆ, ಶಿವನಲ್ಲಿ ತಾನಡಗಿ,", 
        "17161": "ನಾನು ನಾನೆಂಬ ಅಹಂಭಾವವಳಿದು,", 
        "17162": "ತನ್ನೊಳಡಗಿಹ ಅನಂತಕೋಟಿ ಬ್ರಹ್ಮಾಂಡಗಳಿಗೆ", 
        "17163": "ಶಿವನೆಂದರಿದವಂಗೆ ತಾನೆಂಬುದಿಲ್ಲಾಗಿ", 
        "17164": "ಚಂದ್ರ ಚಂದ್ರಿಕೆಯಂತೆ, ಅಗ್ನಿ ಉಷ್ಣದಂತೆ,", 
        "17165": "ಅರಿಯಬಾರದು ಕುರುಹಿಲ್ಲವಾಗಿ,", 
        "17166": "ಶರಣಮಧ್ಯಲಿಂಗ, ಲಿಂಗ ಮಧ್ಯಶರಣನಾದ ಮತ್ತೆ,", 
        "17167": "ನೀರು ನೀರು ಕೂಡಿ, ಕ್ಷೀರ ಕ್ಷೀರವ ಕೂಡಿ,", 
        "17168": "ಆರು ನೆಲೆ ಮಂಟಪದ ಕೋಣೆಯಲ್ಲಿ", 
        "17169": "ಗುರುಮುಖವಿಲ್ಲದೆ ತಾನೆ ಆಯಿತ್ತೆಂಬರು.", 
        "17170": "ದಯದಿಂದ ನೋಡಿ ಬಹು ವಿಚಾರವುಂಟು.", 
        "17171": "ನೀರು ನೇಣಾದಿ ದುರ್ಮರಣದಲ್ಲಿ ಹೋದರುಯೆಂಬ,", 
        "17172": "ಹೊನ್ನು ಹೆಣ್ಣು ಮಣ್ಣು ತನಗೆ ಸಂಬಂಧವೆಂದು ಹೇಳಿಕೊಂಬರು"
    }
}