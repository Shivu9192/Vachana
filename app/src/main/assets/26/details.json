        "8387": "ಲಿಂಗಕ್ಕೆ ಅರ್ಪಿತವ ಮಾಡುವಲ್ಲಿ ಸಕಲಪದಾರ್ಥದ ಗುಣ ವಿವರನರಿದು,", 
        "8388": "ಕಂಗಳಲ್ಲಿ ನೋಡಿ, ಕೈಯ್ಯಲ್ಲಿ ಮುಟ್ಟಿ, ನಾಸಿಕ ವಾಸನೆಯನರಿದು", 
        "8389": "ಭಕ್ತಂಗೆ ಕ್ರೀ, ಜಂಗಮಕ್ಕೆ ನಿಃಕ್ರೀಯೆಂದೆನಬಾರದು.", 
        "8390": "ಶಿವ ಭಕ್ತನಾಗಿ ಮುಂದೆ ಬಂದು ಮಂದಿರವ ಕಟ್ಟಿದ,", 
        "8391": "ಪಾದವ ಕೊಡುವಲ್ಲಿ ಪದಂ ನಾಸ್ತಿಯಾಗಿರಬೇಕು.", 
        "8392": "ಕೈ ಕಲಸಿ ಬಾಯಿ ಉಂಡಲ್ಲಿ ಅದಾವ ಹಂಗೆಂದೆನಬಹುದೆ?", 
        "8393": "ಆಚಾರವೆಂಬ ಭಕ್ತ ಘಟವಾಗಿ, ಅರಿವೆಂಬ ಮೂರ್ತಿಜಂಗಮ ಪ್ರಾಣವಾಗಿ,", 
        "8394": "ಭಕ್ತ ಭೂಮಿಯಾಗಿ, ಜಂಗಮ ಬೀಜವಾಗಿ,", 
        "8395": "ಅಜ ಕೊಂಡ ಗ್ರಾಸದ ಮಲವ", 
        "8396": "ವಸ್ತು ತ್ರಿವಿಧರೂಪಾದ ಪರಿ ಎಂತೆಂದಡೆ:", 
        "8397": "ಒಂದಂಗಕ್ಕೆ ಮೂರು ಯುಕ್ತಿ ಭಿನ್ನವಾದಂತೆ,", 
        "8398": "ಗುರುವಾದಲ್ಲಿ, ಶಿಷ್ಯನ ಅಂಗದಲ್ಲಿ", 
        "8399": "ಮಾಡುವ ಸೇವೆಯನರಿತು ಮಾಡಿಸಿಕೊಂಬುದು ಗುರುಸ್ಥಲ.", 
        "8400": "ತನ್ನಾಸೆಯ ಹರಿದು ಶಿಷ್ಯಂಗೆ ಅನುಜ್ಞೆಯ ಮಾಡಿದಾಗವೆ", 
        "8401": "ತನ್ನಯ ವಿಶ್ವಾಸದಿಂದ ಗುರುವ ಮುಕ್ತನ ಮಾಡಿದ ಭಕ್ತನು,", 
        "8402": "ಬಯ್ಚಿಟ್ಟ ಬಯ್ಕೆಯ ಒಡೆಯ ಕೊಂಡುಹೋದಲ್ಲಿ", 
        "8403": "ನಿಜಗುರುವಿನ ಇರವು ಹೇಗಿರಬೇಕೆಂದಡೆ:", 
        "8404": "ಗುರುಶಿಷ್ಯನ ಇರವಿನ ಸ್ಥಲವೆಂತುಟೆಂದಡೆ:", 
        "8405": "ಗುರುಜಂಗಮಲಿಂಗ ಭಕ್ತಿಮಾರ್ಗಸ್ಥಲ ಸಮರ್ಪಣ:", 
        "8406": "ಪೃಥ್ವಿಜ್ಞಾನ ಪಿಪೀಲಿಕಾಸಂಬಂಧವಾಗಿಹುದು,", 
        "8407": "ಉರಿಯ ಮಡುವಿನಲ್ಲಿ ಒಂದು ಜಲದ ಕುಸುಮ ಹುಟ್ಟಿ,", 
        "8408": "ಊರ ಹೊರಗಳ ಹೊಲತಿಯ ಹಾರುವ ನೆರೆದು", 
        "8409": "ಧರೆಯ ಹಾವು ಆಕಾಶದೊಳಗಳ ಕಪ್ಪೆ ಒಳಗೂಡಿ ಒರುಮೆಯಾಗಿ,", 
        "8410": "ಕೂಂಬಿನ ಮೇರಳ ಹಕ್ಕಿಯ ಆಕಾಶದ ಮೇರಳ ಗಿಡುಗ ಹಿಡಿಯಿತ್ತು.", 
        "8411": "ಮೂರು ಲೋಕವನೆಲ್ಲವ", 
        "8412": "ಕೋಣೆಯೊಳಗಳ ಮಿಂಡ, ನಡುಮನೆಯೊಳಗಳ ಗಂಡ,", 
        "8413": "ಪಂಚಾಕ್ಷರಿಯ ಮಣಿಮಾಲೆಯಲ್ಲಿ", 
        "8414": "ಕಂಗಳ ದೃಷ್ಟಿ ಪುರುಷನಾಗಿ, ಕೈಯ ದೃಷ್ಟಿ ಸತಿಯಾಗಿ,", 
        "8415": "ಊರ ಬಾಗಿಲ ಕಂಬದಲ್ಲಿ ಮೂರು ಮುಖದ ಕೋಡಗ", 
        "8416": "ಉರಿಯ ಸಿರಿಯ ನಡುವೆ ಒಂದು ಸರೋವರದಲ್ಲಿ ನರಿ ತಿರುಗಾಡುತ್ತದೆ.", 
        "8417": "ಹರಿವ ಹಾವು ಕೊಂಬಿನ ಕೋಡಗ ಸಂಧಿಸಿ, ಒಂದಕ್ಕೊಂದಂಜಿ ನಿಂದೈದಾವೆ.", 
        "8418": "ಕೋಡುಗದ ಚೇಷ್ಟೆ ಉಡುಗಿ, ಹಾವಿನ ಹಲ್ಲು ಮುರಿದು,", 
        "8419": "ಬ್ರಹ್ಮಾಂಡದ ಧರೆಯ ಮೇಲೆ ಅರಿಬಿರಿದಿನ ಮಂತಣದ ಗಿರಿ ಹುಟ್ಟಿತ್ತು.", 
        "8420": "ಆ ದುರ್ಗದಲ್ಲಿದ್ದ ದೊರೆಗಳ ವಿರೋಧವನರಿದು,", 
        "8421": "ನಿರಾಳದಲ್ಲಿ ನೀರಕೋಟೆಯನಿಕ್ಕಿ, ಬಯಲ ಅಗಳ ತೆಗೆದು,", 
        "8422": "ನಂಜಿನ ಭೂಮಿಯ ಮೇಲೆ ಸಂದೇಹದ ಕವರವ ನೆಟ್ಟು,", 
        "8423": "ಕಣ್ಣಿನ ಮನೆಯಲ್ಲಿ ಗನ್ನದ ಗರತಿ ಹಡದುಂಬುತ್ತೈದಾಳೆ.", 
        "8424": "ಸುಖದುಃಖದ ನಡುವೆ ಒಂದು ಅತಿರೇಕದ ಕೂಸು ಹುಟ್ಟಿ,", 
        "8425": "ಶ್ರೀಶೈಲದ ಮಧ್ಯದಲ್ಲಿ ಒಂದು ಪರುಷರಸದ ಬಾವಿ ಹುಟ್ಟಿತ್ತು,", 
        "8426": "ಸೂಜಿಯ ಮೊನೆಯಲ್ಲಿ ದಾರವೇರಿ ಹಿನ್ನೆಯ ಹಂಗ ಬಿಡಿಸಿತ್ತು.", 
        "8427": "ಕುರಿಯೊಡೆಯ ಹಲಬರ ನಡುವೆ ನಿಂದಿರೆ ತನ್ನ ಒಡೆಯನನರಿವಂತೆ,", 
        "8428": "ಕ್ರೀಯಲ್ಲಿ ಕಾಬುದ ಕ್ರೀಯಲ್ಲಿ ಕಂಡು,", 
        "8429": "ಮಹಾಮನೆಯೊಳಗೊಂದು ಮಂದಿರವಾಡ:", 
        "8430": "ಹುವ್ವಿನ ಮೇರಳ ತುಂಬಿ ಹುವ್ವ ತಿಂದು ಗಂಧವನುಳುಹಿತ್ತು.", 
        "8431": "ಇನ್ನೇವೆ ? ನಾ ತಂದ ಬೆಂಕಿಯಲ್ಲಿ", 
        "8432": "ನಾ ತೋಡಿದ ಬಾವಿಯಲ್ಲಿ ಡುಂಡುಕ ಭೇಕವೊಂದಾಗಿ ಇದ್ದವು.", 
        "8433": "ಎನ್ನ ಮಡದಿ ಹಾಲ ಕಾಸುವಾಗ,", 
        "8434": "ಬೀಜ ಹುಟ್ಟುವ ತಿರುಳು ಒಳಗಿದ್ದಲ್ಲಿ,", 
        "8435": "ಇರುಹೆ ಅಂಜದೆ ಮದ ಸೊಕ್ಕಿದ ಐದಾನೆಯ ತನ್ನ ಕಡೆಗಾಲಿನಲ್ಲಿ ಕಟ್ಟಿ", 
        "8436": "ಎರಳೆಯ ಕೊಂಬಿನ ನುಲಿಯ ಬಳಸಿನಲ್ಲಿ", 
        "8437": "ಕ್ಷೀರಕ್ಕೆ ವಾರಿ ಅರತಲ್ಲದೆ ಮಧುರಗುಣವಿಲ್ಲ.", 
        "8438": "ಕಾಮನ ಅಂಬಿನ ಕಣೆಯಲ್ಲಿ ಮೂವರು ಬಾಲೆಯರು ಹುಟ್ಟಿದ ವಿವರ :", 
        "8439": "ಭಕ್ತಿಯ ನುಡಿವಲ್ಲಿ ಬಾಹ್ಯವಾಗಿರಬೇಕು, ಸತ್ಯವ ನುಡಿವಲ್ಲಿ ಮರೆಯಾಗಿರಬೇಕು.", 
        "8440": "ವೇದವ ನುಡಿವಲ್ಲಿ ವಿಪ್ರರು", 
        "8441": "ನಿಕ್ಷೇಪವ ನೀಕ್ಷೆಪಿಸುವಲ್ಲಿ ಮತ್ತಾರೂ ಅರಿಯದಂತೆ", 
        "8442": "ಬ್ರಹ್ಮವನರಿತ ಮತ್ತೆ ಸುಮ್ಮನೆ ಇರಬೇಕು.", 
        "8443": "ಮಹಾಪರಂಜ್ಯೋತಿಪ್ರಕಾಶವ ಕಂಡನಿಂದವರಲ್ಲಿ ಹೇಳಬಹುದೆ ?", 
        "8444": "ವಚನರಚನೆಯ ಬಲ್ಲ ಅರುಹಿರಿಯರೆಲ್ಲರು", 
        "8445": "ಒಂದನಹುದು ಒಂದನಲ್ಲಾ ಎಂಬುದಕ್ಕೆ", 
        "8446": "ಒಡೆಯ ನೋಡುತ್ತಿದ್ದಲ್ಲಿ ಅಸುವಿನಾಸೆಯಿಲ್ಲದೆ ಅವಸರಕ್ಕೊದಗಬೇಕು.", 
        "8447": "ವರ್ತಕದಲ್ಲಿ ವರ್ತಿಸುವನ್ನಕ್ಕ ಸತ್ಕ್ರೀಯ ಮಾಡಬೇಕು,", 
        "8448": "ಆತ್ಮ ವಸ್ತುವೆಂದು ಅರಿತಲ್ಲಿ ಆತ್ಮನ ಅಳಿವ ಉಳಿವನರಿತು,", 
        "8449": "ಪಂಚತತ್ವದಲ್ಲಿದ್ದು ಪರತತ್ವವನರಿಬೇಕು.", 
        "8450": "ಅಪ್ಪು ಆಧಾರವಾಗಿ, ಕಮಠ ಶೇಷ ನೆಮ್ಮುಗೆಯಿಂದ", 
        "8451": "ಪಾಷಾಣದ ಘಟದಲ್ಲಿ, ರತ್ನದ ಜ್ಯೋತಿಯ ಬೆಳಗು ತೋರುವಂತೆ,", 
        "8452": "ಆತ್ಮ ಘಟಮಧ್ಯದಲ್ಲಿ ನಿಂದು,", 
        "8453": "ಪೃಥ್ವಿಯ ಅಪ್ಪುವಿನ ಆಧಾರದಿಂದ ಬೀಜವೊಡೆದು ಬೆಳೆವಂತೆ", 
        "8454": "ಹಾಲು ಬತ್ತಿದ ಹಸುವಿಂಗೆ ಕರುವ ಬಿಟ್ಟಡೆ ಒದೆಯುವುದಲ್ಲದೆ ಉಣಲೀಸುವುದೆ?", 
        "8455": "ಆಡಿನ ಮೊಲೆವಾಲ ಕೋಡಗವುಂಡು, ಹಾಲ ಸಿಹಿ ತಲೆಗೇರಿ,", 
        "8456": "ಮಹಾಧರೆಯಲ್ಲಿ ಮನೆಯ ಕಟ್ಟುವುದಕ್ಕೆ,", 
        "8457": "ಹಸಿದ ಹಾವಿನ ಹೆಡೆಯಲ್ಲಿ ಇಲಿ ಬಿಲನ ತೋಡುತ್ತದೆ.", 
        "8458": "ತಾವರೆಯ ನಾಳದ ತೆರಪಿನಲ್ಲಿ,", 
        "8459": "ಜೇನುತುಪ್ಪವ ಹೊತ್ತು ಮಾರುವವಳ ಮಡಕೆಯ ಮೇಲೆ", 
        "8460": "ಕೊಟ್ಟ ಕೂಗಿತ್ತು, ಹಾಲುವಕ್ಕಿ ಬಳಿಯಿತ್ತು, ಹಂಗ ಕಟ್ಟಿತ್ತು;", 
        "8461": "ಮೂಳಿಯ ಮೊದಲಿನಲ್ಲಿ ಮೂವರು ಮಕ್ಕಳು ಹುಟ್ಟಿ.", 
        "8462": "ಮಹಾಮಾಯವೆಂಬ ಶಕ್ತಿರೂಪಿನಲ್ಲಿ", 
        "8463": "ಕಲ್ಲ ಗಿರಿಯ ಹುಲ್ಲಿನ ಮೊನೆಯಲ್ಲಿ ಕುಕ್ಕಿ", 
        "8464": "ಹುತ್ತದೊಳಗಳ ಹಾವು ಹದ್ದಿನ ಹೊಟ್ಟೆಯೊಳಗಳ ತತ್ತಿ ನುಂಗಿತ್ತು.", 
        "8465": "ಒಂದು ಹರಿವ ಹಾವು, ಒಂದು ದನಿ ಸರಕ್ಕೆ ನಿಂದ ಹಾವು,", 
        "8466": "ಮಹಾವಿಕಾರ ರಣಮಯವಾದಲ್ಲಿ", 
        "8467": "ಶರೀರವ ಕುರಿತಲ್ಲಿ ಸಾಧಕಾಂಗವ ನುಡಿಯಬೇಕು.", 
        "8468": "ಉಪ್ಪಿನ ನೀರು ಹೆಪ್ಪ ಬಲಿದು ಘಟ್ಟಿಯಾದಂತೆ", 
        "8469": "ಬೇರಿನ ಬಣ್ಣ ಇಂದುವಿನ ಕಳೆಕೊಂಡಂತಿರಬೇಕು.", 
        "8470": "ಒಂದು ಶರೀರ ನಾನಾ ಲಾಗ ಕಲಿತು, ಆಡುವ ಭೇದ ಬೇರಾದಂತೆ,", 
        "8471": "ಮದ್ದ ಕೂಡಿ ತುಂಬಿದ ನಾಳಿಯಲ್ಲಿದ್ದ ಕಲ್ಲು", 
        "8472": "ಕಾಯವಿದ್ದಲ್ಲಿಯೇ ಸಕಲಕರ್ಮಂಗಳ ಮಾಡು,", 
        "8473": "ಅಸಿಯ ಮೊನೆಯು ಮುರಿದಡೆ ಮಸೆದಡೆ ಮೊನೆಯಾಗದೆ ಅಯ್ಯಾ?", 
        "8474": "ಅರಿವಾಗ ಆ ತನುವಿನಲ್ಲಿದ್ದೆ ಅರಿಯಿತ್ತು,", 
        "8475": "ಕರೆವ ಪಶುವಿಂಗೆ ತೃಣ ದವಸ ಎಯ್ದಾದಲ್ಲಿ", 
        "8476": "ಗಂಡನಿಲ್ಲದ ಸತಿ ಯೋನಿಯಿಲ್ಲದೆ ಮಕ್ಕಳ ಹಡೆದು", 
        "8477": "ಕಣ್ಣಿಲ್ಲದ ಕುರುಡ ಕನ್ನಡಿಯ ನೋಡಿ ಮೂರು ಕಣ್ಣ ಕಂಡ.", 
        "8478": "ಕಾದ ಕಬ್ಬುನವ ಕಾಯದ ಕಬ್ಬುನದಲ್ಲಿ ಹಿಡಿದು,", 
        "8479": "ಸಿದ್ಧರಸ ಲೇಸೆಂದಡೆ ತಾನಿದ್ದ ಕುಡುಕೆ ಹೇಮವಾದುದಿಲ್ಲ.", 
        "8480": "ರಸದ ಬಾವಿಯ ಚೆಲ್ಲಿ, ಅಸಿಯ  ಮಡುವ ಕಲಕಿ,", 
        "8481": "ಕಾಳಿಯ ಕಡೆಗಣ್ಣ ಕಿತ್ತು, ಏಡಿಯ ಸರ್ವಾಂಗವ ಸೀಳಿ,", 
        "8482": "ಅಂಗವರಿಯದೆ ಲಿಂಗವಿದ್ದಲ್ಲಿಯೆ ಸಂಗವ ಮಾಡಿ,", 
        "8483": "ಮೂವರ ಹಂಗಿಂದ ಬಂದುದು ಲಿಂಗವಲ್ಲಾ ಎಂದು,", 
        "8484": "ಶೈವಕ್ಕೆ ಕೈಲಾಸ, ವೈಷ್ಣವಕ್ಕೆ ವೈಕುಂಠ,", 
        "8485": "ಶೈವವಾದ, ತತ್ತ್ವವಾದ, ಮಾಯಾವಾದ", 
        "8486": "ಬ್ರಹ್ಮ ಸೋರೆಯಾಗಿ, ವಿಷ್ಣು ದಂಡವಾಗಿ, ಸಕಲದೇವತಾಕುಲ ದಾರವಾಗಿ,", 
        "8487": "ಆದಿಯೆಂಬ ಅಂತರಾದಿಯಲ್ಲಿ ಅನಾದಿಯ ವಸ್ತುವನರಿತು,", 
        "8488": "ಬ್ರಹ್ಮಂಗೆ ಸರಸ್ವತಿಯಾಗಿ ಕಾಡಿತ್ತು ಮಾಯೆ.", 
        "8489": "ಐದು ಬಹಲ್ಲಿ ಒಂದಾಗಿ ಬಂದ ಮಾಯೆಯ,", 
        "8490": "ಸಕಲವೆಂಬನ್ನಕ್ಕ ನಿಃಕಲವುಂಟು, ನಿಃಕಲವೆಂಬುದು ನಾಮರೂಪು.", 
        "8491": "ನೀತಿ ಲಿಂಗವಾಗಿ, ಸದ್ಗುಣ ಕಳೆಯಾಗಿ,", 
        "8492": "ವಾರಿಯ ಸಾರದ ಶೈತ್ಯದ ಇರವು.", 
        "8493": "ಅಪ್ಪು ಚಿಪ್ಪಿನಲ್ಲಿ ನಿಂದು [ಚಿಪ್ಪ]ವ ಬೆರಸದಂತೆ,", 
        "8494": "ಮಹದಾಕಾಶದ ಮನೆಯಲ್ಲಿ ಪಂಚಕಾಲಿನ ನೆಲಹು ಕಟ್ಟಿರಲಾಗಿ,", 
        "8495": "ಅಂಧಕಾರವೆಂಬ ಮನೆಯ ಬಾಗಿಲಲ್ಲಿ ಆರಂಗದ ಕರಡಿ ಕಟ್ಟಿ", 
        "8496": "ಹರಿವ ಹಾವಿಂಗೆ ಕಾಲ ಕೊಟ್ಟು, ಉರಿವ ಕಿಚ್ಚಿಗೆ ಕಯ್ಯನಿಕ್ಕಿ,", 
        "8497": "ಹುಲಿ ಹುತ್ತವ ಹೊಕ್ಕು ಹಾವಾದ ಭೇದವನಾರು ಬಲ್ಲರು?", 
        "8498": "ಹಾವಿನ ಹುತ್ತದಲ್ಲಿ ಹದ್ದು ತತ್ತಿಯನಿಕ್ಕಿ,", 
        "8499": "ಹುಲ್ಲು ಕೆಂಡವ ಮುಟ್ಟಿ ಹೊತ್ತದ ಪರಿಯ ನೋಡಾ,", 
        "8500": "ಮೂಷಕ ಮಾರ್ಜಾಲ ಕೂಡಿ ಭೇಕನ ಕಂಡು ನೀತಿಯ ಕೇಳಿ,", 
        "8501": "ದತ್ತೂರದ ಬಿತ್ತಿನಲ್ಲಿ ಕಲ್ಪತರು ಹುಟ್ಟಿ,", 
        "8502": "ಹೊಲದೊಳಗಳ ಹುಲ್ಲೆ, ಗವಿಯೊಳಗಳ ಹುಲಿ,", 
        "8503": "ಹುತ್ತದೊಳಗಳ ಉಡು, ಹೊಳೆಯೊಳಗಳ ಮೊಸಳೆಯ ನುಂಗಿತ್ತು.", 
        "8504": "ಜೀವಜ್ಞಾನ ಭಾವಜ್ಞಾನ ಯುಕ್ತಿಜ್ಞಾನ ಚಿತ್ತಜ್ಞಾನ ಶಕ್ತಿಜ್ಞಾನ", 
        "8505": "ಉತ್ಪತ್ಯಕ್ಕೆ ಬ್ರಹ್ಮಂಗೆ ಸೃಷ್ಟಿಯ ಕೊಟ್ಟು,", 
        "8506": "ವೇದ ಪ್ರಣವದ ಮೂಲ, ಶಾಸ್ತ್ರ ಪ್ರಣವದ ಶಾಖೆ,", 
        "8507": "ಅಪ್ಪು ಮಣ್ಣು ಕೂಡಿ ಘಟವಾದಂತೆ,", 
        "8508": "ಅನಲ ನಂದಿದ್ದಲ್ಲಿ ವಾಯು ಸಂಗವಲ್ಲದೆ,", 
        "8509": "ಆದಿ ಮಧ್ಯ ಅವಸಾನವರಿಯಬೇಕೆಂಬರು,", 
        "8510": "ದರ್ಪಣದ ಒಪ್ಪ ಅತ್ತರೆ ಅತ್ತು, ನಕ್ಕರೆ ನಕ್ಕು", 
        "8511": "ಬೀಜದ ನೆಲೆಯಲ್ಲಿ ಹಲವು ತೋರುವವೊಲು,", 
        "8512": "ಮಾತಿಗೆ ಸಿಕ್ಕೆ, ಅವರ ನೀತಿಯ ಬಲ್ಲೆಯಾಗಿ,", 
        "8513": "ವೇದಪಾಠಕ ಶಾಸ್ತ್ರವಿತ್ತು ಪುರಾಣಬಹುಶ್ರುತಿವಂತ ವಾಚಕ", 
        "8514": "ಶೇಷ ಲಂಪಟಕೆಯ್ದುವ ಮುಖದಂತಿರಬೇಕು,", 
        "8515": "ಪಾತಾಳದ ನೀರ ಹುರಿ ಸಂಚದಿಂದ ಧರೆಗೆ ತಾಹಂತೆ", 
        "8516": "ಮೊನೆ ತಪ್ಪಿದಲ್ಲಿ ಅಲಗಿನ ಘನವೇನ ಮಾಡುವುದು?", 
        "8517": "ಯತಿಗುಣದ್ವೇಷ, ಸಮಯಗುಣದ್ವೇಷ, ಆಚಾರಗುಣದ್ವೇಷ", 
        "8518": "ಪಂಚವಿಷಯಂಗಳೆಂದು, ಅಷ್ಟಮದಂಗಳೆಂದು,", 
        "8519": "ಆತ್ಮಗುಣ ವಿವರದ ಪರಿಯೆಂತುಟೆಂದಡೆ:", 
        "8520": "ಮೂಲದ ಜ್ವಾಲೆಯೆತ್ತಿ, ಕಮಲವ ತಾಳಿ, ಮೇಲೊಂದು ಕೊಡನಿಪ್ಪುದು.", 
        "8521": "ಶರೀರದಲ್ಲಿ ಷಡಾಧಾರಚಕ್ರವುಂಟೆಂಬರು.", 
        "8522": "ಅರಸು ಆಲಯವ ಹಲವ ಕಟ್ಟಿಸಿದಂತೆ,", 
        "8523": "ವೇದವೇದಾಂತಂಗಳಿಂದ ಸಿದ್ಧಸಿದ್ಧಾಂತಂಗಳಿಂದ", 
        "8524": "ಸಕಲವನರಿದಡೂ ಮಲತ್ರಯ ನಾಸ್ತಿಯಾಗಿರಬೇಕು.", 
        "8525": "ಒಂದಾಸೆಯ ಕುರಿತು ಸರ್ವಮಾತಿಂಗೆ ಒಡಲಾಯಿತ್ತು,", 
        "8526": "ಉಂಟೆಂಬ ದೃಢ, ಇಲ್ಲಾ ಎಂಬ ಸಂದೇಹ ತಾನಲ್ಲಿಯೇ ತೋರಿ", 
        "8527": "ಕಯ್ಯೊಳಗಳ ಸಂಚ ಕಣ್ಣಿಗೆ ಮರೆಯಾದಂತೆ,", 
        "8528": "ಕಾಯದ ಕಕ್ಕುಲತೆಗಾಗಿ ಜೀವಿಗಳ ಬಾಗಿಲ ಕಾಯದೆ,", 
        "8529": "ವೇಷದಲ್ಲಿ ತಿರುಗುವುದು ಸಮಯದ ಹಂಗು.", 
        "8530": "ಕಾಯವುಳ್ಳನ್ನಕ್ಕ ಮಾಡುವುದು ಲಿಂಗಪೂಜೆಯ,", 
        "8531": "ಜಲದೊಳಗಳ ಮತ್ಸ್ಯದ ನೆಲೆಯ, ಮುಗಿಲೊಳಗಳ ಮಿಂಚಿನ ನೆಲೆಯ,", 
        "8532": "ಮಠದಲ್ಲಿ ತೋರುವ ಜ್ಯೋತಿ ಅಂಗ ಚಿಕ್ಕಿತ್ತಾಗಿ,", 
        "8533": "ಗಂಧವನೊಳಕೊಂಡ ಕುಸುಮವ ಕೊಯ್ಯಬಹುದಲ್ಲದೆ", 
        "8534": "ಗಂಧವೊಂದೆಂದಡೆ ಹಲವು ವಾಸನೆಯ ಕುಸುಮದಲ್ಲಿ ಸಿಕ್ಕಿ,", 
        "8535": "ಉರಿಯಂಗವೆಲ್ಲ ಅಪೆÇೀಷನಕ್ಕೊಡಲು,", 
        "8536": "ಪಾದಪ ಗುರುಭಾವ, ಶಾಖೆ ಚರಭಾವ, ಲಿಂಗ ಫಲಭಾವ,", 
        "8537": "ಕೋಟಿ ಗುರುವಾಗಬಹುದು, ಭಕ್ತನಾಗಬಾರದು.", 
        "8538": "ಗುರುವಿಂಗೂ  ಲಿಂಗಕ್ಕೂ ಜಂಗಮಕ್ಕೂ ಭಕ್ತನಂಗವೆ ಮಂದಿರವಾಗಿ,", 
        "8539": "ತ್ರಿವಿಧಮೂರ್ತಿಯು ಕೂಡಿ ಭಕ್ತನ ಅಂಗದಲ್ಲಿ ನಿಂದು,", 
        "8540": "ಆಗಮಂಗಳ ಶೋಧಿಸಿ ಮಾತಿನ ನೀತಿಯ ಹೇಳುವರೆಲ್ಲರು ಹಿರಿಯರೆ?", 
        "8541": "ಸ್ವಪ್ನದ ಮಂದಿರದಲ್ಲಿ ನಿಃಕಲದ ಕೊಡಗೂಸು", 
        "8542": "ಕಾಳೋಗರನ ಹೆಡೆಯ ಮೇಲೆ ಮಧ್ಯದಲ್ಲಿ ಒಂದು ಕೋಳು ಸಿಕ್ಕದ ಹೆಣ್ಣು", 
        "8543": "ಯೋಗದಲ್ಲಿ ಕಾಬುದು ಶರೀರಕ್ಕೆ ಹೊರಗು.", 
        "8544": "ಮತಿಯಿಂದ ಕಾಬುದು ಚಿತ್ತದ ಹಂಗು,", 
        "8545": "ಬಯಲು ಬಯಲ ನೋಡಿ ಕಾಬುದಿನ್ನೇನು?", 
        "8546": "ಸಕಲಜ್ಞಾನಸಂಪನ್ನ ಕಲೆಯನರಿತೆನೆಂಬ ಅರುಹಿರಿಯರು ಹೇಳಿರಣ್ಣಾ.", 
        "8547": "ಚಿತ್ತ ನೆನೆದು ವಸ್ತುವೆಂದು ಪ್ರಮಾಳಿಸುವಲ್ಲಿ", 
        "8548": "ಕೆಚ್ಚಲೊಳಗೆ ಕ್ಷೀರವಿಪ್ಪಂತೆ", 
        "8549": "ವಸ್ತುಲೀಲಾಲೋಲನಾಗಿ ಅವತಾರಮೂರ್ತಿಯಾದಲ್ಲಿ", 
        "8550": "ನಾದ ಬ್ರಹ್ಮಶಕ್ತಿಯಾಗಿ, ಬಿಂದು ವಿಷ್ಣುಶಕ್ತಿಯಾಗಿ, ಕಳೆ ರುದ್ರಶಕ್ತಿಯಾಗಿ,", 
        "8551": "ಯೋಗಿ ಜೋಗಿ ಶ್ರಾವಕ ಸನ್ಯಾಸಿ ಪಾಶುಪತಿ ಕಾಳಾಮುಖಿ", 
        "8552": "ವಸ್ತುವಿನ ಪಾದಮಂಡಲ ಬ್ರಹ್ಮಲೋಕವಾಯಿತ್ತು,", 
        "8553": "ಯೋಗಿ ಪಾಶುಪತಿ ಕಾಳಾಮುಖಿ ಈ ಮೂರು ಶೈವಪಕ್ಷವಾಗಿಹವು.", 
        "8554": "ಶರೀರ ದಹನ ಮುಖವೆಲ್ಲವು ರುದ್ರತತ್ವ ಆಧೀನವಾಗಿಹುದು.", 
        "8555": "ತನುಸಂಬಂಧ ಬ್ರಹ್ಮನ ತೊಡಕಾದಲ್ಲಿ,", 
        "8556": "ಜಗದ ವರ್ತಕದ ಇರವು ಎಂತೆಂದಡೆ:", 
        "8557": "ಶಕ್ತಿರೂಪು ವಿಷ್ಣುವಿನ ಅವತಾರವಾಯಿತ್ತು.", 
        "8558": "ಯೋನಿಜನಾಗಿ ದಶ ಅವತಾರಕ್ಕೆ ಒಳಗಾದಲ್ಲಿ ದೇವಪದ ಹಿಂಗಿತ್ತು.", 
        "8559": "ಪೃಥ್ವಿ ಅಪ್ಪು ತೇಜ ವಾಯು ಆಕಾಶ ಪಂಚಭೌತಿಕಕ್ಕೆ", 
        "8560": "ವಿಷ್ಣುಮಯ ಜಗವೆಂದಡೆ ಮಹಾಪ್ರಳಯದಲ್ಲಿ ವಟಪತ್ರಶಯನನಾದ.", 
        "8561": "ವಿಷ್ಣು ದೈವವೆಂದಡೆ ಪಾಂಡವರ ಬಂಡಿಯ ಬೋವನಾದ,", 
        "8562": "ವಿಷ್ಣು ಆದಿಶಕ್ತಿಯ ಕಂದನಾಗಿ ಬಂದುದನಾರೂ ಅರಿಯರು.", 
        "8563": "ಸೀತೆ ಸೆರೆಹೋಹಾಗ, ವಾಣಿಯ ನಾಸಿಕ ಅರಿವಾಗ,", 
        "8564": "ಧ್ಯಾನದಿಂದ ಕಾಬುದು ಬ್ರಹ್ಮನ ಕಲ್ಪಾಂತರಕ್ಕೊಳಗು.", 
        "8565": "ಸ್ಥೂಲದಲ್ಲಿ ಸಿಕ್ಕಿ, ಸೂಕ್ಷ್ಮದಲ್ಲಿ ಕಂಡು, ಕಾರಣದಲ್ಲಿ ಅರಿವುದು", 
        "8566": "ವಾಗದ್ವೈತದಲ್ಲಿ ನುಡಿದು ಸ್ವಯಾದ್ವೈತದಲ್ಲಿ ನಡೆದು ತೋರಬೇಕು.", 
        "8567": "ವೇದವನೋದಿದಲ್ಲಿ ಪ್ರಣವವನರಿಯಬೇಕು.", 
        "8568": "ಕೆಯಿ ಬೆಳೆವಲ್ಲಿ ಸದೆಗೆ ಮುನಿವರಲ್ಲದೆ ಬೆಳೆಗೆ ಮುನಿದವರುಂಟೆ ಅಯ್ಯಾ?", 
        "8569": "ವಂದಿಸಿ ನಿಂದಿಸಬಾರದೆಂಬುದೆ ಕ್ರೀವಂತನ ಇರವು.", 
        "8570": "ಉಭಯಚಕ್ಷು ಗುರುಚರವಾದಲ್ಲಿ ಹಿಡಿವ ಬಿಡುವ ಕರ ಸದ್ಭಕ್ತ.", 
        "8571": "ಕನ್ನಡಿಯಲ್ಲಿ ಕಪ್ಪು ಹುಟ್ಟಿದಾಗ ಕಪ್ಪ ಕಳೆದಲ್ಲದೆ ಒಪ್ಪವ ಕಾಣಬಾರದು.", 
        "8572": "ಎನಗೆ ಗುರುವಾಗಬೇಡ ಹೆಣ್ಣು ಹೊನ್ನು ಮಣ್ಣಿನ ಆಸೆ ಉಳ್ಳನ್ನಕ್ಕ,", 
        "8573": "ವೇಷವ ತೊಟ್ಟಡೆ ಜಗಕ್ಕಳುಕಲ್ಲದೆ ಎನಗಳುಕಿಲ್ಲ.", 
        "8574": "ಪಾಶವರತು ಗುರುವಾಗಬೇಕು, ವೇಷವರತು ಚರವಾಗಬೇಕು,", 
        "8575": "ಭಕ್ತಿಹೀನ ಭಕ್ತಿಯ ಮಾಡುವಲ್ಲಿ", 
        "8576": "ಭಕ್ತ ತನ್ನಯ ಪಾಶವ ಗುರುವಿನ ಮುಖದಿಂದ ಕಳೆವ,", 
        "8577": "ಜಂಗಮಲಿಂಗವಾದಲ್ಲಿ ಜಂಗುಳಿಗಳ ಬಾಗಿಲ ಕಾಯದಿರಬೇಕು.", 
        "8578": "ಬಾಗಿಲಲ್ಲಿ ತಡೆವಾತನ ಮನೆಯ ಬಾಗಿಲ ಹೊಕ್ಕು ಉಂಡ ಜಂಗಮದ", 
        "8579": "ರಾಜಸ ತಾಮಸ ಸಾತ್ವಿಕವನರಿದು ಪೂಜಿಸಿಕೊಂಬುದು ಗುರುಚರದ ಇರವು.", 
        "8580": "ಮುನ್ನವೆ ಮೂರ ಹರಿದು ಗುರುಚರವಾಗಿ ಬಂದಲ್ಲಿ ಇನ್ನು ಮೂರರಾಸೆಯೇಕೆ?", 
        "8581": "ಗುರುವಿನ ಉಪಾಧಿಕೆಯ ಶಿಷ್ಯನರಿತಲ್ಲಿ", 
        "8582": "ವಂದಿಸಿ ನಿಂದಿಸಲಿಲ್ಲ, ನಿಂದಿಸಿ ವಂದಿಸಲಿಲ್ಲ.", 
        "8583": "ಭಕ್ತಂಗೆ ವಿಶ್ವಾಸ, ಗುರುವಿಂಗೆ ಅರಿವು,", 
        "8584": "ಮಾತಿನ ಮಾಲೆಯ ಕಲಿತು ಹೋರುವಾತ ಗುರುವಲ್ಲ;", 
        "8585": "ಕಂಚುಕೆಖಚಿತ ಆಭರಣ ವಿಲಾಸಿತಂಗಳಿಂದ", 
        "8586": "ದಿನದಿನಕ್ಕೆ ಫಲರಸ ಬಲಿವಂತೆ ಮಾಡುವ ಕ್ರೀ ಸನ್ನದ್ಧನಾಗಿ,", 
        "8587": "ಅನುವನರಿವನ್ನಕ್ಕ ಅರ್ಚನೆ, ಪುಣ್ಯವನರಿವನ್ನಕ್ಕ ಪೂಜೆ,", 
        "8588": "ತೆಪ್ಪದ ಮೇಲೆ ನಿಂದು", 
        "8589": "ಮಣ್ಣಿನೊಳಗೆ ಚಿನ್ನ ಹುಟ್ಟಿ ಮಣ್ಣ ಬಿಟ್ಟಂತಿರಬೇಕು,", 
        "8590": "ಅರಿವು ಘಟದಲ್ಲಿ ನಿಂದು ನುಡಿವನ್ನಕ್ಕ", 
        "8591": "ಗುರುಭಕ್ತನಾಗಿದ್ದಲ್ಲಿ ಆ ಗುರುವಿನ ಗುಣವ ವಿಚಾರಿಸಬೇಕು,", 
        "8592": "ದಾತ ಗುಣ ವರ್ತಕ ಮಾಟ, ಅರಿವ ಗುಣ ವಿಚಾರ ಮಾಟ.", 
        "8593": "ಮಾಡುವಂಗೆ ಅರಿವೆ ಕಾರಣ, ಮಾಡಿಸಿಕೊಂಬವಂಗೆ ಅರಿವೆ ಕಾರಣ,", 
        "8594": "ಮರವೆಯಿಂದ ಬ್ರಹ್ಮ ಶಿರವ ಹೋಗಾಡಿಕೊಂಡ,", 
        "8595": "ಅರಿದು ಮಾಡುವುದು ಗುರುಭಕ್ತಿ,", 
        "8596": "ಗುರುವಿಂಗೆ ಗುರುವಿಲ್ಲದೆ ಅರಿದ ಪರಿಯಿನ್ನೆಂತೊ?", 
        "8597": "ಗುರುವಾದಡೂ ಮಲತ್ರಯದಾಸೆ ಉಳ್ಳನ್ನಕ್ಕ ಮಲದೇಹಿ.", 
        "8598": "ಗುರುವೆಂದು ಅನುಸರಣೆಯ ಮಾಡಿದಲ್ಲಿ ಲಿಂಗವಿಲ್ಲ,", 
        "8599": "ಕುಂಭಕ್ಕೆ ಜಲವ ತುಂಬುವಲ್ಲಿ ಶೋಧಿಸಿದಲ್ಲದೆ ಶುದ್ಧವಿಲ್ಲ.", 
        "8600": "ಗುರುವಿಂಗಾಸೆಯ ಕಲಿಸದೆ,", 
        "8601": "ತನ್ನ ಅಂಗದ ಮಲಿನವ ತಾನರಿದಂತೆ, ತನ್ನ ಕಂಗಳ ಕಸನ ತಾ ಕಳೆದಂತೆ,", 
        "8602": "ಏಕಮೂರ್ತಿತ್ರಯೋಭಾಗವಾದುದ ತಾನರಿತ ಮತ್ತೆ", 
        "8603": "ಅಂಗದ ಆಪ್ಯಾಯನವ ಆತ್ಮನರಿವಂತೆ,", 
        "8604": "ಸಕಲಬ್ರಹ್ಮಾಂಡ ವಿಷ್ಣುಮಯ, ಉಭಯದ ಆತ್ಮಮಯ ಮಹಾಪ್ರಳಯ,", 
        "8605": "ಅಂಬು ಅಂಬುಜದು[ಭ]ಯ[ದ] ಸಂಗದಲ್ಲಿ", 
        "8606": "ಅನಾದಿಯಿಂದತ್ತಲಾದ ಅಂತರಾದಿಮಧ್ಯದಲ್ಲಿ,", 
        "8607": "ಗುರುಲಿಂಗಜಂಗಮವೆಂಬ ತ್ರಿವಿಧಮೂರ್ತಿ ಕೂಡಿ", 
        "8608": "ನೋಡಿಹೆನೆಂದಡೆ ನೋಡಲಿಲ್ಲ, ಕೂಡಿಹೆನೆಂದಡೆ ಕೂಡಲಿಲ್ಲ.", 
        "8609": "ಮುನ್ನ ಬಯಲೆಂಬುದೇನೋ?", 
        "8610": "ವೇಷವರಿತು ಗುರುವಾಗಬೇಕು,", 
