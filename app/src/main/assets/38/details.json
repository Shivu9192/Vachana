        "9321": "ಕೇಳಾ ಹೇಳುವೆನು:", 
        "9322": "ಕೇಳಿದಾತ ಶಂಕರದಾಸಿಮಯ್ಯ,", 
        "9323": "ಕೊಡುವ ಲಿಂಗವನು ಬೇಡಿಕೊಳಲರಿಯದೆ", 
        "9324": "ಕ್ಷುಧೆಯಾರ್ಥ ಎತ್ತರಹದಲ್ಲಿ ಇಂಬಿಟ್ಟಡೆ", 
        "9325": "ಗುರುಕಾರುಣ್ಯವ ಪಡೆದು ಲಿಂಗವೇ ಪ್ರಾಣವಾಗಿ", 
        "9326": "ಗುರುತ್ವವುಳ್ಳ ಮಹದ್ಗುರುವನರಿಯದೆ", 
        "9327": "ಗುರುಲಿಂಗ ಪರಂಜ್ಯೋತಿರ್ಲಿಂಗವೆಂದರಿದವನು ಬಲ್ಲವನು,", 
        "9328": "ಗುರು ಲಿಂಗ ಜಂಗಮ ಪ್ರಸಾದ-", 
        "9329": "ಗುರುಲಿಂಗಜಂಗಮವೆ ಶಿವನೆಂದು ಅರಿದು", 
        "9330": "ಗುರುವ ಕಂಡಲ್ಲಿ ನಿನ್ನನೇ ಕಾಬೆ,", 
        "9331": "ಗುರುವನರಿವನ್ನಕ್ಕರ ಅಷ್ಟಾದಶ ವಿದ್ಯೆಯನರಿಯಲುಂಟು", 
        "9332": "ಗುರುವಿನ ಮಹತ್ವವನು, ಶಿಷ್ಯನ ಗುರುತ್ವವನು", 
        "9333": "ಗುರುವೆ ಪರಶಿವನು", 
        "9334": "ಗುರುಸ್ವರೂಪನಾಗಿ ಮಹಾಸ್ಥಾನದಲ್ಲಿರುತ್ತಿದ್ದೆ,", 
        "9335": "ಗೃಹ ಗ್ರಾಮ ನಾಡು ದೇಶಂಗಳೆಂಬ ಭೂಮಿಯ", 
        "9336": "ಘನಕ್ಕೆ ಘನ ಮಹಿಮೆಯ ಪಡೆವರೆಲ್ಲರು ರುದ್ರಾಕ್ಷಿಯ ಧರಿಸಿಪ್ಪರು,", 
        "9337": "ಜಗದಗಲ ಮುಗಿಲಗಲ ಮಿಗೆಯಗಲ ನಿಮ್ಮಗಲ", 
        "9338": "ಜಗವೊಂದೆಸೆ, ತಾನೊಂದೆಸೆ,", 
        "9339": "ಜನನವೆಲ್ಲರಿಗುಳ್ಳ ಯೋನಿಗಳಲ್ಲಿ ಜನಿಸದೆ", 
        "9340": "ಜನ್ಮದ ಭವಬಂಧನವ ತೊಡೆಯಬಲ್ಲಡೆ", 
        "9341": "ಜನ್ಮಾಂತರಸಹಸ್ರೇಷು ತಪೆÇೀಧ್ಯಾನಸಮಾಧಿಭಿಃ †", 
        "9342": "ಜನಿತವಿಲ್ಲದ [ಜನಿತ]ನಾದ ಹರನೆ, ನೀನು ನಿರೂಪನು.", 
        "9343": "ಜನಿತವಿಲ್ಲದೆ ಜನಿಸಿ ಸ್ವಯವಾದ ಹರನೇ ನೀನು ನಿರೂಪನೇರಿ", 
        "9344": "ಜಲಕುಂಭಚಂದ್ರವತ್ತೆಂಬ ಯಥಾ ನ್ಯಾಯವನರುಹಿಸಿ", 
        "9345": "ಜಗದೊಳಗೆ ಸೂರ್ಯನಂತಿರ್ಪನಯ್ಯ ಶಿವನು.", 
        "9346": "ಜಲ ಮಲಿನ[ವಪ್ಪು]ವ ತೊಳೆವಂತೆ, ಮನದ ತಮಂಧವ ತೊಳೆ[ದು]", 
        "9347": "ಜಾತವೇದಸ್ಸಿನಲ್ಲಿ ಶೋಧಿಸಿ", 
        "9348": "ಜಾತಿಬ್ರಾಹ್ಮಣಂಗೆ ಕರಸ್ಥಲದಲ್ಲಿ ಲಿಂಗ,", 
        "9349": "ಜ್ಞಾತೃವ ಜ್ಞಾನ ನುಂಗಿ, ಜ್ಞಾನವ ಜ್ಞೇಯ ನುಂಗಿ,", 
        "9350": "ಜ್ಞಾನವೇ ಗುರು, ಜ್ಞಾನವೇ ಲಿಂಗ, ಜ್ಞೇಯವೇ ಜಂಗಮ.", 
        "9351": "ಜ್ಞಾನವೇ ಪ್ರಸಾದಕಾಯ ಜ್ಞೇಯವೇ ಚಿನ್ಮಯಲಿಂಗ,", 
        "9352": "ಜ್ಞಾನ ಸದ್ಭಕ್ತಿ ಸನ್ನಹಿತವಾಗಿ, ಲಿಂಗ ಮುಂತಾಗಿ ಮಾಡಿದ ಕ್ರೀ ಲಿಂಗಕ್ರೀ.", 
        "9353": "ಜ್ಯೋತಿಕರ್ಪುರದ ಸಂಗ ಜ್ಯೋತಿಯಪ್ಪಂತೆ,", 
        "9354": "ತಂದೆ-ತಾಯಿ ಸಂಯೋಗಸಂಭೂತನಲ್ಲದವನು,", 
        "9355": "ತತ್ ಪದ ಲಿಂಗವೆಂದರುಹಿ", 
        "9356": "ತನ್ನ ತಾನರಿಹವೇ ಪರಮಾತ್ಮಯೋಗ,", 
        "9357": "ತನು ತನ್ನದಾದಡೆ, ದಾಸೋಹಕ್ಕೆ ಕೊರತೆಯಿಲ್ಲ,", 
        "9358": "ಏನು ಮನ ಶುದ್ಧವಾದಡೆ", 
        "9359": "ತಾನಾರೆಂದರಿವುದು ಭಕ್ತಿ.", 
        "9360": "ತಾನು ಸಾವ ಕನಸ ಕಂಡಡೆ ಎದ್ದು ಕುಳ್ಳಿರಿ ಎಂಬ ಲೋಕದ", 
        "9361": "ತೃಣಾದಿ ವಿಷ್ಣ್ವಂತ್ಯವಾಗಿ ಸರ್ವರ ಉತ್ಪತ್ತಿಸ್ಥಿತಿಲಯಂಗಳೂ ಶಿವಾಧೀನ.", 
        "9362": "ದಿಟದಿಟ ಈ ಬಂದುತ್ತೆ, ಈ ಬಂದುತ್ತೆ ಸಾವು, ಎನಗಿನ್ನೆಂತಯ್ಯಾ?", 
        "9363": "ದೂರ್ವಾಸ ಉಪಮನ್ಯು ದಧೀಚಿ ಜಮದಗ್ನಿ ಮಾರ್ಕಂಡೇಯ ಪರಾಶರ,", 
        "9364": "ದೇವ ದಾನವ ಮಾನವ ಋಷಿಜಂಗಳೆಲ್ಲರನು", 
        "9365": "ದೇವನೊಂದೆ ವಸ್ತು, ಮನವೊಂದೆ ವಸ್ತು.", 
        "9366": "ದೇವ, ಮಂಗಳಮಜ್ಜನಮಂ ಮಾಡಲು,", 
        "9367": "ದೇವರಾರೋಗಣೆಯಂ ಮಾಡಿಸಿ,", 
        "9368": "ದೇಹವೆಂಬ ಮನೆಯಲ್ಲಿ ಮಹಾಲಿಂಗವೆಂಬರಸು", 
        "9369": "ಧರ್ಮದೊಳಗೆ ಶಿವಧರ್ಮವೇ ಧರ್ಮವೆಂದರಿದೆನಾಗಿ,", 
        "9370": "ನ ಗುರೋರಧಿಕಂ, ನ ಗುರೋರಧಿಕಂ,", 
        "9371": "ನ ಭೂಮಿ ನರವಿಂದಾಪೆÇೀನವಹ್ನಿ ಪ್ರಾಣಾನಿಲಂಗನನೋನ ಚ ವಿದ್ಯತೇ †", 
        "9372": "ನಾನಾ ಪ್ರಯತ್ನದಿಂದ ಹೊನ್ನನಾರ್ಜಿಸುವಂತೆ,", 
        "9373": "ನಾನಾವಿಧದ ಪೂಜೆಗಳ ಪೂಜಿಸಿದವರೆಲ್ಲಾ ಭಕ್ತರೆ, ಹೇಳಿರಣ್ಣಾ.", 
        "9374": "ನಾನು ಕಾಮಿಸುವ ವಸ್ತು ಕರಸ್ಥಲಕೆ ಬಂದ ಬಳಿಕವೂ", 
        "9375": "ನಾನು ಕಾಮಿಸುತ್ತಿಪ್ಪ ವಸ್ತುವಿನಲ್ಲಿ", 
        "9376": "ನಾನೊಂದು ಮಾಡೆಹೆನೆಂಬ ಪ್ರತಿಜ್ಞೆ ಎನಗೊಡ್ಡಿ,", 
        "9377": "ನಿತ್ಯನಿರಂಜನಪರಂಜ್ಯೋತಿಲಿಂಗವು", 
        "9378": "ನಿಮ್ಮ ಶರಣನು ಲಿಂಗಭರಿತನು,", 
        "9379": "ನಿರಾಳದ ಅಷ್ಟದಳಕಮಲದೊಳಗೆ ನಿರಂಜನ ಚೌಕಮಧ್ಯ ನೋಡಾ.", 
        "9380": "ನಿರುಪಾಧಿಕನಾಗಿ, ನಿರ್ವಂಚಕತ್ವದಿಂದ ದಾಸೋಹವ ಮಾಡಿ,", 
        "9381": "ನಿಕ್ಷೇಪನಿಧಿಯ ಸಾಧ್ಯವ ಮಾಡಲೆಂದು", 
        "9382": "ನಿಃಕಾಮಿತ ಫಲವ ಕಾಮಿಸುವ ಮಹಾಕಾಮಿಯೆ ಶಿವಭಕ್ತನು,", 
        "9383": "ನೆಲನನೊಳಕೊಂಡ ಸಲಿಲ, ಸಲಿಲವನೊಳಕೊಂಡ ಅನಲ", 
        "9384": "ನೇತ್ರದಲ್ಲಿ ಷಡುವರ್ಣಸಂಬಂಧವಹ", 
        "9385": "ಪಂಕವಿಲ್ಲದ ಕಮಲಕ್ಕೆ ಸುಗಂಧ ಸುಲಲಿತವೆಂತಪ್ಪುದಯ್ಯಾ?", 
        "9386": "ಪಂಚಗವ್ಯದಿಂದಾದ ಗೋಮಯವಂ ತಂದಾರಿಸಿ", 
        "9387": "ಪಂಚಭೂತದನುವರಿದು", 
        "9388": "ಪಂಚಭೂತವಂಗವಾಗಿಪ್ಪ ಆತ್ಮಂಗೆ", 
        "9389": "ಪಂಚಾಕ್ಷರಿಯೆ ಸರ್ವಮಂತ್ರವೆಲ್ಲವಕ್ಕೆಯು,", 
        "9390": "ಪದ ಪದವಿಗಳ ವಿಚಾರಿಸಲು", 
        "9391": "ಪರಬ್ರಹ್ಮನಿಂದ ಜೀವನಾಯಿತ್ತೆಂಬುವನೊಬ್ಬ ವಾದಿ.", 
        "9392": "ಪರಲೋಕದಲ್ಲಿ ಲಾಭವನರಸುವರು", 
        "9393": "ಪರಶಿವನು ಸುಖಮಯನು, ಸುಖಾತ್ಮನು, ಸುಖಸ್ವರೂಪನು,", 
        "9394": "ಪರಶಿವನೇ ಶ್ರೀಗುರು, ಶ್ರೀಗುರುವೇ ಪರಶಿವನು,", 
        "9395": "ಪರಾಂಗನೆಯರ ಅಂಗವ ಹಿಂಗಿಹುದೆ ಶೌಚ.", 
        "9396": "ಪರುಷದ ಗೃಹದೊಳಗಿದ್ದು, ತಿರಿವನೆ ಮನೆ ಮನೆಯ?", 
        "9397": "ಪರುಷವು ಪಾಷಾಣದಂತೆ ಇಪ್ಪುದು,", 
        "9398": "ಪಾಪ ನನ್ನದೇ? ಪುಣ್ಯ ನಿನ್ನದೆ? ಹೇಳಾ ಅಯ್ಯಾ.", 
        "9399": "ಪುಣ್ಯೆರಾಪೆÇ್ನೀತಿ ದೇವತ್ವಂ ಪಾಪೈಃ ಸ್ಥಾವರಮೇವ ಚ ††", 
        "9400": "ಪುಣ್ಯಾಂಗನೆಯ ಸುತಂಗೆ", 
        "9401": "ಪುತ್ರ ಮಿತ್ರ ಕಳತ್ರಕ್ಕೆ ಸ್ನೇಹಿಸುವಂತೆ", 
        "9402": "ಪುರಾಕೃತಪುಣ್ಯಫಲದ ಪರಿಯ ನೋಡಿರೆ !", 
        "9403": "ಪೃಥ್ವಿ ಅಂಗ, ಚಿತ್ತ ಹಸ್ತ, ನಾಸಿಕ ಮುಖ, ಗಂಧ ಪದಾರ್ಥ,", 
        "9404": "ಪ್ರಕೃತಿವಿಡಿದಿಹುದು ಪ್ರಾಣ,", 
        "9405": "ಪ್ರಣವವೆ ಸರ್ವವರ್ಣಂಗಳ ಕಾರ್ಯಕಾರಣವ್ಯಾಪ್ತಿ ಮೂಲವು,", 
        "9406": "ಪ್ರಥಮದಲ್ಲಿ ಬೀಜವಿಲ್ಲದಿದ್ದಡೆ", 
        "9407": "ಪ್ರಸಾದ ಪ್ರಸಾದವೆನುತಿಪ್ಪಿರಿ.", 
        "9408": "ಪ್ರಸಾದವೆಂದು ಪ್ರಸಾದಿಗಳೆಂದು,", 
        "9409": "ಪ್ರಾಣಲಿಂಗ, ಲಿಂಗಪ್ರಾಣ", 
        "9410": "ಪ್ರಾಣಲಿಂಗ, ಲಿಂಗ ಪ್ರಾಣ ವೆಂಬ ಧ್ಯಾನಾಮೃತದಿಂದ", 
        "9411": "ಪ್ರಾಣ ಲಿಂಗಸಂಬಂಧಿ,", 
        "9412": "ಪ್ರಾಣಲಿಂಗಸಂಬಂಧಿಯಾಗಿ, ಅಂಗಲಿಂಗಸಂಬಂಧಿಯಾಗಿ,", 
        "9413": "ಪ್ರಾಣಲಿಂಗಸಂಬಂಧಿಯಾದ ಸದ್ಭಕ್ತನು", 
        "9414": "ಪ್ರಾಣಲಿಂಗ ಸ್ವಾಯತವಾದ ಶಿವಭಕ್ತಂಗೆ, ಸದ್ಭಕ್ತಿಮಾರ್ಗವ ಕ್ರೀ ನೋಡಾ,", 
        "9415": "ಬಸವ, ಬಸವ, ಬಸವ ಎನುತಿರ್ಪವರೆಲ್ಲರು", 
        "9416": "ಬಹಿರಂಗದಲ್ಲಿ ಶಿವಲಿಂಗ, ಅಂತರಂಗದಲ್ಲಿ ಅನ್ಯದೈವ", 
        "9417": "ಬಳ್ಳ ಬೇವಿನಕೊರಡು ಆಡಿನಹಿಕ್ಕೆ ಲಿಂಗವೆ ?", 
        "9418": "ಬೆಡಗಿನಲಿಪ್ಪ ಅಗ್ನಿ ಮೈದೋರದಿದ್ದಲ್ಲಿ ಆಕಾರ ನಾಸ್ತಿಯಾಗದು.", 
        "9419": "ಬ್ರಹ್ಮದೇವರಾದರೆ ತಾನೇರುವ ಹಂಸೆಯಿಂದಲಿ ಬಿತ್ತಿ ಬೆಳೆದು", 
        "9420": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ಮೊದಲಾದ ದೇವದಾನವ ಮಾನವರೆಲ್ಲರನೂ", 
        "9421": "ಬ್ರಹ್ಮವೆಂಬುದನರಿದ ಬಳಿಕ", 
        "9422": "ಬ್ರಹ್ಮಸೃಷ್ಟಿ ಪಂಚಭೂತ ಸಮ್ಮಿಶ್ರದಿಂದಾದ", 
        "9423": "ಬ್ರಹ್ಮಾಬ್ರಹ್ಮರಿಲ್ಲದಂದು,", 
        "9424": "ಭಕ್ತಕಾಯ ಮಮಕಾಯ ಕಂಡಾ ಲಿಂಗವು,", 
        "9425": "ಭಕ್ತಕಾಯ ಮಮಕಾಯ ಎಂಬ ವಾಕ್ಯದಲ್ಲಿ ಕಾಯಲಿಂಗವೆಂದಿರಿದಿಪ್ಪ,", 
        "9426": "ಭಕ್ತನಂಗಾಫಗಲಿ ಜಂಗಮಕ್ಕಾಗಲಿ ಸಹಪಂತಿ ಶಿವಭೋಜನದಲ್ಲಿ", 
        "9427": "ಭಕ್ತನು ರುದ್ರಗಣಂಗಳ ಮಠಕ್ಕೆ ಬಂದಡೆ", 
        "9428": "ಭಕ್ತ ಮಾಹೇಶ್ವರ ಪ್ರಸಾದಿ ಪ್ರಾಣಲಿಂಗಿ", 
        "9429": "ಭಕ್ತರಲ್ಲದವರನಾಸೆಗೈದಡೆ ಕಕ್ಕುಲತೆಯಲ್ಲದೆ ಕಾರ್ಯವಲ್ಲ.", 
        "9430": "ಭಕ್ತಿ ಭಕ್ತಿಯೆಂಬರು:", 
        "9431": "ಭಕ್ತಿಭಾಂಡದ ಶಿವಭಕ್ತರ ನಿಷೇಧವ ಮಾಡುವ", 
        "9432": "ಭಕ್ತಸ್ಥಲ ಸಾಧ್ಯವಾಯಿತ್ತು ಸಂಗನಬಸವರಾಜದೇವರಿಗೆ.", 
        "9433": "ಭವಿತನಕ್ಕೆ ಹೇಸಿ ಭಕ್ತನಾಗಬೇಕೆಂಬಾತನು ಸದ್ಗುರುವನರಸಿಕೊಂಡು ಬಂದು,", 
        "9434": "ಮಜ್ಜನಕ್ಕೆರೆವ ಶರಣರ ಮಹಿಮೆಯನೇನೆಂಬೆನು !", 
        "9435": "ಮಣ್ಣ ಬೆಲ್ಲವ ಮಾಡಿ, ಮಗುವಿನ ಕೈಯಲ್ಲಿ ಕೊಟ್ಟು,", 
        "9436": "ಮದ್ಗುರುವೆ ಸದ್ಗುರುವೆ ತ್ರಿಜಗದ್ಗುರುವೆ", 
        "9437": "ಮನದಲ್ಲಿ ಮಹವನರಿದು ಮನ ನಿರ್ದೇಶವಾಗಿ", 
        "9438": "ಮನ ಮುಟ್ಟದು, ಬುದ್ಧಿ ಒಂದಾಗದು, ಚಿತ್ತವಿಡಿಯದು,", 
        "9439": "ಮನವೇ, ನಿನ್ನ ನೆನಹು ಸಿದ್ಧಿಯಾದುದಲ್ಲಾ,", 
        "9440": "ಮನೋವಾಕ್ಕಾಯಮೇಕಸ್ಯ ಗುರುಶ್ಚರಃ ಶಿವಸ್ತಥಾ", 
        "9441": "ಮಹಾಜ್ಯೋತಿಯು ಸೋಂಕಿದ ಉತ್ತಮಾಧಮತೃಣ ಮೊದಲಾದವೆಲ್ಲವೂ", 
        "9442": "ಮಹಾರಾಜನನೆಲ್ಲರೂ ಬಲ್ಲರು", 
        "9443": "ಮಹಾಸ್ಥಾನದಲ್ಲಿ ಗುರುಸ್ವರೂಪನಾಗಿರುತಿರ್ದೆ,", 
        "9444": "ಮಾತಂಗ ಪಾರಮೇಶ್ವರೇ :", 
        "9445": "ಮಾತಾಪಿತಸಂಯೋಗಸಂಭೂತನಲ್ಲದವನು", 
        "9446": "ಮೂರು ಮಲ ಮೊದಲಾದ ಸಂಸಾರ ಸಂಗವ ಬಿಟ್ಟು,", 
        "9447": "ಮೃಗಾದಿಗಳಂತೆ [ಆಚರಿಸಿದಡೇನುರಿ] ದಾನಾದಿಗಳ ಕೊಟ್ಟಡೇನು ?", 
        "9448": "ಯತೋ ವಾಚೋ ನಿವರ್ತಂತೇ ಅಪ್ರಾಪ್ಯ ಮನಸಾ ಸಹ", 
        "9449": "ಯಥಾ ಬೀಜಂ ತಥಾsಂಕುರಂ ಎಂಬ ವಾಕ್ಯ ತಪ್ಪದು", 
        "9450": "ಯೋಗಾಭ್ಯಾಸ ಯೋಗಾಭ್ಯಾಸವೆಂದೆಂಬಿರಿ", 
        "9451": "ರತ್ನಂಗಳೊಳಗೆ ಚಿಂತಾಮಣಿ ಮಹಾರತ್ನವೆಂತು", 
        "9452": "ರಾವಣ ಪಡೆದ ಲಿಂಗ ಚಂದ್ರಾಯುಧ ಪಾರ್ವತಿಯ ಗಜಮುಖನ", 
        "9453": "ರುದ್ರಪಿಂಡದಲ್ಲಿ ಉತ್ಪತ್ತಿ", 
        "9454": "ಲಘು ಗುರುವಪ್ಪನೆ ? ಗುರು ಲಘುವಪ್ಪನೆ ? ಆಗದಾಗದು.", 
        "9455": "ಲಿಂಗಕ್ಕೆಯೂ ತನಗೆಯೂ ಭಿನ್ನಭಾವವಿಲ್ಲೆಂಬರು,", 
        "9456": "ಲಿಂಗದಲ್ಲಿ ಮನ ಲೀಯವಾಗಿ,", 
        "9457": "ಲಿಂಗದ ಮರ್ಮವನರಿವುದರಿದು,", 
        "9458": "ಲಿಂಗದೇವನ ನಂಬಿದವಂಗೆ ಜ್ಞಾನವಿದೆ,", 
        "9459": "ಲಿಂಗದೊಡನೆ ಸಹಭೋಜನ ಮಾಡುವ", 
        "9460": "ಲಿಂಗಪ್ರಸಾದವಂ ಚೆಲ್ಲಿ, ಜಂಗಮಪ್ರಸಾದದ ಸುಯಿಧಾನಿಯೆಂಬ", 
        "9461": "ಲಿಂಗಪ್ರಸಾದದಲ್ಲಿ ಸುಯಿಧಾನಿಯಾಗದವರಲ್ಲಿ", 
        "9462": "ಲಿಂಗ ಪ್ರಾಣ, ಪ್ರಾಣ ಲಿಂಗ,", 
        "9463": "ಲಿಂಗಭರಿತ ಶರಣ, ಶರಣಭರಿತ ಲಿಂಗವದೆಂತೆಂದಡೆ:", 
        "9464": "ಲಿಂಗ ಲಿಂಗಿಗಳ ದರ್ಶನ: ಸಾಲೋಕ್ಯಪದ.", 
        "9465": "ಲಿಂಗವಂತಂಗೆ ಲಿಂಗವು ಮಾಡಿದ ಪದವನು ಲಿಂಗವಂತರೆ ಬಲ್ಲರು.", 
        "9466": "ಲಿಂಗವಂತನು ಕೇವಲ ಲಿಂಗಮೂರ್ತಿ.", 
        "9467": "ಲಿಂಗವಂತನು ಲಿಂಗವಂತಂಗೆ", 
        "9468": "ಲಿಂಗವಂತನು ಲಿಂಗವಂತರಲ್ಲಿಯೇ ಸಂಗವ ಮಾಡುವುದು,", 
        "9469": "ಲಿಂಗವಂತನು ಲಿಂಗವಂತರೊಡನೆ ಸತ್ಯವ ನುಡಿವುದು, ಸಹಜದಲ್ಲಿ ನಡೆವುದು,", 
        "9470": "ಲಿಂಗವಂತನು ಲಿಂಗವಿಲ್ಲದರ ಬಯಸಿದಡೆ ಆತಂಗೆ ಲಿಂಗವಿಲ್ಲ,", 
        "9471": "ಲಿಂಗವಂತರ ಲಿಂಗವೆಂಬುದೇ ಶೀಲ,", 
        "9472": "ಲಿಂಗವಂತ ಲಿಂಗವಂತ ಎಂಬಿರಿ,", 
        "9473": "ಲಿಂಗವಂತನು ಲಿಂಗಾಚಾರಿ ಸದಾಚಾರಿ ಭೃತ್ಯಾಚಾರಿಯೆನಿಸಿದಡೆ", 
        "9474": "ಲಿಂಗವಂತನು ಲಿಂಗಾಚಾರ, ಸದಾಚಾರ,", 
        "9475": "ಲಿಂಗವ ನಂಬಿ ಲಿಂಗಾರ್ಚನೆಯ ಮಾಡಿ", 
        "9476": "ಲಿಂಗವನರಿದು ಲಿಂಗಾರ್ಚನೆಯ ಮಾಡಿ", 
        "9477": "ಲಿಂಗವನರಿದ ಲಿಂಗವಂತನು ಸರ್ವಾಂಗಲಿಂಗಮೂರ್ತಿ.", 
        "9478": "ಲಿಂಗವನರಿಯರು ಲಿಂಗದ ಮುಖವನರಿಯರು.", 
        "9479": "ಲಿಂಗವಿದ್ದುದೇ ಕೈಲಾಸ, ಲಿಂಗವಿದ್ದುದೇ ಕಾಶೀಕ್ಷೇತ್ರ", 
        "9480": "ಲಿಂಗವು ಕೊಟ್ಟ ಆಯುಷ್ಯದಲ್ಲಿ", 
        "9481": "ಲಿಂಗವು ಸರ್ವಾಂಗದಲ್ಲಿ ಭರಿತವಾಗಿರಲು", 
        "9482": "ಲಿಂಗವೆಂಬುದು ಪರಶಕ್ತಿಯುತ ಪರಶಿವನ ನಿಜದೇಹ,", 
        "9483": "ಲಿಂಗವೇ ಪ್ರಾಣ, ಪ್ರಾಣವೇ ಲಿಂಗವಾದ ಪ್ರಾಣಲಿಂಗಿಯ ಪ್ರಾಣಲಿಂಗಕ್ಕೆ,", 
        "9484": "ಲಿಂಗಾರ್ಚನೆಯ ಮಾಡಿ", 
        "9485": "ಲಿಂಗಾರ್ಪಿತವ ಮಾಡಿ ಪ್ರಸಾದವನಲ್ಲದೆ ಭೋಗಿಸೆನೆಂಬ", 
        "9486": "ಲಿಂಗಿನಾ ಸಹವರ್ತಿತ್ವಂ ಲಿಂಗಿನಾ ಸಹ ವಾದಿತಾ †", 
        "9487": "ಲೋಕದ ಜಡಜೀವಿಗಳಿಗೆ", 
        "9488": "ಲೋಕಹಿತಾರ್ಥವಾಗಿ, ಪ್ರಾಣಿಗಳೆಲ್ಲ ಕೆಟ್ಟಹರೆಂದು", 
        "9489": "ವಂಚನೆಯಿಲ್ಲದುದೇ ಭಕ್ತಿ,", 
        "9490": "ವಂಚಿಸುವ ಸತಿ ಸುತ ಬಂಧು ಮೊದಲಾದ ಲೋಕವ ನಂಬಿ,", 
        "9491": "ವಟಬೀಜವು ವಟವೃಕ್ಷಕೋಟಿಯನೊಳಕೊಂಡಿಪ್ಪಂತೆ,", 
        "9492": "ವಟಬೀಜ ವಟವೃಕ್ಷಕೋಟಿಯ ನುಂಗಿಪ್ಪಂತೆ", 
        "9493": "ವಶಿಷ್ಠಗೋತ್ರದಲ್ಲಿ ಹುಟ್ಟಿದವನು ವಶಿಷ್ಠಗೋತ್ರದವನೆಂಬಂತೆ,", 
        "9494": "ವಿತ್ತಂ ಚ ರಾಜವಿತ್ತಂ ಚ ಕಾಮಿನೀ ಕಾಮಕಾವಶಾ †", 
        "9495": "ವಿಶ್ವಾಸವೆ ಭಕ್ತಿ, ಗುರುಲಿಂಗಜಂಗಮವೊಂದೆಂದರಿವುದೆ ಜ್ಞಾನ,", 
        "9496": "ವೇದಂಗಳು ದೈವವೆಂಬ ವಿಪ್ರರಂತಹ ಮರುಳರುಂಟೇ ತ್ರಿಜಗದಲ್ಲಿ ?", 
        "9497": "ವೇದಮಾರ್ಗವ ಮೀರಿದರು ಮಹಾವೇದಿಗಳು ಲಿಂಗವಂತರು,", 
        "9498": "ವೇದವನೋದಿ ಕೇಳಿ", 
        "9499": "ವೇದವನೋದಿ ಕೇಳಿದಡೇನು? ವೇದಜ್ಞನಪ್ಪನಲ್ಲದೆ ಭಕ್ತನಲ್ಲ.", 
        "9500": "ವೇದವಾಕ್ಯ ವಿಚಾರಕ್ಕೆ ಬೀಜ,", 
        "9501": "ವೇದವಾವುದು? ವೇದ್ಯವಾವುದು? ಎಂದು ಭೇದವನರಿದಾತ ಕೃತಾರ್ಥನು.", 
        "9502": "ವೇದಶಾಸ್ತ್ರಪುರಾಣಾಗಮಗ್ರಂಥಂಗಳ", 
        "9503": "ವೇದಶಾಸ್ತ್ರಾಗಮಪುರಾಣಿಯಾದವರಿಂದತ್ತತ್ತ ನೀನು.", 
        "9504": "ವೇದಾದಿ ಅಷ್ಟಾದಶವಿದ್ಯೆಗಳ ಸರ್ವಮಂತ್ರಗಳ ಮೂಲವು ಶ್ರೀಪಂಚಾಕ್ಷರಿ.", 
        "9505": "ವ್ಯಾಸಾದಿಗಳಂತೆ ಶ್ವಾನಜ್ಞಾನಿಗಳಪ್ಪರೆ ಸತ್ಯಶುದ್ಧಶರಣರು ?", 
        "9506": "ಶರಣನ ಇಂದ್ರಿಯ ಕರಣಂಗಳು ಶಿವಾಚಾರದಲ್ಲಿ", 
        "9507": "ಶರಣರ ಕೂಡೆ ನುಡಿ ದಿಟವಿಲ್ಲ,", 
        "9508": "ಶರಣರಲ್ಲದವರನಾಸೆಗೈದಡೆ ಕಕ್ಕುಲತೆಯಪ್ಪುದಲ್ಲದೆ ಕಾರ್ಯವಿಲ್ಲ,", 
        "9509": "ಶರಣರ ಸಂಗ ಸಾಲೋಕ್ಯಪದವಯ್ಯಾ.", 
        "9510": "ಶರಣಲಿಂಗಸಂಬಂಧಿಯಾದಾತನು ತನ್ನಂಗದ ಲಿಂಗದಲ್ಲಿ", 
        "9511": "ಶಿವ ಜಗವಾಗಲ್ಲ, ಶಿವ ಜಗವಾಗದಿರಲೂ ಬಲ್ಲ.", 
        "9512": "ಶಿವ ತನ್ನ ವಿನೋದಕ್ಕೆ ರಚಿಸಿದನು, ಅನಂತ ವಿಶ್ವವನು.", 
        "9513": "ಶಿವನು ನರಕಾಯವ ತೊಟ್ಟು ಗುರುರೂಪಾಗಿ ಮತ್ರ್ಯಕ್ಕೆ ಬಂದು", 
        "9514": "ಶಿವನು ಸರ್ವಗತನು, ಸಕಲಬ್ರಹ್ಮಾಂಡಗಳಿಗೆಲ್ಲ ಸಕಲನೆಂದು ಹೇಳಿತ್ತು ಶ್ರುತಿ.", 
        "9515": "ಶಿವನೆ, ನಿಮ್ಮ ನಾ ಬಲ್ಲೆನು.", 
        "9516": "ಶಿವಂ ಪರಾತ್ಪರಂ ಸೂಕ್ಷ್ಮಂ ನಿತ್ಯಂ ಸರ್ವಗತಾವ್ಯಯಮ್ †", 
        "9517": "ಶಿವನೇ ದೈವ, ಶಿವಭಕ್ತನೇ ಕುಲಜ, ಷಡಕ್ಷರವೇ ಮಂತ್ರ,", 
        "9518": "ಶಿವಭಕ್ತನ ಕಂಡಲ್ಲಿ ಪಾಪ ಕೇಡುವುದು", 
        "9519": "ಶಿವಭಕ್ತರ ಮಠವಲ್ಲದೆ ಒಲ್ಲೆನೆಂದು", 
        "9520": "ಶಿವಲಿಂಗದ ಮಹಾತ್ಮೆಯನು", 
        "9521": "ಶಿವಲಿಂಗಧಾರಣವ ಮಾಡಬೇಕೆಂದುದು ಶ್ರುತಿ.", 
        "9522": "ಶಿವಶರಣರಿಗೆ ಶರಣೆನ್ನದೆ", 
        "9523": "ಶಿವ ಶಿವ ! ಪರಶಿವಮೂರ್ತಿ ಮಹಾಲಿಂಗವು", 
        "9524": "ಶಿವ ಶಿವ! ಮಹಾದೇವ,", 
        "9525": "ಶಿವ ಶಿವ! ಮಹಾದೇವ ಶಿವನೇ", 
        "9526": "ಶಿವ ಶಿವ! ಮಾಹೇಶ್ವರರ ಮಹಾತ್ಮೆಯನೇನೆಂದುಪಮಿಸಬಹುದಯ್ಯಾ,", 
        "9527": "ಶಿವ ಶಿವಾ, ನೀನೆನ್ನ ಮನವ ನೋಡಿಹೆನೆಂಬೆ.", 
        "9528": "ಶಿವ ಶಿವಾ, ಬಸವಣ್ಣನೇ ಗುರುತತ್ತ್ವವುಳ್ಳ ಮಹಾವಸ್ತುವಾಗಿ", 
        "9529": "ಶಿವಶಿವಾ, ಭಕ್ತಿಹೀನನ ಬಟ್ಟೆಯಲ್ಲಿ ಕಂಡು", 
        "9530": "ಶಿವ ಶಿವಾ, ಲಿಂಗವಂತರ ಚಿತ್ತಕ್ಕೆ, ಲಿಂಗದ ಚಿತ್ತಕ್ಕೆ", 
        "9531": "ಶಿವಾಚಾರ ಶಿವಕಾರ್ಯಕ್ಕೆ ವಕ್ರವಾದವರ", 
        "9532": "ಶಿಷ್ಯನ ಗುರುತ್ವವನು, ಗುರುವಿನ ಮಹತ್ವವನು,", 
        "9533": "ಶೀಲ ಸಮಾನವಿಲ್ಲೆಂದು ಅಲಿಂಗವಂತರ ಮಗಳಿಗೆ", 
        "9534": "ಶ್ರೀಗಂಧದ ತರು ಕಾಮರನಾದುದುಂಟೆ?", 
        "9535": "ಶ್ರೀಗುರು ಕರುಣಿಸಿ ಪ್ರಾಣಲಿಂಗಸಂಬಂಧವ ಮಾ[ಡೆ]", 
        "9536": "ಶ್ರೀಗುರುಕಾರುಣ್ಯವ ಪಡೆದು ಲಿಂಗಸ್ವಾಯತವಾದ ಬಳಿಕ", 
        "9537": "ಶ್ರೀಗುರುಪ್ರೇಮವಿಲ್ಲದೆ ಲಿಂಗಾರ್ಚನೆಯ ಮಾಡಿದಡೆ", 
        "9538": "ಶ್ರೀಗುರು ಮಾಡಿದ ಗುರುತ್ವ ಉಪಮಾತೀತವು.", 
        "9539": "ಶ್ರೀಗುರುಲಿಂಗ ಸಾಕ್ಷಾತ್ಪರಶಿವಲಿಂಗ ತಾನೆ.", 
        "9540": "ಶ್ರೀಗುರುಲಿಂಗಜಂಗಮಕ್ಕೆ ತನುಮನಧನವನು", 
        "9541": "ಶ್ರೀಗುರು ಲಿಂಗ ಜಂಗಮ ಒಂದೆ ಎಂಬುದು,", 
        "9542": "ಶ್ರೀಗುರು ಲಿಂಗ ಜಂಗಮ ತ್ರಿವಿಧವೆಂದು ನಾನರಿವಂದು", 
        "9543": "ಶ್ರೀಗುರು ಲಿಂಗ ಜಂಗಮ ಪ್ರಸಾದ", 
        "9544": "ಶ್ರೀಗುರುಲಿಂಗದಲ್ಲಿ ಬಸವಣ್ಣನ ಕಂಡೆನು,", 
        "9545": "ಶ್ರೀಗುರುವಿನಲ್ಲಿ ಸಕಲಮಾಹೇಶ್ವರರಲ್ಲಿ, ಅನುಕೂಲವ ನುಡಿಯದೆ", 
        "9546": "ಶ್ರೀಗುರುವೇ ಪರಶಿವಲಿಂಗ,", 
        "9547": "ಶ್ರೀಗುರು ಶಿಷ್ಯಂಗೆ ಮಂತ್ರವ ಮೂರ್ತಿಗೊಳಿಸಬೇಕಾಗಿ,", 
        "9548": "ಶ್ರೀಗುರುಸ್ವಾಮಿ ಕರುಣಿಸಿಕೊಟ್ಟು ಪ್ರಾಣಲಿಂಗವು", 
        "9549": "ಶ್ರೀಪಂಚಾಕ್ಷರಿ", 
        "9550": "ಶ್ರೀಮತ್ಪಂಚಾಕ್ಷರಿಯ ಉಚ್ಚರಣದ ವಿಶ್ವಾಸದಿಂದ", 
        "9551": "ಶ್ರೋತ್ರ, ತ್ವಕ್ಕು, ನೇತ್ರ, ಜಿಹ್ವೆ, ಘ್ರಾಣ,", 
        "9552": "ಸಕಲಜನರು ಹೇಸಿದ ಉದಾನವ ಶ್ವಾನ ಭುಂಜಿಸಿ", 
        "9553": "ಸಕಲವ ಪೂಜಿಸಿಹೆನೆಂಬವಂಗೆ,", 
        "9554": "ಸಟೆಯ ದಿಟವಮಾಡಿ ಕಂಡು ಸ್ವಭಾವವಾಗಿ ಸದ್ಭಾವದಿಂ ಲಿಂಗವಾಗಿ", 
        "9555": "ಸತ್ಯ ಛಲ ಭಾಷೆಯಿಂ ಸತಿಯಾಗಿರಬಲ್ಲ ಶರಣಂಗೆ ಲಿಂಗವೇ ಪತಿ.", 
        "9556": "ಸತ್ಯಸಹಜ ನಿತ್ಯ ಉತ್ತಮ ವಸ್ತು,", 
        "9557": "ಸತ್ಕ್ರೀಯಿಂದ ಸಕಲಪದಾರ್ಥಂಗಳ ಭಾಜನ ತೀವಿ,", 
        "9558": "ಸತ್ತುರೂಪು ಲಿಂಗ,", 
        "9559": "ಸದ್ಭಕ್ತನು ತನು ಮನ ಧನ ಪ್ರಾಣಾದಿಗಳನರ್ಪಿಸಿ,", 
        "9560": "ಸದ್ಭಕ್ತಿಯೆ ಆರು ಸ್ಥಲಕ್ಕೆ ಮುಖ್ಯ", 
        "9561": "ಸದ್ಗುರುವಿನಿಂದ ಮಹತ್ತಪ್ಪ ಮಹಾಲಿಂಗ ಉತ್ಪತ್ತಿ,", 
        "9562": "ಸದ್ಗುರುವೇ ಶಿವಲಿಂಗವು, ಸದ್ಗುರುವೇ ಜಂಗಮಲಿಂಗವು,", 
        "9563": "ಸದ್ಗುರುಶ್ಶಿವಲಿಂಗಂ ಚ ಜಂಗಮಶ್ಚ ಪ್ರಸಾದಕಂ †", 
        "9564": "ಸದ್ಯೋಜಾತಮುಖವೇ ಪೃಥ್ವಿ ಎಂದರಿದಲ್ಲಿ", 
        "9565": "ಸದ್ಯೋಜಾತ ವಾಮದೇವ ತತ್ಪುರುಷ ಅಘೋರ ಈಶಾನ್ಯವೆಂಬ", 
        "9566": "ಸಪ್ತವ್ಯಸನಿಗಳಾದವರಿಗೆ ಆ ವ್ಯಸನಿಗಳ ಸಂಗದಿಂದಲ್ಲದೆ", 
        "9567": "ಸಮ್ಯಜ್ಞಾನ ಸದ್ಭಕ್ತಿ ಸನ್ನಹಿತನಾಗಿ,", 
        "9568": "ಸರ್ವಗತನಾಗಿ ನೀನೆನ್ನೊಳಗಿಹೆ,", 
        "9569": "ಸಾಕಾರವಿಡಿದು ಅರ್ಚನೆ ಪೂಜನೆಯಂ ಮಾಡುವುದಲ್ಲದೆ", 
        "9570": "ಸಾಕಾರವಿಡಿದು ಪರಬ್ರಹ್ಮ, ನಿರಾಕಾರವಿಡಿದು ಪರಬ್ರಹ್ಮವೆಂಬಿರಿ.", 
        "9571": "ಸಾಲೋಕ್ಯವೆಂದೇನೋ,", 
        "9572": "ಸಾವುದು ದಿಟ ದಿಟ ನೋಡಾ.", 
        "9573": "ಸೂರ್ಯನಿಲ್ಲದೆ ಹಗಲುಂಟೆ ಅಯ್ಯಾ?", 
        "9574": "ಸೂಳೆ ಭಕ್ತೆಯಾದಡೆ ಮಿಂಡಜಂಗಮಕ್ಕಲ್ಲದೆ ಮಾಡಳು.", 
        "9575": "ಸೆಜ್ಜೆ ಶಿವದಾರ ವಸ್ತ್ರದಲ್ಲಿ ಹುದುಗಿಸಿ ಲಿಂಗವ ಧರಿಸಿಕೊಂಡು", 
        "9576": "ಸೊಡರ ಹಿಡಿದು ಕುಣಿಯಲ್ಲಿ ಬೀಳುವವರ ಕಂಡು,", 
        "9577": "ಸ್ತನಾಮೃತವ ಸೇವಿಸುವ ಶಿಶು ಸಕ್ಕರೆಯನಿಚ್ಛಿಸುವುದೆ?", 
        "9578": "ಸ್ವಸ್ತಿ ಸಮಸ್ತವಿದ್ಯಾದಿ ಮೂಲವಹ", 
        "9579": "ಸ್ಥೂಲತನು ಸೂಕ್ಷ್ಮತನುವಿಡಿದು ಜಾಗ್ರತೆಯಲ್ಲಿ", 
        "9580": "ಹರನೆ, ನಿಮ್ಮನು ದುಷ್ಟನಿಗ್ರಹ ಶಿಷ್ಟಪರಿಪಾಲನವಿಚಕ್ಷಣನೆಂದೆಂಬರು.", 
        "9581": "ಹಸಿವು ತೃಷೆ ವಿಷಯ ಕಾಮ ಕ್ರೋಧ ಲೋಭ", 
        "9582": "ಹಿಂದಾದ ದುಃಖವನೂ ಇಂದಾದ ಸುಖವನೂ ಮರೆದೆಯಲ್ಲಾ!", 
        "9583": "ಹೆಣ್ಣ ಧ್ಯಾನಿಸಿ ನಿಮ್ಮ ಮರೆದೆನಯ್ಯಾ,", 
        "9584": "ಹೊನ್ನ ಬಿಟ್ಟು, ಹೆಣ್ಣ ಬಿಟ್ಟು, ಮಣ್ಣ ಬಿಟ್ಟು, ಬ್ರಹ್ಮಚಾರಿಗಳಾಗಬೇಕೆಂದು", 
        "9585": "ಹೊನ್ನು, ಹೆಣ್ಣು, ಮಣ್ಣು, ಅನ್ನ, ಉದಕ, ದ್ರವ್ಯಂಗಳು"
    }
}