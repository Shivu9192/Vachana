        "17289": "ಇಂತು ಶಿವ ಸದಾಶಿವ ಮಹೇಶ್ವರ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ", 
        "17290": "ನಾದಸಂಜ್ಞಿತವಾದ ಹಕಾರದ ಮೊದಲಹ ಸ್ ಎಂಬುದನುದ್ಧರಿಸಿ", 
        "17291": "ಮತ್ತಂ, ಗಣಸಂಜ್ಞಿತವಾದ ಗ್ ಎಂಬುದನುದ್ಧರಿಸಿ", 
        "17292": "ಮಂತ್ರೋತ್ಪತ್ತಿ ನಿರೂಪಣಾನಂತರದೊಳ್ತತ್ವಂಗಳಂ ಮಂತ್ರಂಗಳಂ", 
        "17293": "ಬಳಿಕ್ಕಂ,", 
        "17294": "ಮತ್ತಂ, ಕ್ರಮದಿಂ ಹ್ರೌಂ ಎಂಬ ಮೂಲಪ್ರಸಾದವಾಚಕದಲ್ಲಿ", 
        "17295": "ಮತ್ತೆಯುಮಾ, ಮಂತ್ರಮೂರ್ತಿಯಂ ಧ್ಯಾನಿಸಿ", 
        "17296": "ಮತ್ತೆಯುಂ, ಕ್ರಿಯೆಯೆ ಶರೀರಂ ಮಂತ್ರವೆ ಜೀವವಿಮೆವೆರಡರ", 
        "17297": "ಮತ್ತಮಿಂತು ಮಂತ್ರಭೇದ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ", 
        "17298": "ಮರಲ್ದುಮಾ ಪ್ರಣವಂ ಪಂಚಪ್ರಕಾರಮಾದಪುದೆಂತೆನೆ", 
        "17299": "ಮತ್ತಂ, ಸಾಕಲ್ಯಪ್ರಣವ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ", 
        "17300": "ಇಂತು,", 
        "17301": "ಇಂತು ಪಂಚಪ್ರಣವ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ", 
        "17302": "ಇಂತು, ಪೂರ್ವೋಕ್ತ ಪಂಚಬ್ರಹ್ಮವಪ್ಪ ಶಿವಬೀಜವೆ", 
        "17303": "ಬಳಿಕಮೀ ಪಂಚವಿಂಶತಿ ಬ್ರಹ್ಮಂಗಳ ಲಕ್ಷಣಮಂ ಪೇಳ್ವೆನೆಂತೆನೆ-", 
        "17304": "ಮತ್ತಮಾ, ಸದ್ಯಾದಿಗಳಿಂದೊಗೆದ ಪೃಥ್ವಾ ್ಯದಿ ಪಂಚಭೂತಂಗಳ", 
        "17305": "ಮತ್ತಮಾ, ಪೃಥ್ವಾ ್ಯದಿಭೂತಂಗಳಂ ಪೆತ್ತ", 
        "17306": "ಮತ್ತಂತರದಿಂ", 
        "17307": "ಬಳಿಕ್ಕೆಯುಮಾ,", 
        "17308": "ಮತ್ತೆಯುಂ,", 
        "17309": "ಮತ್ತಂ, ಪ್ರಣವಭೇದ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ", 
        "17310": "ಮತ್ತಮೀ ಷಡಂಗಮಂತ್ರಗಳೆ", 
        "17311": "ಮತ್ತಂ, ಶಿವಾಂಗಮಂತ್ರ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ,", 
        "17312": "ಬಳಿಕ್ಕಂ, ಭೂತಾಂಗ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ", 
        "17313": "ಕೂಟಾಂಗ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ ವಿದ್ಯಾಂಗಮಂ ಪೇಳ್ವೆನೆಂತೆನೆ-", 
        "17314": "ಈ ವಿದ್ಯಾಂಗ ನಿರೂಪಣಾನಂತರದಲ್ಲಿ", 
        "17315": "ಮತ್ತಮಾ ಶಕ್ತಿಬೀಜವಾದ ಸಕಾರವೆ ಚತುರ್ಥಸ್ವರದೊಡನೆ ಕೂಡಿ", 
        "17316": "ಮರಲ್ದುಂ, ಪೂರ್ವೋಕ್ತ ಸಕಾರಮಂ ಷಷ್ಠಸ್ವರದೊಡನೆ ಕೂಡೆ", 
        "17317": "ಮೊದಲಾಧಾರ ಶಕ್ತಿಸಂಜ್ಞಿತ ಸಕಾರವೆ", 
        "17318": "ಬಳಿಕ್ಕಾಧಾರಶಕ್ತಿ ಸಂಜ್ಞಿತ ಸಕಾರವು ಚತುರ್ದಶ ಸ್ವರದಿಂ ಕೂಡಿ", 
        "17319": "ಪೂರ್ವೋಕ್ತ ಸಕಾರವುಂ ಸ್ವರಾಂತ ವಿಸರ್ಗದೊಡನೆ ಕೂಡಿಸೆ", 
        "17320": "ಮತ್ತಂ, ಶಕ್ತ್ಯಂಗನಿರೂಪಣಾನಂತರದೊಳ್ಸಾಮಾನ್ಯಾಂಗಮಂ", 
        "17321": "ಬಳಿಕ್ಕಮೀ ಸಾಮಾನ್ಯಾಂಗ ಮಂತ್ರದೇವತೆಗಳಂ", 
        "17322": "ಮತ್ತಮಾ, ಮೂವತ್ತಾರಂಗಗಳೆಂತೆನೆ-", 
        "17323": "ಮತ್ತಂ, ಪೃಥ್ವೀ ಪೀತವರ್ಣಂ.", 
        "17324": "ಶ್ವೇತಂ ರಕ್ತಂ, ನೀಲಂ ಕುಂಕುಮಂ.", 
        "17325": "ಬಳಿಕ್ಕಂ, ಜಪಾಕುಸುಮ ಕುಂದಪಾವಕ ಸುರೇಂದ್ರ", 
        "17326": "ಮತ್ತಂ,", 
        "17327": "ಮತ್ತಂ, ಪರಿವಿಡಿಯಿಂ ಸ್ಫಟಿಕವರ್ಣಂ ರಕ್ತವರ್ಣಂ ಪೀತವರ್ಣಂ", 
        "17328": "ಬಳಿಕ್ಕಂ, ಚತುರ್ಭುಜ, ತ್ರೀನೇತ್ರ, ಜಟಾಮಕುಟ,", 
        "17329": "ಮರಲ್ದುಂ, ಪಾದಾದಿ ಮಸ್ತಕ ಪರ್ಯಂತಂ ಪಂಚಭೂತಶುದ್ಧಿಯಂ", 
        "17330": "ಮತ್ತೆಯುಂ, ಕೂಟಾಂಗ ವಿದ್ಯಾಂಗ[ಗ]ಳಿಂದೆ ಶೈವಾಚಾರ್ಯಂ", 
        "17331": "ಮತ್ತಮುಮಾದಿಶಕ್ತಿ ಸ್ಥಾಪನಾಕಾಲದಲ್ಲಿ ಶಕ್ತ್ಯಂಗಗಳಿಂದವೆ", 
        "17332": "ಬಳಿಕ್ಕಂ", 
        "17333": "ಮತ್ತಂ ಭೂತಜಹೃದಯಮಂ ಗ್ರಂಥವಿರ್ದಲ್ಲಿ ನೋಡಿಕೊಂಬುದು.", 
        "17334": "ಮತ್ತೆಯುಮೀಯೈದು ಹೃದಯಮಂತ್ರಕ್ಕೆಯುಂ ತರದಿಂ", 
        "17335": "ಬಳಿಕ್ಕಂ", 
        "17336": "ಮರಲ್ದುಂ, ಕೇವಲಹೃದಯದಿಂ ಯಜನಂ", 
        "17337": "ಬಳಿಕ್ಕಂ, ಶಿವಾಸ್ತ್ರಮಂತ್ರಂ, ಕ್ಷುರಿಕಾಸ್ತ್ರಮಂತ್ರಂ", 
        "17338": "ಮತ್ತಂ, ಪೂಜಕನಾದ ಸುಶೈವಂ", 
        "17339": "ಮತ್ತೆಯುಮಂಗಭೇದ ನಿರೂಪಣಾನಂತರದೊಳ್ಮಂತ್ರಜಾತಮಂ", 
        "17340": "ಮರಲ್ದುಂ", 
        "17341": "ಮತ್ತಂ, ಪಡುವ ತೊಡಗಿ ಮೂಡಲತನಕಲಿ", 
        "17342": "ಮತ್ತಂ", 
        "17343": "ಬಳಿಕ್ಕಂ, ಷಡಂಗಮಂತ್ರನ್ಯಾಸಮಂ ಪೇಳ್ವೆನೆಂತೆನೆ-", 
        "17344": "ಮತ್ತಮಾ, ಮಹಾಲಿಂಗದ ಶಿವಷಟ್ಸಾ ್ಥನದಲ್ಲಿ-", 
        "17345": "ಬಳಿಕ್ಕಮಾ, ಲಿಂಗದಧಃಕಂಜಮೆ ಸ್ವಾಧಿಷ್ಠಾನಚಕ್ರವೆನಿಸುಗು-", 
        "17346": "ಮರಲ್ದುಮಾ, ಲಿಂಗದ ಕಂಠ, ಸಂಜ್ಞಿತವಾದ ವೃತ್ತಸ್ಥಾನಮೆ", 
        "17347": "ಬಳಿಕ್ಕಮೂಧ್ರ್ವಾಬ್ಜವೆಂಬ ಪೆಸರ ಹೃದಯಸ್ಥಾನದೆ", 
        "17348": "ಮತ್ತೆಯುಮೂಧ್ರ್ವಪಟ್ಟಿಕೆಯೆಂಬ ಕಂಠವೆ ವಿಶುದ್ಧಿಚಕ್ರಮೆನಿಸುಗು-", 
        "17349": "ಬಳಿಕ್ಕಮಾಜ್ಯಪ್ರಧಾರಿಕೆಯೆ ಅಜ್ಞಾಚಕ್ರಮೆನಿಕು-", 
        "17350": "ಬಳಿಕ್ಕಮೀಯೂಧ್ರ್ವದ ಶಿವ ಷಟ್ಚಕ್ರಕೋಷ್ಠದಳ ನ್ಯಸ್ತವಾದೇಕ", 
        "17351": "ಮತ್ತಮೀ ಷೋಡಶ ಸ್ವರಮೂರ್ತಿ ವಿವರಮೆಂ[ತೆ]ನೆ-", 
        "17352": "ಮತ್ತಮಾ ಮಹಾಲಿಂಗದಧಃಕಂಜಾಖ್ಯವಾದನಾಹತಚಕ್ರದ", 
        "17353": "ಬಳಿಕ್ಕಮಾ ಮಹಾಲಿಂಗದ ಕಂಠಸಂಜ್ಞಿತವಾದ", 
        "17354": "ಮರಲ್ದುಮಾ ಮಹಾಲಿಂಗದೂಧ್ರ್ವಾಬ್ಜ ಸಂಜ್ಞೆಯನುಳ್ಳ", 
        "17355": "ಮತ್ತೆಯುಮೀ ಮಹಾಲಿಂಗದೂಧ್ರ್ವಪಟ್ಟಿಕಾಸಂಜ್ಞೆ", 
        "17356": "ಇಂತು ಷೋಡಶರುದ್ರವಾಚ್ಯವಾದ ಹದಿನಾರು ಸ್ವರಾಕ್ಷರಂಗಳುಂ", 
        "17357": "ಮತ್ತಂ ಶಿವಷಟ್ಚಕ್ರನಿರೂಪಣಾನಂತರದಲ್ಲಿ ಶಕ್ತಿ ಷಟ್ಚಕ್ರಗಳೆಂತನೆ-", 
        "17358": "ಮತ್ತೆಯುಂ", 
        "17359": "ಬಳಿಕ್ಕಂ", 
        "17360": "ಮರಲ್ದುಂ", 
        "17361": "ಮತ್ತೆಯುಂ", 
        "17362": "ಇಂತೀ ಮಹಾಲಿಂಗದ ಪೂರ್ವೋಕ್ತ ಪಟ್ಚ ್ರಕ್ರಂಗಳೆಲ್ಲ", 
        "17363": "ಈ ತೆರದಿಂದೀ ಮಹಾಲಿಂಗದ  ಪೂರ್ವೋಕ್ತವಾದಧಶ್ಚಕ್ರಂಗಳೆಲ್ಲ", 
        "17364": "ಮತ್ತಂ, ಪಿಂದಣ  ಚಕ್ರೋದ್ಧರಣಮೆ ಈ ಲಿಂಗೋದ್ಧರಣ", 
        "17365": "ಇಂತೀ ಮಹಾಲಿಂಗದ ಶಿವಶಕ್ತ್ಯಾತ್ಮಕ ಚಕ್ರನ್ಯಾಸ", 
        "17366": "ಬಳಿಕ್ಕಮೀ ಸೂಕ್ಷ ್ಮ ಪಂಚಾಕ್ಷರ ನ್ಯಾಸಾನಂತರದಲ್ಲಿ", 
        "17367": "ಮರಲ್ದು, ಷಡಕ್ಷರನ್ಯಾಸಮಂ ಪೇಳ್ವೆನೆಂತನೆ", 
        "17368": "ಮತ್ತೆಯಂ, ಸ್ಥೂಲ ಪಂಚಾಕ್ಷರ ಮಂತ್ರಕ್ಕೆ ಅಕ್ಕರಮದೋಂಕಾರ", 
        "17369": "ಬಳಿಕ್ಕೆಯುಂ, ವಾಂತವೆ ಸೂಕ್ಷ ್ಮ ರುದ್ರಸಂಜ್ಞಿತವಾದಿಕಾರದೊಳ್ಬೆರೆಯೇ", 
        "17370": "ಮತ್ತಂ, ಷಡಧ್ವನ್ಯಾಸಮೆಂತೆನೆ-", 
        "17371": "ಇಂತು, ಕಾಮಿಕಾದಿ ವಾತೂಲಾಂತವಾಗಿಪ್ಪತ್ತೆಂಟು", 
        "17372": "ಮತ್ತಮೀ ಮಂತ್ರಜಾತಮೆಂತೆನೆ-", 
        "17373": "ಮತ್ತಮೀ, ಪತ್ತೊಂಬತ್ತಕ್ಕೆ ತರದಿಂದೊಂದಕ್ಕೆ", 
        "17374": "ಮತ್ತಂ ಮಂತ್ರರೂಪಮೆಂತೆನೆ,", 
        "17375": "ಬಳಿಕ್ಕಮಾ ಮಂತ್ರಂಗಳ ಪ್ರಕೃತಿಯೆಂತೆನೆಯಾ", 
        "17376": "ಮತ್ತಂ ವಾಹನಮೆಂತನೆ-", 
        "17377": "ಬಳಿಕ್ಕಂ, ವಶಮನ್ಯಾಯದಂತೆ ಪೂರ್ಣಚಿತಿಯೆಂತೆನೆ-", 
        "17378": "ಮತ್ತಂ,", 
        "17379": "ಮತ್ತಮಾ, ಸಹಸ್ರಕಮಲದ ವಿಭಾಗೆಯಂ", 
        "17380": "ಮತ್ತಮಿಲ್ಲಿ ಕಕಾರಾದಿ ಕ್ಷಕಾರಾಂತವಾದ ಮೂವತ್ತೈದಕ್ಕರಂಗಳಲ್ಲಿ", 
        "17381": "ಮತ್ತಂ, ಭವಿಗಳ ಪ್ರಾಕೃತ ದೃಷ್ಟಿ ಪತಿತವಾದ"
    }
}