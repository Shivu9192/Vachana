        "10484": "ಖಂಡಿತವಿಲ್ಲದ ಅಖಂಡಿತ ನೀನೆ.", 
        "10485": "ಗಂಡನುಳ್ಳಮ್ಮನ ಗೌರಿಯೆಂದು ಕಂಡಡೆ", 
        "10486": "ಗಂಡ ಭಕ್ತನಾಗಿ, ಹೆಂಡತಿ ಭವಿಯಾದಡೆ", 
        "10487": "ಗಿರಿಗಳೆಲ್ಲ ಕೂಡಿ ಮೇರುಗಿರಿಗೆ ಸರಿಯಾಗಬಲ್ಲವೆ?", 
        "10488": "ಗುರು ಕರುಣಿಸಲು ಸಂಸಾರ ವಿಷಯ ಕೆಟ್ಟಿತ್ತು.", 
        "10489": "ಗುರು ನಿರೂಪವ ಮರದು", 
        "10490": "ಗುರುಭಕ್ತನಾದಲ್ಲಿ ಘಟಧರ್ಮವಳಿದು", 
        "10491": "ಗುರು ಲಿಂಗ ಒಂದೆಂಬರು.", 
        "10492": "ಘಟದೊಳಗೆ ತೋರುವ ಸೂರ್ಯನಂತೆ", 
        "10493": "ಘಟವನೊಡದು ಬಯಲ ನೋಡಲದೇಕೆ?", 
        "10494": "ಚಿನ್ನದೊಳಗಣ ಬಣ್ಣವ ಆ ಚಿನ್ನ ತನ್ನ ತಾನರಿವುದೆ?", 
        "10495": "ಜಂಬೂದ್ವೀಪವನೆಲ್ಲ ತಿರಿಗಿದಡೇನು?", 
        "10496": "ಜಗದೊಳಗಣ ಮನುಜರು ಸೋಗೆಗನಂತಿಪ್ಪರು.", 
        "10497": "ಜ್ವರ ಪೀಡಿಸಿದ ಮನುಜರಿಗೆ ನೊರೆವಾಲು ಸೊಗಸುವದೆ?", 
        "10498": "ಜ್ವರ ಬಡಿದ ಬಾಯಿಗೆ ನೊರೆವಾಲು ಉರಸುವದೆ?", 
        "10499": "ತನು ತನ್ನ ದೆಸೆಯಲೇಸು ದಿನವಿರ್ದಡೇನು?", 
        "10500": "ತನ್ನೊಳಗಣ ಅರಿವು ತನ್ನಲ್ಲಿಯೇ ತೋರಿದಲ್ಲದೆ", 
        "10501": "ತಲೆಯಿಲ್ಲದ ಗುರು, ಕಾಲಿಲ್ಲದೆ ನಿಂದ ನೋಡ!", 
        "10502": "ತಾಯ ಗರ್ಭದ ಶಿಶು ತಾಯ ಕುರುಹನರಿಯದು.", 
        "10503": "ತೀವಿ ಕುಳ್ಳಿರ್ದ ಸಭೆ ಈಯಬಲ್ಲುದೆ ದಾನವ?", 
        "10504": "ತೊಡೆಯಲ್ಲಿ ಮುದ್ರೆಯನೊತ್ತಿದಡೇನು?", 
        "10505": "ದರುಶನ ತಪ್ಪುಕ ಪ್ರಥಮ ಪಾತಕ.", 
        "10506": "ದಾಸನ ಕುಲವಾದಾತ ಈಶಂಗಲ್ಲದೆ ಶರಣೆನ್ನ.", 
        "10507": "ದಾಸಿ ವೇಶಿ ಮದ್ದು ಮಾಂಸ ಸುರೆ ಭಂಗಿ ಹೊಗೆ", 
        "10508": "ದೇಹಗೊಂಡು ಹುಟ್ಟಿದವರು", 
        "10509": "ಧರಣಿಯ ಪಸರಿಸಿ, ಉರಗನನಡಿಯಿಟ್ಟು", 
        "10510": "ಧರ್ಮವನೆತ್ತುವವರ ಮಹಾಧರ್ಮಿಗಳೆಂದೆಂಬಿರಿ.", 
        "10511": "ನಂಬಿ ನಚ್ಚಿದೆನೆಂದು ಮನವನಿಂಬುಗೊಡದಿರು.", 
        "10512": "ನಂಬಿದ ಚೆನ್ನನ ಅಂಬಲಿಯನುಂಡ.", 
        "10513": "ನಡೆ ನೋಟ ಸೊಲ್ಲೆಡೆಯಲ್ಲಿ ಒಂದು ಕಿಚ್ಚು.", 
        "10514": "ನಾನೊಂದು ಸುರಗಿಯನೇನೆಂದು ಹಿಡಿವೆನು?", 
        "10515": "ನಿಜವುಂಡ ನಿರ್ಮಲದಲೊದಗಿದ ಜ್ಯೋತಿಯನು", 
        "10516": "ನಿಡಿದೊಂದು ಕೋಲುವನು ಕಡಿದು ಎರಡ ಮಾಡಿ", 
        "10517": "ನಿಷ್ಠೆಯುಳ್ಳ ಭಕ್ತ ನಟ್ಟಡವಿಯಲ್ಲಿದ್ದಡೇನು?", 
        "10518": "ನೀನೀಶನೀಯದೆ ಮಾನಿಸನೀವನೆ?", 
        "10519": "ನೆರೆದರೆ ಗಣಂಗಳು! ಹರದಡೆ ಕಂಚುಗಾರರು!", 
        "10520": "ನೆರೆ ನಂಬಿ-ಕರೆದಡೆ ನರಿ ಕುದುರೆಯಾಗಿ ಹರಿವೆ?", 
        "10521": "ಪರವಧುವ ನೆರೆಯದೆ; ಪರಧನವ ತುಡುಕದೆ.", 
        "10522": "ಪ್ರಾಣನ ಕಳೆ ಪ್ರಕೃತಿಯಲ್ಲಿ ಅಡಗಿ", 
        "10523": "ಬಂದುದನರಿದು ಬಳಸುವಳು", 
        "10524": "ಬಯಲ ಬಣ್ಣವ ಮಾಡಿ;", 
        "10525": "ಬರು ಸಠಗನ ಭಕ್ತಿ ದಿಟವೆಂದು ನಚ್ಚಲು ಬೇಡ.", 
        "10526": "ಬಿಂದುವಿನೊಳಗೆ ನಾದ ಹೊಂದಿರ್ದ ಭೇದವನು", 
        "10527": "ಬೆಕ್ಕು ನಾಯಿ ಶಿವಭಕ್ತರೊಕ್ಕು ಮಿಕ್ಕುದ ಕೊಂಡಡೆ", 
        "10528": "ಭಂಜಿಸಿಹೆನೆಂದೆಂಬೆ ಹಲಬರು", 
        "10529": "ಭಕ್ತನ ಮಠವೆಂದು ಭಕ್ತಹೋದಡೆ", 
        "10530": "ಭಕ್ತಿಯ ಬಲ್ಲವರಿಗೆ ಸತ್ಯ ಸದಾಚಾರವ ಹೇಳಿದಡೆ", 
        "10531": "ಭಕ್ತಿಯ ಬಲ್ಲವರು ನಚ್ಚಿ ಮೆಚ್ಚಿ ಮನವನೊಚ್ಚತಗೊಡುವರು.", 
        "10532": "ಭವಿಯ ಕಳೆದು ಭಕ್ತನಾದ ಬಳಿಕ", 
        "10533": "ಭವಿಯ ಮನೆಯ ಅನ್ನಸವಿಯಾಯಿತ್ತೆಂಬ ಭಕ್ತಂಗೆ ಭವಿಪಾಕ ನರಕ.", 
        "10534": "ಮಂಡೆಯ ಬೋಳಿಸಿಕೊಂಡು", 
        "10535": "ಮಡದಿಯ ಪ್ರಾಣಕ್ಕೆ ಮೊಲೆ ಮುಡಿ ಇದ್ದಿತ್ತೆ?", 
        "10536": "ಮಣಿಗಣ ಸೂತ್ರದಂತೆ ತ್ರಿಣೆಯ ನೀನಿಪ್ಪೆಯಯ್ಯ.", 
        "10537": "ಮಣಿವಡೆ ಶಿವಭಕ್ತ ಮಣಿಯ ಕಟ್ಟಲೆ ಬೇಕು.", 
        "10538": "ಮನಮುಟ್ಟಿ ನೆನವಲ್ಲಿ ತನು ನಿಮ್ಮದಾಯಿತ್ತು.", 
        "10539": "ಮನೆಯಲ್ಲಿ ಅಟ್ಟೆನೆಂದಡೆ ಹೊಟ್ಟೆ ತುಂಬಿದುದುಂಟೆ?", 
        "10540": "ಮಹಾಘನವಪ್ಪ ಬೋನವನು", 
        "10541": "ಮಾಡಿದ ಕರ್ಮವನೂಡಯ್ಯ ಎನ್ನ ಮನದಣಿವನ್ನಕ್ಕ.", 
        "10542": "ಮರನೊಳಗಣ ಮಂದಾಗ್ನಿಯ ಉರಿಯದಂತಿರಿಸಿದೆ", 
        "10543": "ಮಾಡಿದಲ್ಲದೆ ಮನೆಯೊಳಗಾಗದು ಬಯಲು", 
        "10544": "ಮಾರಿಯ ಪೂಜಿಸಿ, ಮಸಣಕ್ಕೆ ಹೋಗಿ", 
        "10545": "ಮುಂಡೆಗೆ ಮುಡಿಕಟ್ಟೇಕೊ?", 
        "10546": "ಮೆಳೆಯ ಮೇಲಣ ಕಲ್ಲು ಜಗದೆರೆಯನಾಗಬಲ್ಲುದೆ?", 
        "10547": "ಮೊಲೆ ಮುಡಿ ಬಂದಡೆ ಹೆಣ್ಣೆಂಬರು.", 
        "10548": "ವಿಷಯದ ಪಿತ್ತ ತಲೆಗೇರಿದಲ್ಲಿ", 
        "10549": "ವೇಶಿಯ ಎಂಜಲ ತಿಂದು", 
        "10550": "ವೇಷದ ಕೂಡೆ ವಾಸಿಗೆ ಹೋರುವವನು ಈಶ್ವರದ್ರೋಹಿ.", 
        "10551": "ವೇಷವನೂ ವೇಶಿಯನೂ ಸರಿಯೆಂಬೆ.", 
        "10552": "ವೇಷವ ಹಲ್ಲಣಿಸುವ ಹಿರಿಯಣ್ಣಂಗೇನು?", 
        "10553": "ವೇಷದ ಹೊತ್ತ ಹಿರಿಯರು ಈಶ್ವರಧ್ಯಾನದಲ್ಲಿರಬೇಕು.", 
        "10554": "ವೇಷವ ಹೊತ್ತು ದೋಷದಲ್ಲಿ ನಡೆದಡೆ", 
        "10555": "ವೇಷಕ್ಕೆ ಅಂಜುವೆ ದೋಷಕ್ಕೆ ಹೇಸುವೆ!", 
        "10556": "ಶರಧಿಯ ಮೇಲೆ ಧರೆಯ ಕರಗದಂತಿರಿಸಿದೆ!", 
        "10557": "ಶಿವಪೂಜೆಯೆತ್ತ, ವಿಷಯದ ಸವಿಯೆತ್ತ;", 
        "10558": "ಶಿವಭಕ್ತನು ಹೋಗಿ ತನ್ನೊಡಲಿಚ್ಛೆಗೆ ಭವಿಯ", 
        "10559": "ಶಿವಭಕ್ತರು ತಮ್ಮ ನಿಜ ಕೈಲಾಸಕ್ಕೆ ಹೋದಡೆ", 
        "10560": "ಶ್ರೀ ಗುರು ಲಿಂಗ ಜಂಗಮಕ್ಕೆ", 
        "10561": "ಸಣ್ಣ ನನೆಯೊಳಗಣ ಪರಿಮಳವ", 
        "10562": "ಸತ್ಯದ ನುಡಿ ತೀರ್ಥ, ಭಕ್ತಿಯ ನಡೆ ತೀರ್ಥ,", 
        "10563": "ಸತಿಪತಿಗಳೊಂದಾದ ಭಕ್ತಿ ಹಿತವಾಗಿಪ್ಪುದು ಶಿವಂಗೆ.", 
        "10564": "ಸತಿಯರ ಸಂಗವನು ಅತಿಶಯ ಗ್ರಾಸವನು", 
        "10565": "ಸರ್ಪನೂ ಕೂರ್ಮನೂ ದಿಕ್ಕರಿಗೆಳೆಂಟೂ", 
        "10566": "ಸರ್ವಜ್ಞಾನಿಗಳಾಗಿ ಅಚ್ಚುಗ ಬಡುವಡೆ,", 
        "10567": "ಸ್ವಸ್ತಿಕಾಸನದಲ್ಲಿದ್ದು ಅತ್ತಿತ್ತ ಚಲಿಸದೆ", 
        "10568": "ಸಾಗರದೊಳಗಣ ಕಿಚ್ಚಿನ ಸಾಕಾರದಂತೆ,", 
        "10569": "ಸಾಸಿವೆಯಷ್ಟು ಭಕ್ತಿಯಳ್ಳನ್ನಕ್ಕ", 
        "10570": "ಸೊಡರು ಕೆಟ್ಟಡೆ ದೃಷ್ಟಿಯಡಗಿಪ್ಪ ಭೇದವನು", 
        "10571": "ಹಂದಿ ಶ್ರೀಗಂಧವನೆಂದೂ ಒಲ್ಲದು.", 
        "10572": "ಹಂದಿ ಶ್ರೀಗಂಧವ ಹೂಸಿದಡೇನು?", 
        "10573": "ಹರ ತನ್ನ ಭಕ್ತರ ತಿರಿವಂತೆ ಮಾಡುವ", 
        "10574": "ಹರಿದ ಗೋಣಿಯಲ್ಲೊಬ್ಬ ಕಳವೆಯ ತುಂಬಿದ.", 
        "10575": "ಹಸ್ತಾಬ್ಜ ಮಥನದಿಂದೊತ್ತಿ ಭಸ್ಮವ ಮಾಡಿ", 
        "10576": "ಹಸಿವಿನಲ್ಲಿ ವಿಷಯದಲ್ಲಿ ಭವಿಯ ಬೆರಸಿದೆನಾದಡೆ", 
        "10577": "ಹಸಿವಿಲ್ಲದ ಬೊಂಬೆಗೆ ತೃಷೆಯಿಲ್ಲದ ನೀರೆರೆದು", 
        "10578": "ಹಸಿವೆಂಬ ಹೆಬ್ಬಾವು ಬಸುರ ಬಂದು ಹಿಡಿದಡೆ", 
        "10579": "ಹಾಸೇಕೆ ಹೊರಸೇಕೆ? ಲೇಸೇಕೆ ಹೊಲ್ಲೆಹವೇಕೆ?", 
        "10580": "ಹಿರಿದಪ್ಪ (ಆ) ಹಾರವ ಕೊಂಡು", 
        "10581": "ಹೂವಿನೊಳಗಣ ಕಂಪ ಹೊರಸೊಸಿ ಸುಳಿವ ಅನಿಲನಂತೆ", 
        "10582": "ಹೆಂಬೇಡಿಗೆ ಹಿಡಿಯಬಹುದೆ ಹೇಳಿಗೆಯೊಳಗಣ ಹಾವ?", 
        "10583": "ಹೆಣ್ಣ ಕಂಡು ಹೆಚ್ಚಿ ಹೆಕ್ಕಳಬಡುವಂತೆ", 
        "10584": "ಹೇಳಬಲ್ಲಡೆ ಬೋಳು ಕೇಳಬಲ್ಲಡೆ ಬೋಳು", 
        "10585": "ಹೇಳಿತ್ತ ಕೇಳುವನ ಕೀಳಿನೊಳಗಿರೆ ಲೇಸು!", 
        "10586": "ಹೊಲಬನರಿಯದ ಗುರು", 
        "10587": "ಹೊಲೆಯರ ಬಾವಿಯಲೊಂದು ಎಲುನಟ್ಟಿದ್ದಡೆ"
    }
}