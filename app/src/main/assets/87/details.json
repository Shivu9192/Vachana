    "vachana": {
        "7085": "ಅಂಗಲಿಂಗ ಸಹವಾಗಿ, ಆತ್ಮನರಿವು ಸಹವಾಗಿ,", 
        "7086": "ಅಕ್ಕಿ ಬೇಳೆ ಬೆಲ್ಲ ಉಪ್ಪು ಮೆಣಸು ಅಡಕೆ ಫಲ ರಸ ದ್ರವ್ಯ ಮುಂತಾದ", 
        "7087": "ಅಜ ಎಮ್ಮೆ ಹಾಲಬಿಟ್ಟಲ್ಲಿ ಹೇರಂಡವ ಬೆಳೆಯಲಿಲ್ಲ", 
        "7088": "ಅಡಗು ಸುರೆ ಕಟಕ ಪಾರದ್ವಾರ ಪರಪಾಕ ಮುಂತಾದ", 
        "7089": "ಅನಲನ ತಾಹಲ್ಲಿ, ಅನಿಲನ ಗಂಧ ಒಡಗೂಡಿ ಸೋಂಕುವಲ್ಲಿ,", 
        "7090": "ಅನ್ಯದೈವ ಭವಿನಾಸ್ತಿಯಾದಲ್ಲಿ,", 
        "7091": "ಅನ್ಯರು ಮಾಡಿದುದ ಮುಟ್ಟದೆ ತನ್ನ ತಾ ಮಾಡಿಕೊಂಡು ನಡೆವುದು", 
        "7092": "ಅನ್ಯಶಬ್ದಕ್ಕೆ ಜಿಹ್ವಾಬಂಧನ, ದುರ್ಗಂಧಕ್ಕೆ ನಾಸಿಕಬಂಧನ,", 
        "7093": "ಅರಸಿಗೆ ಆಚಾರ ಅನುಸರಣೆಯಾಯಿತ್ತೆಂದು,", 
        "7094": "ಅರುವತ್ತುನಾಲ್ಕು ಶೀಲ, ಐವತ್ತಾರು ನೇಮ, ಮೂವತ್ತೆರಡು ಕೃತ್ಯ", 
        "7095": "ಅಸಿ ಮಸಿ ಕೃಷಿ ವಾಣಿಜ್ಯ ಮುಂತಾದ ಕಾಯಕವ ಮಾಡಿ,", 
        "7096": "ಆಚಾರ ಅನುಸರಣೆಯಾದಲ್ಲಿ ಅಲ್ಲ ಅಹುದೆಂದು ಎಲ್ಲರ ಕೂಡುವಾಗ", 
        "7097": "ಆಚಾರ ತಪ್ಪಿದಲ್ಲಿ ಪ್ರಾಯಶ್ಚಿತ್ತ ಉಂಟೆಂಬ", 
        "7098": "ಆಚಾರ ತಪ್ಪಿದಲ್ಲಿ, ಶ್ರೇಷ್ಠನು ನಾನೆಂದು", 
        "7099": "ಆಚೆಯ ನೀರ ಈಚೆಯಲ್ಲಿ ತೆಗೆವುದು ಚಿಲುಮೆಯಲ್ಲ ;", 
        "7100": "ಆರಾರ ಭಾವಕ್ಕೆ ಒಳಗಾದ ವಸ್ತು,", 
        "7101": "ಆರೈದು ನಡೆವಲ್ಲಿ ತನ್ನ ಕ್ರೀಯನರಿತು,", 
        "7102": "ಆವಾವ ವ್ರತಕ್ಕೂ ಗುರು ಲಿಂಗ ಜಂಗಮನೆ ಮೂಲಮಂತ್ರ.", 
        "7103": "ಉಳ್ಳೆ ಹೇರಂಡ ಮಹಿಷಿ ಇವೆಲ್ಲವ ನಿಷೇಧವೆಂದು ಬಿಟ್ಟಲ್ಲಿ", 
        "7104": "ಎಂಬತ್ತುನಾಲ್ಕು ಲಕ್ಷ ವ್ರತದೊಳಗಾದ ಶೀಲ", 
        "7105": "ಎನ್ನ ಲಿಂಗದ ಸೀಮೆಯಲ್ಲಿದ್ದ ಗೋವತ್ಸ ಮೊದಲಾದ ಘಟಕ್ಕೆಲ್ಲಕ್ಕೂ", 
        "7106": "ಎನ್ನ ವ್ರತದ ನೇಮ ಅಡಿ ಆಕಾಶದೊಳಗಾದ ವ್ರತಸ್ಥರು ಕೇಳಿರೆ.", 
        "7107": "ಎನ್ನ ಸಮಗ್ರಾಹಕ ಶೀಲಸಂಪಾದಕರನಲ್ಲದೆ", 
        "7108": "ಎಲ್ಲವ ಮೀರಿ ಶೀಲವಂತನಾದಲ್ಲಿ ರೋಗವೆಲ್ಲಿಂದ ಬಂದಿತು ?", 
        "7109": "ಎಲ್ಲಾ ವ್ರತಕ್ಕೂ ಜಂಗಮದ ಪ್ರಸಾದವೆ ಪ್ರಾಣ.", 
        "7110": "ಎರಿಯಕೆಳಗೆ ಬಿದ್ದ ನೀರು ಪೂರ್ವದ ತಟಾಕಕ್ಕೆ ಎರಬಲ್ಲುದೆ ?", 
        "7111": "ಏಳುನೂರೆಪ್ಪತ್ತು ಅಮರಗಣಂಗಳಿಗೆಲ್ಲಕ್ಕೂ ತಮ್ಮ ತಮ್ಮ ಭಾವದಶೀಲ.", 
        "7112": "ಒಂದ ವಿಶೇಷವೆಂದು ಹಿಡಿದು,", 
        "7113": "ಒಡೆಯರ ಕಟ್ಟಳೆಯೆಂದು ಮಾಡಿಕೊಂಡು ಆಡುವ ತನಕ", 
        "7114": "ಒಡೆಯರ ಸಮಯಾಚಾರವೆಂದ ಮತ್ತೆ", 
        "7115": "ಒಡೆಯರು ಭಕ್ತರಿಗೆ ಸಲುವ ಸಹಪಙÂ್ತಯಲ್ಲಿ ಗುರುವೆಂದು, ಅರಸೆಂದು,", 
        "7116": "ಕಂಗಳ ಮುಂದೆ ಕಂಡಂತೆ ಕಾಣಬಹುದೆ", 
        "7117": "ಕಾಮಿಗೆ ವ್ರತವುಂಟೆ, ನಿಃಕಾಮಿಗಲ್ಲದೆ !", 
        "7118": "ಕಾಮುಕಗೆ ದುರ್ಜನಗೆ ಕಪಟಿಗೆ ಹೇಮಚೋರಗೆ ಕಾರುಕ ಸಮ್ಮಗಾರಗೆ", 
        "7119": "ಕಾಯಕಕೃತ್ಯ, ನೇಮಕೃತ್ಯ, ಆಚರಣೆಕೃತ್ಯ, ದಾಸೋಹಕೃತ್ಯ, ಭಾವಕೃತ್ಯ,", 
        "7120": "ಕೀಟಕ ವಿಹಂಗ ಮೊದಲಾದ ಆವ ಜೀವವು", 
        "7121": "ಕೂಪಚಿಲುಮೆ ಬಹುಜಲಂಗಳಲ್ಲಿ ಸ್ವೀಕರಿಸಿಕೊಂಬುದು ಅದೇತರ ಶೀಲ", 
        "7122": "ಕೂಪಜಲವ ಬಳಸುವನ್ನಕ್ಕ, ಲವಣಬಳಕೆ ಸಾರೋಪಿತ ದ್ರವ್ಯವನ್ನರಿಯಬೇಕು.", 
        "7123": "ಕೆಯಿಗೆ ಬೆಚ್ಚು ಬೆದರ ಕಟ್ಟುವಲ್ಲಿ", 
        "7124": "ಕೆರೆ ಪೂದೋಟ ಅರವಟ್ಟಿಗೆ ಬಾವಿ ವಿವಾಹ", 
        "7125": "ಕೊಡುವದು ಬೇಡ ಎಂದಲ್ಲಿ ಸರ್ವರ ಒಡಗೂಡಿ ಕಾಡುವುದು.", 
        "7126": "ಖಂಡಿತಕಾಯಕದ ವ್ರತಾಂಗಿಯ ಮಾಟದ ಇರವೆಂತೆಂದಡೆ", 
        "7127": "ಖಂಡಿತ ವ್ರತ ಅಖಂಡಿತ ವ್ರತ,", 
        "7128": "ಖಂಡಿತವ್ರತದ ನಿರಂಗನ ವ್ರತವೆಂತುಟೆಂದಡೆ", 
        "7129": "ಖ್ಯಾತಿಯ ಲಾಭಕ್ಕೆ ವ್ರತವ ಮಾಡಿಕೊಂಡವನ ನೇಮವ ನೋಡಾ !", 
        "7130": "ಗುರುವಾಗಿಬಂದು ತನುವ ಕೊರೆದಡೂ ಕೇಳೆ,", 
        "7131": "ಗುರುವಾದಡೂ ಆಚಾರಭ್ರಷ್ಟನಾದಡೆ ಅನುಸರಿಸಲಾಗದು.", 
        "7132": "ಚಂದ್ರ ಸೂರ್ಯಾದಿಗಳು ಭವಿಯೆಂದಲ್ಲಿ ಅವೆರಡರ ಸಂಸಂಧಿಯ ಜಗದಲ್ಲಿ", 
        "7133": "ಚತುಷ್ಪಾದಿ ಮುಂತಾದ, ನರ ವಿಹಂಗ ಕೀಟಕ ಮುಂತಾದ ಜೀವಂಗಳೆಲ್ಲವು", 
        "7134": "ಚಾಟಿ ಗುಂಡು ಬಂಧನ ಕಲಕೇತ ಯಾಚಕ ಪಗುಡಿ", 
        "7135": "ಚಿನ್ನ ಒಡೆದಡೆ ಕರಗಿದಡೆ ರೂಪಪ್ಪುದಲ್ಲದೆ,", 
        "7136": "ಜಗಮೆಚ್ಚಬೇಕೆಂಬ ಶೀಲವ ನಾನರಿಯೆ,", 
        "7137": "ಜಾಗ್ರದಲ್ಲಿ ಮಾಡುವ ಹಾಹೆಯ ಗುಣಂಗಳು ಸ್ವಪ್ನದಲ್ಲಿ ತೋರುವಂತೆ", 
        "7138": "ಜಾಗ್ರದಲ್ಲಿ ಹೋಹಡೆ,", 
        "7139": "ತಂದೆಯ ಒಂದಾಗಿ ಬಂದ ಸಹೋದರ ಗಂಡೆಲ್ಲ ತಂದೆಯಾದ ಕಾರಣ,", 
        "7140": "ತನ್ನ ತಾ ಬಂದುದ ಸೋಂಕಿಲ್ಲದುದ ವಿಚಾರಿಸಿ,", 
        "7141": "ತನ್ನ ಸ್ವಕಾರ್ಯದಿಂದ ಮಾಡುವ", 
        "7142": "ತನ್ನಾಚಾರಕ್ಕೆ ಬಂದವರು ತನ್ನವರೆಂದು ಭಾವಿಸಬೇಕಲ್ಲದೆ,", 
        "7143": "ತನುವಿಂಗೆ ಕ್ರೀ, ಆತ್ಮಂಗೆ ವ್ರತ.", 
        "7144": "ತನುವ್ರತ ಮನವ್ರತ ಮಹಾಜ್ಞಾನವ್ರತ -", 
        "7145": "ತನುಶೀಲವಂತರುಂಟು, ಧನಶೀಲವಂತರುಂಟು,", 
        "7146": "ತಮ್ಮ ಆಯತದ ಉಪ್ಪೆಂದು ಬಳಸುವನ್ನಕ್ಕ", 
        "7147": "ತಾ ಮಾಡುವ ಕೃಷಿಯ ಮಾಡುವನ್ನ ಬರ ಮಾಡಿ,", 
        "7148": "ತಾ ಮುಳುಗಿದ ಮತ್ತೆ ಸಮುದ್ರದ ಪ್ರಮಾಣ ತನಗೇನು ?", 
        "7149": "ತಾ ವ್ರತಿಯಾಗಿದ್ದಲ್ಲಿ ತನ್ನ ಸೀಮೆ ಒಳಗಾಗಿ,", 
        "7150": "ತಾ ವ್ರತಿಯಾಗಿ ಮಕ್ಕಳೆಂದು ಮಾಡದಿರ್ದಡೆ ಆ ವ್ರತಕ್ಕೆ ತಾನೆ ಹೊರಗು.", 
        "7151": "ತುಷವ ಕಳೆದ ತಂಡುಲವನಡಬೇಕು.", 
        "7152": "ತೋಡಿದ ಬಾವಿ, ಬಳಸುವ ಭಾಜನಕ್ಕೆ ಮರೆಯಲ್ಲದೆ,", 
        "7153": "ದಾಸಿ ವೇಶಿ ಪರನಾರಿಸಹೋದರನಾಗಿರಬೇಕು.", 
        "7154": "ದುಷ್ಟರಿಗಂಜಿ ಕಟ್ಟಿಕೊಳ್ಳಬಹುದೆ ಕಡ್ಡಾಯದ ವ್ರತವ !", 
        "7155": "ದೇವರಿಗೆಂದು ಬಂದ ದ್ರವ್ಯ ಮತ್ತೆ ಸಂದೇಹವಾಯಿತ್ತೆಂದು", 
        "7156": "ದೃಷ್ಟದಲ್ಲಿ ಆಗಾಗ ಅರ್ಪಿಸಿಕೊಂಡಿಹೆನೆಂಬುದು ನಿನ್ನ ಚಿತ್ತದ ಕಲೆಯೊ !", 
        "7157": "ಧನಶೀಲ ಮನಶೀಲ ತನುಶೀಲ ಸರ್ವಮಯ ದೃಕ್ಕಿಂಗೆ ಕಾಂಬುದೆಲ್ಲವು ಶೀಲ.", 
        "7158": "ಧೂಳುಪಾವಡ ಕಂಠಪಾವಡ ಸರ್ವಾಂಗಪಾವಡ", 
        "7159": "ಧೂಳುಪಾವಡವಾದಲ್ಲಿ ಆವ ನೀರನು ಹಾಯಬಹುದು.", 
        "7160": "ನಚ್ಚುಮಚ್ಚಿನ ವ್ರತ, ನಿಷ್ಠೆಹೀನನ ಪೂಜೆ, ಭಕ್ತಿ ಇಲ್ಲದವನ ಮಾಟಕೂಟ", 
        "7161": "ನಾನಾ ವ್ರತಂಗಳ ಪಿಡಿವುದೆಲ್ಲವು ಮನದ ಶಂಕೆ.", 
        "7162": "ನಾನಾ ವ್ರತದ ಭಾವಂಗಳುಂಟು.", 
        "7163": "ನಾನಾ ವ್ರತ ನೇಮ ಭೇದಂಗಳಲ್ಲಿ ಅರುವತ್ತುನಾಲ್ಕು ವ್ರತ, ಐವತ್ತಾರು ಶೀಲ,", 
        "7164": "ನಿತ್ಯ ಚಿಲುಮೆಯ ಕೃತ್ಯಂಗಳಾದಲ್ಲಿ", 
        "7165": "ನಿತ್ಯ ಚಿಲುಮೆಯ ಕೃತ್ಯವೆಂದು ಮಾಡುವಲ್ಲಿ,", 
        "7166": "ನಿತ್ಯ ಚಿಲುಮೆಯ ನೇಮಕ್ಕೆ ಶಿವಭಕ್ತರ ಸೀಮೆಯಲ್ಲಿಯಲ್ಲದೆ ಇರಲಿಲ್ಲ.", 
        "7167": "ನಿರ್ಧನಿಕರು ನಡೆನುಡಿಗಳಿಂದ ತಾಕುಸೋಂಕು ಬರಲಾಗಿ", 
        "7168": "ನೆರಹಿ ಮಾಡುವ ಮಾಟ ಅಘಹರನ ಮುಟ್ಟದು.", 
        "7169": "ನೇಮವ ಮಾಡಿಕೊಂಡು ತನ್ನಯ ಸಂಸಾರದ ಕಾಮ್ಯಾರ್ಥಕ್ಕಾಗಿ", 
        "7170": "ಪಂಚಾಚಾರವ ಧರಿಸಿದ ಶರೀರಕ್ಕೆ", 
        "7171": "ಪಂಚಾಚಾರ ಶುದ್ಧವಾದ ಸದ್ಭಕ್ತಂಗೆ, ಪಾದತೀರ್ಥಪ್ರಸಾದದಲ್ಲಿ ನಿರತಂಗೆ,", 
        "7172": "ಪಂಚಾಚಾರಶುದ್ಧವಾಗಿ ಇರಬೇಕೆಂಬರಲ್ಲದೆ,", 
        "7173": "ಪರಪಾಕವ ಬಿಟ್ಟಿಹುದೊಂದೆ ವ್ರತ.", 
        "7174": "ಪಾಕಲವಣವ ಬಿಟ್ಟಲ್ಲಿ ಪರವಧುವ ಅಪೇಕ್ಷಿಸಿ ಬೇಡಲಾಗದು.", 
        "7175": "ಬಂದುದ ಸಾಕೆನ್ನದೆ, ಬಾರದುದ ತಾ ಎನ್ನದೆ,", 
        "7176": "ಬತ್ತಲೆ ಇದ್ದವರೆಲ್ಲ ಕತ್ತೆಯ ಮಕ್ಕಳು.", 
        "7177": "ಬಸವಣ್ಣ, ಚೆನ್ನಬಸವಣ್ಣ, ಪ್ರಭು, ವೀರಮಡಿವಳ, ನಿಜಗುಣಶಿವಯೋಗಿ,", 
        "7178": "ಬಹುಜಲವಂ ಬಿಟ್ಟಲ್ಲಿ ಅದು ಶೀಲವಲ್ಲ.", 
        "7179": "ಬೆಳ್ಳೆ, ಭಂಗಿ, ನುಗ್ಗಿ, ಉಳ್ಳೆ ಮೊದಲಾದವನೆಲ್ಲವ ಬಿಡಬೇಕು.", 
        "7180": "ಭಕ್ತಂಗೆ ಬಯಕೆ ಉಂಟೆ ? ನಿತ್ಯಂಗೆ ಸಾವುಂಟೆ ?", 
        "7181": "ಭಕ್ತರ ಮನೆಗೆ ಸತ್ಯಶರಣರು ಬಂದಲ್ಲಿಯೆ", 
        "7182": "ಭಕ್ತರ ಮನೆಯಲ್ಲಿ ಭಕ್ತರು ಬಂದು ಕಳವು ಪಾರದ್ವಾರವ ಮಾಡಿದರೆಂದು", 
        "7183": "ಭರಿತಾರ್ಪಣವೆಂದು ಸ್ಥಲವನಂಗೀಕರಿಸಿದಲ್ಲಿ", 
        "7184": "ಭವಿ ನಿರೀಕ್ಷಣೆಯಾದ ದ್ರವ್ಯಂಗಳ ಮುಟ್ಟೆನೆಂಬಲ್ಲಿ", 
        "7185": "ಭವಿಪಾಕವ ಮುಟ್ಟದೆ ಸಮಸ್ತ ಮತದಿಂದ", 
        "7186": "ಭವಿಯಲಾದ ಪಾಕವ ತಂದು ಮನೆಯಲ್ಲಿರಿಸಿಕೊಂಡು ಭುಂಜಿಸುತ್ತ", 
        "7187": "ಭಾಜನಕ್ಕೆ ಸರ್ವಾಂಗ ಮುಸುಕಿಟ್ಟಲ್ಲಿ ಸರ್ವವ್ಯಾಪಾರ ನಿರಸನವಾಗಿರಬೇಕು ;", 
        "7188": "ಭಾಜನಕ್ಕೆ ಸರ್ವಾಂಗವ ಬಾಸಣಿಸುವಾಗ", 
        "7189": "ಭಾಜನದ ಕಂಠಕ್ಕೆ ಪಾವಡವ ಬಾಸಣಿಸಲಾಗಿ ವ್ರತಕ್ಕೆ ಬೀಜ ಮೊದಲಾಯಿತ್ತು.", 
        "7190": "ಮಣ್ಣಮಡಕೆಯ ಮುಸುಕಿಟ್ಟು ಗನ್ನದಲ್ಲಿ ನೀರು ತುಂಬಿ,", 
        "7191": "ಮನಮುಟ್ಟದ ವ್ರತ, ತನುಮುಟ್ಟದ ಸುಖ,", 
        "7192": "ಮರ್ಕಟ, ಕುಕ್ಕುಟ, ಮಾರ್ಜಾಲ, ಶುನಕ, ಶೂಕರ, ವಿಹಂಗ", 
        "7193": "ಮತ್ರ್ಯಕ್ಕೆ ಬಂದ ಭಕ್ತಕಾಯರು ತಮ್ಮ ತಮ್ಮ ಗೊತ್ತಿನಲ್ಲಿಯೆ ಮುಕ್ತರು.", 
        "7194": "ಮಹಾವ್ರತಸ್ಥರು ವ್ರತಿಗಳ ಮನೆಗೆ ಹೋದಲ್ಲಿ,", 
        "7195": "ಮಾಡಿ ನೀಡುವ ಭಕ್ತನನರಸಿಕೊಂಡು ಹೋಗಿ", 
        "7196": "ಮೂಷಕ ಮಾರ್ಜಾಲ ಮಕ್ಷಿಕ ಇವು ಮೊದಲಾದ ಸಕಲ ಜೀವಪ್ರಾಣಿಗಳಿಗೆ", 
        "7197": "ಮೂಷಕ, ವಿಹಂಗ, ಕುಕ್ಕುಟ,ಮಾರ್ಜಾಲ, ಕುಕ್ಕುರ, ದೌಷ್ಟ್ರ", 
        "7198": "ಲಕ್ಷವಿಲ್ಲದ ವ್ರತ, ಕಟ್ಟಳೆಯಿಲ್ಲದ ನೇಮ,", 
        "7199": "ಲವಣನಿರಶನದ ಆಯತದ ಭೇದವೆಂತೆಂದಡೆ ;", 
        "7200": "ಲವಣವ ಕೊಂಬುದೆಲ್ಲವು ಒಂದೆ ಶೀಲ. ಸರ್ವ ಸಪ್ಪೆ ಎಂಬುದು ಒಂದೆ ಶೀಲ.", 
        "7201": "ಲಿಂಗಕ್ಕೂ ತಮಗೂ ಸಹಭಾಜನ ಸಹಭೋಜನವಾಹಲ್ಲಿ", 
        "7202": "ಲಿಂಗಗೂಡಿ ಭರಿತಾರ್ಪಣವ ಅಂಗೀಕರಿಸುವಲ್ಲಿ,", 
        "7203": "ಲಿಂಗರೂಪ ನೋಡುವಲ್ಲಿ, ಗುರುಲಿಂಗಜಂಗಮವನರಿವಲ್ಲಿ,", 
        "7204": "ಲಿಂಗಸೀಮೆ ಸೀಮೋಲ್ಲಂಘನವಾದಲ್ಲಿ,", 
        "7205": "ವಿಪ್ರಂಗೆ ವೇದಮಂತ್ರವ ಬಿಟ್ಟು ವಿಜಾತಿಯಲ್ಲಿ ಬೆರಸಲಿಕ್ಕೆ", 
        "7206": "ವಿಷವ ಅಂಗಕ್ಕೆ ಕೊಂಡು ವೇಧಿಸಿದಲ್ಲಿ,", 
        "7207": "ವೇಷ ಎಲ್ಲಿರದು ?", 
        "7208": "ವ್ರತ ನೇಮ ಶೀಲಮಂ ಮಾಡಿಕೊಂಡು,", 
        "7209": "ವ್ರತನೇಮಿಯಾಗಿರ್ದಲ್ಲಿ, ಹಿರಿಯರು ಭಕ್ತರು ಮಿಕ್ಕಾದ ಮನುಜರಲ್ಲಿ", 
        "7210": "ವ್ರತವನಾಶ್ರಯಿಸಿದ ಮತ್ತೆ ಹುಸಿ ನುಸುಳು ಕೊಲೆ ಕಳವು", 
        "7211": "ವ್ರತವ ಮಾಡಿಕೊಂಡಲ್ಲಿ ರತಿಗೆಡದೆ, ಕ್ಷೇತ್ರವಾಸಂಗಳ ಬೇಡದೆ,", 
        "7212": "ವ್ರತವ ಹಿಡಿವಲ್ಲಿ, ವ್ರತವ ಉಪದೇಶ ಮಾಡುವಲ್ಲಿ,", 
        "7213": "ವ್ರತವೆಂಬುದೇನು ? ಮನವಿಕಾರಿಸುವುದಕ್ಕೆ ಕಟ್ಟಿದ ಗೊತ್ತು.", 
        "7214": "ವ್ರತವೆಂಬುದೇನು ?ವಸ್ತುವ ಕಾಂಬುದಕ್ಕೆ ನಿಚ್ಚಣಿಕೆ.", 
        "7215": "ವ್ರತವೆಂಬ ಸೀಮೆಯ ವಿವರವೆಂತುಟೆಂದಡೆ", 
        "7216": "ವ್ರತಾಚಾರವ ಅವಧರಿಸಿದ ಭಕ್ತಂಗೆ", 
        "7217": "ವ್ರತಾಚಾರವೆಂದು ಹೆಸರಿಟ್ಟುಕೊಂಡಿಪ್ಪರಯ್ಯಾ !", 
        "7218": "ವ್ರತಾಚಾರವೆಂಬುದು ತನಗೊ, ತನ್ನ ಸತಿಗೊ, ಇದಿರ ಭೂತಹಿತಕೊ ?", 
        "7219": "ವ್ರತಿಗಳ ಮನೆಗೆ ವ್ರತಿಗಳು ಬಂದಲ್ಲಿ", 
        "7220": "ಸಂದೇಹವ ಹರಿದವಂಗಲ್ಲದೆ ಸಹಭೋಜನ ಭಾಜನವಿಲ್ಲ.", 
        "7221": "ಸಂದೇಹವುಳ್ಳನ್ನಕ್ಕ ವ್ರತಾಂಗಿಯಲ್ಲ.", 
        "7222": "ಸದೈವ ಆವಾವ ಗೋತ್ರದಲ್ಲಿ ಬಂದಡೂ ಎಣ್ಣೆ ನೀರಿನಂತೆ,", 
        "7223": "ಸಪ್ಪೆಯ ವ್ರತವೆಂಬುದ ನಾವರಿಯೆವು, ನೀವು ಹೇಳಿರಯ್ಯಾ.", 
        "7224": "ಸಮಶೀಲ ನೇಮ ಒಂದಾದಲ್ಲಿ ಉಡುವ ತೊಡುವ ಕೊಡುವ ಕೊಂಬ", 
        "7225": "ಸಮಶೀಲಸಂಪಾದಕರು ಬಂದಿದ್ದಲ್ಲಿ", 
        "7226": "ಸರಸಕ್ಕೆ ಸತ್ತವರುಂಟೆ ? ವಿನೋದಕ್ಕೆ ಪಾರದ್ವಾರ ಉಂಟೆ ?", 
        "7227": "ಸರ್ವ ಇಂದ್ರಿಯಗಳ ಒಂದು ಮುಖವ ಮಾಡಿ", 
        "7228": "ಸರ್ವಮಯ ಲಿಂಗಾಂಕಿತಸೀಮೆಯಾಗಬೇಕೆಂಬಲ್ಲಿ", 
        "7229": "ಸರ್ವಶೀಲದ ವ್ರತದಾಯತವೆಂತುಟೆಂದಡೆ", 
        "7230": "ಸೀಮೋಲ್ಲಂಘನವೆಂಬುದ ನಾನರಿಯೆ, ನೀವೆ ಬಲ್ಲಿರಿ.", 
        "7231": "ಸೆರೆಗೆ ಸತಿ ಸಿಕ್ಕಿದಲ್ಲಿ ಅಪಮಾನವನರಸಲಿಲ್ಲ.", 
        "7232": "ಸ್ಥಾವರಕ್ಕೆ ಲಿಂಗಮುದ್ರೆ, ಪಾಷಾಣಕ್ಕೆ ಲಿಂಗಮುದ್ರೆ,", 
        "7233": "ಹರಿ ಬ್ರಹ್ಮ ದೇವರ್ಕಳು ಮುಂತಾಗಿ ಇದ್ದವರೆಲ್ಲರು", 
        "7234": "ಹಲವುತೆರದ ಕ್ರೀಯನಾಧರಿಸಿ ನಡೆವಲ್ಲಿ", 
        "7235": "ಹಿಡಿಯೆನೆಂಬುದ ಹಿಡಿದಲ್ಲಿ ಅದು ಸುರಾಪಾನ.", 
        "7236": "ಹೆಣ್ಣು ಹೊನ್ನು ಮಣ್ಣಿಗಾಗಿ ವ್ರತವೆಂಬ ನೇಮವ ಮಾಡಿಕೊಳ್ಳಬಹುದೆ ?", 
        "7237": "ಹೊಳೆಯ ಹರುಗೋಲಲ್ಲಿಯೆ ಮೆಟ್ಟಡಿಯ ಮೆಟ್ಟಿಹೆನೆಂದು", 
        "7238": "ಹೋತಿನ ಗಡ್ಡದಂತೆ ಗಡ್ಡದ ಹಿರಿಯರ ನೋಡಾ."
    }
}