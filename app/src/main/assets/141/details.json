    "id": "141", 
    "name": "ಮನುಮುನಿ ಗುಮ್ಮಟದೇವ", 
    "vachana": {
        "11804": "ಎನಗೆ ಕರಿ ಕಂಡ ಸ್ವಪ್ನದ ಇರವು.", 
        "11805": "ಭೂತಭವಿಷ್ಯದ್ವರ್ತಮಾನಕ್ಕೆ ಮುನ್ನವೆ", 
        "11806": "ವೇದಂಗಳ ನಾಲ್ಕು ಭೇದಿಸಲರಿಯದೆ,", 
        "11807": "ತಟ್ಟು ಕುಂಚ ಕಮಂಡಲಂಗಳೆಂಬ ಲೋಚು ಮುಟ್ಟಿಯ ಹಿಡಿದು,", 
        "11808": "ತರ್ಕವ ನುಡಿವುದಕ್ಕೆ ಪರಸಮಯಿಯಲ್ಲ.", 
        "11809": "ಹುಲ್ಲೆಯ ಕಣ್ಣಿನಲ್ಲಿ ಹುಲಿ ನಿಂದಿದ್ದುದ ಕಂಡೆ.", 
        "11810": "ಬೆಕ್ಕಿನ ತಲೆಯ ಗಿಳಿ ತಿಂದಿತ್ತು.", 
        "11811": "ಕಲ್ಲು ಕವಣೆಯ ನುಂಗಿ, ಇಡುವಾತನ ಹಣೆ ತಾಗಿತ್ತು.", 
        "11812": "ಆಡು[ಹ]ಲ್ಲ ತಿಂಬಲ್ಲಿ, ಕೋಡಗ ನೋಡಿ ಬಾಲವ ತಿಂದಿತ್ತು.", 
        "11813": "ಹಾವಿನ ಹಲ್ಲಿನ ವಿಷದ ಕೊಳಪೆಯ ಮೂಲೆಯಲ್ಲಿ,", 
        "11814": "ಗುಂಡಿಯ ಹಳ್ಳವು ತುಳುಕಲಾಗಿ ಇಳಿಯಿತ್ತು ಮತ್ಸ್ಯ,", 
        "11815": "ಫಣಿಯ ಹೆಡೆಯ ಮೇಲೆ, ಒಂದು ಮಣಿಮಾಡದ ಮಂಟಪ.", 
        "11816": "ಕಲ್ಲು, ಹೆಂಟೆ ಹೋರಿಯಾಡಿ,", 
        "11817": "ಅರುಹಿರಿಯರೆಂಬವರೆಲ್ಲರು ಕೂಡಿ,", 
        "11818": "ಬಿಗಿ[ದಾ] ಜಿನಘಟಕ್ಕೆ ಗು[ಡು]ಗಿಸಲಾಗಿ ಉಡುಗುವುದೆ ನಾದ?", 
        "11819": "ಕೂಟದಿಂದ ಕೂಸು ಹುಟ್ಟುವಡೆ", 
        "11820": "ಬುದ್ಧಿಯಿಂದ ಬದುಕಿಹೆನೆಂಬುದ ನಿರ್ಧರವೆ ?", 
        "11821": "ಎನ್ನ ಮನದ ಮರವೆ ಭಿನ್ನವಾಗದು.", 
        "11822": "ಹಿಮಾಚಲದ ತಳದಲ್ಲಿ, ಮಹೀತಳದ ಮಧ್ಯದಲ್ಲಿ,", 
        "11823": "ಹುಟ್ಟಿದಳೊಬ್ಬ ಮೂದೇವಿ ಜಗವ ರಕ್ಷಿಸಿಹೆನೆಂದು.", 
        "11824": "ಕಣ್ಣು ಮೂರು, ತಲೆಯಾರು,", 
        "11825": "ಇಚ್ಫಾಶಕ್ತಿಯೆಂಬೆನೆ ಬ್ರಹ್ಮನ ಬಲೆಗೊಳಗು.", 
        "11826": "ಕಣ್ಣಿನಲ್ಲಿ ನೋಡುವಡೆ ತೊಗಲಿನವನಲ್ಲ.", 
        "11827": "ಬಾಯಲ್ಲಿ ಬಸುರು ಹುಟ್ಟಿ, ಕಿವಿ ಬೆಸನಾಯಿತ್ತು.", 
        "11828": "ಮಾಯೆಯ ಗೆದ್ದೆ[ಹೆ]ನೆಂದು, ಮತಿ ಮಂದಿರದಲ್ಲಿ", 
        "11829": "ವಾಯು ಹಸಿದು ಆಪೆÇೀಶನವ ಮಾಡುವಲ್ಲಿ", 
        "11830": "ಗಿರಿಯ ಗುಹೆಯಲ್ಲಿ ಒಂದು ಅರಿಬಿರಿದಿನ ಹುಲಿ.", 
        "11831": "ಬಯಲು ಬ್ರಹ್ಮಾಂಡಂಗಳಿಲ್ಲದಲ್ಲಿ,", 
        "11832": "ಆರೂ ಇಲ್ಲದ ಮನೆಯಲ್ಲಿ ಹಾದರಿಗ ಹೊಕ್ಕು,", 
        "11833": "ಕೈ ಗರ್ಭವಾಗಿ ನವಮಾಸ ತುಂಬಿತ್ತು.", 
        "11834": "ಕಾಯದ ಕೈಯಲ್ಲಿಪ್ಪುದು ರೂಪುವಿಡಿದ ಲಿಂಗ.", 
        "11835": "ಸುಜ್ಞಾನ ಅಜ್ಞಾನವೆಂಬ ಉಭಯದ ಭೇದ,", 
        "11836": "ಕಣ್ಣಿನೊಳಗಣ ಕಸ, ಕೈಯೊಳಗಣ [ಮೆ]ಳೆ,", 
        "11837": "ಧರೆಯ ಉದಕ ಮಾರುತನ ಸಂಗದಿಂದ,", 
        "11838": "ವಾರಿಯ ಶಿಲೆ ಕರಗುವುದಲ್ಲದೆ ಒಡೆದುದುಂಟೆ ಅಯ್ಯಾ ?", 
        "11839": "ಸ್ವಪ್ನವ ಕಂಡು ಎಚ್ಚರಿವುದು ಅರಿವೋ, ಮರವೆಯೋ ?", 
        "11840": "ಅಮ್ಮಿಕಲ್ಲಿನಲ್ಲಿ ಅಮಲವ ಅರೆದಡೆ ಹತ್ತುವುದೆ ಬಂಧ ?", 
        "11841": "ಹಾದಿಯ ತೋರಿದವರೆಲ್ಲರು", 
        "11842": "ಕಂಗಳ ಸೂತಕ ಹಿಂಗಿಯಲ್ಲದೆ ಭಕ್ತನಾಗಬಾರದು.", 
        "11843": "ನಾನೆಂಬುದೆ ಸಕಲ, ನೀನೆಂಬುದೆ ಪ್ರತಿರೂಪು.", 
        "11844": "ಶೈವಂಗೆ ಹರವರಿ, ನೇಮಸಂಗೆ ಊಧ್ರ್ವ", 
        "11845": "ತೊಗಲೊಳಗಣ ನಾದ, ಮಾಂಸದೊಳಗಣ ಕ್ಷೀರ", 
        "11846": "ಕುಲ ಜಾತಿ ವರ್ಗದ ಶಿಶುಗಳಿಗೆ ಹಲವು ಭೇದದ ಹಾಲು.", 
        "11847": "ಭಕ್ತಿ ವಿಶ್ವಾಸ ವಿರಕ್ತಿ ಜಗದ ವೈಭವದ ದೇಹಿಗಳು ಕೇಳಿರೊ.", 
        "11848": "ಅಮೃತದ ಗುಟಿಕೆಯ ಮರೆದು,", 
        "11849": "ಕಾಯಸೂತಕವಳಿದು ಜೀವದ ಭವ ಹಿಂಗಿ,", 
        "11850": "ಇಕ್ಷುದಂಡದ ತುದಿಯ ಸುಳಿಯಲ್ಲಿ ಹುಟ್ಟಿತೊಂದು ಗಿಳಿ,", 
        "11851": "ವಟವೃಕ್ಷದ ಘಟದ ಹೊಟ್ಟೆಯಲ್ಲಿ", 
        "11852": "ನಾನೊಂದು ಗಿಳಿಯ ಕಂಡೆ, ಬೆಕ್ಕಿನ ಬಾಧೆ ಘನ.", 
        "11853": "ಹಾರುವ ಹಕ್ಕಿಯ ಹಿಡಿದೆ, ಬೀಸುವ ಗಾಳಿಯ ಹಿಡಿದೆ.", 
        "11854": "ಕಾಲೆರಡರಲ್ಲಿ ನಿನ್ನಾಟವೆ ?", 
        "11855": "ಗುಹೆಯೆಂದಡೂ ಅಂಗ, ಗುಮ್ಮೆಂದಡೂ ಅಂಗ.", 
        "11856": "ಅಷ್ಟಭೂಮಿಯ ಮಧ್ಯದಲ್ಲಿ ಹುಟ್ಟಿತೊಂದು ಬೆಟ್ಟ.", 
        "11857": "ಅರಿದು ಬಲ್ಲವನಾದಡೆ ಮುರಿ ಕಾಲ,", 
        "11858": "ಉತ್ಪತ್ಯ ಗುರುವಿನಲ್ಲಿ, ಸ್ಥಿತಿ ಲಿಂಗದಲ್ಲಿ, ಲಯ ಜಂಗಮದಲ್ಲಿ.", 
        "11859": "ತತ್ ತ್ವಂ ಅಸಿಯೆಂಬ ತ್ರಿವಿಧಭೇದಂಗಳಲ್ಲಿ", 
        "11860": "ಗುಡಿಯೊಡೆಯಂಗೆ ಕಂಬವೊಂದು, ಬಿಗಿಮೊಳೆಯೆಂಟು,", 
        "11861": "ನೋಡುವ ನಯನ ತೆರಪಾದಡೇನು,", 
        "11862": "ಕೊಲುವಂಗೆ ಜೀವದ ದಯವಿಲ್ಲ.", 
        "11863": "ಅಂಬುಧಿಯಲ್ಲಿ ಅಂಬಿಗ ಮನೆಯ ಮಾಡಿ,", 
        "11864": "ಕಣ್ಣೆಂಬ ಹರುಗೋಲದಲ್ಲಿ ಮಾರ ಅಂಬಿಗನಾಗಿ,", 
        "11865": "ಬಂದವನಿವನಾರು, ಕಂಬಳಿಯ ಕಂಥೆಯ ತೊಟ್ಟು,", 
        "11866": "ಮರ್ಕಟನ ಲಂಘನ ವಿಹಂಗನ ದೃಷ್ಟಿ,", 
        "11867": "ಮಾತಿನ ಗೂಢವ ನುಡಿದಡೆ, ನೀತಿವಂತರು ಅರಿಯರು.", 
        "11868": "ಬಲ್ಲವನ ಭಾವ ಶರೀರದ ಹಂಗನರಿಯಬೇಕು.", 
        "11869": "ಶಿಲೆಯೊಳಗಣ ಭೇದದಿಂದ ಒಲವರವಾಯಿತ್ತು.", 
        "11870": "ಇದಿರು ನೋಡುವುದಕ್ಕೆ ತೊಲಗು ಸಾಯೆಂಬ ಮಾತಲ್ಲದೆ,", 
        "11871": "ಪ್ರಾಣ ತುಡುಕಿಗೆ ಬಂದಲ್ಲಿ,", 
        "11872": "ನೋಡುವಲ್ಲಿ ನೆರೆ ಶೃಂಗಾರವಲ್ಲದೆ, ಕೂಡುವಲ್ಲಿ ಉಂಟೆ ?", 
        "11873": "ಆಲೆಯ ಮನೆಯಲ್ಲಿ ಅಕ್ಕಿಯ ಹೊಯಿದು,", 
        "11874": "ಮದನನ ಚಾಪ, ಮನ್ಮಥನ ಗಂಧ, ಚದುರರ ಮಾತು,", 
        "11875": "ತಟಾಕ ಒಡೆದಡೆ ಪ್ರತಿಶಬ್ದವಲ್ಲದೆ ಗತಿಭಿನ್ನವುಂಟೆ ?", 
        "11876": "ಪದವನರಿಯದ ವಾಚಕ, ಘಾತಕನ ಇರವು", 
        "11877": "ಘಟದಲ್ಲಿ ಆತ್ಮ ದಿಟಕರಿಸುವುದಿಲ್ಲವೆಂದು ಅರಿದ ಮತ್ತೆ,", 
        "11878": "ಹೇಮದ ಬಣ್ಣ, ನಾನಾ ಬಗೆಯಲ್ಲಿ ತೋರುವ", 
        "11879": "ಅರಿದೆಹೆನೆಂದು ಭಾವಿಸುವನ್ನಕ್ಕ,", 
        "11880": "ಸಂಸಾರದಲ್ಲಿ ತೋರುವ ಸುಖದುಃಖಭೋಗಾದಿಭೋಗಂಗಳು", 
        "11881": "ರೂಪ ಕಂಡಲ್ಲಿ ಇಷ್ಟಕ್ಕೆ, ರುಚಿಯ ಕಂಡಲ್ಲಿ ಪ್ರಾಣಕ್ಕೆ,", 
        "11882": "ಬೀಜವ ಮೀರಿದ ವೃಕ್ಷವುಂಟೆ ಅಯ್ಯಾ ?", 
        "11883": "ಭಕ್ತನಾದಲ್ಲಿ ಭಕ್ತಿಸ್ಥಲ ಅಳವಟ್ಟು.", 
        "11884": "ಬ್ರಹ್ಮಂಗೆ ಇಚ್ಫಾಶಕ್ತಿಯಾಗಿದ್ದಲ್ಲಿ,", 
        "11885": "ತನುವಿನೊಳಗಣ ತನು, ಮನದೊಳಗಣ ಮನ,", 
        "11886": "ಒಂದು ಚಿಪ್ಪಿನಲ್ಲಿ, ಮೂರು ಮುತ್ತು ಹುಟ್ಟಿದವು:", 
        "11887": "ಕಾಡುಗುರಿ [ತಾ]ನೆಯಾಗಿ ಬಂದು,", 
        "11888": "ಒಂದು ಉಪ್ಪರಿಗೆಯ ಮಂದಿರದಲ್ಲಿ,", 
        "11889": "ತೃಷ್ಣೆ ಹಿಂಗಿದ ಲಿಂಗಾಂಗಿಯ ಅಂಗದ ಇರವು.", 
        "11890": "ಹಾವು ಹಲ್ಲಿ ಮಾರ್ಜಾಲ ಇವ ಹಾಯಿಸಿ", 
        "11891": "ಅರಿದವನ ವಿಶ್ವಾಸ, ತೊರೆಯ ಗುಂಡಿಗೆಯಿಂದ ಕಡೆಯೆ ?", 
        "11892": "ಕಾಲನ ಗೆದ್ದೆಹೆನೆಂದು ಮಾಡಿ ಬೇಡದ ನಿತ್ಯಾರ್ಚನೆ.", 
        "11893": "ರಸ ಉಂಬಲ್ಲಿ, ಗಂಧ ವಾಸಿಸುವಲ್ಲಿ,", 
        "11894": "ಕರಣಂಗಳ ಕಳೆದು ಕಂಡೆಹೆನೆಂದಡೆ,", 
        "11895": "ಕಣ್ಣಿನಲ್ಲಿ ಕಂಡೆಹೆನೆಂದಡೆ, ಕಣ್ಣು ಎನ್ನವಲ್ಲ.", 
        "11896": "ಮಾಣಿಕದ ಮಣಿ, ಉರಿಯ ಬೆಳಗಲ್ಲದೆ ಸುಡುವುದಿಲ್ಲ.", 
        "11897": "ಆತುರ ಹಿಂಗದವಂಗೆ ವೇಶೆಯ ಪೆÇೀಷಿಸಲೇಕೆ ?", 
        "11898": "ಪೃಥ್ವಿಯಿಂದಾದ ರೂಪು, ಅಪ್ಪುವಿನಿಂದಾದ ಘಟ್ಟಿ,", 
        "11899": "ಅರಿದು ಮರೆಯಲಿಲ್ಲ, ಕುಕ್ಕುರಜ್ಞಾನವಾಗಿ.", 
        "11900": "ಕಾಯವಿಡಿದು ಸೋಂಕಿದುದೆಲ್ಲ, ಪ್ರಕೃತಿಗೆ ಒಳಗು.", 
        "11901": "ನೋಡುವ ಮುಕುರ ತಾನಾಡಿದಂತೆ.", 
        "11902": "ಪಂಚಭೂತಿಕದೊಳಗಾದ ಭೂತಭವಿಷ್ಯದ್ವರ್ತಮಾನ"
    }
}