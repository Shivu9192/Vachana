        "14552": "ಏಳುಕೊಳ್ಳದ ಕಲ್ಲು ತಂದು,", 
        "14553": "ಕಣ್ಣಿಲ್ಲದ ಒಡ್ಡರು ಕಲ್ಲು ತಂದು", 
        "14554": "ಶೈವರುಕಟ್ಟಿದ ಗುಡಿಯ ಬಿಚ್ಚದೆ,", 
        "14555": "ಎಮ್ಮ ಶಿವಗಣಂಗಳು ಕಲ್ಯಾಣಪುರದಲ್ಲಿ", 
        "14556": "ಇಂತಪ್ಪ ಭೇದವ ತಿಳಿಯದೆ", 
        "14557": "ನೀಲವರ್ಣದ ಮಾಣಿಕಕ್ಕೆ ಐದುಹೊನ್ನು ಕೊಟ್ಟೆ.", 
        "14558": "ಇಬ್ಬರಲ್ಲಿ ಹುಟ್ಟದೆ ಕರದಲ್ಲಿ ಹುಟ್ಟಿ,", 
        "14559": "ಬೆಳ್ಳಿ, ಬಂಗಾರ, ತಾಮ್ರ, ಹಿತ್ತಾಳಿಯ ಕೊಡದೆ,", 
        "14560": "ಮಹಾಮಲಸರೋವರದ ವರಾಹ ಆಕಾಶವ ನುಂಗಿ", 
        "14561": "ಇಂತಪ್ಪ ಲಿಂಗೈಕ್ಯದ ಭೇದವ ತಿಳಿಯದೆ", 
        "14562": "ಲಿಂಗೈಕ್ಯವಾಗಬೇಕೆಂದು ಅಜ್ಞಾನದೊಳಗೆ", 
        "14563": "ಲೌಕಿಕದ ಮಧ್ಯದಲ್ಲಿ ಬಾಗಿಣವನಿಕ್ಕಿದ ಸ್ತ್ರೀಯರ", 
        "14564": "ತಲೆಯಿಲ್ಲದ ಪುರುಷನಿಗೆ ಕಾಲಿಲ್ಲದ ಸ್ತ್ರೀ.", 
        "14565": "ಕಾಮವಿಲ್ಲದ ಸ್ತ್ರೀಗೆ  ನಪುಂಸಕನೆಂಬ ಪುರುಷನು.", 
        "14566": "ಯೋನಿಯಿಲ್ಲದ ಸ್ತ್ರೀಗೆ ಶಿಶ್ನವಿಲ್ಲದ ಪುರುಷನು.", 
        "14567": "ವಿಭೂತಿ ರುದ್ರಾಕ್ಷಿಯ ಧರಿಸಿ", 
        "14568": "ಆನೆಯ ಏರುವಾತ ಮಹೇಶ್ವರನಲ್ಲ.", 
        "14569": "ಭಕ್ತರಮನೆಗೆ ಕರೆಯದೆ ಅಶನಕ್ಕೆ ಪೆÇೀಗುವ ಮಹೇಶ್ವರರೆಲ್ಲ", 
        "14570": "ಕಲ್ಲು ದೇವರೆಂದು ಪೂಜಿಸುವರೆಲ್ಲ", 
        "14571": "ಒಂದು ಜನ್ಮದಲ್ಲಿ ನಿನ್ನ", 
        "14572": "ದೇವರು ದೇವರು ಎಂಬಿರಿ ದೇವರಿಗೆ ಪ್ರಳಯವುಂಟೆ ?", 
        "14573": "ಮಾನವರಿಗೆ ಮತ್ರ್ಯಲೋಕ,", 
        "14574": "ಮುಷ್ಟಿಯ ಚಡಾಯಿಸಿ ಮಾನವರ ಕೊಲ್ಲುವಾತ", 
        "14575": "ಸತ್ತವರ ಸಮಾಧಿಯಲ್ಲಿ ತಡಿಗಿಯ ಕಟ್ಟಿಸಿ,", 
        "14576": "ಶೀಲವಂತರೆಲ್ಲ ಶೂಲದ ಹೆಣನೆಂಬೆ.", 
        "14577": "ವಿಭೂತಿಯ ಪೂಸುವ ಪಣೆಗೆ ಬಂಡಾರವ ಪೂಸುವವರು", 
        "14578": "ಹಳದಿಯ ಪೂಸಿ, ವಸ್ತ್ರವ ಪೆÇದ್ದು,", 
        "14579": "ಪುಣ್ಯಕ್ಷೇತ್ರಯಾತ್ರೆ ಪುಣ್ಯತೀರ್ಥಸ್ನಾನವ ಮಾಡಬೇಕೆಂಬಿರಿ.", 
        "14580": "ಆದಿ ಅನಾದಿ ಸುರಾಳ ನಿರಾಳ", 
        "14581": "ಕಾಶಿಗೆ ಹೋಗಬೇಕೆಂಬಣ್ಣಗಳು ನೀವು ಲಾಲಿಸಿರಯ್ಯ,", 
        "14582": "ಪರ್ವತಮಲ್ಲಯ್ಯನ ಯಾತ್ರೆಗೆ ಹೋಗಬೇಕೆಂಬರಯ್ಯಾ,", 
        "14583": "ಪರ್ವತಕ್ಕೆ ಹೋಗಬೇಕಾದರೆ", 
        "14584": "ಇಂತಪ್ಪ ವಿಚಾರವನು", 
        "14585": "ಒಂದು ಕ್ಷೇತ್ರಕ್ಕೆ ಹೋಗಿ", 
        "14586": "ಹುಟ್ಟದ ಮುನ್ನ ಹೋಗಾಡಿ ಕೊಟ್ಟು ಕೊಂಡೆ.", 
        "14587": "ಶುಕ್ಲಶೋಣಿತಾತ್ಮಸಂಬಂಧವಾದ", 
        "14588": "ನಾನು ಪರಶಿವತತ್ವದಿಂ ಭಿನ್ನವಾಗಿ", 
        "14589": "ರಾತ್ರಿಯಲ್ಲಿ ಬಹುಕುಲದಲ್ಲಿ ಪುಟ್ಟಿದೆ.", 
        "14590": "ಎನ್ನ ತಾಯಿ ಹೊಲೆಯನ ಸಂಗವಮಾಡಿ", 
        "14591": "ಪ್ರಸಾದಿಗೆ ಪರದ್ರವ್ಯದ ಪ್ರೇಮವುಂಟೆ ?", 
        "14592": "ಒಳ್ಳೆ ಪದಾರ್ಥಬಂದಡೆ ನೀಡೆಂಬಿರಿ,", 
        "14593": "ಪ್ರಸಾದಿ ಪ್ರಸಾದಿಗಳೆಂದು ಹೆಸರಿಟ್ಟುಕೊಂಡು", 
        "14594": "ಅಚ್ಚಪ್ರಸಾದಿಗಳೆಂದು ಹೆಸರಿಟ್ಟುಕೊಂಡು", 
        "14595": "ಅಚ್ಚಪ್ರಸಾದ ನಿಚ್ಚಪ್ರಸಾದ ಸಮಯಪ್ರಸಾದ", 
        "14596": "ಎಮ್ಮ ಶಿವಶರಣರ ಪ್ರಸಾದವಿವರ ಎಂತೆಂದಡೆ :", 
        "14597": "ಅರಣ್ಯದ ಅಡ್ಡಗುಡ್ಡದ ಸರೋವರದಲ್ಲಿರುವ ಹಂದಿಯ", 
        "14598": "ಇಂತಪ್ಪ ಶಿವಪ್ರಸಾದದ ಮಹಾಘನವನರಿಯದೆ", 
        "14599": "ಪ್ರಾಣಲಿಂಗಿಗೆ ಪದಾರ್ಥಪ್ರೇಮವುಂಟೆ ?", 
        "14600": "ಅಂಗದ ಮೇಲೆ ಇಷ್ಟಲಿಂಗವ ಧರಿಸಿ", 
        "14601": "ಒಂದು ಜಡಿಯ ಬಿಚ್ಚಿ ಸಹಸ್ರ ಹುರಿಗೂಡಿದ ಒಂದು ಹಗ್ಗ.", 
        "14602": "ಮೇಲುಗಿರಿಯಮೇಲಣ ಪಕ್ಷಿ ಉತ್ತಾಯವಾಗಿ", 
        "14603": "ಪಂಚವರ್ಣದ ವಟವೃಕ್ಷದ ಘಟ್ಟದಲ್ಲಿ", 
        "14604": "ಇಂತೀ ವಿಚಾರವನು ತಿಳಿಯದೆ", 
        "14605": "ಶರಣರು ಶರಣರು ಎಂದು ಹೆಸರಿಟ್ಟುಕೊಂಬಿರಿ,", 
        "14606": "ನೀರಲ ಫಲ, ಉತ್ತತ್ತಿ, ಮಾವಿನ ಹಣ್ಣಿನಂತೆ", 
        "14607": "ಆರೂ ಇಲ್ಲದ ಅರಣ್ಯಕ್ಕೆ ಪೆÇೀಗಿ,", 
        "14608": "ನೆಲನಿಲ್ಲದ ಭೂಮಿಯಲ್ಲಿ ಪುಟ್ಟಿದ", 
        "14609": "ಕತ್ತೆಯ ಕಟ್ಟಲಿಲ್ಲಾ, ಕತ್ತೆಯಿಲ್ಲದೆ ಅರಣ್ಯಕ್ಕೆ ಹೋಗಲಿಲ್ಲ.", 
        "14610": "ಮೂರಾರು ಬಾಗಿಲಲ್ಲಿ ಹೋಹ ಕರುವ", 
        "14611": "ಅಮ್ಮನ ಮೊಮ್ಮಗಳ ಗಂಡ", 
        "14612": "ಹಗಲಳಿದು ಇರುಳಲ್ಲಿ ಒಂದು ಕತ್ತೆಯನೇರಿ,", 
        "14613": "ಕಾಲಿಲ್ಲದೆ ನೂರಾವಂದುಯೋಜನ ನಡೆದು,", 
        "14614": "ಈರಾರುಮಣಿಯ ಲೆಕ್ಕಕ್ಕೆ ನಿಲುಕದು.", 
        "14615": "ಅರಗಿನ ಭೂಮಿ ಅಗ್ನಿಕುಂಡದ ಉದಕದಲ್ಲಿ", 
        "14616": "ಮಹಾಮಲೆಯ ವ್ಯಾಘ್ರನ ನೀರ ನಕ್ರ ಕಚ್ಚಿ,", 
        "14617": "ಅಷ್ಟಪರ್ವತ ಮೇಲುಗಿರಿಯ ಮೇಲೊಂದು", 
        "14618": "ನಾ ಪುಟ್ಟಿದುದ ಎಲ್ಲರೂ ಬಲ್ಲರು.", 
        "14619": "ಅಂಗೈಯೊಳಗಿನ ಕೂಸು ಆಕಾಶವ ನುಂಗಿ,", 
        "14620": "ಇಂತಪ್ಪ ಲಿಂಗೈಕ್ಯವಹ ಭೇದವ ತಿಳಿಯದೆ", 
        "14621": "ಪ್ರಸಾದ ಪ್ರಸಾದವೆಂದು ನುಡಿಯಬಹುದಲ್ಲದೆ,", 
        "14622": "ಪ್ರಾಸದವೆಂಬುದು ಪುಣ್ಯಪಾಪವೆಂಬುಭಯ ಕರ್ಮವ", 
        "14623": "ಪ್ರಸಾದವೆಂಬುದು ಆಕಾರವಲ್ಲ ನಿರಾಕಾರವಲ್ಲ,", 
        "14624": "ಶಿವ ಶಿವಾ, ನಾವು ಗುರುಲಿಂಗಜಂಗಮದ", 
        "14625": "ಇಂತಪ್ಪ ನಿರ್ಣಯವನು ಸ್ವಾನುಭಾವಜ್ಞಾನದಿಂ ತಿಳಿದು", 
        "14626": "ಅಂತಪ್ಪ ಭವಭಾರಿಗಳಾದ ಜೀವಾತ್ಮರು", 
        "14627": "ಪಾದೋದಕ ಪಾದೋದಕವೆಂದು ಕೊಂಬಿರಿ,", 
        "14628": "ಪ್ರಸಾದ ಪ್ರಸಾದವೆಂದು ನುಡಿದುಕೊಂಡುಂಬಿರಿ.", 
        "14629": "ಕೇಳು ಕೇಳಯ್ಯ ಎನ್ನ ಮತ್ಪ್ರಾಣನಾಥ", 
        "14630": "ಶಿವ ಶಿವಾ, ನಾವು ಗುರುಲಿಂಗಜಂಗಮದ", 
        "14631": "ಪ್ರಸಾದಗ್ರಾಹಕನಾದ ಸದ್ಭಕ್ತನು", 
        "14632": "ಗುರುಲಿಂಗಜಂಗಮದ ಪಾದೋದಕ", 
        "14633": "ಗುರುಲಿಂಗಜಂಗಮದಲ್ಲಿ ಅತ್ಯಂತ ಪ್ರೇಮಿಗಳೆಂದು", 
        "14634": "ಗುರುಪ್ರೇಮಿಯಾದಡೆ ಚತುರ್ವಿಧಭಕ್ತಿಯಿಂದ", 
        "14635": "ಇಂದಿನ ಚಿಂತೆಯುಳ್ಳವರೆಲ್ಲ ಹಂದಿಗಳು.", 
        "14636": "ಉಣ್ಣಬೇಕು, ಉಡಬೇಕು, ಇಡಬೇಕು,", 
        "14637": "ಉಂಡು ಕಾರುವಾತ ಇಸವ ಕಂಡ.", 
        "14638": "ಇಂತಪ್ಪ ವಿಚಾರವ ತಿಳಿದು", 
        "14639": "ಇಂತಪ್ಪ ಗುರುಶಿಷ್ಯರ ಮೇಳಾಪವ ಕಂಡು", 
        "14640": "ಎನಗೆ ಭಕ್ತನೆಂಬರು,", 
        "14641": "ಲಿಂಗನಿಷ್ಠೆಯುಳ್ಳ ವೀರಶೈವ ಮಹೇಶ್ವರರು", 
        "14642": "ದಿನಚರಿ ಮಾಸದೊಳಗೆ ಏಳು ವಾರದೊಳಗೆ", 
        "14643": "ಇಂತಪ್ಪ ಲಿಂಗಾಂಗದ ಸಮರಸವ ತಿಳಿಯದೆ,", 
        "14644": "ಈ ಲೌಕಿಕದ ಮಧ್ಯದಲ್ಲಿ ಜೀವಾತ್ಮರು ದಿನಚರಿ,", 
        "14645": "ದ್ವಾದಶಮಾಸದೊಳಗೆ ಶ್ರಾವಣಮಾಸದ ಫಲಪುಣ್ಯ ಮಹಾದೊಡ್ಡದೆಂದು,", 
        "14646": "ದ್ವಾದಶಮಾಸ, ಚತುದರ್ಶಿ, ದ್ವಾದಶಿ, ಅಮವಾಸಿಯೊಳಗೆ", 
        "14647": "ಇಂತಪ್ಪ ತ್ರಿವಿಧಾಚರಣೆಯಲ್ಲಿ ಭಿನ್ನಜ್ಞಾನಿಗಳಾಗಿ", 
        "14648": "ಅಡ್ಡಗುಡ್ಡದ ಬೆಟ್ಟವನೊಡೆದು,", 
        "14649": "ನವಖಂಡಪೃಥ್ವಿ ಎಂಬುದು ಪಂಚಶತಕೋಟಿ ಯೋಜನ.", 
        "14650": "ಭೂಮಿಯಲ್ಲಿ ಲಿಂಗಸ್ಥಾಪನೆಯಿಲ್ಲದೆ", 
        "14651": "ಇಂತಪ್ಪ ಶಿವಾಚಾರದ ನಿರ್ಣಯವನರಿಯದೆ", 
        "14652": "ಪ್ರಾಣಲಿಂಗಿ ಪ್ರಾಣಲಿಂಗಿ ಎಂಬಿರಿ,", 
        "14653": "ಮೂರಾರು ಪರ್ವತದ ಮೇಲುಗಿರಿ ಅಗ್ರದ ಕಮಲದೊಳಗೆ,", 
        "14654": "ಬಯಲ ಪಟ್ಟಣದ ರಾಜಕುಮಾರನು", 
        "14655": "ನೀರಿಲ್ಲದ ಭೂಮಿಯಲ್ಲೊಂದು ಬೇರಿಲ್ಲದ ವೃಕ್ಷಪುಟ್ಟಿ,", 
        "14656": "ಮಂಡಲತ್ರಯದ ಚಿತ್ತದೊಳಗೆ", 
        "14657": "ಪಂಚವರ್ಣದ ಚಿತ್ತದ", 
        "14658": "ಅವಿದ್ಯಾಪಟ್ಟಣದ ಹೊರಕೇರಿಯಲ್ಲಿ", 
        "14659": "ಅತ್ತೆ ಅಳಿಯನ ನೆರೆದು ಮಾವ ಸೊಸೆಯ ಬೆರೆದು,", 
        "14660": "ಊರ ಬಾರಿಕನ ಬಾಗಿಲಲ್ಲಿ", 
        "14661": "ಸಂದಿ ಕೇರಿ ಗೊಂದಿ ಪಕ್ಷಿಯ ಕೋಡು ಕಿತ್ತು,", 
        "14662": "ಮಲೆಯಮಂದಿರದ ವಟವೃಕ್ಷದ ಘಟದಲ್ಲಿ", 
        "14663": "ಮನೆಯೊಳಗಣ ಜ್ಯೋತಿ ಮನೆಯ ಸುಟ್ಟು,", 
        "14664": "ನೆಲನಿಲ್ಲದ ಭೂಮಿಯ ಪಕ್ಷಿ", 
        "14665": "ಅಂಬರದ ಪಕ್ಷಿಗೆ ಕಾಲಾರು, ತಲೆ ಮೂರು,", 
        "14666": "ರಂಡೆಮುಂಡೆಯ ಮಗ ರಾಜಕುಮಾರ", 
        "14667": "ಭೂಮಿಯಿಲ್ಲದೆ ಊರೊಳಗೆ ಒಂದು ಶಿಶು ಹುಟ್ಟಿ,", 
        "14668": "ಅಂಬರದ ಕೂಸು ಆನೆಯ ನುಂಗಿ,", 
        "14669": "ನೀರಿಲ್ಲದ ಸಮುದ್ರದಲ್ಲಿ ಅಗ್ನಿಯು ಪುಟ್ಟಿ,", 
        "14670": "ಪಂಚಾಗ್ನಿಪುರವೆಂಬ ಪಟ್ಟಣದಲ್ಲಿ", 
        "14671": "ಆಕಾರ ಪ್ರಸಾದ ಅಂಗೈ ನುಂಗಿ,", 
        "14672": "ಪ್ರಾಣಲಿಂಗಿ ಪ್ರಾಣಲಿಂಗಿ ಎಂಬರಯ್ಯಾ ;", 
        "14673": "ಪ್ರಾಣಲಿಂಗಿಯ ನಿಲವ ಪೇಳ್ವೆ.", 
        "14674": "ಪ್ರಾಣಲಿಂಗಿಗೆ ಚತುರ್ವಿಧಭಕ್ತಿಯಿಂದ", 
        "14675": "ಪ್ರಾಣಲಿಂಗಿಗೆ ಪರದೈವ ಪೂಜೆಯುಂಟೆ ?", 
        "14676": "ಮನೆಯ ಕಟ್ಟುವಾತ ಪ್ರಾಣಲಿಂಗಿಯಲ್ಲ ;", 
        "14677": "ಕಟ್ಟಿದ ಮನೆ ಬಿಚ್ಚದೆ, ಮಾಡಿದ ಮನೆ ಕೆಡವದೆ,", 
        "14678": "ರಂಜನಿಪುರದಲ್ಲಿ ಕಪಿ ಉಡವ ಕಚ್ಚಿ", 
        "14679": "ಕತ್ತಲಿಪುರದಲ್ಲಿ ಎತ್ತೆಮ್ಮೆ ಮೈಥುನ ಮಾಡುವದ ಕಂಡೆ.", 
        "14680": "ಮನೆಯೊಳಗೊಂದು ಮಾಯದ ಕೂಸು ಹುಟ್ಟಿ,", 
        "14681": "ವಿರಕ್ತನಾದೆನೆಂಬರು, ವಿರಕ್ತನ ಬಗೆಯ ಪೇಳ್ವೆ.", 
        "14682": "ವಿರಕ್ತನಾದಡೆ ಕುಳ್ಳಬೂದಿಯ ಧರಿಸಲಾಗದು.", 
        "14683": "ವಿರಕ್ತನಾದಡೆ ಮನೆಯ ಹೊಂದಿ ಎಲ್ಲರಲ್ಲಿರಬೇಕು.", 
        "14684": "ವಿರಕ್ತನಾದಡೆ ರಂಡೆಯ ಸಂಗವಮಾಡಿ,", 
        "14685": "ಇಂತಪ್ಪ ವಿರಕ್ತನ ನಿಲುಕಡೆಯ", 
        "14686": "ಸಾಣಿಯ ಮೇಲೆ ಶ್ರೀಗಂಧವನಿಟ್ಟು,", 
        "14687": "ಕಲ್ಲಮರದ ಕೋಡಗದ ತಲೆಯೆಂಟು ಸ್ಥಾನವ ಮೆಟ್ಟಿ", 
        "14688": "ಆರು ಇಲ್ಲದ ಅರಣ್ಯದಲ್ಲೊಂದು", 
        "14689": "ಮಹಾಮಲೆಯಲ್ಲಿ ಮಕ್ಷಿಕವಿರ್ಪುದು.", 
        "14690": "ಬೇರಿಲ್ಲದೆ ಭೂಮಿಗೆ ಹೊಂದದೆ", 
        "14691": "ಮೋಟುಗಿಡದಲ್ಲಿ ಹೂವು ಕಾಯಿಲ್ಲದೆ", 
        "14692": "ವಂದಿಸಿದಲ್ಲಿ ಶತಕೋಟಿ ಯೋಜನವಾಯಿತ್ತು.", 
        "14693": "ಗುರುಪೂಜೆ ಮಾಡುವಾತ ಶಿಷ್ಯನಲ್ಲ.", 
        "14694": "ವೇದವನೋದುವರೆಲ್ಲ ಬಂಜೆಯ ಮಕ್ಕಳು.", 
        "14695": "ಮಲವ ಭುಂಜಿಸುವ ಶೂಕರನಿಗೆ ಮದಗಜವ ಹೋಲಿಸಿದರೆ", 
        "14696": "ಹೆಂಡದ ಹರವಿಗೆ ಹಾಲಹರವಿಯ ಹೋಲಿಸಿದರೆ,", 
        "14697": "ಹಾಲುಕುಡಿದ ಶಿಶು ಸತ್ತು", 
        "14698": "ಮೃತ್ಯುವೆಂಬ ಪಟ್ಟಣದಲ್ಲಿ ಒಂದು ಚಿತ್ರವ ಕಂಡೆ.", 
        "14699": "ಮುದುಕಿ ಮುದುಕನ ಸಂಗದಿಂದೊಂದು", 
        "14700": "ಗುಂಗರಿಯ ಮೂರು ಮುಖದಲ್ಲಿ ಊರು ಇರ್ಪುದು.", 
        "14701": "ಅರಸನ ಹೆಂಡತಿಯ ತಂದು", 
        "14702": "ಬೇವಿನಮರಕ್ಕೆ ಬೆಲ್ಲದ ಕಟ್ಟೆಯ ಕಟ್ಟಿ,", 
        "14703": "ಮೋರೆಯಿಲ್ಲದವರಿಗೆ ಕನ್ನಡಿಯ ತೋರಿದಂತೆ,", 
        "14704": "ಅಂಗವಿಲ್ಲದ ಪುರುಷನ ಸಂಗವಿಲ್ಲದ ಸ್ತ್ರೀ", 
        "14705": "ಮೋಟ ಮೂಕಾರ್ತಿಯ ಮದುವೆಗೆ", 
        "14706": "ಪಾದೋದಕ ಪ್ರಸಾದಗಳೆಂದೆಂಬಿರಿ,", 
        "14707": "ಕರಿಯ ಕಂಬಳಿಯ ಮೇಲೆ ಬಿಳಿಯ ಪಾವಡವ ಹಾಕಿ,", 
        "14708": "ಗುರು ಮುನ್ನವೋ ಶಿಷ್ಯ ಮುನ್ನವೋ ಲಿಂಗ ಮುನ್ನವೋ", 
        "14709": "ಪೃಥ್ವಿಯಲ್ಲಿ ಹುಟ್ಟಿದ ಶಿಲೆಯ ತಂದು", 
        "14710": "ತಮ್ಮನ ಪ್ರಸಾದ, ಅಣ್ಣಗಲ್ಲದೆ", 
        "14711": "ಕಮಲಪ್ರಸಾದ ಬ್ರಹ್ಮನ ನುಂಗಿತ್ತು.", 
        "14712": "ಒಬ್ಬ ಪುರುಷಂಗೆ ಮೂವರು ಸಸ್ತ್ರೀಯರು.", 
        "14713": "ಮಂಗಳಾಂಗಿ ಮಧುರವಾಣಿ ಕೇಳವ್ವಾ,", 
        "14714": "ವನಾಂತರದಲ್ಲಿ ಕೋಗಿಲೆ ಸ್ವರಗೈದಿತೆಂದು", 
        "14715": "ವೀರಮಾಹೇಶ್ವರರು ಸರ್ವಾಂಗದಲ್ಲಿ", 
        "14716": "ಎರಡಿಲ್ಲದ ಭೂಮಿಯಲ್ಲಿ ಬಯಲ ಪಟ್ಟಣ.", 
        "14717": "ಕಾಳಿಯ ಉದರದ ಮೂರು ಕೋಣೆ", 
        "14718": "ತಾಡಿನಮರದಮೇಲೆ ತೆಂಗಿನಮರವ ಕಂಡೆ.", 
        "14719": "ತ್ರಿಪುರಮಧ್ಯದಲ್ಲಿ ಮೂರಾರು ಮಂಡಲ ಇರ್ಪುದ ಕಂಡೆ.", 
        "14720": "ಪಂಚಶತಕೋಟಿ ಯೋಜನದಾಚೇಲಿ", 
        "14721": "ಆರುವರ್ಣದ ಭೂಮಿಯ ಮೂರು ಬೆಟ್ಟದ ನಡುವಣ", 
        "14722": "ಹರಿವ ನೀರ ಉರಿಕೊಂಡದಲ್ಲಿ", 
        "14723": "ಆಡ ಅಣ್ಣ ತಿಂದ, ಕುರಿ ತಮ್ಮ ತಿಂದ,", 
        "14724": "ಮೂರುದಿಬ್ಬದ ಅರಣ್ಯದಲ್ಲಿ ಚರಿಸುವ ಕುರಿತಂಡನು", 
        "14725": "ಆಡ ಕೊಂದು ಗಡಿಗೆಯಲಿಕ್ಕಿ,", 
        "14726": "ಆಡಿನ ಹಾಲ ಹರವಿಯ ತುಂಬಿ,", 
        "14727": "ಮೂರು ಸುತ್ತಿನ ಹಟ್ಟಿಯ ಸುಟ್ಟು", 
        "14728": "ಕುರಿಯ ಹಾಲ ಮರಿಯು ಕುಡಿದು", 
        "14729": "ಶರಣರ ನಿಲುಕಡೆ ದಾರುಬಲ್ಲರಯ್ಯ ?", 
        "14730": "ಶರಣನ ನಿಲವು ಜ್ಯೋತಿಯಂತೆ,", 
        "14731": "ಶರಣನಾದೊಡೆ ಅಚ್ಚೊತ್ತಿದ ಅರಿವಿಯಂತಿರಬೇಕು ಲಿಂಗದಲ್ಲಿ.", 
        "14732": "ಎರಡುಗಾಲಿಯ ಬಂಡಿಯ ಮುರಿದು", 
        "14733": "ಕೀಲಿಲ್ಲದ ಬಂಡಿಯ ಗಾಲಿಯನುಚ್ಚಿ,", 
        "14734": "ಕಪ್ಪಿನ ಭೂಮಿಗೆ ಮೂರುಗಜ ಸೂರ್ಯಕಾಂತದ ಕಲ್ಲು ಹಾಕಿ,", 
        "14735": "ಗುರುಪೂಜೆ ಮಾಡುವಣ್ಣಗಳ", 
        "14736": "ಶೀಲಮಾಡುವಣ್ಣಗಳ ಶುಚಿ ಅವಶುಚಿಯೆಂಬ", 
        "14737": "ಅರಣ್ಯದೊಳಗಿರುವಣ್ಣಗಳ", 
        "14738": "ಮೂರಾರು ಕಂಡಿಕಿ ದಂಡಿಗಿಗೆ", 
        "14739": "ಮೂರುಗಂಟಿನ ದಂಡಿಗಿ,", 
        "14740": "ಲಿಂಗಪೂಜೆಯುಳ್ಳನ್ನಕ್ಕ ಲಿಂಗವಿಲ್ಲ.", 
        "14741": "ಹಾಳುಗೋಟಿನ ಭಿತ್ತಿ ಬೇಲಿಯಲ್ಲಿ", 
        "14742": "ಅರಸು ಪ್ರಧಾನಿಗಳು ಬಿಟ್ಟಿಯ ಮಾಡಲಿಲ್ಲ.", 
        "14743": "ಆನೆಯ ಹಿಂಡು ಹೊಗದೆ, ಕುದುರೆಯ ತೂರ ಹಿಡಿಯದೆ,", 
        "14744": "ಅಟಮಟವೃಕ್ಷದ ಘಟದಲ್ಲಿ", 
        "14745": "ಮರಿದುಂಬಿ ಮರದ ಮೂಲಗೂಡಿನಲ್ಲಿರುವ", 
        "14746": "ಗಜಿಬಿಜಿಯೆಂಬ ಪಟ್ಟಣದ ಗುಜ್ಜದೇವಿ ಎಂಬ ಸ್ತ್ರೀಯಳ", 
        "14747": "ಎಮ್ಮೆ ಕೋಣನ ಚರ್ಮವ ತೆಗೆದು,", 
        "14748": "ಹಾಳುಬಾವಿ ಹಿಡಿಮೊಟ್ಟೆಗೆ", 
        "14749": "ಮೂರು ಜರಿಬಾವಿಯ ನೀರಿನಲ್ಲಿ", 
        "14750": "ಕನ್ಯಾಸ್ತ್ರೀಯಳ ಕುಚವ ಪಿಡಿದು,", 
        "14751": "ಪಂಕದೊಳಗಿನ ಪಶುವಿಗೆ ಒಬ್ಬ ಪುರುಷನಿಂದ ಚೆೃತನ್ಯವಲ್ಲದೆ", 
        "14752": "ಭವಬಂಧನಂಗಳ ಹಿಂಗಿಸಬೇಕೆಂಬಣ್ಣಗಳು", 
        "14753": "ಕುರಿತೊಗಲು ಬಿಸಿಲಿಗೆ ಹದಮಾಡಿ,", 
        "14754": "ಹಸಿತೊಗಲಿಗೆ ಬಿಸಿಯೆಳಿಯನಿಕ್ಕಿ,", 
        "14755": "ಕಾಶಿಗೆ ಹೋಗಬೇಕೆಂಬಣ್ಣಗಳು", 
        "14756": "ಕರಿಚರ್ಮದ ಮಚ್ಚಿ ಮೆಟ್ಟಿದವರು ಕೆಳಗಾದರು.", 
        "14757": "ಚಮ್ಮಾರನಲ್ಲಿ ಮಚ್ಚೆಯ ಕೊಂಡು", 
        "14758": "ಇಹವ ತಿಳಿಯದೆ, ಮೂರಿಟ್ಟು ಕಮಲವ ಕಾಣದೆ", 
        "14759": "ಮೂರು ಹೊನ್ನಿಗೆ ಹಚ್ಚಡವ ಮಾರಿದೆ.", 
        "14760": "ಶಕಲಾತಿ ಬ್ರಹ್ಮಂಗೆ ಕೊಟ್ಟು,", 
        "14761": "ಶ್ವೇತವರ್ಣದ ವಸ್ತ್ರವನೆಲ್ಲ ಭಕ್ತಂಗೆ ಕೊಟ್ಟು,", 
        "14762": "ಕೊಟ್ಟ ಹಣವ ಮುಟ್ಟದೆ,", 
        "14763": "ಪುರುಷ ಸತ್ತಬಳಿಕ ಗಂಧ ಅಕ್ಷತೆ ಧರಿಸಿ", 
        "14764": "ಹಿಡಿಯಿಲ್ಲದ ಉಳಿ, ಕಾವಿಲ್ಲದ ಬಾಚಿ, ಹಲ್ಲಿಲ್ಲದ ಕರಗಸ", 
        "14765": "ಮಲೆಯ ಕಡಿದು ಒಂದು ವೃಕ್ಷವ ತಂದು,", 
        "14766": "ಪರ್ಣ ಉದುರಿದ ವೃಕ್ಷ ಕಡಿದು, ಕೊಂಗೆಯ ಸವರದೆ ಮನೆಯ ಕಟ್ಟಿ,", 
        "14767": "ಪಂಚಲೋಹದ ಕಬ್ಬಿಣವ ಬೆಂಕಿಯಿಲ್ಲದೆ", 
        "14768": "ಮಣ್ಣಿಲ್ಲದ ಮುಶಿಯೊಳಗೆ ಪಂಚರಸದ ಲೋಹವ ಹಾಕಿ", 
        "14769": "ಭವಿಗಳ ಕಪ್ಪಡ ಕವುದಿ ಕಂಬಳಿ ಕರಿಯಶಾಲಿಯ ತೊಳೆಯದೆ", 
        "14770": "ಕತ್ತೆಯ ಕಟ್ಟದೆ, ಮಡಿಯ ಮುಟ್ಟದೆ,", 
        "14771": "ಎಡಭಾಗದ ಮುರಗಿಂದ ಬ್ರಹ್ಮನ ಹೊಡೆದು ಕೊಂದೆ,", 
        "14772": "ಕಲ್ಯಾಣಪುರದೊಳಗೆ ಬಿಜ್ಜಳಾಂಕನೆಂಬ ರಾಜನು", 
        "14773": "ನೆಲವಿಲ್ಲದ ಭೂಮಿಯ ಮಣ್ಣನ್ನು ತಂದು,", 
        "14774": "ಮಣ್ಣ ತುಳಿದು ಮಡಿಕೆಯ ಮಾಡಿ", 
        "14775": "ಅಟ್ಟುಂಬ ಜನರಿಗೆ ಮಡಿಕೆಯ ಕೊಡಲಿಲ್ಲ;", 
        "14776": "ರಂಜಣಿಗೆಯ ಉದಕವ ಕುಡಿದವರು ಸಾಯಲಿಲ್ಲ.", 
        "14777": "ಕಾಳಮ್ಮನ ಪೂಜಿಸದೆ, ಕಾಳಿಶಾಲಿಯನುಡದೆ", 
        "14778": "ಕುಬಟಗಿಯನೊಡೆದು ಇದ್ದಲಿಯ ಹಾಕದೆ,", 
        "14779": "ಮನುಜರಿಲ್ಲದ ಊರ ಬಳೆಯ ತಂದು,", 
        "14780": "ಹಡಿಯದವರಿಗೆ ಬಳೆಯನಿಡಿಸುವೆ,", 
        "14781": "ಪ್ರಪಂಚ ಪಾರಮಾರ್ಥ, ಲೌಕಿಕ ಪಾರಮಾರ್ಥ", 
        "14782": "ಹೊನ್ನು ಹೆಣ್ಣು ಮಣ್ಣೆಂಬ ತ್ರಿವಿಧವನಳಿದಲ್ಲದೆ", 
        "14783": "ಮದುವೆಯಿಲ್ಲದವರಿಗೆ ಮುಹೂರ್ತವ ಪೇಳಲಿಲ್ಲ.", 
        "14784": "ಪಂಚಾಂಗವ ಬರೆದು ಓದಿ", 
        "14785": "ಲಿಂಗವಂತರಿಗೆ ಮುಹೂರ್ತವ ಪೇಳಿ", 
        "14786": "ಮೂರಳಿದು ಮೂರುಳಿದು ಮೂರೊಂದುಗೂಡಿದವರ", 
        "14787": "ಆಶೆಯುಳ್ಳವರಿಗೆ ವೇಷವ ಧರಿಸಿ", 
        "14788": "ಪಂಚಾಂಗವ ಸುಟ್ಟವರಿಗೆ ಪಂಚಾಂಗವ ಕೊಟ್ಟು", 
        "14789": "ಮೂರು ಹೊನ್ನು ಕೊಟ್ಟು ಮೂರಾರು ಹೊನ್ನಿನ ಊರಕೊಂಡು", 
        "14790": "ತನ್ನ ಗ್ರಾಮವಬಿಟ್ಟು ಅನ್ಯಗ್ರಾಮದಲ್ಲಿರಲು,", 
        "14791": "ಅರಸು ಪ್ರಧಾನಿ ಗೌಡರಿಗೆ ಅಂಜದೆ,", 
        "14792": "ಭೂಮಿ ಬಿಟ್ಟವರಿಗೆ ಭೂಮಿಕೊಟ್ಟು ಹಣವ ಕೊಂಬುವರು ;", 
        "14793": "ಇಬ್ಬರಿಗೆ ಮೂರು, ಮೂರಿಟ್ಟವರು", 
        "14794": "ಬಯಲ ಹೊತ್ತವರು ಅಳಲಿಲ್ಲ;", 
        "14795": "ಮನೆ ಸುಟ್ಟು ಸತಿಸುತರು ಸತ್ತು", 
        "14796": "ಕರಿಶಾಲಿಯ ಹರಕ ಬಿಡಿಹೊಲಿಗೆಯ ಕರಿಕುಪ್ಪಸ,", 
        "14797": "ಅನೇಕ ಹೊಲಿಗೆಯ ಕಪ್ಪಡವ ಬಿಚ್ಚಿ,", 
        "14798": "ಕೀಲಿಲ್ಲದ ಕತ್ತರಿ, ಹಿನ್ನಿಯಿಲ್ಲದ ಸೂಜಿ, ನೂಲಿಲ್ಲದ ದಾರ.", 
        "14799": "ಸೂಜಿಯ ಹಿನ್ನಿಯ ಹರಿದು ದಾರವ ಪೆÇೀಣಿಸಿ,", 
        "14800": "ಲೋಡಿಗೆ ಮೂರು ಹಣವ ಕೊಂಬರು.", 
        "14801": "ಹರಕರವಿಯ ಹುರಿಯಿಲ್ಲದ ದಾರದಲ್ಲಿ ಹೊಲಿದು,", 
        "14802": "ಗಿಲಾಯದ ಮಣ್ಣಿನ ಗೋಡೆಗೆ,", 
        "14803": "ಹಸಿಯ ಗೋಡೆಗೆ ಚಿತ್ರವ ಬರೆಯದೆ", 
        "14804": "ಅಗ್ನಿವರ್ಣದ ಚಿತ್ರ ಆಕಾಶವ ನುಂಗಿತ್ತು.", 
        "14805": "ಚಿತ್ರವನರಿಯದವರು", 
        "14806": "ಮೂದೇವಿಯ ಮಕ್ಕಳೆಲ್ಲಾ ಚಿತ್ರವ ಪೂಜಿಸಿ", 
        "14807": "ಕರ್ಲಭೂಮಿಯ ಹರಳ", 
        "14808": "ನೀರಿಲ್ಲದ ಭೂಮಿಯ ಹರಳು ತಂದು", 
        "14809": "ಸುಟ್ಟ ಸುಣ್ಣವ ಹಸಿಮಡಕೆಗೆ ತುಂಬದೆ,", 
        "14810": "ಇರುವ ಮನೆಯ ವಿಸರ್ಜಿಸಿ ನೆರೆಮನೆಯಲ್ಲಿ ಚರಿಸಿ,", 
        "14811": "ಒಮ್ಮನದ ಯೋನಿಯಲ್ಲಿ ಪುಟ್ಟಿದವರು", 
        "14812": "ಪಂಚವರ್ಣದ ಗಾಣಕ್ಕೆ ಎಂಟೇಣಿನ ಕಣಿಯನಿಕ್ಕಿ,", 
        "14813": "ಪಂಚವರ್ಣದ ಗಾಣ, ಆರೇಣಿನ ಕಣಿ,", 
        "14814": "ಬುದ್ದಲಿಕಿ ಎಣ್ಣೆಯ ಊರೊಳಗಿನ ಶೀಲವಂತರಿಗೆ ಮಾರುವಳು.", 
        "14815": "ಸೌಟೆಣ್ಣೆಯ ಪ್ರೇಮಿಗಳು ಘನ ಎಣ್ಣೆಯ ಪ್ರೇಮವನೆತ್ತಬಲ್ಲರು?", 
        "14816": "ಆನೆ ಕುದುರೆ ವೈಲಿ ಪಾಲಿಕಿ ರೂಢರಾದವರು,", 
        "14817": "ಭವಿಗಳ ನೂಲಪಿಡಿದು ನೆಯ್ದು ವಸ್ತ್ರವ ಮಾರಿ", 
        "14818": "ಕರಿಹಾಸ ಆರುಮಳ ಹೂಡಿ,", 
        "14819": "ಆರೇಣಿನ ಬೆಸೆ, ಒಂಬತ್ತುಹುರಿ ಬೇವ,", 
        "14820": "ಒಮ್ಮನದವರಿಗೆ ಹಣವಿಲ್ಲ;", 
        "14821": "ಹುಟ್ಟಿದ ಶಿಶುವಿಂಗೆ ಸುನತಿ ಇಲ್ಲ;", 
        "14822": "ಬಯಲಭೂಮಿಯಲ್ಲಿ ನಿಂತು ನಮಾಜ ಮಾಡಲು", 
        "14823": "ಸಹೀದರ ಖುರಾನವ ನೋಡಿ ದಾಡಿ ಜವನ ಲೋಚವ ಬಿಟ್ಟು", 
        "14824": "ಸಹೀದರು ಕೊಟ್ಟ ಖುರಾನವ ಮುಂದಿಟ್ಟು", 
        "14825": "ಅಲ್ಲಂಗೆ ದ್ವಯಜ್ಯೋತಿ ಬೆಳಗಿಲ್ಲ.", 
        "14826": "ತೀಸಹಿದರು ಆ ಕರ್ ಬಚ್ಚ್ಯಾಕು ಘರ್‍ಮೇ", 
        "14827": "ಖುದಾ ಜಾತಕು ಬಮ್ಮನ ಆದ್ಮಿಕು ಗರಕಾ ದಂದೇಗನೈ", 
        "14828": "ಏಕ ಪಾವುಮೇ ಶತಯೇಕ ಪಾವು ಗಈಬಾ.", 
        "14829": "ಅರಳಿ ಅತ್ತಿ ಆಲದ ಮರವೆಂಬ", 
        "14830": "ಭೂಲೋಕದಲ್ಲಿ ಪುಟ್ಟಿದ ವೃಕ್ಷ ಪರ್ಣಪಾತ್ರೆಯಲ್ಲಿ", 
        "14831": "ಮೋಟಮರದ ಹಾಟ ಕುಡಿದವರು", 
        "14832": "ಮದನಂಗದ ಗುಹ್ಯದ ಮುದಿಕೋಡಗದ ಹಾಲು", 
        "14833": "ಆರೂ ಇಲ್ಲದ ಅರಣ್ಯದ ಹುಲ್ಲತಂದು,", 
        "14834": "ಸೂರ್ಯವರ್ಣದ ಚಾಪಿ ಬ್ರಹ್ಮಲೋಕವ ನುಂಗಿತ್ತು.", 
        "14835": "ಕರಿಭೂಮಿಯ ಬಿದಿರ ಬೊಂಬವ ಕಡಿದು", 
        "14836": "ಭೂಮಿಯ ಬೆಳೆ ಕಳೆಯದೆ,", 
        "14837": "ಗೊಲ್ಲಂಗೆ ಕಲ್ಲಿನ ಕಷ್ಟ, ಅಗ್ನಿಯ ತಪದ್ಯೋಗಿಗಳ", 
        "14838": "ಬೆಳ್ಳನ್ನವರ ಭಾಷೆಯ ಹಿಂಗಿ", 
        "14839": "ಪಶ್ಚಿಮದೇಶದ ಧಾನ್ಯಕ್ಕೆ ಒಂದು ಸುಂಕ.", 
        "14840": "ಆನೆ ಕುದುರೆ ಒಂಟೆ ನಾಯಿ ಕೋತಿಗಳು", 
        "14841": "ಇರುವೆ ಕುಕ್ಕಟ ವಿಹಂಗ ಮರ್ಕಟ ಗಜ ಶ್ವಾನವೆಂಬ", 
        "14842": "ನೆಲವಿಲ್ಲದ ಭೂಮಿಯಲ್ಲಿ ಪುಟ್ಟಿದ ವೃಕ್ಷದ ಬೇರ ತಂದು", 
        "14843": "ಔಷಧಿಯ ಕೊಂಡು ಬಿಸಿಹಾಲು ಕುಡಿಯದೆ", 
        "14844": "ಹಿರಿಬೇನೆಯನಳಿಯಬೇಕೆಂಬಣ್ಣಗಳು", 
        "14845": "ಶಿಕಾರಿಯ ಹೋದಲ್ಲಿ ಶುನಿಗಳ ಕೊಂದು", 
        "14846": "ಕರೆಯದೆ ಬಂದವರು ಕೆಲಬರು,", 
        "14847": "ಕೊಂಬಿಲ್ಲದ ಪಶುವಿನ ಮೇಲೆ", 
        "14848": "ಉದಯದ ಉದಕವ", 
        "14849": "ಸರ್ಪನ ಧ್ವನಿಗೆ ಆಕಾಶದ ಟೆಂಗಿನುದಕ ಉಕ್ಕಿ", 
        "14850": "ಲಿಂಗೈಕ್ಯರು ಲಿಂಗೈಕ್ಯರು ಎಂದೆಂಬಿರಿ.", 
        "14851": "ಲಿಂಗೈಕ್ಯಂಗೆ ಕಾಮವಿಲ್ಲ, ಕ್ರೋಧವಿಲ್ಲ,", 
        "14852": "ಶಶಿರವಿಕಿರಣಕೋಟಿ ಮಾಣಿಕದ ದೀಪ್ತಿಯಂತೆ,", 
        "14853": "ಬ್ರಹ್ಮಯ್ಯನವರು ನಿತ್ಯದಲ್ಲಿ ಹಾದರವನಾಡಿ", 
        "14854": "ಹಾದರವನಾಡುವರಿಗೆ ಹನ್ನೆರಡು ಮಂದಿ,", 
        "14855": "ಹಾದರದಲ್ಲಿ ಹುಟ್ಟಿದ ಕೂಸಿಂಗೆ", 
        "14856": "ಹಾದರಗಿತ್ತಿಗೆ ಹಲವು ಶಬ್ದ,", 
        "14857": "ಕಲ್ಯಾಣನಗರದ ಬಸವೇಶ್ವರನ ಪ್ರಮಥಗಣಂಗಳ", 
        "14858": "ಪ್ರಮಥಗಣಂಗಳ ಪ್ರಸಾದಕೊಂಡದಲ್ಲಿ", 
        "14859": "ಅರಸಿಯ ಪುತ್ರನು ಶಿರವಿಲ್ಲದೆ ಐದರಲ್ಲಿರಲು,", 
        "14860": "ಪಂಚಲೋಹವ ಕರಗಿಸಿ ರಸ ಬೆರೆಸಲು", 
        "14861": "ಲೌಕಿಕ ಪಾರಮಾರ್ಥವೆಂಬುಭಯವನು ಪಿಡಿದು", 
        "14862": "ಹೊನ್ನು ಹೆಣ್ಣು ಮಣ್ಣೆಂಬ ತ್ರಿವಿಧಮಲವ ಪಿಡಿದು", 
        "14863": "ಅಂಗಭಾವ ಹಿಂಗಿ, ಲಿಂಗಭಾವ ಮುಂದುಗೊಂಡಿರುವುದೇ", 
        "14864": "ಹಿತ್ತಾಳಿಯ ಭಾಂಡಕ್ಕೆ ಹೊನ್ನು ಕೊಟ್ಟು", 
        "14865": "ಎರಕದ ಭಾಂಡಕ್ಕೆ ತೂಕಿಲ್ಲ, ಹಣವಿಲ್ಲ.", 
        "14866": "ಜ್ಯೋತಿ ಉದಕದಂತೆ ಲಿಂಗೈಕ್ಯರು.", 
        "14867": "ಕರ್ಲಭೂಮಿ, ನೀರಭೂಮಿ, ಹಾಳಭೂಮಿ", 
        "14868": "ಮಾಳಿಯ ಉಪ್ಪ ಬಡವರು ಉಣ್ಣಲಾಗದು.", 
        "14869": "ಮಾಳಿ ಮೂರಳಿದು, ಕಾವಲಿ ಆರಳಿದು,", 
        "14870": "ನೆಲವಿಲ್ಲದ ಭೂಮಿ ಭಾಗವ ಮಾಡಿ", 
        "14871": "ನೆಲೆಯಿಲ್ಲದ ಬಾವಿಗೆ ಕಾವಲಿಲ್ಲದ ಯಾತ,", 
        "14872": "ಸವುಳಭೂಮಿಯ ಉಳ್ಳಿ ಮೂಲಂಗಿ ಬಳ್ಳೊಳ್ಳಿ", 
        "14873": "ಕೆಚ್ಚಿಲ್ಲದ ವೃಕ್ಷಕ್ಕೆ ಫಲವಾದುದೆ ಕಡೆ.", 
        "14874": "ಶೀಲವಂತರ ಶೀಳ ಕೊಳ್ಳದೆ, ಭವಿಗಳ ಶೀಳ ಕೊಂಬುವರು.", 
        "14875": "ಕರಿದು ಕೆಂಪು ಬಿಳಿದು ಮೊದಲಾದ ಬಣ್ಣವನೆದ್ದದೆ", 
        "14876": "ಕರಿಯಬಣ್ಣದ ನೂಲು ಉತ್ಪತ್ಯದವನ ನುಂಗಿತ್ತು.", 
        "14877": "ದೇವರು ದೇವರು ಎಂದು ಬಳಲುತ್ತಿರ್ಪರು", 
        "14878": "ಆಯತಲಿಂಗ ಮತ್ರ್ಯವ ನುಂಗಿತ್ತು.", 
        "14879": "ದೇವರಿಗೆ ಅಗ್ನಿಯಿಲ್ಲ, ಪೂಜಾರಿಗೆ ಜಲವಿಲ್ಲ.", 
        "14880": "ದೇವನಾದಡೆ ಅಗ್ನಿಯ ನುಂಗಬೇಕು.", 
        "14881": "ಗುರು ಗುರುವೆಂದು ಬೊಗಳುತ್ತಿರ್ಪರು", 
        "14882": "ಪಂಚರಸದ ಪಡಿಗೆ", 
        "14883": "ಮಣ್ಣಪಡಿಯಿಂದ ಮತ್ರ್ಯವನಳದು,", 
        "14884": "ತುಡುಗುವ್ಯಾಪಾರದವರಿಗೆ, ಶೀಲವಂತರಿಗೆ", 
        "14885": "ಕೊಟ್ಟ ಪಡಿಯನುಣ್ಣದೆ", 
        "14886": "ನೀರಿಲ್ಲದ ಸಾಗರದಲ್ಲಿರ್ಪ ಮತ್ಸ್ಯಕ್ಕೆ", 
        "14887": "ಸೆಳವು ನೆಲೆಯಿಲ್ಲದ ಮಡುವಿನ", 
        "14888": "ಸೆಳವಿನ ಮಡದ ಮತ್ಸ್ಯವು ವ್ಯಾಧಂಗೆ ವಶ.", 
        "14889": "ಒಂದನೆಯ ಗಾಳಕ್ಕೆ ಬ್ರಹ್ಮನ ಕೆಡವಿದೆ.", 
        "14890": "ಕುರುಡ, ಕುಂಟ, ಅಧಮರು ಹಾಕಿದ", 
        "14891": "ಕರ್ಲಭೂಮಿ ಸವುಳ ಭೂಮಿ ಕಲ್ಲು ಭೂಮಿ", 
        "14892": "ಹಸಿಹುಲ್ಲಿನ ರಸವ ಕುಡಿದವರು ಬರುವರು.", 
        "14893": "ಪುಲ್ಲ ಕೊಯ್ಯದೆ ಹೊಡೆಯ ಹಿರಿದು", 
        "14894": "ಅರಸು ಪ್ರಧಾನಿ ಬಂಟರು ವೀರರು", 
        "14895": "ಗಂಡನಿಲ್ಲದ ಸ್ತ್ರೀಯರು ಗಂಡನ ಮದುವೆಯಾಗಿ,", 
        "14896": "ಪುರುಷನನಗಲಿ ಪರಪುರುಷನ ನೆನೆದರೆ", 
        "14897": "ಬ್ರಹ್ಮಂಗೆ ಕಾಲಕೊಟ್ಟು, ವಿಷ್ಣುವಿಂಗೆ ಕೈಯಕೊಟ್ಟು,", 
        "14898": "ಒಬ್ಬನ ಮಾತಕೊಟ್ಟು ಬಾಹ್ಯದಲ್ಲಿಟ್ಟು ಕೊಂದೆ.", 
        "14899": "ಒಳ್ಳೆಯವರ ಸಂಗವ ಮಾಡಿ", 
        "14900": "ಹುಟ್ಟಲಿಲ್ಲದ ಮರದಲ್ಲಿ ಹಾರದ ಪಕ್ಷಿ ಗೂಡನಿಕ್ಕಿ,", 
        "14901": "ಅಣ್ಣಗಳು ಅಕ್ಕನ ಕೂಡ", 
        "14902": "ಅರಣ್ಯದ ಬೆಟ್ಟದಲ್ಲಿ ಮೂವರು ಕಳ್ಳರು ಮನೆಮಾಡಿ", 
        "14903": "ಅಕ್ಕನಪುರುಷನ ಬಲದಿಂದ", 
        "14904": "ಪಶುವಿಗೆ ಕರುವಿನ ಮಮಕಾರ,", 
        "14905": "ನಿನ್ನಿನವರೂ ಇಂದಿನವರೂ ನಾಳಿನವರೂ ಹೋದ ದಾರಿ", 
        "14906": "ಆಳಿಂಗೆ ವರುಷದ ಚಿಂತೆ.", 
        "14907": "ಎನ್ನ ಕಾಳಿಯ ಧ್ವನಿಕೇಳಿ ಬ್ರಹ್ಮ ಬೆದರಿದ,", 
        "14908": "ಕಾಳಿಯ ಧ್ವನಿ ಒಳ್ಳೆಯವರು ಕೇಳರು, ಬಡವರು ಕೇಳ್ವರು.", 
        "14909": "ಗೊಲ್ಲನ ಕೊಳಲಧ್ವನಿ ಕೇಳಿ", 
        "14910": "ಆರುಕಾಲವನ ಸುದ್ದಿ ಉಭಯ ಜಿಹ್ವೆಯನುಳ್ಳವರಿಗೆ ಹೇಳಿದಡೆ", 
        "14911": "ಪಂಚವರ್ಣದ ಗೋವಿನ ಹಾಲ ಕರದು", 
        "14912": "ಎಮ್ಮಿಯ ಕೊಂದು ಹಾಲು ಕರದು,", 
        "14913": "ನೀರಿಲ್ಲದೆ ಬೆಳೆದ ಹುಲ್ಲು ಮೆಯ್ದ", 
        "14914": "ಹಸಿಹುಲ್ಲು ಮೆಯ್ದ ಪಶುವಿಗೆ", 
        "14915": "ಸಾಯದ ಪಶುವಿಂಗೆ ಒಂದು ಗೂಟ,", 
        "14916": "ಸಾಯದವರು ಸತ್ತವರ ಸುದ್ದಿಯ ಕೇಳಿ,", 
        "14917": "ಹಗಲೋದಿ ಬ್ರಹ್ಮಂಗೆ ಪೇಳಿದೆ,", 
        "14918": "ಆಧಾರ, ಸ್ವಾಧಿಷ್ಠಾನ, ಮಣಿಪೂರಕ,", 
        "14919": "ಓದಿದರೆ ಓದಬಹುದು ಅಧಮ ಮೂಢರಮುಂದೆ ;", 
        "14920": "ಮುಡಿಮೂಡದ ಮುನ್ನ ಪುರುಷನ ಹುಡುಕಲೇಕೆ ?", 
        "14921": "ಪುರುಷನ ಮರೆದು ಮಾವನ ಪೇಳುವರು.", 
        "14922": "ಪಾಕವಾದನ್ನವನುಣ್ಣದೆ ಪಾಕವ ಮಾಡುಂಬೆವೆಂಬರು.", 
        "14923": "ಹುಟ್ಟಿ ಸಾಯಬೇಕೆಂಬಾತ ಶರಣನಲ್ಲ.", 
        "14924": "ತೊಟ್ಟ ಕಾಶಿಯ ಕಳೆಯದೆ ಸುಟ್ಟು,", 
        "14925": "ಒಂದು ಟಗರು ಮೂರು ಸೊನ್ನಿಗೆ ಹತವಾಗಿ,", 
        "14926": "ಸತ್ತವರ ಸಂಗ ಸಾಯದವರು ಮಾಡಿ", 
        "14927": "ಒಕ್ಕಲಿಗನ ಸಂಗದಲ್ಲಿದ್ದವರಿಗೆ ಬಡತನವಿಲ್ಲ.", 
        "14928": "ಮುದ್ರಿಕಿಪಶುವಿಂಗೆ ಭಯವಿಲ್ಲ.", 
        "14929": "ಸತ್ತವರ ಸುದ್ದಿ ಇದ್ದವರು ಕೇಳಿ", 
        "14930": "ಹೊಲತಿ ಹೊಲೆಯನು ಕೂಡಿ", 
        "14931": "ಸತ್ತವರು ಸಾಯದವರ ನುಂಗಿ, ಸಾಯದವರು ಸತ್ತವರ ನುಂಗಿ,", 
        "14932": "ಕಂಡವರಿಗೆ ಒಂದು, ಕಾಣದವರಿಗೆ ಎರಡು.", 
        "14933": "ಸತ್ತ ಹೆಣದ ಮುಂದೆ ಅತ್ತವರು", 
        "14934": "ಕರೆದವರಿಗೆ ಕೊಡುವರು;", 
        "14935": "ಸತ್ತವರ ಮನೆಗೆ ಸಾಯದವರು ಪೆÇೀಗಿ"
    }
}