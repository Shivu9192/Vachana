        "10365": "ಮನರಥದ ಮೇಲೆ ನಿಂದುದ ಭೇದಿಸಬಾರದಯ್ಯ,", 
        "10366": "ಮುಟ್ಟದ ಮುನ್ನವೆ ಹುಟ್ಟಿ", 
        "10367": "ಮುನ್ನವೆ ಮೂರರ ಹಂಬಲ ಹರಿದು", 
        "10368": "ಮುನ್ನಿನ ಜನ್ಮದ ಭಕ್ತಿ ಹೋಯಿತ್ತಾಗಿ ಬಾರದು.", 
        "10369": "ಮೂದಲೆಯ ಮಾತು ಕೂದಲ ಕೀಲಿನಂತಲ್ಲದರಿಯಬಾರದು.", 
        "10370": "ಯತಿಗಳ ವ್ರತಿಗಳ ಧೃತಿಗೆಡಿಸಿತ್ತು ಮಾಯೆ.", 
        "10371": "ಲೋಕಗತಿ ಬೇರೆ ತನ್ನ ಗತಿ ಬೇರೆ ನಿಜಗುಣನಿಗೆ,", 
        "10372": "ಲೋಕವ ತಾ ಹೇಸಿದ ಬಳಿಕ", 
        "10373": "ವಸುಧೆಯೊಳಗುಬ್ಬಸವಾದ ಪ್ರಾಣಿಗಳನೆಲ್ಲ", 
        "10374": "ವ್ಯಾಸ ವಾಲ್ಮೀಕಿ ಶುಕನು", 
        "10375": "ವಿಧಿಯ ಮೀರಿ ಮೈದೋರುವ", 
        "10376": "ವಿಧಿ ನಿಷೇಧ ಜನನಿ ಜನಕ ಕುಲಗೋತ್ರ", 
        "10377": "ವಿಷಮದಶ ಪದನವನು ವಶಕ್ಕೆ ತಂದು ನಿಲಿಸಿ", 
        "10378": "ವಿಷಯ ವಿರಕ್ತನಾಗಿ ದೃಢ ಭಕ್ತಿಯಿಂದ", 
        "10379": "ವೇದವಾವುದು, ವೇದ್ಯವಾವುದು?", 
        "10380": "ಶಬ್ದಾದಿ ಸಕಲ ವಿಷಯಗುಣಂಗಳ ನೀ ರಾಗ ವಿರಾಗಕಾರಣವಾಗಿ", 
        "10381": "ಶಾಂತಸುಖವಿರಲೊಲ್ಲದೆ ನೀನೇಕೆ ಭ್ರಮಿಸುವೆಯೊ?", 
        "10382": "ಶಿವಗಣಂಗಳ ಬರವ ಕಂಡು ಕೈಮುಗಿದು ಅಂಜಲೇಬೇಕು.", 
        "10383": "ಶಿರವ ಸೀರೆಯ ಕರವ ಕಂಗಳ ಶಿಶುವನಿಕ್ಕಿಯೆರದರು.", 
        "10384": "ಸಕಲ ಸಂಸಾರದ ಆಸುರತೆಯನರಿದು, ಅಹಂಮಮತೆಯನಳಿದು,", 
        "10385": "ಸಂಕಲ್ಪವೆಂಬ ಸಂಕಲೆ ಹರಿಯಿತ್ತಲ್ಲಾ!", 
        "10386": "ಸತ್ಯ ಜ್ಞಾನ ಪರಿಪೂರ್ಣಾನಂದ ಪರಮವಿದ್ಯೆಯದೇಕೊ?", 
        "10387": "ಸನ್ಮಣಿ ಎಂತು ಹೊದ್ಧಿದಂತೆ ತೋರುತಿಪ್ಪುದು?", 
        "10388": "ಸರ್ವಂ ಖಲ್ವಿದಂ ಬ್ರಹ್ಮ ಎಂದುದು ವೇದ.", 
        "10389": "ಸಂಸಾರವೆಂಬ ಅತ್ತೆಗೆ ನಿರ್ಬುದ್ಧಿ ಸೊಸೆಯೊಬ್ಬಳು,", 
        "10390": "ಸಂಸಾರ ಮಾಯೆ ಅತಿದುಃಖ ಹೊಲ್ಲೆಂದು,", 
        "10391": "ಸಂಸಾರವೆಂಬ ಹೇರಡವಿಯ ಅಂಧಕಾರದ,", 
        "10392": "ಸಮವೇದ್ಯನು ಮನವೇನ ಬಯಸಿತ್ತು?", 
        "10393": "ಸಮುದ್ರದಿಂದಾದ ತೆರೆಗಳು ಸಮುದ್ರದೊಳಗಡಗುತ್ತ", 
        "10394": "ಸಾಕಾರಸ್ವರೂಪಿ ಸಂಗನಬಸವಣ್ಣ ನೋಡಾ.", 
        "10395": "ಸಾವು ತಡವಲ್ಲ, ನರಕ ದೂರವಲ್ಲ,", 
        "10396": "ಸ್ಥಾಣು ಚೋರರಜ್ಜುಸರ್ಪ ಮೃಗ ತೃಷ್ಣೆಕನಸು,", 
        "10397": "ಸುರರಿಗೆ ನಿರಂತರ ಜಾಗ್ರ,", 
        "10398": "ಹತ್ತರ ಹಸುಗೆಯಲಚ್ಚಾದ ಪರಿಗಳ", 
        "10399": "ಹಾವ ಹೊತ್ತುಕೊಂಡು ಹೋಗುತ್ತ ಹಾವಾಡಿಗ", 
        "10400": "ಹಿರಿದಪ್ಪ ಮಾಯೆಯನೆಂತು ಕೆಡಿಸುವೆನೆಂದು", 
        "10401": "ಹುಸಿಯ ವ್ಯಾಧನು ನಿಜದಿಂದ ಕ್ಷತ್ರಿಯನೆಂದು ಬೋಧಿಸಲು", 
        "10402": "ಹುಲ್ಲ ಮನುಷ್ಯನ ಕಂಡು ಹುಲ್ಲೆ ತಾ ಬೆದರುವಂತೆ", 
        "10403": "ಹೆಡತಲೆಯ ಮಾತ ಬಲ್ಲಡೆ ಪ್ರಸಾದಿ.", 
        "10404": "ಹೆರದ ಮುನ್ನವೆ ಹುಟ್ಟಿ", 
        "10405": "ಹೇಳಿದ್ದ ಕೇಳಲೊಂದು ಶಬುದವಾಯಿತ್ತಯ್ಯ.", 
        "10406": "ಹೊತ್ತುದ ಹುಸಿಮಾಡಲರಿಯದೆ ದುಃಖ ದುರ್ಮತಿ ಬಿಡದು.", 
        "10407": "ಹಂದಿ ಹೈನವಲ್ಲ! ಸಂಸಾರಿ ಜಂಗಮವಲ್ಲ"
    }
}