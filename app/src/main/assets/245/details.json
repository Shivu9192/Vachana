    "vachana": {
        "15174": "ಷಡ್ವಿಧ ಚಕ್ರಂಗಳಿಲ್ಲದಂದು,", 
        "15175": "ಪೃಥ್ವಿ ಅಪ್ಪುಗಳಿಲ್ಲದಂದು,", 
        "15176": "ಅಹಂಕಾರಗಳಿಲ್ಲದಂದು,", 
        "15177": "ಸತ್ತು ಚಿತ್ತಾನಂದ ನಿತ್ಯಪರಿಪೂರ್ಣವೆಂಬ ಐದಂಗವಿಲ್ಲದಂದು,", 
        "15178": "ರೇಚಕ ಪೂರಕಂಗಳಿಲ್ಲದಂದು,", 
        "15179": "ಬದ್ಧಜ್ಞಾನಿಗಳು ಇಲ್ಲದಂದು,", 
        "15180": "ನಕಾರ ಮಕಾರಗಳಿಲ್ಲದಂದು,", 
        "15181": "ಲೋಕ ಲೌಕಿಕಂಗಳಿಲ್ಲದಂದು,", 
        "15182": "ಬ್ರಹ್ಮ ವಿಷ್ಣುಗಳಿಲ್ಲದಂದು,", 
        "15183": "ಮಹಾಘನ ಅಪರಂಪರ ಅಗಮ್ಯ ಅಗೋಚರ", 
        "15184": "ಪರಮಚಿತ್ಕಲೆಯಲ್ಲಿ ಪರಿಣಾಮವನೆಯ್ದಿದ ಮಹಾಶರಣನು,", 
        "15185": "ನಾದಬಿಂದುಕಳಾತೀತಲಿಂಗವನು,", 
        "15186": "ಸಪ್ತದ್ವೀಪಂಗಳ ರಚಿಸಿದ ಸಾವಿರೆಸಳಮುತ್ತಿನಮಂಟಪದೊಳಗೆ", 
        "15187": "ಆದಿ ಅನಾದಿಗಳಿಲ್ಲದಂದು,", 
        "15188": "ಪರಮಾನಂದಪ್ರಭೆಯಲ್ಲಿ ನಿರ್ಮಲವಾದ ಶರಣನು,", 
        "15189": "ದೇಶದ ಮುಂದೆ ನಿರ್ದೇಶವ ಕಂಡೆನಯ್ಯ.", 
        "15190": "ಮಂಡಲವನಳಿದು ನಿರ್ಮಂಡಲವಾದ ಶರಣನು,", 
        "15191": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರ ಈಶ್ವರ ಸದಾಶಿವ ಪರಶಿವರೆಂಬ", 
        "15192": "ಸತ್ಯಜ್ಞಾನದಿಂದ ನಿರ್ಮಲವಾದ ಮಹಾಶರಣನು,", 
        "15193": "ಅಂಬರದ ಮನೆಯೊಳಗೆ ಸಾವಿರೆಸಳಮಂಟಪವ ಕಂಡೆನಯ್ಯ!", 
        "15194": "ಸದಾಸನ್ನಹಿತವಾದ ಶರಣನು ನಿತ್ಯನಿಜದಾರಂಭಕೆ ಹೋಗಿ", 
        "15195": "ಚಿದ್ರೂಪ ಚಿದಾನಂದ ನಿರಾಮಯ", 
        "15196": "ಬ್ರಹ್ಮರಂಧ್ರದಲ್ಲಿಪ್ಪ ಮಹಾಘನಲಿಂಗವನು", 
        "15197": "ತನುಮನದ ಕೊನೆಯ ಮೇಲೆ ಅಘಟಿತಲಿಂಗವಿಪ್ಪುದು ನೋಡಾ!", 
        "15198": "ಆರು ಮೂರ್ತಿಗಳ ಮೇಲೆ ಮೂರು ದೇವರ ಕಂಡೆನಯ್ಯ!", 
        "15199": "ನಿತ್ಯನಿರಾಳದಲ್ಲಿ ಪರಿಪೂರ್ಣವನೈದಿದ ಮಹಾಶರಣಂಗೆ", 
        "15200": "ಚಿತ್ಪ್ರಕಾಶಲಿಂಗದಲ್ಲಿ ಚಿದಾನಂದವಾದ ಶರಣನು", 
        "15201": "ಪರತತ್ವದಲ್ಲಿ ಬೆರಸಿಪ್ಪ ಭೇದವನು ಸಹಜ ಸಮ್ಯಕ್ಜ್ಞಾನದಿಂದ ತಿಳಿದು,", 
        "15202": "ಪರಮಾನಂದಪ್ರಭೆಯಲ್ಲಿ ಅಖಂಡವಾದ ಶರಣನು,", 
        "15203": "ಭಾವನಿಬ್ಬೆರಗಿನೊಳು ಐಕ್ಯವಾದ ಮಹಾಜ್ಞಾನಿಗಳ ಸಂಗದಿಂದ", 
        "15204": "ಕಿಚ್ಚಿನ ಮನೆಯೊಳಗೆ ಒಬ್ಬ ಮಚ್ಚಗಾರ ಕುಳಿತು,", 
        "15205": "ಸಪ್ತದ್ವೀಪಂಗಳಲ್ಲಿ ಸಪ್ತ ಋಷಿಗಳ ಕಂಡೆನಯ್ಯಾ!", 
        "15206": "ಬರಿಯಬಯಲಲ್ಲಿ ಬರಿದಾದ ಸತಿಯಳ ಕಂಡೆನಯ್ಯಾ!", 
        "15207": "ತಾನು ತಾನಾದ ಭೇದವ ಮಹಾಜ್ಞಾನದಿಂದ ತಿಳಿದು,", 
        "15208": "ನಿರಂಜನಲಿಂಗದಲ್ಲಿ ನಿರಪೇಕ್ಷವಾದ ಶರಣನು,", 
        "15209": "ಚಿತ್ಪ್ರಕಾಶವಾದ ಲಿಂಗದಲ್ಲಿ ಚಿನ್ಮಯವಾದ ಶರಣನು", 
        "15210": "ವಿಮಲಜ್ಞಾನದಿಂದ ಮಹಾಮಹಿಮನಾಗಿ ಪರಕೆ ಪರವನಾಚರಿಸಿ,", 
        "15211": "ಆಚಾರಲಿಂಗ ಗುರುಲಿಂಗ ಶಿವಲಿಂಗ ಜಂಗಮಲಿಂಗ", 
        "15212": "ಭಕ್ತ ಮಹೇಶ್ವರ ಪ್ರಸಾದಿ ಪ್ರಾಣಲಿಂಗಿ ಶರಣೈಕ್ಯರೆಂಬ", 
        "15213": "ಪರಮಾನಂದದ ಪ್ರಭೆಯಲ್ಲಿ ನಿರ್ಮಲವಾದ ಶರಣನ ಸಂಗದಿಂದ", 
        "15214": "ಮಹಾಜ್ಞಾನಿಗಳ ಸಂಗದಿಂದ ವಾಸನಾಧರ್ಮಂಗಳನಳಿದು,", 
        "15215": "ನಿಟಿಲತಟದಲ್ಲಿ ಮಠವ ಮಾಡಿಕೊಂಡು,", 
        "15216": "ಬಟ್ಟಬಯಲಲಿ ಘಟಿತವಾದ ಲಿಂಗವನು", 
        "15217": "ತುಟ್ಟತುದಿಯಲೊಂದು ಬಟ್ಟಬಯಲಾದ ಹಣ್ಣ", 
        "15218": "ನಾದಬಿಂದುಕಳಾತೀತವನು ಗರ್ಭೀಕರಿಸಿಕೊಂಡು ಇರಲ್ಪಟ್ಟಂಥಾ", 
        "15219": "ಆಟವ ಮೀರಿದ, ನೀರಾಟವನಾಡಿದ,", 
        "15220": "ಲಕ್ಷವ ನೋಡಿದರೇನಯ್ಯ? ಲಕ್ಷವ ಹೇಳಿದರೇನಯ್ಯ?", 
        "15221": "ವೇದವ ನೋಡುವಣ್ಣಗಳ ವಾದಿಸಿತ್ತು ಮಾಯೆ.", 
        "15222": "ಆಕಾಶದ ಮೇಲೆ ಏಕಾಂತ ಸೂಳೆಯ ಕಂಡೆನಯ್ಯ!", 
        "15223": "ಆದಿಯಲ್ಲಿ ಉದಯವಾದ ದೇವನು,", 
        "15224": "ಆರು ಕಂಬದ ಶಿವಾಲಯದ ಮೇಲೆ", 
        "15225": "ದೇಶದ ಮೇಲೆ ಒಂದು ಪರಿಪರಿಯ ಬಣ್ಣದ ಪಕ್ಷಿಯ ಕಂಡೆನಯ್ಯ.", 
        "15226": "ಊರೊಳಗೊಬ್ಬ ನಾರಿಯ ಕಂಡೆನಯ್ಯ!", 
        "15227": "ಪ್ರಥಮಕಾಲದಲ್ಲಿ ನಿರಂಜನಗಣೇಶ್ವರನ ಸಂಗದಿಂದ", 
        "15228": "ಪರಮಾನಂದಸುಖದಲ್ಲಿ ಪರಿಣಾಮವನೆಯ್ದಿದ", 
        "15229": "ಲಿಂಗಸೌಖ್ಯದಲ್ಲಿ ಪರಿಪೂರ್ಣವಾದ ಶರಣನ ಅಂತರಂಗದಲ್ಲಿ", 
        "15230": "ಲೋಕ ಲೌಕಿಕದ ರಂಜನೆಗಳಿಲ್ಲದ", 
        "15231": "ಅಮಲಬ್ರಹ್ಮದಲ್ಲಿ ಸಕಲ ಬ್ರಹ್ಮಾಂಡಂಗಳು ಅಡಗಿಪ್ಪವಯ್ಯ.", 
        "15232": "ಪಿಂಡ ಬ್ರಹ್ಮಾಂಡಕೆ ನಿವಾಸವಾದ ಅಪರಂಪರಲಿಂಗದಲ್ಲಿ", 
        "15233": "ಪರಶಿವತತ್ವದಲ್ಲಿ ಪರಿಪೂರ್ಣವಾದ ಮಹಾಶರಣನು,", 
        "15234": "ಶಿವಾನುಭಾವಿಗಳ ಸಂಗದಿಂದ ಮಹಾಸುಖಿಯಾಗಿರ್ದೆನಯ್ಯ.", 
        "15235": "ಪಂಚತತ್ವದ ಮೇಲೆ ನಿತ್ಯಪರತತ್ವವ ಕಂಡು ನಿರ್ಲೇಪಕನಾದ ಶರಣನು", 
        "15236": "ಅಖಿಳಕೋಟಿ ಬ್ರಹ್ಮಾಂಡಗಳಲ್ಲಿ ಪರಾತ್ಪರೈಕ್ಯವಾದ ಲಿಂಗವು", 
        "15237": "ಮೊರಡಿಯ ಮೇಲೆ, ಹಾರುವ ಗರುಡನ ಕಂಡೆ ನೋಡಾ!", 
        "15238": "ಬೆಟ್ಟದ ತುದಿಯ ಮೇಲೆ ಉರಿವ ಕಂಬವ ಕಂಡೆ ನೋಡಾ!", 
        "15239": "ತನ್ನಂತರಂಗದಲ್ಲಿ ಸಹಜ ಸಮ್ಯಕ್ಜ್ಞಾನವನರಿತು", 
        "15240": "ಅಂಬರದ ಮನೆಯೊಳಗೆ ಸ್ವಯಂಭೂಲಿಂಗವ ಕಂಡೆ ನೋಡಾ!", 
        "15241": "ಪರಮಚಿದಂಶಿಕದಲ್ಲಿ ತತ್ಪರನಾದ ಶರಣನು,", 
        "15242": "ಚಿತ್ತಸುಖದಲ್ಲಿ ಪರಿಣಾಮವನೈದಿ", 
        "15243": "ಓಂಕಾರವೆಂಬ ಮೂಲಮಂತ್ರದಲ್ಲಿ", 
        "15244": "ಐದು ಮೇರುವೆಯ ಮೇಲೆ ಪರಮಲಿಂಗವ ಕಂಡೆನಯ್ಯ!", 
        "15245": "ಹೃದಯಕಮಲದಲ್ಲಿಪ್ಪ ಲಿಂಗವ ಸುಮನಜ್ಞಾನದಿಂದ ತಿಳಿದು,", 
        "15246": "ಸಾವಿರಕಂಬದ ಮುತ್ತಿನ ಗದ್ದುಗೆಯ ಮೇಲೆ", 
        "15247": "ನವನಾಳಪಟ್ಟಣದೊಳಗೆ ನವಸ್ಥಳವ ಕಂಡೆನಯ್ಯ!", 
        "15248": "ಮೂಲಪ್ರಣವದಲ್ಲಿ ಅಮೃತಸಾರಲಿಂಗವ ಕಂಡೆನಯ್ಯ!", 
        "15249": "ನಾದಬಿಂದುಕಳಾತೀತಲಿಂಗವನು ಮಹಜ್ಞಾನದಿಂದ ತಿಳಿದು,", 
        "15250": "ಆದಿ ಅನಾದಿ ಇಲ್ಲದಂದು, ಸುರಾಳ ನಿರಾಳ ಇಲ್ಲದಂದು,", 
        "15251": "ಬ್ರಹ್ಮವಿಷ್ಣುರುದ್ರಾದಿಗಳಿಲ್ಲದಂದು,", 
        "15252": "ನಾಮರೂಪಕ್ರಿಯೆಯಿಲ್ಲದ ಮೂರ್ತಿಯೊಬ್ಬ.", 
        "15253": "ಪ್ರಥಮಕಾಲದಲ್ಲಿ ಅನಾದಿ ಜಂಗಮವು", 
        "15254": "ಒಬ್ಬ ಸಹಜಗಳ್ಳನು ನಿಟಿಲಮುಂದಳ ಚಾವಡಿಯಲ್ಲಿ ನಿಂದು", 
        "15255": "ಕಾಯವಿಲ್ಲದ ಹಂಸನು ಗೂಡನಿಕ್ಕುವದ ಕಂಡೆನಯ್ಯ.", 
        "15256": "ಆದಿಯಲ್ಲಿ ಒಬ್ಬ ಶಿವಶರಣನು", 
        "15257": "ತನ್ನಿಂದ ತಾನಾದವನು, ಉನ್ಮನಿಯ ಬಾಗಿಲ ಮುಂದೆ ನಿಂದು", 
        "15258": "ಓಂ ನಮೋ ಶಿವಾಯವೆಂಬ ಮಂತ್ರವು ನೆನಹಿಂಗೆ ಬಾರದಂದು.", 
        "15259": "ನೊಸಲಕಣ್ಣು, ಪಂಚಮುಖ, ದಶಭುಜ,", 
        "15260": "ಆದಿಯ ಸಂಗದಿಂದ ಆದವನಲ್ಲ,", 
        "15261": "ಅಗ್ನಿಗಿರಿಯ ಪಟ್ಟಣದ ಚಂದ್ರಗಿರಿಯ ಪಟ್ಟಣದ ನಡುವೆ", 
        "15262": "ಸರ್ವಾಂಗಪಟ್ಟಣದೊಳಗೆ ಐದು ಮೊದಲಗಿತ್ತೇರಿಗೆ", 
        "15263": "ಬೇರು ಇಲ್ಲದ ಮರ ಹೋಗಿ ಆದಿಯ ಸೇರಿತ್ತಯ್ಯ,", 
        "15264": "ಒಂದು ಮುಳ್ಳುಮೊನೆಯ ಮೇಲೆ", 
        "15265": "ಆರು ನೆಲೆಯ ಮಂಟಪದೊಳಗೆ ಸಾರುತಿರ್ದವಯ್ಯ ಶ್ರುತಿಗಳು.", 
        "15266": "ದೇಗುಲದೊಳಗಣ ಲಿಂಗಕ್ಕೆ ಒಂಬತ್ತು ಶಿಖರ,", 
        "15267": "ಅಂತರಂಗದ ಸುಳುವಿನ ಭೇದವ ಚಿತ್ತವೆಂಬ ಹಸ್ತದಲ್ಲಿ ಹಿಡಿದು,", 
        "15268": "ಊರೊಳಗಣ ಸೂಳೆಯ ಕರೆದು", 
        "15269": "ನಡುವಿಲ್ಲದ ಬಾಲೆಯು ಕಣ್ಣಿಲ್ಲದ ಅಂಧಕನ ಕೂಡಿಕೊಂಡು", 
        "15270": "ಅಂಗಡಿಯ ರಾಜಬೀದಿಯಲ್ಲಿ ಶಿಶುವಿಪ್ಪುದ ಕಂಡೆನಯ್ಯ.", 
        "15271": "ಹರಿತವರ್ಣದ ಮರಕ್ಕೆ ಶ್ವೇತವರ್ಣದ ಕೊಂಬೆಗಳು ಹುಟ್ಟಿದವು ನೋಡಾ!", 
        "15272": "ಭಾವಬತ್ತಲೆಯಲ್ಲಿ ನಿರಾಳಕೋಣನಿಪ್ಪುದು ನೋಡಾ !", 
        "15273": "ಉಚ್ಫ್ವಾಸ ನಿಶ್ವಾಸಂಗಳ ಬ್ರಹ್ಮರಂಧ್ರದ ತನುಮನದ ಕೊನೆಯಲ್ಲಿ ಹಿಡಿದು", 
        "15274": "ವಾಚಾತೀತ ಮನೋತೀತ ಭಾವಾತೀತ", 
        "15275": "ಆಕಾರನಿರಾಕಾರವಿಲ್ಲದಂದು,", 
        "15276": "ಆನೆ ಮೊದಲು ಇರುವೆ ಕಡೆ ಎಂಬತ್ತುನಾಲ್ಕುಲಕ್ಷ ಜೀವರಾಶಿಯಲ್ಲಿ", 
        "15277": "ಹದಿನಾರು ಎಸಳ ಸ್ಥಾವರಗದ್ದುಗೆಯಿಂದತ್ತತ್ತ,", 
        "15278": "ಒಬ್ಬಳ ಬಸಿರಲಿ ಶಿಶುವು ಹುಟ್ಟಿ,", 
        "15279": "ಒಬ್ಬಳ ಸಂಗದಿಂದ ಐವರು ಮೊದಲಗಿತ್ತೇರು ಬಂದ ಬೆಡಗ ನೋಡಾ!", 
        "15280": "ತಂದೆಯೊಡನೆ ಒಬ್ಬ ಮಗ ಹುಟ್ಟಿ ಐವರ ಸಂಗವ ಮಾಡುತ್ತಿರಲು,", 
        "15281": "ಊರೊಳಗಣ ಕಪ್ಪೆ ಮತ್ತೆ ಸರ್ಪನ ನುಂಗುವುದ ಕಂಡೆನಯ್ಯ!", 
        "15282": "ಸಹಸ್ರದಳಕಮಲಮಂಟಪದಲ್ಲಿ ಓಂಯೆಂಬ ತಾಯಿ", 
        "15283": "ಮೂರು ಬಾಯುಳ್ಳ ಪಕ್ಷಿಂಗೆ ಆರು ಕಾಲುಗಳುಂಟು,", 
        "15284": "ಕೋಳಿಯ ಧ್ವನಿಯ ಕೇಳಿ ಒಬ್ಬಳು", 
        "15285": "ಪೂರ್ವದಲ್ಲಿ ಓಂಕಾರವೆಂಬ ಲಿಂಗದ ಸಂಗದಿಂದ", 
        "15286": "ಆದಿಯಲ್ಲಿ ಒಂದು ಲಿಂಗ ಬೇರೆ ಮೃದಂಗವ ನುಡಿಸಾಡುತ್ತಿರಲು", 
        "15287": "ಹುಲ್ಲುಮಾರುವ ಹಾವಾಡಿಗಂಗೆ", 
        "15288": "ಓಂ ಪ್ರಥಮದಲ್ಲಿ ಲಜ್ಜೆಗೆಟ್ಟು ಕಳ್ಳನು ಐವರ ಕೂಡಿಕೊಂಡು", 
        "15289": "ತನ್ನೊಳಗೆ ಮಹಾಲಿಂಗವಿಪ್ಪ ಸುಳುವಿನ ಭೇದವ", 
        "15290": "ನಾನಾ ದೇಶವ ತಿರುಗಿ ಒಂದು ಮುಳ್ಳುಮೊನೆಯ ಮೇಲೆ", 
        "15291": "ಕಾಡಾರಣ್ಯದೊಳಗೆ ಒಬ್ಬ ಸೂಳೆ ಕರೆದು", 
        "15292": "ಮೇರುವೆಯ ಗುಡಿಯಲ್ಲಿ ಒಬ್ಬ ಪುರುಷನು", 
        "15293": "ಕೋಳಿ ಕೂಗಿದ ಕೂಗ ಕೇಳಿ ಒಬ್ಬ ತಳವಾರನು", 
        "15294": "ವಿಶ್ವತೋಬ್ರಹ್ಮಾಂಡವನೊಳಕೊಂಡು ಇರ್ದ ಲಿಂಗವ", 
        "15295": "ಮುಳ್ಳುಮೊನೆಯ ಕೆಳಗೆ ಒಂದೂರು ಹುಟ್ಟಿದುದ ಕಂಡೆನಯ್ಯ.", 
        "15296": "ಧರೆಯಾಕಾಶವೆಂಬ ಅಂಡವ", 
        "15297": "ಒಂಬತ್ತು ಮನೆಯೊಳಗೆ ಸುಳಿದಾಡುವ ಮಾನವಂಗೆ", 
        "15298": "ಆಕಾಶವೆಂಬ ಲಿಂಗಕ್ಕೆ ಐದು ಮುಖಂಗಳು ತೋರಿದವು ನೋಡಾ !", 
        "15299": "ಆತ್ಮನೆಂಬ ಅಂಗಕ್ಕೆ ನಿರಾತ್ಮನೆಂಬ ಲಿಂಗವು", 
        "15300": "ಇಪ್ಪತ್ತೈದು ಗ್ರಾಮದ ಮುಂದೆ", 
        "15301": "ತನ್ನೊಳಿಹ ಮೂರು ಲೋಕವ ನುಂಗಿ", 
        "15302": "ಆರು ಕೇರಿಯ ಮುಂದೆ ಮೂರು ಗ್ರಾಮವ ಕಂಡೆನಯ್ಯ.", 
        "15303": "ಆರು ನೆಲೆಯ ಮಂಟಪದ ಮುಂದೆ", 
        "15304": "ವಸ್ತು ವಸ್ತುವೆಂದು ಹೆಸರಿಟ್ಟು ನುಡಿವ ಅಣ್ಣಗಳು ನೀವು ಕೇಳಿರಯ್ಯ.", 
        "15305": "ಊರೊಳಗೆ ಒಬ್ಬ ಮಾನವನ ಕಂಡೆನಯ್ಯ,", 
        "15306": "ಬಯಲಲ್ಲಿ ಒಂದು ಪಕ್ಷಿ ಗೂಡನಿಕ್ಕುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15307": "ಐವತ್ತೆರಡು ಎಸಳ ತಾವರೆಯ ಗದ್ದುಗೆಯ ಮೇಲೆ", 
        "15308": "ಐದು ಕೇರಿಯ ಮುಂದೆ ಒಂದು ಗ್ರಾಮವ ಕಂಡೆನಯ್ಯ,", 
        "15309": "ಆದಿಯ ಸಂಗದಿಂದ ಮೂವರು ಹುಟ್ಟಿ,", 
        "15310": "ಒಂದು ಕಪ್ಪೆ ನವಗ್ರಹಗಳ ನುಂಗಿ ಕೂಗುತ್ತಿದೆ ನೋಡಾ.", 
        "15311": "ಒಂದು ಲಿಂಗದ ಸಂಗದಿಂದ ಮೂವರು ಪುರುಷರು ಹುಟ್ಟಿದರು ನೋಡಾ!", 
        "15312": "ಒಂಬತ್ತು ಬಾಗಿಲ ತ್ರಿಪುರದ ಮುಂದೆ", 
        "15313": "ಸಂಗಸಂಯೋಗವೆಂಬ ಪಟ್ಟಣದಲ್ಲಿ", 
        "15314": "ಊರ ಮುಂದಳ ಕಡೆಯ ಬಾಗಿಲಲ್ಲಿ", 
        "15315": "ತನ್ನೊಳಗಿಪ್ಪ ಲಿಂಗದ ಭೇದವನು ಮಹಾಜ್ಞಾನದಿಂದ ತಿಳಿದು,", 
        "15316": "ಒಬ್ಬ ಸತಿಯಳು ಪಂಚಮುಖದ ಸರ್ಪನ ಸಾಕುವುದ ಕಂಡನಯ್ಯ.", 
        "15317": "ಒಬ್ಬ ಕನ್ನೆಯು ಐದು ಮುಖದ ಪಕ್ಷಿಯ ಸಂಗವ ಮಾಡಲು", 
        "15318": "ಪೃಥ್ವಿ ಸಲಿಲ ಪಾವಕ ಪವನ ಅಂಬರ ರವಿ ಶಶಿ ಆತ್ಮರೆಂಬ", 
        "15319": "ಬಯಲಿಂಗೆ ಹಾರಿದ ಪಕ್ಷಿಯ ತಳವಾರನೆಸೆಯಲು,", 
        "15320": "ಊರೊಳಗೊಬ್ಬ ನಾರಿಯು", 
        "15321": "ದೇಗುಲದೊಳಗೆ ಒಬ್ಬ ಗಂಡಂಗೆ ಐವರು ಹೆಂಡರಾಗಿಪ್ಪರು ನೋಡಾ.", 
        "15322": "ಐವತ್ತೆರಡು ದೇಶವನು ಒಂದು ಕಪ್ಪೆ ನುಂಗಿ ಕೂಗುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15323": "ಇರುವೆಯ ಒಡಲಲ್ಲಿ ಐವರು ಹುಟ್ಟಿದುದ ಕಂಡೆನಯ್ಯ.", 
        "15324": "ಉರಿಯ ಮೇಲೆ ಹರಿವ ಹಾವ ಕಂಡೆನಯ್ಯ.", 
        "15325": "ಊರೊಳಗೊಬ್ಬ ಹಗಲುಗಳ್ಳನು ಮನೆಯ ಮಾಡಿಕೊಂಡು", 
        "15326": "ಆಧಾರದಲ್ಲಿ ಬ್ರಹ್ಮನೆಂಬ ಮೂರ್ತಿ.", 
        "15327": "ಆದಿಯಲ್ಲಿ ಒಬ್ಬ ಪುರುಷನು ಎಳೆಯ ಗನ್ನಕಿಯ ಕೈವಿಡಿಯಲು", 
        "15328": "ಸರ್ವಮಯವ ಗರ್ಭೀಕರಿಸಿಕೊಂಡಿರ್ದ ಲಿಂಗವ", 
        "15329": "ಮೇಲೆ ಭಾಮಿನಿಯೊಳಗೆ ಮೂರು ರತ್ನವ ಕಂಡೆನು.", 
        "15330": "ಆರು ಬಾಗಿಲ ಮುಂದೆ ಮೂರು ದೇಗುಲವ ಕಂಡೆನಯ್ಯ.", 
        "15331": "ಆಕಾಶದ ಮೇಲೆ ಏಕಾಂತ ಸೂಳೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15332": "ಪರಮಾನಂದಪ್ರಭೆಯೊಳಗೆ ಸುಜ್ಞಾನ ಮಹಾಜ್ಞಾನವೆಂಬ ಸತಿ ಪುರುಷರು", 
        "15333": "ಆದಿಯಲ್ಲಿ ಪರಬ್ರಹ್ಮವೆಂಬ ಮೂರ್ತಿಯು ಪಾತಾಳಲೋಕಕ್ಕೆ ಬಂದು", 
        "15334": "ಪಂಚಮುಖದ ಮೇಲೆ ಮಿಂಚುವ ಶಿವಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15335": "ಅಷ್ಟಕುಲಪರ್ವತದ ಮೇಲೆ ದೃಷ್ಟಲಿಂಗವ ಕಂಡೆ ನೋಡಾ !", 
        "15336": "ಸರ್ವಬ್ರಹ್ಮಾಂಡವ ಗರ್ಭೀಕರಿಸಿಕೊಂಡಿರ್ದ ಮೂರ್ತಿಗೆ", 
        "15337": "ನವಗೃಹದ ಮೇಲೆ ನಿರ್ವಯಲೆಂಬ ಸತಿಯಳು", 
        "15338": "ಬಯಲನೇರಿದ ಪಕ್ಷಿಂಗೆ ಮುಖ ಮೂರು, ಒಡಲಾರು,", 
        "15339": "ಪ್ರಥಮಕಾಲದಲ್ಲಿ ಓಂಕಾರವೆಂಬ ಲಿಂಗಕ್ಕೆ ನಾದಮೂರ್ತಿಯಾದ.", 
        "15340": "ಬ್ರಹ್ಮಂಗೆ ಸರಸ್ವತಿಯಾಗಿ ವಿಷ್ಣುವಂ ಪೂಜೆಯಂ ಮಾಡಲು,", 
        "15341": "ಆತ್ಮನೆಂಬ ಬೆಳಗಿನೊಳು ನಿರಾತ್ಮನೆಂಬ ಉದಯದೋರಿ", 
        "15342": "ಆದಿ ಅಪಾನದಲ್ಲಿ ಸೋಪಾನವಿಡಿದು ನಿಂದು", 
        "15343": "ಆಧಾರವೆಂಬ ಹಾವುಗೆಯ ಮೆಟ್ಟಿ,", 
        "15344": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರಾದಿಗಳನೊಳಗೊಂಡಿದ್ದಂತಾತನೇ ಈಶ್ವರ.", 
        "15345": "ಧರ್ಮವೆಂಬ ದಾರಿಯಲ್ಲಿ ಒಬ್ಬ ಸತಿಯಳು ನಿಂದು", 
        "15346": "ಅಂತಃಕರಣಚತುಷ್ಟಯಂಗಳೆಂಬ", 
        "15347": "ಒಂದು ಗುಡಿಯೊಳಗೆ ಮೂರು ಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15348": "ಕರ್ಮವೆಂಬ ಕತ್ತಲೆಯಲ್ಲಿ ಒಬ್ಬ ಮಾನವನು", 
        "15349": "ಆಕಾರ ನಿರಾಕಾರವಿಲ್ಲದಂದಿನ", 
        "15350": "ಒಂಬತ್ತು ಬಾಗಿಲ ಮನೆಯೊಳಗೆ ಆರು ಮೂರು ಕೋಣೆಯ ಮುಂದೆ", 
        "15351": "ಆರು ಮೂರು ಕೇರಿಯ ಮುಂದೆ ಸುಳಿದಾಡುವ", 
        "15352": "ಘನದ ಮೇಲೊಂದು ಇರುವೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15353": "ಒಂದು ಲಿಂಗದ ಸಂಗದಿಂದ ಒಬ್ಬ ಭಾಮಿನಿಯು ಹುಟ್ಟಿದಳು ನೋಡಾ!", 
        "15354": "ನವದ್ವಾರಬೀದಿಗಳಲ್ಲಿ ಸುಳಿದಾಡುವ ಹಂಸನ ಕೊರಳಲ್ಲಿ", 
        "15355": "ಉಲುವಡಗಿದ ಮರದ ಮೇಲೆ ಒಬ್ಬ ಸತಿಯಳು ನಿಂದು", 
        "15356": "ಪೃಥ್ವಿಯೊಳಗಣ ಬಾಲಿಂಗೆ", 
        "15357": "ಮೂರು ರತ್ನದ ಒಂದು ಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15358": "ಐದು ಎಲೆಯ ಮಂಟಪದ ಮೇಲೆ", 
        "15359": "ಪ್ರಥಮಕಾಲದಲ್ಲಿ ನಿರವಯನೆಂಬ ಸತಿಯಳಂಗದಲ್ಲಿ", 
        "15360": "ಓಂಕಾರವೆಂಬ ಲಿಂಗದಲ್ಲಿ ನುಡಿದಾಡುವನಾರಯ್ಯ ?", 
        "15361": "ನವಸಾಗರದ ಮುಂದೆ ಕಪ್ಪೆ ಕೂಗುತಿದೆ ನೋಡಾ !", 
        "15362": "ಊರೊಳಗಣ ಮಾನವನು", 
        "15363": "ಮಹಾಮೇರುವೆಂಬ ಪಟ್ಟಣದರಸಂಗೆ ಮೂರು ಪ್ರಧಾನಿಗಳು,", 
        "15364": "ನಾನು ಇಲ್ಲದಂದು, ನೀನು ಇಲ್ಲದಂದು,", 
        "15365": "ಪ್ರಣವಪಂಚಾಕ್ಷರಿಯೆಂಬ ಕೊನೆಯ ಮೇಲೆ", 
        "15366": "ನಾದಮೂರ್ತಿಗಳು ತಮ್ಮ ನಿಲವು ಕಾಣಬಾರದಂದಿಗೆ,", 
        "15367": "ಆವು ಹೋಗಿ ಆರುದಿನವೆಂಬುದ ನಾನು ಬಲ್ಲೆನಯ್ಯ.", 
        "15368": "ಒಂಬತ್ತು ತಲೆಯ ಮೇಲೆ ಶಂಭುಲಿಂಗದ ಗುಡಿಯ ಕಂಡೆನಯ್ಯ.", 
        "15369": "ಆರು ಕೋಟೆಯಮೇಲೆ ಮೂರು ಕೊತ್ತಳವ ಕಂಡೆನಯ್ಯ,", 
        "15370": "ಕಾಯವಿಲ್ಲದ ಹೆಂಗೂಸು ಬಸುರಿಲ್ಲದ ಮಗನ ಹಡೆದು", 
        "15371": "ದಶನಾದಗಳು ಇಲ್ಲದಂದು, ಸುನಾದಗಳು ನೆಲೆಗೊಳ್ಳದಂದು,", 
        "15372": "ನಾನೇಯೆಂದು ವಾದಿಸದಂದು, ಓಂಯೆಂದು ಓಂಕರಿಸದಂದು,", 
        "15373": "ಜೀವಾತ್ಮ ಅಂತರಾತ್ಮ ಪರಮಾತ್ಮಯೆಂಬ ಭೇದವ ತಿಳಿದು", 
        "15374": "ನಾದಲಕ್ಷವ ನೋಡಿದರೇನಯ್ಯ ?", 
        "15375": "ನಾದಲಕ್ಷವ ನೋಡಿದೆನೆಂದು, ಬಿಂದುಲಕ್ಷವ ಕಂಡೆನೆಂದು,", 
        "15376": "ಭೂಮಿಯಿಲ್ಲದ ಪೃಥ್ವಿಯ ಕಂಡೆನಯ್ಯ.", 
        "15377": "ಮೂರು ಮುಖದ ಸೂಳೆ, ಆರನೆಯ ಸ್ಥಳದಲ್ಲಿ ನಿಂದು", 
        "15378": "ಅಂಗದೊಳಗೊಂದು ಮಂಗಳದ ಹಕ್ಕಿ ಕುಳಿತಿಪ್ಪುದ ಕಂಡೆನಯ್ಯ.", 
        "15379": "ಧ್ಯಾನ ಮೌನವ ನುಂಗಿರ್ದುದ ಕಂಡೆನಯ್ಯ.", 
        "15380": "ಓಣಿಯೊಳಗೆ ಒಬ್ಬ ಬಾಣತಿ ಕುಳಿತು ಏಳೆಂಟನೆಣಿಸುತಿರ್ಪಳು ನೋಡಾ!", 
        "15381": "ಹೊರಗೆ ನೋಡಿದರೆ ಬಯಲು, ಒಳಗೆ ನೋಡಿದರೆ ಬಯಲು,", 
        "15382": "ಒಳಗೆ ನೋಡಿದರೆ ತಾನೇ, ಹೊರಗೆ ನೋಡಿದರೆ ತಾನೇ,", 
        "15383": "ತಾನು ತಾನಾದಲ್ಲಿ ಒಬ್ಬ ಸತಿಯಳು", 
        "15384": "ಐವರು ಕನ್ನೆಯರು ಮೇರುವೆಯ ಗುಡಿಯ ಹತ್ತಿ", 
        "15385": "ತಾಳಮರದ ಮೇಲೆ ಒಂದು ಕಪ್ಪೆ ಕುಳಿತು", 
        "15386": "ನಾನು ಇಲ್ಲದೆ ನೀನು ಇಲ್ಲದೆ", 
        "15387": "ನಾನೆಂಬ ನಾದಗಳು ಇಲ್ಲದಂದು, ನೀನೆಂಬ ನಿನಾದಗಳಿಲ್ಲದಂದು,", 
        "15388": "ಕಾಡೊಳಗೆ ಒಬ್ಬ ಸೂಳೆ ಕುಳಿತು", 
        "15389": "ಬತ್ತಲೆ ಬಯಲಾದ ಹೆಂಗಸು", 
        "15390": "ಮೂರು ಕೊಂಬೆಯ ಮೇಲೆ,", 
        "15391": "ನವನಾಳಮಧ್ಯದಲ್ಲಿ ಅಷ್ಟದಳಪಟವ ಮೆಟ್ಟಿ ನಿಂದೆನಯ್ಯ.", 
        "15392": "ಬಯಲಿಂಗೆ ಬಯಲಾದ ಮನೆಯಲ್ಲಿ ಮೂವರು ಮಕ್ಕಳು", 
        "15393": "ಜ್ಞಾನ ಜ್ಞಾತೃವಿನ ಮೇಲೆ ಒಂದು ಶಿವಾಲಯವ ಕಂಡೆನಯ್ಯ.", 
        "15394": "ಸ್ವರ್ಗ ಮತ್ರ್ಯ ಪಾತಾಳವ ಒಂದು ಹಂಸ ನುಂಗಿ,", 
        "15395": "ಊರ ಮುಂದಳ ದಾರಿಯಲ್ಲಿ ಸರ್ಪನು", 
        "15396": "ಬಿದುರಿನ ಮಲೆಯೊಳಗೆ ಚದುರೆರೊಂದೈವರು ಇಪ್ಪರು ನೋಡಾ.", 
        "15397": "ಅರುವಿನ ಮನೆಯೊಳಗೆ ಕುರುವಾದ ಸೂಳಿಯು", 
        "15398": "ಅಂಗವೆಂಬ ಗದ್ದುಗೆಯ ಮೇಲೆ", 
        "15399": "ಸ್ವರ್ಗಮತ್ರ್ಯಪಾತಾಳವ ನುಂಗಿತ್ತಯ್ಯ ಒಂದು ಎರಳೆ.", 
        "15400": "ಕತ್ತಲೆಮನೆಯೊಳಗೆ ಒಬ್ಬ ಸೂಳೆ ನಾಲ್ವರ ಕೂಡಿಕೊಂಡು", 
        "15401": "ಒಬ್ಬ ಶಿವಶರಣನು ಶಿವರಾತ್ರಿಯಲ್ಲಿ", 
        "15402": "ನಾಲ್ಕು ಕಾಲುಳ್ಳ ಪಕ್ಷಿಂಗೆ ಆರು ರೆಕ್ಕೆಗಳುಂಟು,", 
        "15403": "ಆರು ಕಂಬದ ಮೇಲೆ ಮೂರು ದೇಗುಲವ ಕಂಡೆನಯ್ಯ.", 
        "15404": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರ ಈಶ್ವರ ಸದಾಶಿವ ಪರಶಿವನೆಂಬ", 
        "15405": "ಇಪ್ಪತ್ತೈದು ಕಂಬದ ಶಿವಾಲಯದೊಳಗೆ", 
        "15406": "ಐದು ತಲೆಯ ಮೇಲೆ ಒಂದು ದೇಗುಲವ ಕಂಡೆನಯ್ಯ.", 
        "15407": "ಜ್ಞಾನವೆಂಬ ಗದ್ದುಗೆಯ ಮೇಲೆ ಸದಮಲವೆಂಬ ಪಾವಡವ ಹಾಸಿ,", 
        "15408": "ಊರ ಮುಂದಳ ಕೇರಿಯಲ್ಲಿ ಐದು ಬೇರಿನ ಮರನ ಕಂಡೆನಯ್ಯ.", 
        "15409": "ಅಷ್ಟ ಮಂದಿಗಳೊಳಗೆ ಪುಟ್ಟಿರ್ದ ಈತನಾರಯ್ಯ?", 
        "15410": "ಅರಿವು ಮರವ ನುಂಗಿತ್ತು ಅಯ್ಯಾ.", 
        "15411": "ಕರ್ಮವೆಂಬ ಕೋಟಲೆಯ ಹರಿದು", 
        "15412": "ಮೇರುವೆಯೊಳಗಣ ಪುರುಷನು", 
        "15413": "ಒಬ್ಬ ಪುರುಷನು ಐದು ಮುಖದ ಸೂಳೆಯ ಸಂಗವ ಮಾಡಲು", 
        "15414": "ಒಂದು ಲಿಂಗ ಮೂರಾಗಿತ್ತು ನೋಡಾ.", 
        "15415": "ಅನಂತಕೋಟಿಬ್ರಹ್ಮಾಂಡವ ನುಂಗಿಕೊಂಡಿರ್ಪುದಯ್ಯ ಒಂದು ಲಿಂಗ.", 
        "15416": "ಧರೆ ಆಕಾಶವಿಲ್ಲದಂದು, ಅಪ್ಪು ವಾಯುಗಳಿಲ್ಲದಂದು,", 
        "15417": "ಮಾತು ಮಥನಗಳಿಲ್ಲದಂದು, ನೋಟ ಬೇಟಗಳಿಲ್ಲದಂದು,", 
        "15418": "ರವಿ ಶಶಿಗಳಿಲ್ಲದಂದು, ಕತ್ತಲೆ ಕಾಳಾಂಧಗಳಿಲ್ಲದಂದು,", 
        "15419": "ಮೂರು ರತ್ನದ ಮೇಲೆ ಒಂದು ಮಂಡೂಕ ಕುಳಿತು", 
        "15420": "ಆರು ಕಂಬದ ದೇಗುಲದ ಮೇಲೆ", 
        "15421": "ಮೂರು ಲೋಕದ ಮೇಲೆ ಒಂದು ಎರಳೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15422": "ಆದಿಯಲ್ಲಿ ಒಬ್ಬ ಶರಣನು ಮನೆಯ ಮಾಡಿ,", 
        "15423": "ಎಂಟು ಮೇರುವೆಯ ಮೇಲೆ", 
        "15424": "ಒಬ್ಬ ಮಾನವನ ಕರಸ್ಥಲದಲ್ಲಿ ಮೂರು ಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15425": "ಏನೂ ಇಲ್ಲದಲ್ಲಿ ಒಂದು ಮರನ ಕಂಡೆನಯ್ಯ.", 
        "15426": "ಒಬ್ಬ ಶಿವನ ಕರಸ್ಥಲದಲ್ಲಿ ಐದು ಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15427": "ಧರೆಯಾಕಾಶದ ಮೇಲೆ ಒಬ್ಬ ಸೂಳೆ ನಿಂದು,", 
        "15428": "ಊರ ಮೇಲೆ ಆಡುವ ಗಿಳಿಯ ಕಂಡೆ ನೋಡಾ.", 
        "15429": "ಆರು ಬುಡದ ಮೇಲೆ ಮೂರು ಕೊನೆಯ ಕಂಡೆ ನೋಡಾ.", 
        "15430": "ಐದು ಕೇರಿಯ ಮೇಲೆ ಒಂದು ಗ್ರಾಮವಿರ್ಪುದು ನೋಡಾ.", 
        "15431": "ಹೃದಯದೊಳಗಿರ್ಪ ಜ್ಯೋತಿಯ ಬೆಳಗಿನೊಳಗೆ", 
        "15432": "ಅಂಗವೆಂಬ ಪಟ್ಟಣದೊಳಗೆ", 
        "15433": "ಸಪ್ತೇಳುಸಾಗರದ ಮೇಲೆ ಅಷ್ಟ ಕುಲಪರ್ವತವ ಕಂಡೆನಯ್ಯ.", 
        "15434": "ಈರೇಳುಭುವನವ ಹದಿನಾಲ್ಕುಲೋಕವನೊಂದು ಇರುವೆ ನುಂಗಿತ್ತುನೋಡಾ.", 
        "15435": "ಊರೊಳಗೆ ಆಡುವ ಹಕ್ಕಿಯ ಕಂಡೆನಯ್ಯ.", 
        "15436": "ಮನವೆಂಬ ಕೋಗಿಲೆಯ ಮೇಲೆ ಘನವೆಂಬ ನಿಜವ ತೋರಿ", 
        "15437": "ಸರ್ವಾಂಗದೊಳಹೊರಗಿರ್ಪ ಲಿಂಗವು ಹಿಂಗಿ ತೋರುತಿದೆ ನೋಡಾ.", 
        "15438": "ಅಜ್ಞಾನವೆಂಬ ಕರ್ಮಕೋಟಲೆಯ ಹರಿದು", 
        "15439": "ಹುಚ್ಚಮನದೊಳು ಸಿಕ್ಕಿ ಕಿಚ್ಚಾಗಿ ಬೆಂದಿರಲ್ಲಾ,", 
        "15440": "ಮನದ ಅಂಕುರಗಳ ಹರಿದು ಜ್ಞಾನವ ನಿಶ್ಚೈಸಿ", 
        "15441": "ಜ್ಞಾನ ಉಪಾಧಿಯಲ್ಲಿ ಮನವ ನಿಶ್ಚೈಸಿ, ಮಹಾಜ್ಞಾನದಲ್ಲಿ ನಿಂದು,", 
        "15442": "ಅಂಗಗುಣಾದಿಗಳನಳಿದು, ಲಿಂಗಸಾವಧಾನಿಯಾಗಿ,", 
        "15443": "ಮಂಚದ ಎಕ್ಕೆಯಲ್ಲಿ ಸಣ್ಣ ಮಣಿಯ ಪೆÇೀಣಿಸಬಲ್ಲೆನೆಂಬುವರು", 
        "15444": "ಒಬ್ಬ ಕನ್ನೆಯ ಮನೆಯೊಳಗೆ ಐವರು ಪುರುಷರ ಕಂಡೆನಯ್ಯ.", 
        "15445": "ನಾದಲಕ್ಷ ಬಿಂದುಲಕ್ಷ ಕಳಾಲಕ್ಷದ ಮೇಲೆ", 
        "15446": "ಒಂಬತ್ತು ಸೋಪಾನದ ಮೇಲೆ", 
        "15447": "ಹುಟ್ಟಿದ ಮನವಿಲ್ಲ, ಬಂದ ಬಟ್ಟೆಗಳಿಲ್ಲ,", 
        "15448": "ಕರ್ಮದ ವಾಸನೆಗಳ ಹರಿದು, ಶಿವಧರ್ಮದಲ್ಲಿ ನಿಂದು", 
        "15449": "ಜ್ಞಾನದ ಉಪಾಧಿಯಲ್ಲಿ ಸಂಗವನರಿತು", 
        "15450": "ಸಕಲ ನಿಃಕಲಗಳು ಇಲ್ಲದಂದು, ಪರಾಪರಗಳಿಲ್ಲದಂದು,", 
        "15451": "ನಿಃಕಲಶಿವತತ್ವದಿಂದ ಜ್ಞಾನಚಿತ್ತು ಉದಯಿಸಿತ್ತು ನೋಡಾ.", 
        "15452": "ಆ ಜ್ಯೋತಿಯ ಬೆಳಗಿನೊಳಗೆ ಅನಂತಕೋಟಿಬ್ರಹ್ಮರು,", 
        "15453": "ಅಂತಿರ್ದ ಬ್ರಹ್ಮದ ಅಂಗದಲ್ಲಿ ಜ್ಞಾನಚಿತ್ತು ಉದಯಿಸಿತ್ತು ನೋಡಾ.", 
        "15454": "ಕಾಯವೆಂಬ ಹುತ್ತಿನಲ್ಲಿ", 
        "15455": "ಜ್ಞಾನಸಂಬಂಧವನರಿತು, ಸುಜ್ಞಾನದೊಳು ನಿಂದು", 
        "15456": "ಬಯಲೆ ಅಂಗವಾಗಿ, ನಿರ್ವಯಲೆ ಲಿಂಗವಾಗಿ,", 
        "15457": "ಇಪ್ಪತ್ತೈದು ದೇಗುಲದ ಮೇಲೆ ಒಂದು ಶಿವಾಲಯವ ಕಂಡೆನಯ್ಯ.", 
        "15458": "ಸಾಗರದ ತೆರೆಯ ಮೇಲೆ ಮಂಡೂಕ ಕುಳಿತು ಕೂಗುತಿದೆ ನೋಡಾ.", 
        "15459": "ಮಹಾಮನೆಗೆ ಹೋಗುವ ದಾರಿಯಲ್ಲಿ", 
        "15460": "ಅಂತರಂಗದಲ್ಲಿ ಜ್ಞಾನಸಂಬಂಧವಾದ ಶರಣಂಗೆ", 
        "15461": "ಸಮುದ್ರದೊಳಗೊಂದು ಮೊಸಳೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15462": "ಕರ್ಮದ ಪಾಶವ ಹರಿದು ನಿರ್ಮಲಶರಣನಾಗಿ", 
        "15463": "ಅಹಂಕಾರವೆಂಬ ಕೊಂಬ ಮುರಿದು", 
        "15464": "ಕತ್ತಲೆ ಕವಿದ ಮಾನವರಿಗೆ ಜ್ಞಾನದ ಪ್ರಭೆಯುಂಟೇನಯ್ಯ ?", 
        "15465": "ಒಲ್ಲೆನೆಂಬ ಭಾವವು ಎಲ್ಲರಿಗೂ ಸಲ್ಲದು ಬಿಡಿರೊ.", 
        "15466": "ಕಾಯ ನನ್ನದೆಂದು ನಚ್ಚಬೇಡಿರೋ,", 
        "15467": "ಸಂಚಲಿಸುವ ಮನಗಳನಳಿದು, ನಚ್ಚುಮಚ್ಚುಗಳನಳಿದು,", 
        "15468": "ಅಂಗಪ್ರಕೃತಿಯನಳಿದು, ಲಿಂಗಧ್ಯಾನವ ಮಾಡಿ,", 
        "15469": "ಅಂಗದ ಕಳವಳವ ಲಿಂಗದಲ್ಲಿ ಅಳಿದು,", 
        "15470": "ಮಾತು ಇಲ್ಲದ ಮನೆಯಲ್ಲಿ ಒಬ್ಬ ಸತಿಯಳು ಕುಳಿತು", 
        "15471": "ಅಂಗವಿಲ್ಲದ ಬಾಲೆಯು ಅಂಗಳದೊಳಗೆ ಕುಳಿತು", 
        "15472": "ಕಪ್ಪೆಯ ಒಡಲಲ್ಲಿ ಮುಪ್ಪಾಗಿ ಆರುಮಂದಿ ಸತ್ತಿರುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15473": "ಮನೆಮನೆಗೆ ಹಾರುವ ಕೋಡಗನ ಒಡಲಲ್ಲಿ", 
        "15474": "ಋತುವಿಲ್ಲದ ಹಂಸನು ತತ್ತಿಯನಿಕ್ಕುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15475": "ಸಂಚಲಿಸುವ ಮನಕ್ಕೆ ಮಿಂಚುದೋರುವುದೇನಯ್ಯ ?", 
        "15476": "ಸಜ್ಜನವೆಂಬ ಮಾರ್ಗದಲ್ಲಿ ಒಬ್ಬ ಬಾಲೆಯು ನಿಂದು", 
        "15477": "ಕಂಗಳ ಮುಂದಳ ರೂಪವನಳಿದು,", 
        "15478": "ಕತ್ತಲೆ ಮನೆಯೊಳಗೆ ಬೆಳಗುಂಟೇನಯ್ಯ ?", 
        "15479": "ಚಿಪ್ಪಿನೊಳಗಣ ಮುತ್ತಿನಂತೆ, ಕಲ್ಲೊಳಗಣ ವಜ್ರದಂತೆ,", 
        "15480": "ಆಕಾಶಕ್ಕೆ ಬೈಗುಬೆಳಗುಂಟೇನಯ್ಯ ?", 
        "15481": "ತಲೆ ಒಂದು, ಮುಖ ಮೂರು, ಹಸ್ತವಾರು,", 
        "15482": "ನಾಲ್ಕು ಮುಖದ ಸೂಳೆಯು", 
        "15483": "ಕಳವಳಿಸುವ ಕರಣಂಗಳಿಗೆ ಮುಖಗೊಡದೆ", 
        "15484": "ಕಾಯದ ಕರಣಂಗಳಿಗೆ ನಿಲ್ಲದೆ", 
        "15485": "ಆದಿಯಲ್ಲಿ ಸ್ವಯಜ್ಞಾನಿ ಉದಯವಾದ ಕಾರಣ,", 
        "15486": "ಮನದ ವಾಸನೆಗಳ ಹರಿದು, ಘನತರ ಲಿಂಗವನಾಚರಿಸಿ,", 
        "15487": "ಆದಿಯಲ್ಲಿ ಶಿವಾತ್ಮನು ಉದಯವಾದ ಕಾರಣ", 
        "15488": "ಆರು ಸ್ಥಲದಲ್ಲಿ ಆರು ಮೂರ್ತಿಗಳ ಕಂಡೆನಯ್ಯ.", 
        "15489": "ತನ್ನೊಡನೆ ಒಬ್ಬ ಭಾಮಿನಿ ಪುಟ್ಟಿದಳು ನೋಡಾ.", 
        "15490": "ಆರು ಮೂರು ದೇಶದ ಮುಂದೆ", 
        "15491": "ಕಣ್ಣು ಇಲ್ಲದವ ಕಂಡನಯ್ಯ ಒಂದು ಲಿಂಗವ.", 
        "15492": "ತಂದೆಯೊಡನೆ ಒಬ್ಬ ಮಗ ಹುಟ್ಟಿ", 
        "15493": "ಆರು ಕಂಬದ ಶಿವಾಲಯದ ಮೇಲೆ", 
        "15494": "ಓಂಕಾರವೆಂಬ ಉಲುಹಿನಲ್ಲಿ ಪ್ರಣವಸೋಹಂಕಾರವಿಡಿದು", 
        "15495": "ಆಧಾರಚಕ್ರದಲ್ಲಿ ಬ್ರಹ್ಮಜ್ಯೋತಿ,", 
        "15496": "ಏನೇನೂ ಇಲ್ಲದಲ್ಲಿ ಒಬ್ಬ ಪುರುಷ ನಿಂದಿರುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15497": "ಅಂಗದ ಗುಣಾದಿಗಳನಳಿದು, ಭಾವ ಬೆಳಗಿನೊಳು ಕೂಡಿ,", 
        "15498": "ಕಾಯದ ಗುಣಗಳನಳಿದು, ಮಾಯದ ಬಲೆಗೆ ಸಿಲ್ಕದೆ.", 
        "15499": "ಊರೊಳಗೆ ಇರುವ ನಾರಿಯ ಕಂಡೆನಯ್ಯ.", 
        "15500": "ಹೃದಯದೊಳಗಿರ್ಪ ಪ್ರಾಣಲಿಂಗವನರಿತು,", 
        "15501": "ಹಲವು ಕಡೆಗೆ ಹರಿವ ಮನವ ಚಿತ್ತದಲ್ಲಿ ನಿಲಿಸಿ,", 
        "15502": "ಹುಟ್ಟುವ ಕರಣಂಗಳ ಮುರಿದು,", 
        "15503": "ಪೃಥ್ವಿ ಆಕಾಶದ ಮೇಲೆ ಏಕಾಂತಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15504": "ಮೂರುಲೋಕದ ಮೇಲೆ", 
        "15505": "ಬಯಲೆ ಅಂಗವಾದ ಶರಣಂಗೆ, ನಿರ್ವಯಲೇ ಲಿಂಗ ನೋಡಾ.", 
        "15506": "ನಾದ ಬಿಂದು ಕಲೆಗಳು ಇಲ್ಲದ ಮುನ್ನ,", 
        "15507": "ನಾದಘೋಷವೆಂಬ ಲಿಂಗದಲ್ಲಿ ಪಾತ್ರ ಸತ್ಪಾತ್ರವನರಿತು,", 
        "15508": "ಕಾಡುವ ಕರಣಂಗಳೇ ಮಕ್ಕಳು, ಬೇಡುವ ಮನವೇ ಭ್ರಾಂತಿ.", 
        "15509": "ಮೂರು ತನುಗಳ ಗತಿಗೆಡಿಸಿ, ಜ್ಞಾನೈಕ್ಯದಲ್ಲಿ ನಿಂದು,", 
        "15510": "ಮೂರು ಮೇರುವೆಯ ಮೇಲೆ", 
        "15511": "ಅಂತಃಕರಣಚತುಷ್ಟಯಂಗಳೆಂಬ ನಾಲ್ಕು ಕಂಬದ ಮೇಲೆ", 
        "15512": "ಅಂಗವಿಲ್ಲದ ಹಂಸಗೆ ಪಾದವೊಂದು, ಮುಖ ಮೂರು, ರೆಕ್ಕೆ ಆರು,", 
        "15513": "ಅಷ್ಟಕುಲಪರ್ವತದ ಮೇಲೆ ದೃಷ್ಟಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15514": "ಕಣ್ಣಮೇಲೆ ಕಣ್ಣು ಹುಟ್ಟಿ ಕಂಡೆನಯ್ಯ ಒಂದು ಲಿಂಗವ,", 
        "15515": "ಕತ್ತಲೆಯ ಮನೆಯೊಳಗೊಂದು ಪಶುವಿಪ್ಪುದು ನೋಡಾ.", 
        "15516": "ಆದಿಯ ಲಿಂಗವು, ಆರು ಕಂಬದ ಶಿವಾಲಯವ ಪೆÇಕ್ಕು ನೋಡಲು,", 
        "15517": "ಸಾಕಾರವಳಿದು ನಿರಾಕಾರವಾದ ಶರಣಂಗೆ", 
        "15518": "ಒಳಗೆ ನೋಡಿದರೆ ನಿರಾಕುಳಲಿಂಗವು.", 
        "15519": "ಆರು ಮತಗಳಿಲ್ಲದೆ, ತನ್ನ ಮನವ ತಾನೇ ಶುದ್ಧ ಮಾಡಿ,", 
        "15520": "ಪಾತಕಾಂಗದವನಿಗೆ ನೀತಿ ನಿರ್ಮಳಲಿಂಗವು ಕಾಣಬರ್ಪುದೇನಯ್ಯಾ?", 
        "15521": "ಒಂದೊಂದಾಗಿ ಕೂಡಿದಲ್ಲಿಗೆ ಬಂದನಯ್ಯ ಒಬ್ಬ ವಿಶ್ವಾಸಘಾತಕನು.", 
        "15522": "ಅಜಹರಿಸುರನಾರದರು ಮೊದಲಾದವರಿಂಗೆ", 
        "15523": "ಕಾಯದ ಕರಣಂಗಳಿಗೆ ಸಿಲ್ಕಿ", 
        "15524": "ಹಲವು ಕಡೆಗೆ ಹರಿದಾಡುವ ಮನವ ನಿಲಿಸಿ,", 
        "15525": "ಬತ್ತಲೆಯಾದ ಭಾಮಿನಿಯ ಅಂಗದಲ್ಲಿ", 
        "15526": "ಕಾಳಮನವ ಪರಿಹರಿಸಿ, ತೊಳಲಿ ಬಳಲುವ ಜಲ್ಮವನಳಿದು,", 
        "15527": "ಅಂಗಕಳೆ ಲಿಂಗದಲ್ಲಿ ಅರತು, ಲಿಂಗಕಳೆ ಭಾವದಲ್ಲಿ ಅರತು,", 
        "15528": "ಅಂತರಂಗ ಬೆಳಗಿನ ಸುಳುವನರಿತು ಶಾಂತಲಿಂಗವ ಕೂಡಿ", 
        "15529": "ಮಾತುಮಥನಗಳಿಲ್ಲದೆ, ಜಾತಿಸೂತಕವಿಲ್ಲದೆ,", 
        "15530": "ನಿತ್ಯವನರಿತು, ಪಾತಕವ ಕಳೆದು,", 
        "15531": "ಮನ  ಬುದ್ಧಿ ಚಿತ್ತ ಅಹಂಕಾರವೆಂಬ ನಾಲ್ಕು ಗುಣಾದಿಗಳ ಹಿಡಿದು,", 
        "15532": "ಅಂತರಂಗ ಬಹಿರಂಗ ಶುದ್ಧನಾದ ಶರಣನ ದಿಟಪುಟವ ನೋಡಿರಯ್ಯ.", 
        "15533": "ಧರೆಯಾಕಾಶದ ಮೇಲೆ ಒಬ್ಬ ಪುರುಷ ನಿಂದು", 
        "15534": "ಆಕಾರವಳಿದು ನಿರಾಕಾರವಾದ ಶರಣನು", 
        "15535": "ಆರು ನೆಲೆಯ ಮಂಟಪದ ಮೇಲೆ", 
        "15536": "ಆತ್ಮನೆಂಬ ಅಂಗದಲ್ಲಿ ನಿರಾತ್ಮನೆಂಬ ಲಿಂಗವು ತೋರಿತ್ತು ನೋಡಾ.", 
        "15537": "ಆಕಾಶದ ಮೇಲೊಬ್ಬ ಸೂಳೆ ನಿಂದು", 
        "15538": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರಾದಿಗಳ ಮೇಲೆ ಈಶ್ವರಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15539": "ಮೂರು ಮಂಟಪದ ಶಿವಾಲಯದ ಶಿಖರದ ಮೇಲೆ", 
        "15540": "ನಾಲ್ಕು ಕಂಬದ ಪೌಳಿಯ ಮೇಲೆ", 
        "15541": "ತಾಪತ್ರಯಂಗಳ ಕಳೆದು, ತತ್ವರೂಪಾದಿಗಳನರಿದು,", 
        "15542": "ನಿತ್ಯವಿಡಿದು ಮುಕ್ತನಾಗಿ,", 
        "15543": "ಹಲವು ಕಡೆಗೆ ಹರಿದಾಡುವ ಮನವ", 
        "15544": "ಕೊಂಬೆಕೊಂಬೆಗೆ ಹಾರುವ ಕೋಡಗನ ತಲೆಯ ಮೇಲೆ", 
        "15545": "ಐದು ಕಂಬದ ಗುಡಿಯ ಶಿಖರದ ಮೇಲೆ", 
        "15546": "ಮನ ನಿರ್ಮಳವಾದ ಶರಣನು", 
        "15547": "ಕರ್ಮಕೋಟಲೆಯ ಹರಿದು, ಧರ್ಮದಲ್ಲಿ ನಿಂದು,", 
        "15548": "ಊರೊಳಗೊಂದು ಮನೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15549": "ಸರ್ವಗುಣಾದಿಗಳನಳಿದು, ಲಿಂಗಸಂಬಂಧಿಯಾಗಿ", 
        "15550": "ಸಪ್ತೇಳುಸಾಗರಗಳಿಲ್ಲದಂದು, ಅಷ್ಟಕುಲಪರ್ವತಂಗಳಿಲ್ಲದಂದು,", 
        "15551": "ಯೋಗೀಶ್ವರನಾದರೇನಯ್ಯ ?", 
        "15552": "ಅಂತರಂಗದಲ್ಲಿ ಗುರುಲಿಂಗಜಂಗಮವ ಕಂಡೆವೆಂದು,", 
        "15553": "ಅನಾದಿಲಿಂಗವು ಕರಕ್ಕೆ ಬಂದಿತು ನೋಡಾ.", 
        "15554": "ಅಂತರಂಗದಲ್ಲಿ ವಸ್ತುವ ಕಂಡೆನಯ್ಯ.", 
        "15555": "ಗುರು ಲಿಂಗ ಜಂಗಮ ಪಾದೋದಕ ಪ್ರಸಾದವನರಿಯದ", 
        "15556": "ಷಡುರುಚಿಪದಾರ್ಥಂಗಳ ಇಷ್ಟಲಿಂಗಕೆ ತೋರಿ,", 
        "15557": "ಅಂಗಕೆ ಗುರುವಾದನಯ್ಯ, ಪ್ರಾಣಕೆ ಲಿಂಗವಾದನಯ್ಯ,", 
        "15558": "ತಂದೆ ಸತ್ತ ಮೇಲೆ ಆ ಲಿಂಗವ ತೆಗೆದು", 
        "15559": "ಧರೆಯ ಮೇಲೆ ಹರಡಿರುವ ಶಿಲೆಯ ತಂದು", 
        "15560": "ಸರ್ವಾಂಗದೊಳಹೊರಗಿಪ್ಪ ಪರಬ್ರಹ್ಮಲಿಂಗವು", 
        "15561": "ಇಷ್ಟಲಿಂಗ ಪ್ರಾಣಲಿಂಗ ಭಾವಲಿಂಗವೆಂಬ ಲಿಂಗತ್ರಯವನರಿತು", 
        "15562": "ಷಡುರುಚಿಪದಾರ್ಥಂಗಳ ಇಷ್ಟಲಿಂಗಕೆ ತೋರದೆ", 
        "15563": "ಆಚಾರವನರಿತು ನಡೆವ ಭಕ್ತನು ಶಿವಾಚಾರಿಯೆಂಬೆನಯ್ಯ.", 
        "15564": "ಶಿವಾಚಾರದಲ್ಲಿ ನಡೆವ ಶರಣರ ಪಾದದ ಅಂಘ್ರಿಯಲ್ಲಿ ಎನ್ನನಿರಿಸಯ್ಯ.", 
        "15565": "ಆದಿಯ ಬೀಜ ಶಿವಸಂಸ್ಕಾರಿ ಅನಾದಿಶರಣನಯ್ಯ.", 
        "15566": "ಆಜ್ಞಾÉೀಯಚಕ್ರದೊಳಗೆ ಗಂಭೀರನಿಪ್ಪ ಬೆಡಗ ನೋಡಾ.", 
        "15567": "ಇಪ್ಪತ್ತೈದು ಗ್ರಾಮದ ಮೇಲೆ ಸುಳಿದಾಡುವ", 
        "15568": "ಕುರುಡಂಗೆ ಕನ್ನಡಿಯ ತೋರಿದಡೆ", 
        "15569": "ಶಿವಭಕ್ತನ ಅಂತರಂಗದಲ್ಲಿ ಭಾವಲಿಂಗವಿಪ್ಪುದು ನೋಡಾ.", 
        "15570": "ಇಷ್ಟಲಿಂಗದ ಭೇದವನರಿಯದೆ", 
        "15571": "ವಂತಿಗೆಲಿಂಗವ ಕಟ್ಟಿಕೊಂಡು", 
        "15572": "ಅಂಗದ ಮೇಲೆ ಶಿವಲಿಂಗವ ನೆಲೆಗೊಂಡು,", 
        "15573": "ಯೋಗಿ ಜೋಗಿ ಶ್ರವಣ ಸನ್ಯಾಸಿ ಕಾಳಾಮುಖ", 
        "15574": "ಸಂಗನಬಸವಣ್ಣನೆ ಅಂಗ, ಚನ್ನಬಸವಣ್ಣನೆ ಲಿಂಗ,", 
        "15575": "ಇಷ್ಟಲಿಂಗ ಪ್ರಾಣಲಿಂಗ ಭಾವಲಿಂಗವೆಂಬ ಲಿಂಗತ್ರಯವನರಿತು", 
        "15576": "ಸಂಗನಬಸವಣ್ಣನೇ ಎನ್ನ ಗುರುವು.", 
        "15577": "ಐದು ಮೇರುವೆಯ ಮೇಲೆ ಒಬ್ಬ ಪುರುಷ ನಿಂದಿರುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15578": "ಸತ್ತುಚಿತ್ತಾನಂದ ನಿತ್ಯಪರಿಪೂರ್ಣವೆಂಬ ಐದಂಗವ ಗರ್ಭೀಕರಿಸಿಕೊಂಡು,", 
        "15579": "ಅಂತರಂಗದಲ್ಲಿ ಶುದ್ಧವಾದ ಶಿವಭಕ್ತನು ನಿತ್ಯವಿಡಿದು,", 
        "15580": "ಆತ್ಮನೆಂಬ ಅಂಗಕ್ಕೆ ನಿರಾತ್ಮನೆಂಬ ಲಿಂಗವು ನೋಡಾ.", 
        "15581": "ಹಸಿವು ತೊರೆದರೇನಯ್ಯ ? ಮನದ ಕೊನೆಯ ಮೆಟ್ಟಬೇಕಯ್ಯ.", 
        "15582": "ಆದಿಯಲ್ಲಿ ಒಬ್ಬ ದೇವನು ಮೂವರ ಕೂಡಿಕೊಂಡು", 
        "15583": "ಸ್ಥೂಲವಾದರೇನಯ್ಯ ?", 
        "15584": "ಪ್ರಥಮ ಕಾಲದಲ್ಲಿ ಒಬ್ಬ ಶಿವಶರಣನಿಪ್ಪ ನೋಡಾ.", 
        "15585": "ವೇದದಲ್ಲಿ ನಾಲ್ಕು ನುಡಿಯ ಕಲಿತರೇನು ?", 
        "15586": "ವೇದ ದೊಡ್ಡದೆಂದು ನುಡಿವ ವಾದಿಯ ಮಾತ", 
        "15587": "ಮಾತಿನಲ್ಲಿ ಬ್ರಹ್ಮ, ನೀತಿಯಲ್ಲಿ ಕ್ರೋಧಿ.", 
        "15588": "ನುಡಿಯಲ್ಲಿ ವಾಚಾಳತ್ವ, ಹೃದಯದಲ್ಲಿ ಕರ್ಮೇಂದ್ರಿ.", 
        "15589": "ಹಿಂದಳ ಮಾತ ಕೇಳಿ ಮುಂದೆ ನಿಂದ್ಯವನಾಡುವ", 
        "15590": "ವೇದ ಶಾಸ್ತ್ರ ಪುರಾಣ ಆಗಮಂಗಳ ನೋಡಿದರೇನಯ್ಯ ?", 
        "15591": "ನಿಂದಕರು ನಿಂದಿಸಿದರೆ ಸ್ವಯಜ್ಞಾನಿ ಅಂಜುವನೇನಯ್ಯ ?", 
        "15592": "ಇಷ್ಟಲಿಂಗದ ಕಳಾಭೇದವನರಿತು, ಪ್ರಾಣಲಿಂಗದಲ್ಲಿ ಕೂಡಿ,", 
        "15593": "ಪರಬ್ರಹ್ಮವೆಂಬ ಲಿಂಗದಿಂದ ಭಾವಲಿಂಗ ಉದಯವಾಯಿತ್ತು.", 
        "15594": "ಆಧಾರ ಸ್ವಾಧಿಷ್ಠಾನ ಮಣಿಪೂರಕ ಅನಾಹತ ವಿಶುದ್ಧಿ ಆಜ್ಞಾÉೀಯಯೆಂಬ", 
        "15595": "ಆರು ಮೂರು ಮಂಟಪದ ಮೇಲೆ", 
        "15596": "ಇಪ್ಪತ್ತೈದು ದೇಶದ ಮೇಲೆ", 
        "15597": "ಅಂಗಕ್ಕೆ ಆಚಾರ ನೆಲೆಗೊಂಡಲ್ಲದೆ", 
        "15598": "ಅಂಗದ ಮೇಲೆ ಶಿವಲಿಂಗವ ಧರಿಸಿ", 
        "15599": "ಈ ಧರೆಯ ಮೇಲೆ ಹುಟ್ಟಿ", 
        "15600": "ಇಷ್ಟಲಿಂಗದ ಭೇದವನರಿತು", 
        "15601": "ಇಷ್ಟಲಿಂಗದಲ್ಲಿ ಗುರುವಿಡಿದು, ಪ್ರಾಣಲಿಂಗದಲ್ಲಿ ಲಿಂಗವಿಡಿದು,", 
        "15602": "ಇಷ್ಟಲಿಂಗದಲ್ಲಿ ಶುದ್ಧವಾಗಿ, ಪ್ರಾಣಲಿಂಗದಲ್ಲಿ ಸಿದ್ಧವಾಗಿ,", 
        "15603": "ಭಕ್ತನಾದರೇನಯ್ಯ", 
        "15604": "ಆರ ಗೊಡವೆಯಿಲ್ಲದೆ ಗುರುವು ಹೇಳಿದಂತೆ", 
        "15605": "ಆಚಾರ ಶುದ್ಧವಾದ ಭಕ್ತನ ಅಂತರಂಗದಲ್ಲಿ ಶಿವನಿಪ್ಪನು ನೋಡಾ.", 
        "15606": "ಆರು ಕೇರಿಗಳಲ್ಲಿ ಆರು ಮೂರ್ತಿಗಳಿಪ್ಪರು ನೋಡಾ.", 
        "15607": "ಇಷ್ಟಲಿಂಗವಿಡಿದು ಕಾಯ ಶುದ್ಧವಾಯಿತ್ತಯ್ಯ.", 
        "15608": "ಈ ಧರೆಯ ಮೇಲೆ ಭಕ್ತರೆಂದು ಗುರು ಲಿಂಗ ಜಂಗಮಕ್ಕೆ ನಡೆವರಯ್ಯ.", 
        "15609": "ಈ ಲೋಕದೊಳಗೆ ಶೀಲವಂತರೆಂದು ಪಾದೋದಕ ಪ್ರಸಾದವ ಕೊಂಬರಯ್ಯ.", 
        "15610": "ಅಂತರಂಗದ ಬೆಳಗಿನೊಳು ಸದಮಲಜ್ಞಾನವನರಿತು", 
        "15611": "ಅಂತಃಕರಣಚತುಷ್ಟಯಂಗಳೆಂಬ ನಾಲ್ಕು ಗುಣಂಗಳವಿಡಿದು", 
        "15612": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರಾದಿಗಳ ಮೇಲೆ ಈಶ್ವರನಿಪ್ಪ ನೋಡಾ.", 
        "15613": "ಒಂದು ದಾರಿಯ ಮೇಲೆ ಎರಡು ಮಿಕವಿಪ್ಪವು ನೋಡಾ.", 
        "15614": "ಕರ್ಮವೆಂಬ ಕತ್ತಲೆಯಲ್ಲಿ ವರ್ಮಗೆಟ್ಟು ಮೂವರು ಬಿದ್ದಿರ್ಪರು ನೋಡಾ.", 
        "15615": "ಶಿವಾತ್ಮಜ್ಞಾನ ಉದಯವಾದ ಮಹೇಶ್ವರನ ಅಂತರಂಗದಲ್ಲಿ", 
        "15616": "ಒಬ್ಬ ಪುರುಷನು ಮೂರು ಶಿವಾಲಯವಂ ಪೆÇಕ್ಕು,", 
        "15617": "ಆರು ದೇಶದ ಮೇಲೆ ಮೂರು ಮಂಡಲವ ಕಂಡೆನಯ್ಯ,", 
        "15618": "ಉರೊಳಗಣ ಸೂಳೆಯು ಮೇರುವೆಯೊಳಗಣ ಪುರುಷನ ಕರೆದು", 
        "15619": "ಮೂರು ಲೋಕದ ಮೇಲೆ ಒಂದು ಪಕ್ಷಿ ಕುಳಿತು", 
        "15620": "ಅಂತರಂಗದ ಬೆಳಗಿನೊಳು ನಿಂತಾತನೆ ನಿರ್ಮಳಜ್ಞಾನಿ ನೋಡಾ.", 
        "15621": "ಅನಾದಿಯ ಜಂಗಮವು ಶಿವಭಕ್ತನ ಮಠಕೆ ಬಂದು", 
        "15622": "ಭಕ್ತ ಮಹೇಶ್ವರನಲ್ಲಿ ಅಡಗಿ ಪ್ರಸಾದಿಯಾದನಯ್ಯ.", 
        "15623": "ಆಚಾರಲಿಂಗ ಗುರುಲಿಂಗದಲ್ಲಿ ಅಡಗಿ ಶಿವಲಿಂಗವಾಯಿತ್ತಯ್ಯ.", 
        "15624": "ಇಷ್ಟಲಿಂಗವು ಪ್ರಾಣಲಿಂಗದಲ್ಲಿ ಅಡಗಿ ಭಾವಲಿಂಗವಾಯಿತ್ತಯ್ಯ.", 
        "15625": "ಪೃಥ್ವಿ ಅಪ್ಪು ತೇಜ ವಾಯು ಆಕಾಶ ಸೂರ್ಯ ಚಂದ್ರ ಆತ್ಮರೆಂಬ", 
        "15626": "ಸರ್ವಾಂಗದೊಳಹೊರಗೆ ಲಿಂಗಮಯವಾಗಿ", 
        "15627": "ಒಳಹೊರಗೆ ಪರಿಪೂರ್ಣವಾದ ಸ್ವಯಜ್ಞಾನಿ", 
        "15628": "ನಿಃಕಲಪರಬ್ರಹ್ಮಲಿಂಗವನಾಚರಿಸಿದ ಶರಣನು", 
        "15629": "ನಿತ್ಯವಿಡಿದು ಮುಕ್ತನಾಗಿ,", 
        "15630": "ಆಕಾಶದ ಮೇಲೆ ಏಕಾಂತಮಂಟಪವ ಕಂಡೆನಯ್ಯ.", 
        "15631": "ಸರ್ವಾಂಗದೊಳಹೊರಗೆ ಪರಿಪೂರ್ಣವಾಗಿ", 
        "15632": "ಆತ್ಮನೆಂಬ ಪ್ರಭೆಯಲ್ಲಿ ನಿಂದು", 
        "15633": "ಆಚಾರಲಿಂಗದ ಬೆಳಗಿನೊಳು ಶಾಂತ ಸದಮಲಜ್ಞಾನವನರಿತು", 
        "15634": "ಇರುಳು ಹಗಲುಗಳೆಂಬ ಸಂದೇಹಗಳನಳಿದು ನಿಂದು", 
        "15635": "ಅಂಗದ ಗುಣಾದಿಗಳನಳಿದು ಲಿಂಗಸಂಗಿಯಾಗಿ", 
        "15636": "ಮನದ ವಾಸನೆಗಳನಳಿದು, ಜ್ಞಾನದಲ್ಲಿ ಕೂಡಿ,", 
        "15637": "ಹೃದಯದೊಳಗಿಪ್ಪ ಪ್ರಾಣಲಿಂಗವನು ತ್ರಿಕೂಟದಲ್ಲಿ ತಂದು,", 
        "15638": "ಭಕ್ತ ಮಹೇಶ್ವರ ಪ್ರಸಾದಿ ಪ್ರಾಣಲಿಂಗಿ ಶರಣ ಐಕ್ಯರ ಮೇಲೆ", 
        "15639": "ತನ್ನ ತಾನೇ ತಿಳಿದು, ಗನ್ನಘಾತಕವನಳಿದು, ಸನ್ನಹಿತನಾಗಿ", 
        "15640": "ತತ್ವಮಸಿ ವಾಕ್ಯದಿಂದ ಪರಂಜ್ಯೋತಿಲಿಂಗವ ಕಂಡು,", 
        "15641": "ಐದು ತತ್ವದ ಮೇಲೆ ಒಬ್ಬ ಸತಿಯಳು ಇಪ್ಪಳು.", 
        "15642": "ಪ್ರಸಾದ ಪ್ರಸಾದವೆಂದು ಹೆಸರಿಟ್ಟುಕೊಂಡು ನುಡಿವಿರಿ,", 
        "15643": "ಪೃಥ್ವಿಗುಣವಳಿದು ಭಕ್ತನಾದೆನಯ್ಯ,", 
        "15644": "ಅಂತರಂಗದ ಬೆಳಗಿನೊಳು ಸದಮಲಜ್ಞಾನವನರಿತು,", 
        "15645": "ಉಲುಹುವಡಗಿದ ವೃಕ್ಷದ ಮೇಲೆ ಫಲಗಳಿಪ್ಪುದ ಕಂಡೆನಯ್ಯ.", 
        "15646": "ಹೃದಯದಲ್ಲಿಪ್ಪ ಪ್ರಾಣಕ್ಕೆ ವಾಯುವೇ ಆಧಾರ.", 
        "15647": "ಅಜನಸ್ಥಾನಗಳಿಂದ ನಾಭಿಯ ಪವನವತ್ತಿ", 
        "15648": "ಮನ ಪವನಾದಿಗಳ ತ್ರಿಕೂಟದಲ್ಲಿ ತಂದು", 
        "15649": "ಇಷ್ಟಲಿಂಗವಿಡಿದು ಗುರುಪ್ರಸಾದವ ಕಂಡೆನಯ್ಯ.", 
        "15650": "ಪೃಥ್ವಿ-ಅಪ್ಪು-ತೇಜ-ವಾಯು-ಆಕಾಶವಿಲ್ಲದಂದು,", 
        "15651": "ಮಾತು ಮಥನಂಗಳಿಲ್ಲದಂದು, ನೀತಿ ನಿರ್ಮಲವಿಲ್ಲದಂದು,", 
        "15652": "ಹೃದಯದಲ್ಲಿಪ್ಪ ಪ್ರಾಣಲಿಂಗವನು ತ್ರಿಕೂಟದಲ್ಲಿ ತಂದು,", 
        "15653": "ನಿರಾಳ ಝೇಂಕಾರ ನಿಜಲಿಂಗಪ್ರಭು ನುಡಿಸಿದರೆ ನುಡಿದೆನಲ್ಲದೆ,", 
        "15654": "ಒಳಹೊರಗೆ ಪರಿಪೂರ್ಣವಾಗಿಹ ಮೂಲ ಪ್ರಣವವ ತಿಳಿದು", 
        "15655": "ಬಯಲೆ ಅಂಗವಾದ ಶರಣಂಗೆ ನಿರ್ವಯಲೆ ಲಿಂಗ ನೋಡಾ.", 
        "15656": "ರೂಪ ಕುರೂಪವನೊಳಕೊಂಡು", 
        "15657": "ಹಲವು ಕಡೆಗೆ ಹೊಯ್ದಾಡುವ ಮನವನೊಬ್ಬುಳಿಮಾಡಿ", 
        "15658": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರಾದಿಗಳೆಂಬ ಭವಸಾಗರವ ಹರಿದು", 
        "15659": "ಒಳಹೊರಗೆ ಪರಿಪೂರ್ಣವಾಗಿಹ ಲಿಂಗದಲ್ಲಿ", 
        "15660": "ಅಂಗಲಿಂಗಸಂಬಂಧವ ಗರ್ಭೀಕರಿಸಿಕೊಂಡು", 
        "15661": "ಶ್ರೋತೃ ತ್ವಕ್ಕು ನೇತ್ರ ಜಿಹ್ವೆ ಘ್ರಾಣವೆಂಬ ಪಂಚಮುಖವನು", 
        "15662": "ಪೃಥ್ವಿ-ಅಪ್ಪು-ತೇಜ-ವಾಯು-ಆಕಾಶವೆಂಬ ಐದಂಗವ", 
        "15663": "ಅಂಗದೊಳಹೊರಗಿಪ್ಪ ಲಿಂಗವನು ಅರಿತು", 
        "15664": "ಸಕಲ ನಿಃಕಲದಲ್ಲಿ ಅಡಗಿ, ನಿಃಕಲ ಸಕಲದಲ್ಲಿ ಅಡಗಿ,", 
        "15665": "ಅಂಗಕ್ಕೆ ಲಿಂಗವೇ ಆಧಾರ, ಲಿಂಗಕ್ಕೆ ಸಂಬಂಧವೆ ಆಧಾರ,", 
        "15666": "ಊರೊಳಗೆ ಆನೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15667": "ಮೂರೂರ ಮೇಲೆ ಸುಳಿದಾಡುವ ಸೂಳೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15668": "ಒಳಗ ಹೊರಗು ನುಂಗಿ, ಹೊರಗ ಒಳಗು ನುಂಗಿ,", 
        "15669": "ಭಕ್ತಿ-ಜ್ಞಾನ-ವೈರಾಗ್ಯದಿಂದತ್ತತ್ತ ಅಗಮ್ಯಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15670": "ಅಷ್ಟಮದಂಗಳ ಸುಟ್ಟು, ಕಷ್ಟಕರ್ಮವ ಹರಿದು,", 
        "15671": "ಇಪ್ಪತ್ತೈದು ನೆಲೆಯ ಮೇಲೆ ಸುಪ್ಪಾಣಿಯ ಕಂಡೆನಯ್ಯ.", 
        "15672": "ಅಂಗಪ್ರಕೃತಿಯನಳಿದು, ಲಿಂಗಸಮರಸವಾಗಿ,", 
        "15673": "ಮನ ಬುದ್ಧಿ ಚಿತ್ತ ಅಹಂಕಾರ ಕರಣಚತುಷ್ಟಯಂಗಳ", 
        "15674": "ಊರೊಳಗಣ ನಾರಿಯು ಮೇರುವೆಯ ಗುಡಿಯ ಪೆÇಕ್ಕು", 
        "15675": "ಮೂರು ದಾರಿಯಲ್ಲಿ ಮಾರಣಿ ಕುಳಿತಿಪ್ಪಳು ನೋಡಾ.", 
        "15676": "ಸರ್ಪ ಸತ್ತು ಕಪ್ಪೆಯ ನುಂಗುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15677": "ಒಂದು ಬಿಚ್ಚಿ ಮೂರಾದುದ ಕಂಡೆನಯ್ಯ.", 
        "15678": "ಗುರುವಿಡಿದು ಕಾಯದ ಕರ್ಮವ ಹರಿದೆನಯ್ಯ.", 
        "15679": "ಮನ ಘನವ ನುಂಗಿ, ಪರಕೆಪರವಶನಾಗಿ,", 
        "15680": "ಹಲವು ಯಜ್ಞದ ಮನೆಯಲ್ಲಿ", 
        "15681": "ಒಂಬತ್ತು ಬಾಗಿಲ ಮನೆಯೊಳಗೆ", 
        "15682": "ಆರು ಸ್ಥಲದಲ್ಲಿ ಆರು ಮೂರ್ತಿಗಳ ಕಂಡೆನಯ್ಯ.", 
        "15683": "ಕರ್ಮದ ಕಂಡಣಿಯ ಹರಿದು,", 
        "15684": "ಹೃದಯದಲ್ಲಿಪ್ಪ ಪ್ರಾಣಲಿಂಗವನು ನಿಟಿಲಭ್ರೂಮಧ್ಯದಲ್ಲಿ ತಂದು,", 
        "15685": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರಾದಿಗಳೆಂಬ ಭವಸಾಗರವ ಹರಿದು", 
        "15686": "ಹಲವು ಬಣ್ಣದ ಪಕ್ಷಿಯ", 
        "15687": "ಭೂತದೇಹಾದಿಗಳನಳಿದು, ತತ್ವಾದಿಗಳನೆಲ್ಲ ಲಿಂಗವ ಮಾಡಿ,", 
        "15688": "ಉದಯಕಾಲದಲ್ಲಿ ಒಬ್ಬ ಮದಲಿಂಗನು", 
        "15689": "ಕೂಗುವ ಕೋಗಿಲೆಯ ಒಡಲಲ್ಲಿ", 
        "15690": "ಆದಿಲಿಂಗದ ಸಂಗದಿಂದ ಒಬ್ಬ ಚಿದಂಗನೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15691": "ಆರು ವರ್ಣದ ಪಕ್ಷಿ, ಮೂರು ಬಾಗಿಲವ ಪೆÇಕ್ಕು,", 
        "15692": "ನಯನದ ಸಂಚಲವನಳಿದವರ ಅಂಗವ ತೋರಿಸಯ್ಯ.", 
        "15693": "ಒಂದು ಲಿಂಗಕ್ಕೆ ಮುನ್ನೂರು ಮುಖ,", 
        "15694": "ಬೆಟ್ಟದ ತುದಿಯ ಮೇಲೊಂದು ಬಟ್ಟಬಯಲ ಕಂಡೆನಯ್ಯ.", 
        "15695": "ಗಿರಿಯ ತುದಿಯ ಮೇಲೆ ಹಾರುವ ಹಂಸನ ಕಂಡೆನಯ್ಯ.", 
        "15696": "ದೇಗುಲದೊಳಗೊಂದು ಲಿಂಗವಿಪ್ಪುದು ನೋಡಾ.", 
        "15697": "ಮಾತು-ಮಥನಗಳಿಲ್ಲದೆ, ಜಾತಿ-ಜನನವಿಲ್ಲದೆ,", 
        "15698": "ಹೊಲೆಹದಿನೆಂಟುಜಾತಿ ನೂರೊಂದು ಕುಲದಲ್ಲಿ", 
        "15699": "ಗುರುಲಿಂಗಜಂಗಮದ ಪಾದೋದಕ ಪ್ರಸಾದವ ಸ್ವೀಕರಿಸುವವರಿಂಗೆ", 
        "15700": "ಭಕ್ತ ಮಹೇಶ್ವರರು ಆರಾದರೂ ಆಗಲಿ", 
        "15701": "ಆರು ಕಂಬದ ದೇಗುಲದೊಳಗೆ", 
        "15702": "ಪಶುವಿನ ಒಡಲಲ್ಲಿ ಒಂದು ಶಿಶುವಿಪ್ಪುದ ಕಂಡೆನಯ್ಯ.", 
        "15703": "ಹಾಳೂರಲ್ಲಿ ಒಂದು ಕೋಡಗ ಕುಳಿತಿರುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15704": "ಸಂತೆಯ ಬೀದಿಯಲ್ಲಿ ಒಬ್ಬ ಸೂಳೆ ಕುಳಿತಿರುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15705": "ಕೋಣನ ಹಣೆಯ ಮೇಲೆ ಗಾಣಗಾರನ ಕಂಡೆನಯ್ಯ.", 
        "15706": "ಕರ್ಮದ ಕೋಟಲೆಯ ಹರಿದು, ನಿರ್ಮಲಂಗವ ಪೆÇಕ್ಕು,", 
        "15707": "ತನ್ನೊಳಗೆ ತಾನು ತಾನಾದ ಬಳಿಕ", 
        "15708": "ಆರು ವರ್ಣದ ಅಂಗನೆಯು", 
        "15709": "ಐದು ದಾರಿಯ ಮೇಲೆ ಒಬ್ಬ ಪುರುಷ ನಿಂದಿರುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15710": "ಉಸಿರು ಉನ್ಮನಿಗೆ ಸಿಲ್ಕಿದ ಬಳಿಕ ಪರಿಮಳದ ಹಂಗಿನ್ನ್ಯಾತಕಯ್ಯ?", 
        "15711": "ಬತ್ತಲೆಯಾದ ಭಾಮಿನಿಯ ಅಂಗದಲ್ಲಿ", 
        "15712": "ನಡೆದು ನಡೆದು ಬಯಲಾದುದ ಕಂಡೆನಯ್ಯ.", 
        "15713": "ಅಂಗಪಟ್ಟಣದೊಳಗೆ ಭೃಂಗನಾಟ್ಯವನಾಡಿ,", 
        "15714": "ಕರ್ಮದೋಷಂಗಳ ಹರಿದು, ಕನಸು ಕಳವಳಂಗಳನಳಿದು,", 
        "15715": "ಕತ್ತಲೆ ಮನೆಯೊಳಗೆ ಜ್ಯೋತಿಯ ಮುಟ್ಟಿಸಿದರೆ", 
        "15716": "ತಾಮಸಕೆ ಸಿಲ್ಕಿ ಭ್ರಮಿತನಾಗಬೇಡ.", 
        "15717": "ಹತ್ತುಕಡೆಗೆ ಹರಿದಾಡುವ ಕರಣಂಗಳಿಗೆ ಮುಖಗೊಡದೆ", 
        "15718": "ನಾನು ನೀನೆಂಬುಭಯವನಳಿದು,", 
        "15719": "ಆರು ಸ್ಥಲದಲ್ಲಿ ಆರು ಮೂರ್ತಿಗಳು", 
        "15720": "ಐವತ್ತೆರಡು ಸೋಪಾನದ ಮೇಲೆ", 
        "15721": "ಹೃದಯದಲ್ಲಿಪ್ಪ ಅಂಜನವ ತೆಗೆದು", 
        "15722": "ನಿತ್ಯವಾದ ಶರಣನು ಐವರ ಕೂಡಿಕೊಂಡು", 
        "15723": "ಬಟ್ಟಬಯಲನೊಳಕೊಂಡ ಶರಣನು", 
        "15724": "ಸುಟ್ಟಸುಡುಗಾಡಿನೊಳಗೊಬ್ಬ ಬಟ್ಟಬಯಲಾದ ಹೆಂಗಸು ಹುಟ್ಟಿ", 
        "15725": "ಇಪ್ಪತ್ತೈದು ಶಿವಾಲಯದ ಮೇಲೆ", 
        "15726": "ಆರು ಕಂಬದ ಮನೆಯೊಳಗೆ", 
        "15727": "ನಾದ ಬಿಂದು ಕಳಾತೀತಲಿಂಗದಲ್ಲಿ ಒಬ್ಬ ಭಾಮಿನಿಯು", 
        "15728": "ಐದು ರತ್ನದ ಮೇಲೆ ಒಂದು ಮಾಣಿಕ್ಯವ ಕಂಡೆನಯ್ಯ.", 
        "15729": "ಸರ್ವಾಂಗಪಟ್ಟಣದೊಳಗೆ ಇರುತಿಪ್ಪ ಪರಮನ ಕಂಡೆನಯ್ಯ.", 
        "15730": "ಇರುವೆಯ ಮಸ್ತಕದ ಮೇಲೆ ಇರುತಿಪ್ಪ", 
        "15731": "ಹಾರುವಗೇರಿಯಲ್ಲಿ ಇರುತಿಪ್ಪ ಐವರು ನಾರಿಯರ ಕಂಡೆನಯ್ಯ.", 
        "15732": "ಉರಿಯೊಳಗಣ ಪ್ರಕಾಶದಂತೆ", 
        "15733": "ಮೂರು ಮುಖದ ಭಾಮಿನಿಯು", 
        "15734": "ಆರು ಕಾಲುಳ್ಳ ಗೋವಿಂಗೆ", 
        "15735": "ಕಾಮದ ಕಳವಳವಳಿದು, ಸೀಮೆಯ ದಾಂಟಿ ನಿಸ್ಸೀಮನಾಗಿ,", 
        "15736": "ಕೊಂಬಿನ ತುದಿಯ ಮೇಲೆ ಎಂಟಾನೆ ಹುಟ್ಟಿದುದ ಕಂಡೆನಯ್ಯ.", 
        "15737": "ಹಾದಿಯಲ್ಲಿ ಬಿದ್ದಿಪ್ಪ ಮಾಣಿಕ್ಯವ ಕಂಡೆನಯ್ಯ.", 
        "15738": "ಊರಿಗೆ ಹೋಗುವ ದಾರಿಯಲ್ಲಿ ಉರಗನ ಕಂಡೆನಯ್ಯ.", 
        "15739": "ಆತ್ಮನೆಂಬ ಪ್ರಭೆಯಲ್ಲಿ ತ್ರಿಕೂಟಶಿವಾಲಯವ ಕಂಡೆನಯ್ಯ.", 
        "15740": "ಸ್ಥೂಲ ಸೂಕ್ಷ್ಮ ಕಾರಣ ಸಂಗದೊಳು ನಿಂದು", 
        "15741": "ಆರು ಬಣ್ಣದ ಪಟ್ಟಣದೊಳಗೆ", 
        "15742": "ಸಾಗರವ ನುಂಗಿದ ಕಪ್ಪೆ", 
        "15743": "ಬೆಟ್ಟದ ತುದಿಯ ಮೇಲೆ ತೊಟ್ಟಿಡುವ ಅಮೃತವ ಕಂಡೆನಯ್ಯ.", 
        "15744": "ಮೂರು ಮನೆಯ ಮೇಲೆ ಇಪ್ಪ ನಿರ್ವಾಣವ ಕಂಡೆನಯ್ಯ.", 
        "15745": "ಮೂರು ರತ್ನವ ಒಂದು ಕೋಗಿಲೆ ನುಂಗಿ ಕೂಗುತಿದೆ ನೋಡಾ.", 
        "15746": "ಹೃದಯದೊಳಗಿಪ್ಪ ಪ್ರಾಣಲಿಂಗವನು", 
        "15747": "ಜ್ಞಾನಶಕ್ತಿಯ ಸಂಗದಿಂದ", 
        "15748": "ಮನ ಬುದ್ಧಿ ಚಿತ್ತ ಅಹಂಕಾರವೆಂಬ", 
        "15749": "ಒಂಬತ್ತು ಬಾಗಿಲ ಪಟ್ಟಣದೊಳಗೆ", 
        "15750": "ಸುಂದರವಾದ ಮೇರುವೆಯೊಳಗೆ", 
        "15751": "ಒಳಹೊರಗೆ ಪರಿಪೂರ್ಣವಾದ ನಿಃಕಲಪರಬ್ರಹ್ಮಲಿಂಗದೊಳು ಕೂಡಿ", 
        "15752": "ಓಣಿಯೊಳಗೆ ಒಬ್ಬ ವಾಣಿಗಿತ್ತಿ ಕುಳಿತು", 
        "15753": "ಪೃಥ್ವಿ ಅಪ್ಪು ತೇಜ ವಾಯು ಆಕಾಶವೆಂಬ ಐದು ಮನೆಯಲ್ಲಿ", 
        "15754": "ತಾಯ ಒಡಲಲ್ಲಿ ಒಂದು ಮರಿ ಹುಟ್ಟಿ,", 
        "15755": "ಬೆಟ್ಟದ ತುದಿಯ ಮೇಲೆ ಘಟ್ಟಿಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15756": "ಐದು ಮೇರುವೆಯ ಮೇಲೆ ಒಂದು ಶಿವಾಲಯವ ಕಂಡೆನಯ್ಯ.", 
        "15757": "ಪಿಂಡಬ್ರಹ್ಮಾಂಡವ ಗರ್ಭೀಕರಿಸಿಕೊಂಡು", 
        "15758": "ಸರ್ವಬ್ರಹ್ಮಾಂಡವ ಒಬ್ಬ ದೇವ ನುಂಗಿಕೊಡಿಪ್ಪ ನೋಡಾ.", 
        "15759": "ಬಯಲೇ ಅಂಗವಾದ ಶರಣಂಗೆ", 
        "15760": "ಅಷ್ಟದಳದ ಮೇಲೆ ನಲಿದಾಡುವ ಹಂಸನ", 
        "15761": "ಲಿಂಗದ ನೆನಹಿನಿಂದ ಮಂಗಳಪ್ರಭೆಯ ನೋಡಿ,", 
        "15762": "ಐದು ಮನೆಯೊಳಗೆ ಏಳುಮಂದಿ ಹೆಂಡರ ಕಂಡೆನಯ್ಯ.", 
        "15763": "ಕಾಮ ಕ್ರೋಧ ಲೋಭ ಮೋಹ ಮದ ಮತ್ಸರಂಗಳೆಂಬ", 
        "15764": "ಸಚ್ಚಿದಾನಂದವೆಂಬ ಬೆಳಗಿನೊಳು", 
        "15765": "ಲಕ್ಷವ ನೋಡಿದರೇನಯ್ಯ? ಲಕ್ಷವ ಹೇಳಿದರೇನಯ್ಯ?", 
        "15766": "ಕರಿಯ ಮುಖದ ಸೂಳೆ ಹತ್ತೆಂಟು ಮುಖದೋರಿ", 
        "15767": "ಕತ್ತಲಕಾವುಳದ ಮನೆಯಲ್ಲಿ ಒಬ್ಬ ಪುರುಷನು ಸೀಮೆಯ ಪೆÇಕ್ಕು,", 
        "15768": "ಅಂಗಕ್ಕೆ ಆರುದಿನವೆಂಬುದ ನೀನು ಬಲ್ಲೆಯಯ್ಯಾ?", 
        "15769": "ಕರ್ಮದೋಷಂಗಳ ಪರಿಹಾರವಂ ಮಾಡಿ", 
        "15770": "ಗುಡ್ಡದೊಳಗೊಬ್ಬ ಮಡ್ಡ ಕುಳಿತು", 
        "15771": "ಒಳಹೊರಗೆ ಪರಿಪೂರ್ಣವಾದ ನಿಜಬೆಳಗಿನೊಳಗೆ", 
        "15772": "ಎನ್ನ ನಾನರಿಯದಂದು", 
        "15773": "ಅನಾದಿ ಸದ್ಗುರುವೆ, ಎನ್ನ ಕರ್ಮದೋಷಂಗಳ ಪರಿಹಾರವಂ ಮಾಡಿ", 
        "15774": "ದೇವರ ನಿರೂಪದಿಂದ ಎನ್ನಂತರಂಗದಲ್ಲಿ ಶಿವಾತ್ಮಜ್ಞಾನ ಉಕ್ಕಿ,", 
        "15775": "ಕಾಯದ ಕರಣಂಗಳನಳಿದು ಗುರುಸಂಬಂಧಿಯಾದನಯ್ಯ.", 
        "15776": "ಗುರುಲಿಂಗಜಂಗಮವೆಂಬ ತ್ರಿವಿಧಭೇದವನು ಏಕಾಗ್ರದಲ್ಲಿ ನೋಡಿ,", 
        "15777": "ಇಷ್ಟಲಿಂಗ ಪ್ರಾಣಲಿಂಗ ಭಾವಲಿಂಗವೆಂಬ ಲಿಂಗತ್ರಯವನರಿತು,", 
        "15778": "ಆರರಿಂದತ್ತತ್ತ ಮೀರಿದ ಮಹಾಮಹಿಮನ ಸಂಗದಿಂದ", 
        "15779": "ಅಂಗವೆಂಬ ಶಿವಾಲಯದೊಳಗೆ", 
        "15780": "ಕಣ್ಣಿಲ್ಲದಂಧಕಂಗೆ ಮೂವರು ಮಕ್ಕಳು ಹುಟ್ಟಿ,", 
        "15781": "ಆದಿಯನರಿತು ಭಕ್ತನಾಗಿ,", 
        "15782": "ಹೊನ್ನು ಹೆಣ್ಣು ಮಣ್ಣು ಬಿಟ್ಟು", 
        "15783": "ದೇಹವೆಂಬ ದೇಗುಲದೊಳಗೆ", 
        "15784": "ಅಂಗವೆಂಬ ದೈಗುಲದೊಳಗೆ ಒಬ್ಬ ಸತಿಯಳು ನಿಂದು", 
        "15785": "ಭೂತದೇಹಿಕವನಳಿದು ಚಿದಾತ್ಮಕನಾಗಿ,", 
        "15786": "ಊರಿಗೆ ಹೋಗುವ ಮಾರ್ಗದಲ್ಲಿ ಒಬ್ಬ ನಿರ್ಮಲಗಿತ್ತಿ ಕುಳಿತು", 
        "15787": "ದೇಶಕ್ಕೆ ಹೋಗುವ ಮಾರ್ಗದಲ್ಲಿ ಒಬ್ಬ ಭಾಷೆಗಳ್ಳ ಕುಳಿತು", 
        "15788": "ವಾಸನೆಗಳ ಹರಿದು ನಿಂದು, ಲೇಸಾದ ಜ್ಞಾನವ ಹಿಡಿದು,", 
        "15789": "ಹಲವು ಕಡೆಗೆ ಹರಿದಾಡುವ ಮನವ ಏಕಾಗ್ರದಲ್ಲಿ ನಿಲಿಸಿ", 
        "15790": "ಕರ್ಮದ ಗುಣವನಳಿದು ಅಸಮಾಯಲಿಂಗದೊಳು ಕೂಡಿ", 
        "15791": "ಸರ್ವಗುಣವ ಕಳೆದುಳಿದ ಪರಮನ ಸಂಗವ ಮಾಡಿ", 
        "15792": "ಸಾಗರದ ತುದಿಯ ಮೇಲೆ ಆಗರದ ಹಣ್ಣಾಗಿಪ್ಪುದು ನೋಡಾ.", 
        "15793": "ಐವರು ಅಂಗನೆಯರು", 
        "15794": "ಆರು ಮಂದಿರದೊಳಗೆ ಮೀರಿದ ಸತಿಯಳ ಕಂಡೆನಯ್ಯ.", 
        "15795": "ಗುರುವಿನಲ್ಲಿ ಗುಣವನರಸಲಿಲ್ಲ,", 
        "15796": "ಗುರುವು ಕಾಯಸುಖಿಯ ಮಾಡಿದನಯ್ಯ.", 
        "15797": "ಊರೊಳಗೆ ಆರು ನೆಲೆಯ ಮಂಟಪವ ಕಂಡೆನಯ್ಯ.", 
        "15798": "ಸಾಜವೆಂಬ ನಿಜಮಾರ್ಗದಲ್ಲಿ ವಾಜಿಯುಳ್ಳ ಸತಿಯಳು ನಿಂದು,", 
        "15799": "ಹಲವು ಕಡೆಗೆ ಹರಿದಾಡುವ ಹೊಲೆಮನವ ನಿಲಿಸಿ,", 
        "15800": "ಜ್ಞಾನಸಂಪನ್ನರಾದವರಿಂಗೆ ಹೀನವೃತ್ತಿವುಂಟೇನಯ್ಯ?", 
        "15801": "ತಾಮಸ ತಮಂಧಗಳಿಲ್ಲದೆ, ನೇಮ ನಿತ್ಯಗಳಿಲ್ಲದೆ,", 
        "15802": "ದೇವರ ನಿರೂಪದಿಂದ ದೇವಿಯ ಸಂಗವ ಮಾಡಿ", 
        "15803": "ಕಾಲಿಲ್ಲದ ಪುರುಷನು ಕೈಯಿಲ್ಲದ ನಾರಿಯ ಸಂಗವ", 
        "15804": "ಹುಟ್ಟಿಲ್ಲದ ನಾರಿ, ಹೊಂದಿಲ್ಲದ ಪುರುಷನ ಸಂಗವ ಮಾಡಿ,", 
        "15805": "ಕಟ್ಟಕಡೆಯಲೊಂದು ಬಟ್ಟಬಯಲ ಮನೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15806": "ತನುವಿನೊಳಗಿಪ್ಪ ಅನುಪಮ ಲಿಂಗವನು", 
        "15807": "ಉಲುಹು ಅಡಗಿದ ವೃಕ್ಷದಲ್ಲಿ ಫಲವಾದ ಹಣ್ಣ ಸವಿದು", 
        "15808": "ಅಂಬರದ ಮನೆಯೊಳಗೆ", 
        "15809": "ಧರೆ ಆಕಾಶವೆಂಬ ಅಂಡವನು ಒಂದು ಇರುವೆ ನುಂಗಿ,", 
        "15810": "ಹಲವು ಬಣ್ಣದಲ್ಲಿ ಒಬ್ಬ ಕಾಲಗಿತ್ತಿಯು", 
        "15811": "ಜ್ಞಾನಶಕ್ತಿಯ ಸಂಗದಿಂದ ಅವಿರಳಸ್ವಾನುಭಾವಸಿದ್ಧಾಂತವನರಿತು", 
        "15812": "ತನುಮನದ ಕೊನೆಯ ಮೇಲೆ", 
        "15813": "ನಿರುತ ನಿರಂಜನ ನಿರ್ದೇಶದಲ್ಲಿ ನಿಜಶರಣನಿಪ್ಪ ನೋಡಾ.", 
        "15814": "ಪೃಥ್ವಿ ಅಪ್ಪು ತೇಜ ವಾಯು ಆಕಾಶವೆಂಬ", 
        "15815": "ನಾಲ್ಕು ಕಂಬದ ಗುಡಿಗೆ ಇಪ್ಪತ್ತೈದು ರತ್ನಂಗಳ ಕೆತ್ತಿಸಿ,", 
        "15816": "ಹಗಲಾರುದಿನವೆಂಬುದ ನೀನೇ ಬಲ್ಲೆಯಯ್ಯಾ,", 
        "15817": "ನಿರುತನಿಜಸ್ವರೂಪದಲ್ಲಿ ಪರಮಾನಂದ ಲಿಂಗವಿಪ್ಪುದು ನೋಡಾ.", 
        "15818": "ಘಟ್ಟದ ತುದಿಯ ಮೇಲೆ ಮುಟ್ಟಿ ಆಡುವ ಬೆಳ್ಳಕ್ಕಿಯ ಕಂಡೆನಯ್ಯ.", 
        "15819": "ಶರೀರವೆಂಬ ಆಲಯದೊಳಗೆ ಮೂವರು ಭಾಮಿನಿಯರ ಕಂಡೆನಯ್ಯ.", 
        "15820": "ಪೃಥ್ವಿ ಅಂಗವಾದ ಭಕ್ತನು, ಅಪ್ಪು ಅಂಗವಾದ ಮಹೇಶ್ವರನು,", 
        "15821": "ದ್ವೈತ ಅದ್ವೈತಾನಂದದಲ್ಲಿ ಚಿದ್ರೂಪ ಚಿದಾನಂದಲಿಂಗವ ಕೂಡಿ", 
        "15822": "ಆದಿಯಿಂದಲಾದ ಪುರುಷನು ಮೇದಿನಿಗೆ ಬಂದು,", 
        "15823": "ವಾಸನಿಗಳಿಲ್ಲದೆ ನಿರ್ವಾಸನಿಯಾದನಯ್ಯ.", 
        "15824": "ಮನವು ಮನವ ನುಂಗಿ, ನೆನವು ನೆನವ ನುಂಗಿ,", 
        "15825": "ತ್ರಿಕೂಟದ ಶಿವಾಲಯದೊಳಗಿಪ್ಪ", 
        "15826": "ಐದು ಮೇರುವೆಯ ಮೇಲೆ ಐದಿಪ್ಪ ಸತಿಯಳ ಕಂಡೆನಯ್ಯ.", 
        "15827": "ಆರುಮಂದಿ ಅಂಗನೆಯರು ಮೂವರ ಸಂಗವ ಮಾಡಿ", 
        "15828": "ಕಾಲಕರ್ಮಿಗಳಿಗೆ ಸಿಲ್ಕದೆ ಲಿಂಗಾಂಗಸಮರಸವನರಿತು", 
        "15829": "ಹಲವು ಕಡೆಗೆ ಹರಿದಾಡುವ ಕರಣಂಗಳಿಗೆ ಮುಖಗೊಡದೆ", 
        "15830": "ತನುವಿನ ಗುಣಕ್ರಿಯಂಗಳಿಗೆ ಮುಖಗೊಡದೆ", 
        "15831": "ರವಿಶಶಿಯ ಬೆಳಗನೊಳಕೊಂಡಿಹ", 
        "15832": "ಮಾತುಮಥನಗಳಿಲ್ಲದೆ, ಜಾತಿಜನನವಿಲ್ಲದೆ, ಭ್ರಾಂತಿಸೂತಕವಿಲ್ಲದೆ,", 
        "15833": "ಹತ್ತು ಮುಖದಲ್ಲಿ ಕಾಡುವ ದಶಯಿಂದ್ರಿಯಗಳಿಗೆ ಮುಖಗೊಡದೆ", 
        "15834": "ಬೆಟ್ಟದ ತುದಿಯ ಮೇಲೆ", 
        "15835": "ಬತ್ತಲೆಯಾದ ಭಾಮಿನಿಯು,", 
        "15836": "ಐವರು ಭಾಮಿನಿಯರು ಐದು ಮುಖದಲ್ಲಿ ನಿಂದು,", 
        "15837": "ಅಂಗವೆಂಬ ಭೂಮಿಯಲ್ಲಿ ಸಂಗಸಮರಸವೆಂಬ ಪುರುಷನು", 
        "15838": "ಮಹಾಜ್ಞಾನಿಯ ಸಂಗದಿಂದ", 
        "15839": "ನಿರಪೇಕ್ಷಲಿಂಗದಲ್ಲಿ ನಿರ್ಭರಿತವಾದ ಸತಿಯಳು", 
        "15840": "ನಾದಬಿಂದುಕಲೆಗಳಿಂದತ್ತತ್ತ ಮಹಾಪುರುಷನ ಕಂಡೆನಯ್ಯ.", 
        "15841": "ಜ್ಞಾನ ತನ್ನೊಳಗಾದ ಬಳಿಕ ಲೋಕದ ಹಂಗಿನ್ನ್ಯಾತಕಯ್ಯ?", 
        "15842": "ಪಂಚಮುಖದ ಸರ್ಪನ ತಲೆಯ ಮೇಲೆ", 
        "15843": "ಈಡಾ ಪಿಂಗಳ ಸುಷಮ್ನನಾಳದಿಂದತ್ತತ್ತ", 
        "15844": "ಆರು ಚಕ್ರಂಗಳಲ್ಲಿ ಆರು ಮೂರ್ತಿಗಳ ಕಂಡೆನಯ್ಯ,", 
        "15845": "ಸ್ವಯಂಪ್ರಕಾಶಲಿಂಗದಲ್ಲಿ ಸ್ವಯಂಜ್ಞಾನಿ ನಿಂದು,", 
        "15846": "ಐದು ತತ್ವ, ಐದು ಭೂತ, ಐದು ಅಂಗ,", 
        "15847": "ಆರು ಚಕ್ರ, ಆರು ಮೂರ್ತಿಗಳು,", 
        "15848": "ಆಜ್ಞಾÉೀಯಚಕ್ರದಲ್ಲಿಪ್ಪ ಲಿಂಗವನು ಬ್ರಹ್ಮಚಕ್ರಕೆ ತಂದು,", 
        "15849": "ನಿರ್ಮಲಸ್ವರೂಪನಾದ ಶರಣನು ನಿತ್ಯನಿಜದಲ್ಲಿ ನಿಂದು", 
        "15850": "ಕಪ್ಪೆಯ ನುಂಗಿದ ಕೋಗಿಲೆ ಸತ್ತು ಕೂಗುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15851": "ನಾದದೊಳಗಣ ನಾದವ ತೋರಿಸಯ್ಯ.", 
        "15852": "ಸದಮಲದರುಹಿನಿಂದ ಸಾಧಿಸಿದಮಲಲಿಂಗವ ಕಂಡು", 
        "15853": "ಸಮುದ್ರಘೋಷವೆಂಬ ಲಿಂಗದಲ್ಲಿ ಮುದ್ರಿತನ ಕಂಡೆನಯ್ಯ.", 
        "15854": "ಸುಜ್ಞಾನಪ್ರಭೆಯಲ್ಲಿ ನಿಂದು,", 
        "15855": "ನಿಃಕಲನ ಸಂಗದಿಂದ ಝೇಂಕಾರನಾದನಯ್ಯ.", 
        "15856": "ಮಹಾಜ್ಞಾನಸಂಬಂಧದಿ ಅವಿರಳಸ್ವಾನುಭವಸಿದ್ಧಾಂತವನರಿತು", 
        "15857": "ನಾದದೊಳಗಣ ಚಿನ್ನಾದವ ತೋರಿಸಯ್ಯ.", 
        "15858": "ಸ್ಥೂಲ ಸೂಕ್ಷ್ಮ ಕಾರಣ ಅವಸ್ಥೆಯನರಿತು", 
        "15859": "ನಾನೂ ಇಲ್ಲದೆ, ನೀನೂ ಇಲ್ಲದೆ,", 
        "15860": "ನಿರ್ಮಲಸ್ವರೂಪವಾದ ಶರಣನು ನಿತ್ಯನಿಜದಲ್ಲಿ ನಿಂದು", 
        "15861": "ಹೃತ್ಕಮಲದಲ್ಲಿಪ್ಪ ಜಂಗಮವು ನಿತ್ಯನಿಜದಲ್ಲಿ ನಿಂದು,", 
        "15862": "ಅಂತರಂಗ ಬಹಿರಂಗ ಭೇದವನರಿತು", 
        "15863": "ನಿರಂಜನದಿಂದತ್ತತ್ತ ಪರವಸ್ತು ತಾನೇ ನೋಡಾ.", 
        "15864": "ಅಂಗಕೆ ಆರುದಿನ, ಲಿಂಗಕೆ ಮೂರುದಿನ,", 
        "15865": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರ ಈಶ್ವರ ಸದಾಶಿವ", 
        "15866": "ಬೆಟ್ಟದ ತುದಿಯ ಮೇಲೆ", 
        "15867": "ಅಗಮ್ಯ ಅಗೋಚರ ಅಪ್ರಮಾಣಲಿಂಗದಲ್ಲಿ", 
        "15868": "ಸರ್ವಾಂಗಲಿಂಗಮಯವಾದ ಶರಣನು", 
        "15869": "ಸಾವಿರೆಸಳಮಂಟಪದಲ್ಲಿ ಒಬ್ಬ ಸತಿಯಳು ನಿಂದಿರುವುದ ಕಂಡೆನಯ್ಯ.", 
        "15870": "ಭಕ್ತಗಣ, ಮಹೇಶ್ವರಗಣ, ಪ್ರಸಾದಿಗಣ,", 
        "15871": "ತತ್ವಮಸಿ ವಾಕ್ಯದಿಂದತ್ತತ್ತ ಸ್ವಯಂಜ್ಯೋತಿಲಿಂಗವಿಪ್ಪುದು ನೋಡಾ.", 
        "15872": "ತನುಮನದ ಕೊನೆಯ ಮೇಲೆ ಮಹಾಘನಲಿಂಗವಿಪ್ಪುದು ನೋಡಾ.", 
        "15873": "ಕಂಗಳಾಟದ ಮಧ್ಯದಲ್ಲಿಪ್ಪ ಮಂಗಳಮಹಾಲಿಂಗವು", 
        "15874": "ಉಪ್ಪು ಅಪ್ಪು ಕೂಡಿದಂತೆ,", 
        "15875": "ಶಿವಾನುಭಾವಿಗಳ ಸಂಗದಿಂದ ಲಿಂಗಾನುಭಾವವ ಕಂಡೆನಯ್ಯ.", 
        "15876": "ನಿರಾಮಯವೆಂಬ ಭಕ್ತನ ಅಂಗದಲ್ಲಿ", 
        "15877": "ಬದ್ಧಜ್ಞಾನಿಯ ಸಂಗದಿಂದ ಅಭೇದ್ಯನಾದನಯ್ಯ.", 
        "15878": "ಅಣೋರಣಿಯಾನ್ ಮಹತೋಮಹೀಯಾನ್ ಎಂಬ ಶ್ರುತಿಗಳಿಲ್ಲದಂದು,", 
        "15879": "ಪೃಥ್ವಿ ಸಲಿಲ ಪಾವಕ ಪವನ ಅಂಬರ ರವಿ ಶಶಿ ಆತ್ಮಗಳು ಇಲ್ಲದಂದು,", 
        "15880": "ವಾಸನಿಧರ್ಮಗಳಿಲ್ಲದೆ ನಿರ್ವಾಸಕಧರ್ಮಿಯಾಗಿ,", 
        "15881": "ವಿಮಲಜ್ಞಾನದಿಂದ ಪರತತ್ವವನರಿತು", 
        "15882": "ಪರಮಾನಂದ ಸುಖ, ಮಹಾಲಿಂಗದ ಬೆಳಗು,", 
        "15883": "ಸಾವಿರ ಕಂಬದ ಮುತ್ತಿನಮಂಟಪದೊಳಗೆ", 
        "15884": "ಪರಮಸುಖದಿಂದ ಪರವಶವೆಂಬ ಸತಿಯಳ ಸಂಗವ ಮಾಡಿ,", 
        "15885": "ದೇಗುಲದೊಳಗಿರುವ ದೇವರ ಕಂಡೆನಯ್ಯ.", 
        "15886": "ಅನಂತಮುಖ, ಅನಂತಲೋಚನ, ಅನಂತಪ್ರಕಾಶ,", 
        "15887": "ಅಂಗವಿಲ್ಲದ ನಾರಿಯ ಮನೆಯಲ್ಲಿ", 
        "15888": "ದೇಹವೆಂಬ ದೇಗುಲದೊಳಗೆ", 
        "15889": "ಮಹಾಮೇರುವೆಯೊಳಗೆ", 
        "15890": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರಾದಿಗಳಿಂದತ್ತತ್ತ", 
        "15891": "ನಾಲ್ಕು ಕಂಬದ ದೇಗುಲದೊಳಗೆ ಸುಳಿದಾಡುವ ಶಿಶುವ", 
        "15892": "ಐವರು ನಾರಿಯರು ತ್ರಿಕೂಟದ ಗಿರಿಯನೇರಿ", 
        "15893": "ಆರು ಶಿಲೆಯ ಮಂಟಪದೊಳಗೆ ಮೂವರು ಪುರುಷರು ಕೂಡಿ", 
        "15894": "ಆರುತತ್ವದ ಮೇಲೆ ನಿತ್ಯತ್ವ ನಿಜಪರಬ್ರಹ್ಮಲಿಂಗವು ತಾನೇ ನೋಡಾ.", 
        "15895": "ಅಷ್ಟಕುಲಪರ್ವತದ ಮೇಲೆ ಮಹಾ ದೃಷ್ಟಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15896": "ಐವತ್ತೆರಡೆಸಳಿನ ಸ್ಥಾವರಗದ್ದುಗೆಯ ಮೇಲೆ", 
        "15897": "ಒಂಬತ್ತು ಮಂದಿರದೊಳಗೆ", 
        "15898": "ಜಾಗ್ರಾವಸ್ಥೆಯ ಸ್ಥೂಲದಿಂದರಿದು ಭಕ್ತನಾದೆನಯ್ಯ.", 
        "15899": "ಸಹಜಸಮ್ಯಕ್ಜ್ಞಾನದಿಂದ ಪರಮಾನಂದಬೆಳಗಿನೊಳು ನಿಂದು,", 
        "15900": "ನಿರಂಜನಸ್ಥಲದಲ್ಲಿ ನಿರಾವರಣವಾದ ಶರಣನು", 
        "15901": "ಬ್ರಹ್ಮನಿಲ್ಲದಂದು, ವಿಷ್ಣುವಿಲ್ಲದಂದು, ರುದ್ರನಿಲ್ಲದಂದು,", 
        "15902": "ಬೇರೊಂದು ಸ್ಥಾನದಲ್ಲಿ ಒಬ್ಬ ಸತಿಯಳು ನಿಂದು,", 
        "15903": "ನಿರಂಜನದೇಶದಲ್ಲಿ ಮಹಾಮಹಿಮನ ಕಂಡೆನಯ್ಯ.", 
        "15904": "ಓಂ ನಮಃ ಶಿವಾಯವೆಂಬ ಷಡಕ್ಷರಮಂತ್ರವ", 
        "15905": "ತನುಮನದ ಕೊನೆಯ ಮೇಲೆ ಮಹಾಘನಲಿಂಗವಿಪ್ಪುದು ನೋಡಾ.", 
        "15906": "ಬ್ರಹ್ಮರಂಧ್ರದಲ್ಲಿಪ್ಪ ಮಹಾಜ್ಞಾನಲಿಂಗವೇ ಚಿನ್ನಾದ ನೋಡಾ.", 
        "15907": "ಆಕಾರಲಿಂಗದ ಸಂಗದಿಂದ ನಿರಾಕಾರಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15908": "ನಿರ್ಮಲಸ್ವರೂಪದಿಂದ ಜ್ಞಾನಸಂಬಂಧಿಯಾಗಿ,", 
        "15909": "ಊರಮುಂದಳ ಗುಡಿಯಲ್ಲಿ ಕೋಳಿ ಕುಳಿತು ಕೂಗಲೊಡನೆ", 
        "15910": "ಇರುಳು ಹಗಲೆಂಬೆರಡು ಮಹಾಘನಲಿಂಗದೊಳಡಗಿ", 
        "15911": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ರುದ್ರ ಈಶ್ವರ ಸದಾಶಿವ ಪರಶಿವನಿಂದತ್ತತ್ತ,", 
        "15912": "ಆರು ಸ್ಥಲದಲ್ಲಿ ಆರು ಲಿಂಗವ ಕಂಡೆನಯ್ಯ.", 
        "15913": "ಉತ್ತಮಜ್ಞಾನದಿಂದ ಪರಮಾನಂದಲಿಂಗವ ನೋಡಿ,", 
        "15914": "ಪೃಥ್ವಿ ಅಪು ತೇಜ ವಾಯು ಆಕಾಶದಿಂದತ್ತತ್ತ ಮಹಾಲಿಂಗದ ಬೆಳಗು.", 
        "15915": "ಅಂಗವಿಲ್ಲದ ನಾರಿಯು ಸಂಗವಿಲ್ಲದ ಪುರುಷನ ನೆರೆದು", 
        "15916": "ಅಂಗದೊಳಗಿಪ್ಪ ಲಿಂಗವನು ಸಹಜಸಮ್ಯಕ್ಜ್ಞಾನದಿಂದ ತಿಳಿದು,", 
        "15917": "ಒಂಬತ್ತು ಬಾಗಿಲ ಮನೆಯೊಳಗೆ ಅಂಬರಗಿತ್ತಿಯ ಕಂಡೆನಯ್ಯ.", 
        "15918": "ಇರುಳು ಹಗಲ ನುಂಗಿ, ಹಗಲು ಇರುಳ ನುಂಗಿ,", 
        "15919": "ಒಳಹೊರಗೆ ಪರಿಪೂರ್ಣವಾದ ಮಹಾಶರಣನ ಸಂಗದಿಂದ", 
        "15920": "ಕುಲbsಲಕ್ಕೆ ಹೋರಿ ಆಡುವರೆಲ್ಲ ಶಿವಭಕ್ತರೆ ?", 
        "15921": "ಸಕಲ ಜಗಂಗಳೆಲ್ಲ ನಿಃಕಲದ ಭೇದವ ಬಲ್ಲರೇನಯ್ಯ ?", 
        "15922": "ಪೃಥ್ವಿ ಅಪ್ಪುಗಳಿಲ್ಲದಂದು, ತೇಜ ವಾಯುಗಳಿಲ್ಲದಂದು,", 
        "15923": "ಬ್ರಹ್ಮ ವಿಷ್ಣು ಆದಿಗಳಿಲ್ಲದಂದು,", 
        "15924": "ಭಕ್ತ ಮಹೇಶ್ವರರಿಲ್ಲದಂದು,", 
        "15925": "ಗುರು ಲಿಂಗ ಜಂಗಮವಿಲ್ಲದಂದು,", 
        "15926": "ಲಿಂಗವೇ ತಾನಾಗಿ, ತಾನೇ ಲಿಂಗವಾಗಿ,", 
        "15927": "ಅಂಗವಿಲ್ಲದ ಪುರುಷನು ಲಿಂಗಾರ್ಚನೆಯಂ ಮಾಡಿ,", 
        "15928": "ಆತ್ಮನೆಂಬ ಬೆಳಗಿನಿಂದತ್ತತ್ತ ನಿರಾತ್ಮನೆಂಬ ಲಿಂಗವು,", 
        "15929": "ಐದರಿಂದತ್ತತ್ತ ಮಹಾಮಹಿಮನ ಕಂಡೆನಯ್ಯ,", 
        "15930": "ಪರಶಿವತತ್ವದಿಂದ ಪರಮಜ್ಞಾನಿಯಾಗಿ,", 
        "15931": "ಆರು ಚಕ್ರಂಗಳಲ್ಲಿ ಆರು ಮೂರ್ತಿಗಳು", 
        "15932": "ಸರ್ವಾಂಗಪಟ್ಟಣದೊಳಗೆ ಇರುತಿಪ್ಪ ಮಹಾಮಹಿಮನ", 
        "15933": "ಅತ್ತೆಯ ಬಸುರಲಿ ಮಗಳು ಹುಟ್ಟಿ,", 
        "15934": "ಅಂಗಲಿಂಗ ಸಮರಸವಾದಲ್ಲದೆ,", 
        "15935": "ನಾದಬಿಂದುಕಳಾತೀತಲಿಂಗವನು ಮಹಾಜ್ಞಾನದಿಂದ ತಿಳಿದು,", 
        "15936": "ಹೃತ್ಕಮಲದಲ್ಲಿ ನೆಲೆಸಿಪ್ಪ ಪ್ರಾಣಲಿಂಗವನು ಸ್ವಯಜ್ಞಾನದಿಂದ ತಿಳಿದು,", 
        "15937": "ಸರ್ಪನ ತಲೆಯ ಮೇಲೆ ಒಂದು ರತ್ನವಿಪ್ಪುದು ನೋಡಾ.", 
        "15938": "ಅಂಗನೆಯರು ಆರುಮಂದಿ, ಸಂಗಸಮರಸದಿಂದ", 
        "15939": "ನಾನು ಇಲ್ಲದೆ, ನೀನು ಇಲ್ಲದೆ,", 
        "15940": "ಒಂಬತ್ತು ಬಾಗಿಲ ಮನೆಯೊಳಗೆ", 
        "15941": "ಅಂಗಲಿಂಗಸಮರಸವಾದ ಬಳಿಕ ಕರಣದ ಹಂಗಿನ್ನ್ಯಾತಕಯ್ಯ?", 
        "15942": "ಅಂಬರದ ಮನೆಯೊಳಗೆ ಗಾಂಭೀರ್ಯತ್ವದ ಅಂಗನೆಯ ಕಂಡೆನಯ್ಯ.", 
        "15943": "ಇಪ್ಪತ್ತೈದು ತಲೆಯ ಮೇಲೆ ಸುಳಿದಾಡುತಿಪ್ಪ", 
        "15944": "ಬ್ರಹ್ಮ ವಿಷ್ಣುವಿನಲ್ಲಿ ಎಯ್ದಿ, ವಿಷ್ಣು ರುದ್ರನಲ್ಲಿ ಎಯ್ದಿ,", 
        "15945": "ಭಕ್ತ ಮಹೇಶ್ವರನಲ್ಲಿ ಅಡಗಿ,", 
        "15946": "ಆಚಾರಲಿಂಗ ಗುರುಲಿಂಗದಲ್ಲಿ ಅಡಗಿ,", 
        "15947": "ಚಿದ್ರೂಪ ಚಿದಾನಂದದಲ್ಲಿ", 
        "15948": "ಅಷ್ಟಾಂಗಯೋಗವ ದಾಂಟಿ,", 
        "15949": "ಭೂಚರ ಖೇಚರ ಅಗೋಚರ ಚರಾಚರ", 
        "15950": "ಷಣ್ಮುದ್ರೆಗಳಿಂದತ್ತತ್ತ ಮಹಾಘನ ಪರಾಪರಜ್ಞಾನ,", 
        "15951": "ಆಜ್ಞಾÉೀಯಚಕ್ರದಲ್ಲಿ ಮಹಾಗೋಚರಲಿಂಗವ ಕಂಡೆನಯ್ಯ."
    }
}