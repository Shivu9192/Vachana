    "vachana": {
        "8717": "ಅಂಗದ ಕಳವಳ, ಮನದ ಸಂಚಲ, ಭಾವದ ಭ್ರಾಂತು,", 
        "8718": "ಅಂಗದ ಕಳೆ ಲಿಂಗಸಂಗವಾಗಿ, ಅಂಗವೆಂಬ ಶಂಕೆಯ ಭಂಗವ ತೊರದು.", 
        "8719": "ಅಂಗದಂತೆ ಲಿಂಗವಾಗಿರ್ದ ದೇಹ,", 
        "8720": "ಅಂಗದ ಮೇಲಣ ಲಿಂಗ ಅಂಗದಲ್ಲಿ ಪೂರ್ಣವಾಗಿ", 
        "8721": "ಅಂಗದ ಮೇಲಣ ಲಿಂಗ ಹಿಂಗಿದಾತ ಭವಿ ಎಂಬರು.", 
        "8722": "ಅಂಗದ ಮೇಲೆ ಶಿವಲಿಂಗಸಾಹಿತ್ಯವಿಲ್ಲದ ಅಂಗ", 
        "8723": "ಅಂಗದ ಮೇಲೆ ಸಾಕಾರವಿಡಿದು", 
        "8724": "ಅಂಗದಲ್ಲಿ ಲಿಂಗವಿರಲು ಎನ್ನ ತನು ನಿರ್ಮಲವಾಯಿತ್ತು,", 
        "8725": "ಅಂಗದಾಸೆಯುಳ್ಳನ್ನಕ್ಕ ಭಯ ಬಿಡದು ನೋಡಾ ಅಯ್ಯಾ.", 
        "8726": "ಅಂಗಲಿಂಗ ಮನಲಿಂಗ ಪ್ರಾಣಲಿಂಗ ಭಾವಲಿಂಗ", 
        "8727": "ಅಂಗಲಿಂಗರತಿಯಿಂದ ಆಯತವಾಯಿತ್ತು,", 
        "8728": "ಅಂಗ ಲಿಂಗವಾದಡದ್ಭುತಕ್ಕಾಳಪ್ಪುದೆ?", 
        "8729": "ಅಂಗವು ಲಿಂಗವೇಧೆಯಾದ ಬಳಿಕ", 
        "8730": "ಅಂಗವೆಂಬ ಅಂಗನೆ ಆತ್ಮನೆಂಬ ಪುರುಷನನಪ್ಪಿ", 
        "8731": "ಅಂಗವೆ ಲಿಂಗ, ಲಿಂಗವೆ ಅಂಗವೆಂದೆಂಬರು.", 
        "8732": "ಅಂಗಸುಖಿಗೆ ಲಿಂಗಸುಖವಳವಡದು,", 
        "8733": "ಅಂಗೈಯೊಳಗಣ ಹಂಸೆ ಹಾಲನೊಲ್ಲದೆ ನೀರ ಕುಡಿಯಿತ್ತು.", 
        "8734": "ಅಂತರಂಗದ ಆತ್ಮಜ್ಯೋತಿ ಬಹಿರಂಗವ ಮುಟ್ಟಿ", 
        "8735": "ಅಂದಿನವರು! ಇಂದಿನವರು! ಎಂಬ ಸಂದೇಹಿಗಳಿಗೆ", 
        "8736": "ಅಕಲ್ಪಿತ ಲಿಂಗವು ಕಲ್ಪಿತಕ್ಕೆ ಬಂದು", 
        "8737": "ಅಖಂಡಿತನಾದ ಬ್ರಹ್ಮ ಸೂಕ್ಷ್ಮ ಸುನಾದದ ಬೆಳಗು,", 
        "8738": "ಅಗಣಿತನದ್ವಯನನುಪಮನಪ್ಪ", 
        "8739": "ಅಜಕಲ್ಪನೆಗೊಳಗಾದ ಊರೊಳಗೆ", 
        "8740": "ಅಜಾಂಡ ಪಿಂಡಾಂಡ ಚಿದ್ಬ್ರಾಂಡವೆಂಬ ತ್ರಿಭಾಂಡದೊಳಗೆ", 
        "8741": "ಅತಕ್ರ್ಯ ಅಪ್ರಮಾಣ ಅನಾಮಯ ಅನುಪಮ ಸರ್ವಗತ", 
        "8742": "ಅತ್ಯತಿಷ್ಠದ್ದಶಾಂಗುಲವೆಂದೆಂಬಿರಿ,", 
        "8743": "ಅದ್ವೈತನ ಹಸ್ತದಲ್ಲಿ ಅಪ್ರತಿಮನೆಂಬ ಲಿಂಗವಿದ್ದಿತ್ತು,", 
        "8744": "ಅದ್ವೈತವನೋದಿ ಎರಡಳಿದೆವೆಂಬ ಅಣ್ಣಗಳು ನೀವು ಕೇಳಿರೆ.", 
        "8745": "ಅಧೋಮುಖದಷ್ಟದಳಕಮಲದಲ್ಲಿ", 
        "8746": "ಅಧೋಮುಖವಾಗಿಪ್ಪ ಹೃದಯಕಮಲವನೂಧ್ರ್ವಮುಖವ ಮಾಡಿ,", 
        "8747": "ಅನವರತ ಲಿಂಗಭಾವದಲ್ಲಿ ನೆನಹು ನೆಲೆಗೊಂಡು, ಬಿಡುವಡಗಿ,", 
        "8748": "ಅನಾದಿಯ ಲಿಂಗವ ಕಂಡು, ಆದಿಯ ಪ್ರಸಾದ ಕೊಂಡು,", 
        "8749": "ಅನುಭಾವ ಸಾಹಿತ್ಯವಾದ ಬಳಿಕ ಅನುಭವಿಸಲೆಲ್ಲಿಯದೊ?", 
        "8750": "ಅಯ್ಯಾ, ತನುವಿದ್ದಂತೆ", 
        "8751": "ಅಯ್ಯಾ, ನಿಮ್ಮನೆನ್ನ ಕರಸ್ಥಲದಲ್ಲಿ ಧರಿಸಿದಡೆ", 
        "8752": "ಅಯ್ಯಾ, ನಿಶೆಯ ಮೊತ್ತ ಸವಿತನ ಕಣ್ಗೆ ತಮವನೀಯಬಲ್ಲುದೆ?", 
        "8753": "ಅಯ್ಯಾ, ಹುಲ್ಲುಮೊರಡಿಯೊಳಗೆ ಕಲ್ಪತರುವನರಸುವರೆ?", 
        "8754": "ಅರಿವರತು ಅರಿವ ತಿಳಿದು, ಮರಹ ಮರೆದು ಮರಹಳಿದು,", 
        "8755": "ಅರಿವರತು ಅರಿವುಗೆಟ್ಟಿತ್ತು,", 
        "8756": "ಅರಿವರತು, ಮರಹರತು, ಕುರುಹಳಿದು,", 
        "8757": "ಅರುವಿನ ಕ್ರಿಯೆಯಲ್ಲಿ ತೊಳತೊಳಗಿ ಬೆಳಗುವ", 
        "8758": "ಅರಿವಿನ ತರಹರದಿಂದ ಭಾವ ಸದ್ಭಾವವನೆಯ್ದಿದ ಶರಣನ", 
        "8759": "ಅರಿವು ಮರಹಳಿದುದಕ್ಕೇನು ದೃಷ್ಟ?", 
        "8760": "ಅರಿವು ಮರಹಳಿದು ನಿಜದಲ್ಲಿ ನಿಂದ ನಿರ್ಧರಂಗೆ", 
        "8761": "ಅರಿವುಳ್ಳವರೆಲ್ಲಾ ಎಡೆಯಲ್ಲಿ ಉಂಟು,", 
        "8762": "ಅವಿರಳಾತ್ಮಕನಾಗಿ ನಿಃಕಲಬ್ರಹ್ಮದೊಳೊಡವೆರಸಿ", 
        "8763": "ಅಷ್ಟಭೋಗಂಗಳ ಕಾಮಿಸಿ,", 
        "8764": "ಅಷ್ಟಭೋಗಂಗಳಭಿಲಾಷೆಯಿಂದಿಪ್ಪ ವೇಷಧಾರಿಗಳಿಗೆ", 
        "8765": "ಅಷ್ಟಭೋಗಂಗಳ ಶಕ್ತಿ ಕೆಟ್ಟಲ್ಲಿ ರೋಧಮಲ ನಷ್ಟ.", 
        "8766": "ಅಷ್ಟಾಷಷ್ಠಿ ತೀರ್ಥಂಗಳ ಮೆಟ್ಟಿದವರೆಲ್ಲಾರೂ ಭಕ್ತರಪ್ಪರೇ?", 
        "8767": "ಅಶ್ವತ ಕಂಬಳರ ಕರ್ಣದಲ್ಲಿ ಧರಿಸಿದೆಯಯ್ಯಾ,", 
        "8768": "ಅಸಮಾನಾಢ್ಯರಪ್ರತಿಮಸ್ವತಂತ್ರ ಸ್ವಲೀಲರಪ್ಪ ಶರಣರೆಂತಿಪ್ಪರಯ್ಯಾ?", 
        "8769": "ಅಸ್ತಿ ಭಾತಿ[ಪ್ರಿ]ಯವೆಂಬ ತ್ರಿವಾಕ್ಯದಿಂದಲ್ಲವಾದುದಲ್ಲ.", 
        "8770": "ಅಹಂಕಾರ ಮಮಕಾರಂಗಳಲ್ಲಿ ಸಂದು, ತ್ರಿಗುಣದಲ್ಲಿ ಕೂಡಿ,", 
        "8771": "ಆಕಾರವಿಡಿದು ಅರ್ಚನೆಯ ಮಾಡಬೇಕಲ್ಲದೆ", 
        "8772": "ಆಕಾಶದಲಾಡುವ ಹದ್ದನೊಂದು ಸರ್ಪನುಂಗಿತ್ತ ಕಂಡೆ.", 
        "8773": "ಆಕಾಶದಲ್ಲಿ ಪಂಚಮಹಾನಾದಂಗಳುಂಟು,", 
        "8774": "ಆಕೃತಿ ನಿರಾಕೃತಿಗಳೆಂಬ ಆಕೃತಿಯಳಿದು,", 
        "8775": "ಆಚಾರದನುವಳವಟ್ಟು, ಕರಣಂಗಳನುಡುಗಿ, ತನುವ ಸವೆವುದು ಗುರುವಿನಲ್ಲಿ.", 
        "8776": "ಆಚಾರಲಿಂಗ ನಾಸ್ತಿಯಾದಲ್ಲದೆ ಭಕ್ತನಲ್ಲ.", 
        "8777": "ಆಡಿನ ತತ್ತಿಯೊಳಗೊಂದು ಹೇರಡವಿಯಿದ್ದಿತ್ತು.", 
        "8778": "ಆತ್ಮತೇಜದಿಂ ತೋರ್ಪ ಚಿದಹಮ್ಮಿನ ಮೂಲಾಹಂಕಾರವೆ", 
        "8779": "ಆತ್ಮ ತೇಜ, ದೇಹೋ[ಹಂ] ಹಮ್ಮು, ಮನದ ಬಿಮ್ಮು,", 
        "8780": "ಆತ್ಮನೆಂಬ ಅಂಬುಧಿಯಲ್ಲಿ", 
        "8781": "ಆದಿ ಅನಾದಿಯಿಲ್ಲದಂದು, ನಾದ ಬಿಂದು ಕಳೆ ಮೊಳೆದೋರದಂದು,", 
        "8782": "ಆದಿಬಿಂದುವಿನಲ್ಲಿ ಅಖಂಡಿತ ಬಯಲು", 
        "8783": "ಆನೆಂಬ ಹಮ್ಮು, ನೀನೆಂಬ ಚಿದಹಮ್ಮು,", 
        "8784": "ಆ ಪರಬ್ರಹ್ಮವೆ ಓಂಕಾರವಪ್ಪ ಪ್ರಣವಸ್ವರೂಪು, ಪರಮಾತ್ಮನೆನಿಸಿ", 
        "8785": "ಆಯತಲಿಂಗ ಅಂಗದಲ್ಲಿ ವೇಧಿಸಿಕೊಂಡ ಬಳಿಕ", 
        "8786": "ಆಯತ ಸ್ವಾಯತ ಸನ್ನಿಹಿತವೆಂದು ನುಡಿವ", 
        "8787": "ಆರು ದರುಶನಂಗಳು ತಮತಮಗೆ", 
        "8788": "ಆರು ಬಣ್ಣದ ಆರು ಲಿಂಗದಲ್ಲಿ", 
        "8789": "ಆಶ್ರಮಿಗಳಿದ್ದು ಅತ್ಯಾಶ್ರಮವ ತಳೆದು", 
        "8790": "ಆಶ್ರಯನಲ್ಲ ಅನಾಶ್ರಯನಲ್ಲ,", 
        "8791": "ಇಂದ್ರಿಯದ ಬಾಗಿಲಲ್ಲಿ ಮನವಿಪ್ಪುದು.", 
        "8792": "ಇನನ ಕರದಲ್ಲಿ ತಮವಿದ್ದು ಬದುಕಬಲ್ಲುದೆ?", 
        "8793": "ಇಲ್ಲವುಂಟೆಂಬ ಶಬ್ದನಿಃಶಬ್ದವೆಲ್ಲಾ", 
        "8794": "ಇಷ್ಟಲಿಂಗಕ್ಕೆ ರೂಪನರ್ಪಿಸಿ,", 
        "8795": "ಈ ತಾಮಸ ರುದ್ರತ್ವದಿಂ ಸ್ಥಿರಚಿತ್ತದಿಂ", 
        "8796": "ಉಂಟಾದುದುಂಟೆ? ಉಂಟಾದುದನಿಲ್ಲವೆಂಬುದು ಮಿಥ್ಯೆ.", 
        "8797": "ಊರೊಳಗೈವರು ಮಕ್ಕಳು.", 
        "8798": "ಎಂತೆಂತು ನೋಡಿದಡಂತಂತು ತೋರ್ಪುದೆಲ್ಲಾ ಜಡದೃಶ್ಯ,", 
        "8799": "ಎಂಬತ್ತುನಾಲ್ಕು ಲಕ್ಷ ಜೀವಜಂತುವಿನೊಳಗಿಪ್ಪ ಆತ್ಮನು", 
        "8800": "ಎಚ್ಚರು, ಕನಸು, ನಿದ್ರೆ, ಮೂರ್ಛೆ, ಅರಿವು, ಮರವೆ,", 
        "8801": "ಎತ್ತನೇರಿ ಎತ್ತನರಸುವನಂತೆ,", 
        "8802": "ಎನ್ನಂಗ ಮನ ಪ್ರಾಣ ತ್ರಿವಿಧದಲ್ಲಿ ತ್ರಿವಿಧಲಿಂಗ ಸನ್ನಿಹಿತವೆಂತೆಂದಡೆ:", 
        "8803": "ಎಲುವಿನ ನಿಲವು ಅಮೇಧ್ಯದಾಗರ ಉಚ್ಛಿಷ್ಟದೊರತೆ", 
        "8804": "ಎಲ್ಲೆಲ್ಲಿ ಸುಳಿದಡೆ ಅಲ್ಲಲ್ಲಿ ಉಪಾಧಿ ಬಿಡದಯ್ಯಾ.", 
        "8805": "ಏಕಮುಖದ ರುದ್ರಾಕ್ಷಿಯೊಂದನೆ ಶಿಖಿಯಲ್ಲಿ ಧರಿಸುವುದಯ್ಯಾ.", 
        "8806": "ಏಕಾಕಾರ ಲೋಕಾಕಾರ ನೀನಾಗಿ ತೋರ್ಪೆಯಯ್ಯಾ ಸದ್ಭಾವಿಗಳಿಗೆ.", 
        "8807": "ಏಕೋತ್ತರದಲ್ಲಿ ಎಚ್ಚತ್ತಡೇನಯ್ಯಾ,", 
        "8808": "ಏಳು ನೆಲೆಯ ಕರುಮಾಡದುಪ್ಪರಿಗೆಯೊಳಗೆ,", 
        "8809": "ಒಂದು ಕಪಿಗೆ ಹತ್ತು ಮುಖ,", 
        "8810": "ಒಡಲೋಪಾದಿಯಿಂದ ತನುವ ಹೊರೆವವನಲ್ಲ.", 
        "8811": "ಒಬ್ಬನಿಗೆ ರೂಹಿಲ್ಲ, ಒಬ್ಬನಿಗೆ ರೂಹುಂಟು,", 
        "8812": "ಕಂಗಳ ಬೆಳಗು ತವೆಯುಡುಗಿ ತೊಳತೊಳಗಿ ಬೆಳಗುತಿರ್ದಿತ್ತಯ್ಯಾ.", 
        "8813": "ಕಂಗಳ ಬೆಳಗು ದೃಕ್ಕಿಂಗೆ ದೃಷ್ಟವಾಗದೆ?", 
        "8814": "ಕಂಡುದ ಕಂಡು ಕೈಕೊಂಬರಲ್ಲದೆ,", 
        "8815": "ಕಂಡೆ ಕಾಣೆನೆಂಬುದು ಕಂಗಳ ಭ್ರಮೆ,", 
        "8816": "ಕಟ್ಟಣೆಯೊಳಗಣ ಕಾಂಸ್ಯಕದಂತೆ,", 
        "8817": "ಕಡಲೊಳಗಣ ವಡಬ ಹೊದ್ದಿಯೂ ಹೊದ್ದದಂತೆ,", 
        "8818": "ಕಡವರವಿರ್ದು ಬಡತನವೇಕೊ?", 
        "8819": "ಕಣ್ಣು ತನ್ನ ತಾ ಕಾಣಲರಿಯದಂತೆ,", 
        "8820": "ಕತ್ತೆಯ ಗರ್ಭದಿಂದ ಪ್ರಸೂತವಾದ ಎತ್ತು", 
        "8821": "ಕನಸಿಂಗೆ ಕಲ್ಪನೆಯಿಲ್ಲ. ನೆನಹಿಂಗೆ ಸೂತಕವಿಲ್ಲ,", 
        "8822": "ಕರಸ್ಥಲದಲ್ಲಿ ಲಿಂಗವಿರಲು", 
        "8823": "ಕರ್ಮಮಲಮಾಯೆಗಳಿಲ್ಲದ, ಈಷಣತ್ರಯಂಗಳಿಗೆ ಸಲ್ಲದ,", 
        "8824": "ಕರ್ಮವಶದಿಂದುದಯಿಸಿದ ಶರೀರವ", 
        "8825": "ಕಲ್ಲಿನಲ್ಲಿ ಕನಕ ಒಗೆದಡೆ ಕನಕ ಕಲ್ಲಿಂಗೆ ಕಿಂಕರನಪ್ಪುದೆ?", 
        "8826": "ಕಳವು ಪಾರದ್ವಾರ ಜೀವಹಿಂಸೆಯೆಂಬಿವನತ್ತತ್ತಲೆ ಕೆಡೆನೂಂಕಿ", 
        "8827": "ಕಾಮಂಗೆ ಕೈತಲೆಗೊಟ್ಟ ಕೈವಾರಿಗಳಿಗೆಲ್ಲಿಯದೊ, ಕೈವಲ್ಯ ಪದವಿ?", 
        "8828": "ಕಾಮದ ಸೀಮೆಯ ಕಳೆಯದನ್ನಕ್ಕ, ಕೋಪದ ಕೂಪ ಹೂಳದನ್ನಕ್ಕ,", 
        "8829": "ಕಾಮಧೇನು ಕಾಮಿಸಿದಲ್ಲದೆ ಈಯಲರಿಯದು,", 
        "8830": "ಕಾಮಿತವಿಲ್ಲ ಕಲ್ಪಿತವಿಲ್ಲ ನಾಮಸೀಮೆಯೆಂಬುದಿಲ್ಲ.", 
        "8831": "ಕಾಯದ ಅವಗುಣಂಗಳ ಕಾಣದೆ ವಾಯುಕ್ಕೆ ವಾಯವ ನುಡಿವ", 
        "8832": "ಕಾಯದ ಕೈಮುಟ್ಟದೆ, ಜೀವದ ಕೈಮುಟ್ಟದೆ,", 
        "8833": "ಕಾಯದ ಕೈಮುಟ್ಟಿ ಲಿಂಗಕ್ಕರ್ಪಿಸಿದೆನಂದಡೆ ಅನರ್ಪಿತ,", 
        "8834": "ಕಾಯದ ಗತಿಯಿಲ್ಲ, ನಡೆವವನಲ್ಲ.", 
        "8835": "ಕಾಯದ ಜನನಸ್ಥಾನ ಅಷ್ಟತನುವಿನ ಕೊನೆಯ ಮೊನೆಯಲ್ಲಿ,", 
        "8836": "ಕಾಯವೆ ಜೀವವೊ? ಜೀವವೆ ಕಾಯವೊ?", 
        "8837": "ಕಾಯವೆಂಬ ಕಲ್ಲಮೇಲೆ ಮನವೆಂಬ ಮಚ್ಚವ", 
        "8838": "ಕಾಯದಲ್ಲಿ ಜೀವವು ಪ್ರಚ್ಛನ್ನವೆಂದಡೆ", 
        "8839": "ಕಾಲ ಕರ್ಮ ಬಿಂದು ಮಾಯೆ ಜೀವ ಪ್ರಕೃತಿ", 
        "8840": "ಕಾಷ್ಠದೊಳಗೆ ಅಗ್ನಿ ಇಪ್ಪಂತೆ,", 
        "8841": "ಕಾಳ ರಕ್ಕಸಿಗೆ ಮೂರು ಬೆನ್ನು, ಐದು ಬಸುರು,", 
        "8842": "ಕಿಗ್ಗೊಂಬಿನ ಕಪಿ ಅಗ್ಗೊಂಬಿನ ಕಪಿಯನಣಕಿಸಲು", 
        "8843": "ಕುಂಜರನ ಪಂಜರವೆ ಸಂಜೀವನವಾದವರೆಲ್ಲರೂ ಭಕ್ತರೆಂತಪ್ಪರಯ್ಯಾ?", 
        "8844": "ಕುಂಡಲಿಯ ಮುಖದಲ್ಲಿ ಪ್ರಾಣನಿವಾಸಿಯಾಗಿಪ್ಪ", 
        "8845": "ಕುರುಹಿಲ್ಲದಠಾವ ನೋಡುವ ಪರಿಯೆಂತೊ,", 
        "8846": "ಕುರುಹಿಲ್ಲದಠಾವಿನಲ್ಲಿ ತೆರಹಿಲ್ಲದಾತ್ಮಂಗೆ", 
        "8847": "ಕೂಳ ಪ್ರಸಾದವೆಂದು ನುಡಿವ ಜಾಳು ಮಾತ ಕೇಳಿ,", 
        "8848": "ಕೃಶ ಮಧ್ಯ ಸ್ಥೂಲ, ಹ್ರಸ್ವ ದೀರ್ಘವಪ್ಪ ಬಹುಘಟದಂಬುವಿನಲ್ಲಿ", 
        "8849": "ಕೆಲರು ಚತುರ್ವೇದಭಾರಕರಾದರು,", 
        "8850": "ಕೊಡಗೂಸಿಂಗೆ ಕಡುಬೇನೆಯಾದುದ ಕಂಡೆ,", 
        "8851": "ಕೋಣನೊಂದು ಕುದುರೆಯ ನುಂಗಿತ್ತು.", 
        "8852": "ಕ್ಷೀರಸಮುದ್ರದಲ್ಲಿ ಬೆರಸಿದ ಜಲವೆಲ್ಲ ಕ್ಷೀರವಾಗಿಪ್ಪುದೆಂತಂತೆ", 
        "8853": "ಖಂಡಿತವಳಿದು ಇಂದ್ರಿಯಂಗಳು ಅಖಂಡಿತವಾದವಯ್ಯಾ.", 
        "8854": "ಖ್ಯಾತಿಗೆ ಜೋತು, ಲಾಭಕ್ಕೆ ಲೋಭಿಸಿ,", 
        "8855": "ಗಂಧಘ್ರಾಣ ಆಚಾರಲಿಂಗಸಹಿತ ಲಿಂಗಾರ್ಪಿತ,", 
        "8856": "ಗಗನದ ಪುತ್ಥಳಿ ಬಯಲ ಬಣ್ಣವನುಟ್ಟು.", 
        "8857": "ಗತಿಗೆಟ್ಟು, ಮತಿಯ ಹಂಗ ಮರೆದು,", 
        "8858": "ಗರುಡನನುರಗನ ಬಾಣದಿಂ ಪರಿಹರಿಸುವೆನೆಂಬವನಂತೆ,", 
        "8859": "ಗಾಳಿಯ ನಾರಿನಲ್ಲಿ ಬೆಟ್ಟ ಕಟ್ಟುವಡೆಯಿತ್ತ ಕಂಡೆ.", 
        "8860": "ಗುರುಕರಣವೆಂಬ ಪಾದೋದಕದಲ್ಲಿ ನಾಂದು ನಾಂದು,", 
        "8861": "ಗುರುಕರಾಬ್ಜದಿಂದ ಉದಯಿಸಿದ ಶಿಷ್ಯನು", 
        "8862": "ಗುರುಕಾರುಣ್ಯವ ಪಡೆದು ಲಿಂಗಾನುಗ್ರಾಹಕನಾಗಿ", 
        "8863": "ಗುರುಕಾರುಣ್ಯ ವೇದ್ಯವಾದ ಬಳಿಕ ಜನನ ಸೂತಕವಿಲ್ಲ,", 
        "8864": "ಗುರುಲಿಂಗಜಂಗಮ ಒಂದಾದ ಕ್ರಿಯೆಯಲ್ಲಿ ನಿಂದ", 
        "8865": "ಗುರುಲಿಂಗದ ದೃಷ್ಟಿಯಲ್ಲಿ  ದೃಷ್ಟಿನಟ್ಟು, ಕಂಗಳು ಕರಗಿ,", 
        "8866": "ಗುರುವಿಡಿದು ಲಿಂಗವಾವುದೆಂದರಿಯಬೇಕು,", 
        "8867": "ಗುರುವಿನಂತರಂಗದೊಳಗೆ ಶಿಷ್ಯ, ಶಿಷ್ಯನಂತರಂಗದೊಳಗೆ ಗುರು.", 
        "8868": "ಗುರುವಿನಲ್ಲಿ ಗುಣವನರಸಲಿಲ್ಲ, ಲಿಂಗದಲ್ಲಿ ಲಕ್ಷಣವನರಸಲಿಲ್ಲ,", 
        "8869": "ಗುರುವೆ ಅಂಗ, ಲಿಂಗವೆ ಮನ, ಜಂಗಮವೇ ಪ್ರಾಣವಯ್ಯಾ.", 
        "8870": "ಗೊತ್ತ ಮೆಟ್ಟಿ ದಾಂಟಿ ಹುಟ್ಟದೆ ಹೋದ ಪರಿಯ ನೋಡಾ.", 
        "8871": "ಘಟಜಲ ಬಾಹ್ಯಾಗ್ನಿಯ ಉಷ್ಣ ಸೋಂಕಿ ಉಷ್ಣೋದಕವಾದಂತೆ,", 
        "8872": "ಘಟವ ಮಾಡಿದ ಕುಲಲಾನು ಆ ಘಟದೊಳಿಪ್ಪುದಿಲ್ಲವೆಂತಂತೆ,", 
        "8873": "ಘಟಾಕಾಶ ಮಠಾಕಾಶ ಬಿಂದ್ವಾಕಾಶ ಭಿನ್ನಾಕಾಶ", 
        "8874": "ಘಟಾಕಾಶ ಮಹದಾಕಾಶದ ಪರಿಯಲ್ಲವೆಂಬಿರಿ.", 
        "8875": "ಘನಕ್ಕೆ ಘನ ಮಹಾಘನ.", 
        "8876": "ಘನಲಿಂಗದಲ್ಲಿ ಮನ ನಟ್ಟು, ಭಾವ ಬೆರಸಿ,", 
        "8877": "ಘೋರಘೋರವಪ್ಪ ಮುಖ್ಯನರಕ ಒಂದು ಕೋಟಿ.", 
        "8878": "ಚಕ್ರೋದ್ಧರಣದ ಬಹಿರಾವರಣದ ತಮೋಗುಣದಲ್ಲಿ ಏಕಾಕ್ಷರವದೆ,", 
        "8879": "ಚತುರ್ದಶಭುವನವನಗ್ನಿ ಕೊಂಡಡೆ, ಇದ ಕಂಡು ಬೆರಗಾದೆ.", 
        "8880": "ಚರಪಾದೋದಕದಿಂದ ಲಿಂಗಾಂಗಗಳಿಗೆ", 
        "8881": "ಚರಿಸಿ ವರ್ತಿಸುವುದೆ ಆಚಾರವಾದ ಕಾರಣ,", 
        "8882": "ಚಿದ್ಬ್ರಹ್ಮಾಂಡವೆಂಬ ಭಾಂಡದಲ್ಲಿ", 
        "8883": "ಚೈತನ್ಯಕ್ಕೆ ಚೈತನ್ಯ ನೀನಾಗಿ", 
        "8884": "ಚೈತನ್ಯಾತ್ಮಕವಪ್ಪ ನಾದಬಿಂದುವಿನೊಳಡಗಿಪ್ಪ", 
        "8885": "ಚೌಷಷ್ಠಿವಿದ್ಯೆಗಳ ಕಲಿತಡೇನೊ?", 
        "8886": "ಜಗತ್ಪ್ರಪಂಚ ಮಾಡಿ,", 
        "8887": "ಜಗದಗಲದ ಬಲೆಯನಗಲಬಲ್ಲರ ಕಾಣೆ,", 
        "8888": "ಜಗಭರಿತಲಿಂಗ ಶರಣಭರಿತಲಿಂಗ ನಾಮರಹಿತಲಿಂಗ:", 
        "8889": "ಜಪತಪ ನೇಮವಲ್ಲ, ಮಂತ್ರತಂತ್ರ ನೇಮವಲ್ಲ,", 
        "8890": "ಜಿಹ್ವೆ ಗುಹ್ಯೇಂದ್ರಿಯದಿಚ್ಛೆಯುಳ್ಳನ್ನಕ್ಕ ಜಂಗಮವಲ್ಲ,", 
        "8891": "ಜೀವಂಗೆ ಜೀವವಾದಾತನೆ ಪರಮಾತ್ಮನು, ಪರಬ್ರಹ್ಮವು.", 
        "8892": "ಜೀವಭಾವದ ಹಂಸ ಜಪದಲ್ಲಿ ದ್ವಾದಶಾಂತ ಕೂಡಿ ಶಿವಜಪವಾಯಿತ್ತು.", 
        "8893": "ಜ್ಞಾತೃಸ್ವರೂಪದಿಂದರುಹಿಸುವ ಬುದ್ಧಿ,", 
        "8894": "ಜ್ಞಾನಕಾಯಂಗೆ ಬಹುಕಾಯವಿಲ್ಲ,", 
        "8895": "ಟಿಕ್ಕೆಯೊಳಗೆ ಮಾಣಿಕ್ಯ ಉಂಟೆ ಅಯ್ಯಾ?", 
        "8896": "ಟಿಕ್ಕೆಯಲ್ಲಿ ಮಾಣಿಕವನರಸಲುಂಟೆ?", 
        "8897": "ತತ್ವಮೂವತ್ತಾರ ಮೀರಿ ನಾದಬಿಂದುಕಳಾತೀತನಾದ", 
        "8898": "ತನಗೊಬ್ಬರು ಮುನಿದರು ತಾನಾರಿಗೂ ಮುನಿಯಲಾಗದು.", 
        "8899": "ತನ್ನ ತಾನರಿವುದೇನೋ, ತನ್ನ ತಾ ಮರೆವುದು ಮತ್ತದೇನೋ?", 
        "8900": "ತನ್ನನೆ ಅರ್ಪಿಸಿಹ ಶರಣಂಗೆ ಲಿಂಗವೆಂದೂ ಭಿನ್ನವಿಲ್ಲ ನೋಡಯ್ಯಾ.", 
        "8901": "ತನು ಒದಗಿದಲ್ಲಿ ಮನವಾಯಿತ್ತು,", 
        "8902": "ತನುಕರಣ ಭುವನಭೋಗಂಗಳ ಶಕ್ತಿಯುಳ್ಳನ್ನಕ್ಕ", 
        "8903": "ತನುಗುಣ ಮನಗುಣ ಪ್ರಾಣಗುಣಾದಿಗಳಲ್ಲಿ ಹುದುಗಿದಡೆ,", 
        "8904": "ತನುಗುಣ ಮನಗುಣ ಪ್ರಾಣಗುಣಾದಿಗಳು ಹುಸಿಯೆಂಬ", 
        "8905": "ತನುಗುಣವಳಿದಲ್ಲಿ ತ್ಯಾಗಾಂಗ,", 
        "8906": "ತನುಗುಣವಳಿದಾತನಲ್ಲದೆ ಭಕ್ತನಲ್ಲ,", 
        "8907": "ತನುಗುಣವಿರಹಿತ ಗಮನರಹಿತ ಅನುಪಮಜ್ಞಾನಸನ್ನಹಿತ ಶರಣ,", 
        "8908": "ತನು ನಿರ್ವಾಣವಾದಡೇನೋ, ಆಶೆ ನಿರ್ವಾಣವಾಗದನ್ನಕ್ಕ!", 
        "8909": "ತನುಭಾವವಳಿದು ಲಿಂಗಾನುಭಾವದಲ್ಲಿ ನಿಲಲರಿಯದೆ ದೇಹೋಹಂ ಎಂದು", 
        "8910": "ತನುಮನಧನ ತನ್ನವೆಂಬ ಬಳಕೆಯಳಿದು,", 
        "8911": "ತನು ಮನ ಧನದ ಲೋಭ ಎನಗೇಕೋ?", 
        "8912": "ತನು ಮನ ನೆನಹು, ಹಲವು ನೆನಹಾಗಿ ಜನಿಸಬಲ್ಲವೆ ಆತ್ಮನಿಲ್ಲದಿರಲು?", 
        "8913": "ತನು ಮಾಯೆ, ತನುವಿಂಗೆ ಮನ ಮಾಯೆ,", 
        "8914": "ತನುಮುಟ್ಟಿ ಮನಮುಟ್ಟದೆ ಒಂದೊಂದನೆ ನೆನೆದು", 
        "8915": "ತನುವಿನಲ್ಲಿ ನಿರ್ಮೋಹ, ಮನದಲ್ಲಿ ನಿರಹಂಕಾರ,", 
        "8916": "ತನುವಿನಿಂ ಮನ ಜನಿಸಿ, ಮನದಿಂ ಹಲವು ನೆನಹುಗಳು ಜನಿಸಿದವಲ್ಲದೆ,", 
        "8917": "ತನುವೆಂಬ ಮನೆಗೆ ಆಸ್ಥಿಯ ಗಳು, ನರದ ಭೀಮಗಟ್ಟು,", 
        "8918": "ತನು ಸೋಂಕಿ ಅರ್ಪಿಸುವುದೇ ಸಂದೇಹ,", 
        "8919": "ತಮೋಗುಣವೆಂಬ ಮಯಣದಲ್ಲಿ ರಜೋಗುಣವೆಂಬ ರಜವನೆತ್ತಿ", 
        "8920": "ತಲೆಯಿಲ್ಲದ ಕಾಗೆ ನೆಲದಲ್ಲಿ ನಡೆಯಿತ್ತು.", 
        "8921": "ತಳದಲೊಂದು ಕೋಲ ಕುಳಿ ಮಾಡಿ, ಮೇಲೊಂದು ಕೋಲ ಮೊನೆ ಮಾಡಿ", 
        "8922": "ತಾನು ತನ್ನನರಿಯದ ಮುನ್ನ", 
        "8923": "ತಾನೆ ಶಿವನೆಂದು ಕುರುಹ ತೋರುವ ಹಾಂಗೆ", 
        "8924": "ತಿಳಿದ ತಿಳಿಗೊಳದ ತಳಿತ ತಾವರೆಯರಳ ತಂಡುಲದ ರುಚಿಯ", 
        "8925": "ತಿಳಿದ ತಿಳಿಗೊಳದ ತಳಿತ ತಾವರೆಯರಳ ತಂಡುಲದ ರುಚಿಯ", 
        "8926": "ತೀರ್ಥಯಾತ್ರೆಯ ಮಾಡಿ ಪಾಪವ ಕಳೆದಿಹೆನೆಂಬ", 
        "8927": "ತೆರಹಿಲ್ಲದ ಘನ ತನು ಮನ ಮಹವ ಮೀರಿತ್ತು,", 
        "8928": "ತೆರೆಯಡಗಿದ ಸಾಗರದೊಳಗೆ ಆಕಾಶ ಬಿಂಬಿಸಿ", 
        "8929": "ತ್ರಿಪುರಸಂಹಾರವ ಮಾಡುವಲ್ಲಿ", 
        "8930": "ದಕ್ಷಿಣದ್ವಾರದ ವೃಕ್ಷದ ತಂಪಿನಲ್ಲಿ ಸ್ವಯಂಜ್ಯೋತಿ ಉರಿವುದ ಕಂಡೆ.", 
        "8931": "ದಿವಾರಾತ್ರಿಯ ಮಧ್ಯದಲ್ಲಿ ಕತ್ತಲೆ ಬೆಳಗು ತಳೆದಿಪ್ಪ ಬಯಲು", 
        "8932": "ದೇವದೇಹಿಕ ಭಕ್ತನಾಗಿ", 
        "8933": "ದೇಶ ಉಪದೇಶವಾಗದನ್ನಕ್ಕ, ಜೀವ ಉಜೀವವಾಗದನ್ನಕ್ಕ,", 
        "8934": "ದೇಹ ಪ್ರಾಣದಂತೆ ಕೂಡಿದ ಭಕ್ತ ಜಂಗಮ[ದ]", 
        "8935": "ದೇಹಾಭಿಮಾನವಳಿದು,", 
        "8936": "ದೇಹಾರ್ಥಪ್ರಾಣಾಭಿಮಾನಕ್ಕೆ", 
        "8937": "ದೇಹಿಯಲ್ಲ ನಿರ್ದೇಹಿಯಲ್ಲ ನಿತ್ಯ,", 
        "8938": "ದ್ವೈತಾದ್ವೈತದ ಬಳಿವಿಡಿದರಸುವನಲ್ಲ.", 
        "8939": "ಧರೆಯಾಕಾಶವಿಲ್ಲದಂದು, ಅಂದೇನೊ ಅಂದೇನೊ!", 
        "8940": "ಧರ್ಮಾರ್ಥಕಾಮಮೋಕ್ಷಂಗಳ ಕಾಮಿಸಿ", 
        "8941": "ನಟ್ಟಡವಿಯಲ್ಲಿ ಕೆಂಡದ ಮಳೆ ಕರೆದು", 
        "8942": "ನಾದಬ್ರಹ್ಮ ನಾದಾತೀತಬ್ರಹ್ಮ ಶಬ್ದಬ್ರಹ್ಮ ನಿಶ್ಶಬ್ದಬ್ರಹ್ಮ", 
        "8943": "ನಾದಬಿಂದುಕಳೆಯ ಸಂದನರಿತು", 
        "8944": "ನಾದವೇ ಲಿಂಗ, ಬಿಂದುವೇ ಪೀಠವಾಗಿ", 
        "8945": "ನಾಲ್ಕು ವೇದಂಗಳುಪಮೆ, ಹದಿನಾರು ಶಾಸ್ತ್ರಂಗಳುಪಮೆ,", 
        "8946": "ನಾಲ್ಕುವೇದ, ಹದಿನಾರು ಶಾಸ್ತ್ರ, ಹದಿನೆಂಟು ಪುರಾಣ,", 
        "8947": "ನಾಸಿಕಾಗ್ರ ಭ್ರೂನಿಟಿಲ ಮಧ್ಯದೊಳ್ಪ್ರಜ್ವಲಿಸಿ ಬೆಳಗಿದ", 
        "8948": "ನಿಜಸುಖದ ಸಂಗದಿಂದ ನೆನಹಳಿದು,", 
        "8949": "ನಿಮ್ಮ ನೆನಹಿನ ಪಂಚಾಕ್ಷರವೆಂಬ ಪಂಚಾಮೃತದಿಂ ಮಜ್ಜನಕ್ಕೆರದು,", 
        "8950": "ನಿರಿಂದ್ರಿಯನಾದ ಶಿವನು ತಾನೇ ದೇವದೇಹಿಕ ಭಕ್ತನಾಗಿ", 
        "8951": "ನಿಷ್ಕಲ ಷಟ್‍ಸ್ಥಲಲಿಂಗದ ಮೂಲಾಂಕುರವೆನಿಸುವ ಪರಮ ಕಳೆ,", 
        "8952": "ನೀರಮೇಲೆ ಒಂದು ನಟ್ಟ ಕಂಬವಿದ್ದಿತ್ತು.", 
        "8953": "ನೀರು ನೀರ ಕೂಡಿದಂತೆ, ಕ್ಷೀರಕ್ಷೀರ ಬೆರಸಿದಂತೆ", 
        "8954": "ನೆಣದ ಕೊಣ, ರಕ್ತದ ಹುತ್ತ, ಕೀವಿನ ಬಾವಿ, ರೋಮದ ಸೀಮೆ,", 
        "8955": "ನೆಲದ ಮೇಲಣ ನಿಧಾನವ ಕಂಡವರಲ್ಲದೆ", 
        "8956": "ನೆಳಲಂತೆ ಮಾಯಾತಂತ್ರಿಯಲ್ಲ. ಆಕಾಶದಂತೆ ಶೂನ್ಯವಾದುದಲ್ಲ.", 
        "8957": "ನೆಳಲ ನುಡಿಸಿಹೆನೆಂದು ಬಳಲುವನಂತೆ,", 
        "8958": "ನೇತ್ರಂಗಳ ಮುಚ್ಚಿ ರೂಹಿಲ್ಲದ ನಿಲವ ಕಾಣಬೇಕು.", 
        "8959": "ಪಂಚಗವ್ಯದಿಂದಾದ ಗೋಮಯವ ತಂದಾರಿಸಿ", 
        "8960": "ಪಂಚತತ್ವಂಗಳುವಿಡಿದಾಡುವ", 
        "8961": "ಪಂಚಬ್ರಹ್ಮದಿಂದಾದುದಲ್ಲ,", 
        "8962": "ಪಂಚಬ್ರಹ್ಮದಿಂದುದಯಿಸಿದ", 
        "8963": "ಪಂಚಬ್ರಹ್ಮದಿಂದೊಗೆದ ಪಂಚಭೂತಾಂಶದ", 
        "8964": "ಪಂಚೇಂದ್ರಿಯಂಗಳು ಲಿಂಗಮುಖವಾಗಿ ನಿಂದು,", 
        "8965": "ಪತಿವ್ರತೆಯಾದಡೆ ಅನ್ಯಪುರುಷರ ಸಂಗವೇತಕ್ಕೆ?", 
        "8966": "ಪರತತ್ವ ಪರಬ್ರಹ್ಮ ಪರಶಿವನಪ್ಪ", 
        "8967": "ಪರದಿಂದಾದ ಅಪರಸ್ವರೂಪಂಗೆ ಉಪಾಧಿಯೆಂಬುದಿಲ್ಲ.", 
        "8968": "ಪರಬ್ರಹ್ಮಸ್ವರೂಪನಾದ ಪರಾತ್ಮನೆ ಸರ್ವಾತ್ಮನು.", 
        "8969": "ಪರಮಜ್ಞಾನಕ್ಕೆ ಕರ್ತೃವಾದ ಸದಾಶಿವನೆ ಜೀವಭಾವದಿಂ", 
        "8970": "ಪರಶಿವನೆ ಅಪಸ್ವರೂಪನಾಗಿ", 
        "8971": "ಪರಸತಿಗಳುಪುವ ಪಾರದ್ವಾರಿಯರಿವು ಪರವಕ್ಕು ಕಾಣಿಭೋ.", 
        "8972": "ಪರಿಪೂರ್ಣ ಪರತತ್ವದಿಂದೊದಗಿದ ಆನಂದಭರಿತ", 
        "8973": "ಪರಿಯಾಣವೇ ಭಾಜನವೆಂಬರು.", 
        "8974": "ಪಾತಾಳದಿಂ ಕೆಳಗೆ ಪಾದ, ಸತ್ಯರ್ಲೋಕದಿಂ ಮೇಲೆ ಉತ್ತಮಾಂಗ,", 
        "8975": "ಪಿಂಡಪ್ರಸಾದಿಗಳ ಕಂಡುಬಲ್ಲೆ, ಇಂದ್ರಿಯಪ್ರಸಾದಿಗಳ ಕಂಡುಬಲ್ಲೆ,", 
        "8976": "ಪುತ್ರೇಷಣ ವಿತ್ತೇಷಣ ದಾರೇಷಣಂಗಳೆಂಬ ಈಷಣತ್ರಯವನುಳಿದು", 
        "8977": "ಪೃಥ್ವಿ ಅಪ್ಪು ತೇಜ ವಾಯು ಆಕಾಶ ಆತ್ಮ", 
        "8978": "ಪೃಥ್ವಿಯ ಕಠಿಣವಳಿದ ಭಕ್ತಂಗೆ ಜಾಗ್ರಾವಸ್ಥೆಯಿಲ್ಲ.", 
        "8979": "ಪೃಥ್ವಿಯ ಕೂಡಿ ನೆಲನಹನಲ್ಲ, ಅಪ್ಪುವ ಕೂಡಿ ಜಲವಹನಲ್ಲ,", 
        "8980": "ಪೃಥ್ವಿಯ ಪೂರ್ವಾಶ್ರಯವಳಿದ ಭಕ್ತಂಗೆ ಆಚಾರಲಿಂಗಸಾಹಿತ್ಯ,", 
        "8981": "ಪ್ರಣವಾಂಕುರದಿಂ ತೋರಿತ್ತು ಚಿತ್ತು,", 
        "8982": "ಪ್ರತಿಯಿಲ್ಲದ ಲಿಂಗದಲ್ಲಿ ಪ್ರತಿರಹಿತವಾದ ಮಹಿಮನಿಗೆ", 
        "8983": "ಪ್ರಸಾದ ಪ್ರಸಾದವೆಂಬರು, ಪ್ರಸಾದವಾವುದೆಂದರಿಯರು.", 
        "8984": "ಪ್ರಸಾದವೇ ಪರಮಜ್ಞಾನ, ಪ್ರಸಾದವೇ ಪರಾಪರ,", 
        "8985": "ಪ್ರಾಣದ ಮೇಲೆ ಪ್ರಾಣಲಿಂಗ ಸ್ವಾಯತವಾದ ಬಳಿಕ", 
        "8986": "ಪ್ರಾಣಲಿಂಗಧಾರಣದಲ್ಲಿಯಲ್ಲದೆ", 
        "8987": "ಬಂಧಮೋಕ್ಷಂಗಳ ಭ್ರಾಂತನಳಿದು", 
        "8988": "ಬಟ್ಟಬಯಲ ದೃಷ್ಟಾಕಾಶದಲ್ಲಿ ನಾನೊಂದು ದೃಷ್ಟವ ಕಂಡೆ.", 
        "8989": "ಭದ್ದನಲ್ಲ ಮುಕ್ತನಲ್ಲ, ಎರಡಿಲ್ಲವಾಗಿ ಸಹಜಭರಿತನಯ್ಯಾ.", 
        "8990": "ಬಲ್ಲೆನೆಂಬುದು ಮಾಯೆ, ಅರಿಯೆನೆಂಬುದು ಮಾಯೆ,", 
        "8991": "ಬಸವಣ್ಣನ ಭಕ್ತಿಸ್ಥಲ, ಮಡಿವಾಳ ಮಾಚಣ್ಣನ ಮಾಹೇಶ್ವರಸ್ಥಲ,", 
        "8992": "ಬಸವಣ್ಣನೆನ್ನ ಅಂಗ, ಮಡಿವಾಳನೆನ್ನ ಮನ,", 
        "8993": "ಬಾಣತಿ ರಕ್ಕಸಿಯಾದಡೆ ಮಕ್ಕಳ ಕಾಯುವರಿನ್ನಾರಯ್ಯಾ ?", 
        "8994": "ಬಾಹಿರವ ಕಳೆದು, ಎಡೆನಾಣ್ಯವನುಳಿದು,", 
        "8995": "ಬೀಜದೊಳಗಣ ವೃಕ್ಷ, ಅಂಕುರ, ಪಲ್ಲವ, ಪತ್ರ,", 
        "8996": "ಬೀಜವೃಕ್ಷ ಯೋಗದಂತೆ ಭಕ್ತಸ್ಥಲ,", 
        "8997": "ಬುದ್ಧಿಯೊಡನೆ ಸಂಬಂಧವಾದ ಪುಣ್ಯಪಾದ ರೂಪವೆ", 
        "8998": "ಬೆಚ್ಚು ಬೇರಿಲ್ಲದ ಸುಖವಚ್ಚಾದ ಪರಿಯ ನೋಡಾ ಅಯ್ಯಾ.", 
        "8999": "ಬೆಟ್ಟದ ಮೇಲೊಂದು ದಳ್ಳುರಿ ನಿಮುರಿ ಬಳ್ಳಿವರಿದು", 
        "9000": "ಬೇರು ಮೊದಲಾಗಿ ಚರಿಸುವ ವೃಕ್ಷದೊಳಗೆ", 
        "9001": "ಬ್ರಹ್ಮಜ್ಞಾನದಿಂದ ಬ್ರಹ್ಮವನರಿತರಿವು", 
        "9002": "ಬ್ರಹ್ಮನೆ ಬಟ್ಟಲು ವಿಷ್ಣುವೆ ನೇಣು ರುದ್ರನೆ ತೊಲೆ.", 
        "9003": "ಬ್ರಹ್ಮವೆಂಬ ವೃಕ್ಷದಲ್ಲಿ ಫಲಪತ್ರಕುಸುಮಂಗಳೆಂಬ", 
        "9004": "ಬ್ರಹ್ಮಸ್ಥಾನದಲ್ಲಿ ಜ್ಯೋತಿರ್ಲಿಂಗವಿಪ್ಪುದನರಿತು,", 
        "9005": "ಭಕ್ತನೊಂದು ಕುಲ, ಭವಿಯೊಂದು ಕುಲವೆಂದೆಂಬರು.", 
        "9006": "ಭಕ್ತಿಯಾಚಾರದ ಪಥವಿಡಿದು ನಲಿನಲಿದುಲಿದಡೂ ಲಿಂಗಸಾಹಿತ್ಯವಿಲ್ಲ.", 
        "9007": "ಭಕ್ತಿಯೆಂಬ ಸತಿಯಲ್ಲಿ ಜ್ಞಾನವೆಂಬ ಸುತ ಹುಟ್ಟಲು", 
        "9008": "ಭವಿಯೊಂದು ಕುಲ ಭಕ್ತನೊಂದು ಕುಲವೆಂಬ", 
        "9009": "ಭಾನುಮಂಡಲವಂಜುವದೆ ಮಂಜು ಮುಸುಕಿದಡೆ ?", 
        "9010": "ಭಾವದ ಮುಂದೆ ತೋರುವ ಜ್ಞಾನದ ಕುರುಹಿದೇನೋ !", 
        "9011": "ಭಾವ ದುರ್ಭಾವ ಅಳಿದು, ಸ್ವಭಾವಿಯಾದ ಸದ್ಭಾವಿಗೆ", 
        "9012": "ಭಾವಭೇದವಿಲ್ಲದೆ ಬೆರೆಸಿ,", 
        "9013": "ಭ್ರಮರ ಸೋಂಕಿದ ಕೀಟ ಭ್ರಮರನಾಗದೆ, ಮರಳಿ ಕೀಟನಪ್ಪುದೆ ?", 
        "9014": "ಮಂಡೆ ಬೋಳಾದಡೇನೊ ! ಹುಟ್ಟು ಬೋಳಾಗದನ್ನಕ್ಕರ,", 
        "9015": "ಮಧ್ಯನಿರಾಳದಲ್ಲಿ ನಿಂದು, ಊಧ್ರ್ವ ನಿರಾಳವನೆಯ್ದಿದಲ್ಲಿ,", 
        "9016": "ಮನದ ನಯನದಿಂದ ಕಂಡು", 
        "9017": "ಮನಮುಕ್ತನಾದ ಶರಣ ತಾನೆ ಲಿಂಗವಾಗಿ", 
        "9018": "ಮನವೆ ಮಹ, ತನುವೆ ಪೃಥ್ವಿ,", 
        "9019": "ಮನ ಸೋಂಕಿದ ಸುಖವನೊಂದು ಶ್ರುತಕ್ಕೆ ತಂದಡೆ", 
        "9020": "ಮನೋವಾಕ್ಕಾಯದಲ್ಲಿ ಲಿಂಗವ ನಂಬಿ ನಚ್ಚಿ ಒಚ್ಚತವೋಗಿ,", 
        "9021": "ಮರವೆ ಅರಿವಿನ ಮರೆಗೊಂಡು ತಲೆದೋರಿ ಅರಿವೆಂದೆನಿಸಿತ್ತು.", 
        "9022": "ಮರ್ಕಟ ದರ್ಪಣವ ಹಿಡಿದು ನೋಡಿ ತನ್ನ ಪ್ರತಿಬಿಂಬವ ಕಂಡು", 
        "9023": "ಮಲಜಲವ ತೊಳೆಯಲುಂಟಲ್ಲದೆ, ನಿರ್ಮಲಜಲವ ತೊಳೆಯಲುಂಟೆ ?", 
        "9024": "ಮಹಿಯೆಂಬ ಕುಟ್ಟಿಮದ ದಿಗ್ಭಿತ್ತಿಯ ಮೇಲೆ ಮುಚ್ಚಿದ ಅಂಡಕಟಾಹ ?", 
        "9025": "ಮಾತ ಕಲಿತು ಮಂಡೆಯ ಬೋಳಿಸಿ ವೇಷ ಭಾಷೆಗಳಿಂ", 
        "9026": "ಮಾಯೆಯಿಂದಾದ ಸಂಸಾರದಡವಿಯೊಳಗೆ ತಿರಿಗಿ ತಿರಿಗಿ ಘಾಸಿಯಾಗಿ,", 
        "9027": "ಮಾಯೆಯೆಂದೇನೊ ಮದವಳಿದಂಗೆ ?", 
        "9028": "ಮುಖದಲ್ಲಿ ರುದ್ರನಿಪ್ಪನಯ್ಯಾ, ನಾಬಿsಯಲ್ಲಿ ವಿಷ್ಣುವಿಪ್ಪನಯ್ಯಾ,", 
        "9029": "ಮುತ್ತು ನೀರೊಳಗೆ ಹುಟ್ಟಿ ಮರಳಿ ನೀರಾಗದಂತೆ,", 
        "9030": "ಮುನ್ನ ಮುನ್ನ ತಾನೆ ಸ್ವಯಂಭು.", 
        "9031": "ಮೂರು ಪಂಚಭೂತದಿಂದ ಗಾರಪ್ಪುದು [ಬೇರೆ] ಮಾಡಿ,", 
        "9032": "ಯೋಗ ವಿಯೋಗವೆಂಬೆರಡಳಿದು,", 
        "9033": "ರಸವನುಗುಳ್ದು ಕಸವನಗಿವವನಂತೆ,", 
        "9034": "ರಸವುಂಡ ಅಂಗದಲ್ಲಿ", 
        "9035": "ರಾಗದ್ವೇಷರಹಿತನಾಗಿ ಮೋಹಕ್ಕೆ ಕಾರಣ ಶೋಕ,", 
        "9036": "ರೂಪು ರುಚಿ ತೃಪ್ತಿಗಳು ಬೇಸರವಿಲ್ಲದೆ ಇರ್ಪ ಮರ್ಮವನರಿತು,", 
        "9037": "ಲಿಂಗದ ಸಂಗದಲ್ಲಿಪ್ಪ ಅಂಗ ಹಿಂಗದೆ", 
        "9038": "ಲಿಂಗಪ್ರಸಾದ ತನಗಾಗಬೇಕೆಂಬ ಪ್ರಸಾದಿ", 
        "9039": "ಲಿಂಗವೇ ಪ್ರಾಣವಾಗಿ ಪ್ರಾಣಲಿಂಗವೆಂಬ ಪ್ರಪಂಚನರಿಯನಯ್ಯಾ !", 
        "9040": "ಲಿಂಗಶರಣನ ಪಂಚೇಂದಿಂ್ರಯಂಗಳು ಲಿಂಗಮುಖವಾಗಿ ನಿಂದು,", 
        "9041": "ಲಿಂಗೋದಕ ಪಾದೋದಕ ಪ್ರಸಾದೋದಕವಾದ ತ್ರಿವಿಧೋದಕದಲ್ಲಿ", 
        "9042": "ಲಯಭೋಗಾಧಿಕಾರವನುಳ್ಳ ಪರಶಿವನು", 
        "9043": "ವಾಯುವಿನ ಹಸ್ತದಲ್ಲಿ ಸೊಡರಿದ್ದು ಬೆಳಗಬಲ್ಲುದೆ ?", 
        "9044": "ವಿಶ್ವ ತೈಜಸ ಪ್ರಾಜ್ಞವೆಂಬ ಜೀವತ್ರಯವಿಡಿದ", 
        "9045": "ವೀರಶೈವ, ಶುದ್ಧಶೈವವೆಂಬುಭಯ ಪಕ್ಷದ", 
        "9046": "ವೇದಂಗಳ ಹಿಂದೆ ಹರಿಯದಿರು ಹರಿಯದಿರು.", 
        "9047": "ವೇದವಿತ್ತುಗಳು ಅಗ್ನಿಪುರುಷನೆ ದೇವರೆಂಬರಯ್ಯಾ.", 
        "9048": "ವೇದಶಾಸ್ತ್ರಪುರಾಣಂಗಳ ಗತಿಯಲ್ಲಿ ನಡೆದು", 
        "9049": "ವೇದಾಗಮಂಗಳ ದ್ವೈತಾದ್ವೈತದ ಬಗೆಗೆ ನಿಲುಕುವನಲ್ಲ,", 
        "9050": "ವೇದಾಗಮಂಗಳು ಹೋದ ಸರಣಿಯಲ್ಲಿ ಹೋದರಲ್ಲದೆ", 
        "9051": "ವೈದಿಕರಪ್ಪ ವೇದವಿತ್ತುಗಳೊಂದು ಕೋಟಿಗೆ", 
        "9052": "ವ್ಯಾಪಕಾಕ್ಷರದಲ್ಲಿ ನಾಲ್ಕಕ್ಷರ, ಸ್ಪರ್ಶನಾಕ್ಷರದಲ್ಲಿ ಏಕಾಕ್ಷರ,", 
        "9053": "ಶಬ್ದಶಾಸ್ತ್ರ ತರ್ಕಾಗಮಂಗಳ ಹೇಳಿ ಕೇಳಿ,", 
        "9054": "ಶಬ್ದಶಾಸ್ತ್ರ ಶ್ರುತಿಸ್ಮøತಿಗೆ ವಾಙ್ಮ[ಹ]ವೊ ? ಮಹವೊ ?", 
        "9055": "ಶರೀರಧಾರಕನಾಗಿ ಇಂದ್ರಿಯಂಗಳಿಚ್ಛೆಯಲ್ಲಿ ಸುಳಿವನಲ್ಲ.", 
        "9056": "ಶಿವತತ್ವದ ಮೂಲಾಧಾರದ ಜ್ಞಾನಶಕ್ತಿಯೆ ಚಿದ್ವಿಭೂತಿ.", 
        "9057": "ಶಿವತತ್ವ ಶಿವಜ್ಞಾನ ಶಿವೈಕ್ಯವಪ್ಪ ತ್ರಿವಿಧವನರಿಯಬಲ್ಲಡೆ", 
        "9058": "ಶಿವಪಥವನರಿದು, ಲಿಂಗನಿಷ್ಠೆಯಿಂದೇಕೋಭಾವ ಬಲಿದು,", 
        "9059": "ಶಿವಭಕ್ತಿಯೆಂಬುದು ಯುಕ್ತಿಯ ಪರಿಯಂತಲ್ಲ, ಸಾಧಕದ ಪರಿಯಂತಲ್ಲ,", 
        "9060": "ಶಿವಭಾವದಿಂದ ಆತ್ಮ ಹುಟ್ಟಿ, ಶಿವ ತಾನೆಂಬ ಉಭಯವನಲಂಕರಿಸಿದನಾಗಿ", 
        "9061": "ಶಿವಯೋಗಿಗೆ ಕರ್ಮವಿಲ್ಲ, ಶಿವಶರಣರ ಪಥ ಲಿಂಗಾದಿsೀನ.", 
        "9062": "ಶಿವಲಾಂಛನವ ಧರಿಸಿ, ಚರಲಿಂಗಮೂರ್ತಿಯಾದ ಬಳಿಕ", 
        "9063": "ಶಿವಲಿಂಗಾರ್ಚನಾಕ್ರಿಯಾಶಕ್ತಿಗೆ", 
        "9064": "ಶಿಷ್ಟೋದನವ ಲಿಂಗಕ್ಕಿತ್ತು ಪ್ರಸಾದಭೋಗ ಮಾಡುವುದಯ್ಯಾ.", 
        "9065": "ಶುಕ್ಲಶೋಣಿತದಲ್ಲಿ ಬಲಿದ ಕರುಳು", 
        "9066": "ಶುಕ್ಲಕಶೋಣಿತದಿಂದ ತನು ಮನ ಜನಿಸಿತ್ತು.", 
        "9067": "ಶುದ್ಧ ಕೇವಲಜ್ಞಾನಸ್ವರೂಪನಾದಾತ ನೀನಯ್ಯಾ.", 
        "9068": "ಶುದ್ಧ ಮಿಶ್ರ ಸಂಕೀರ್ಣ ಪೂರ್ವ ಮಾರ್ಗಶೈವಂಗಳಲ್ಲಿ", 
        "9069": "ಶುದ್ಧಶೈವದ ನಿರ್ಧರವನರಿವ ಬುದ್ಧಿಯನನುಕರಿ[ಸೆ]", 
        "9070": "ಶ್ರದ್ಧೆ, ನೈಷ್ಠೆ, ಅವಧಾನ, ಅನುಭಾವ, ಆನಂದ, ಸಮರಸವೆಂಬ", 
        "9071": "ಶ್ರಮಿಸಿದುದೆ ಶ್ರಾದ್ಧ, ಪ್ರೇತವಾದುದೆ ಪ್ರೇದ್ಧ.", 
        "9072": "ಶ್ರೀಗುರು ಕರುಣದಿಂದ ಜ್ಞಾನಮೂರುತಿ ಗುರುವನರಿತ ಬಳಿಕ", 
        "9073": "ಶ್ರೀಗುರುವಿನ ಕರುಣದಿಂದ ಪವಿತ್ರಗಾತ್ರನಾದೆ.", 
        "9074": "ಶ್ರೀಗುರುವಿನ ಶ್ರೀಚರಣದಲ್ಲಿ ಅರುವತ್ತೆಂಟು ತೀರ್ಥಗಳು", 
        "9075": "ಶ್ರೀಗುರುಸೋಂಕಿದ ಶಿಷ್ಯ ತಾನಾ ಗುರುವಾಗದೆ ಮಾಣನು.", 
        "9076": "ಶ್ರೋತ್ರ, ತ್ವಕ್ಕು, ಚಕ್ಷು, ಜಿಹ್ವೆ, ಘ್ರಾಣವೆಂಬ ಪಂಚೇಂದ್ರಿಯಂಗಳಲ್ಲಿ", 
        "9077": "ಷಟ್‍ತ್ರಂಶತತ್ವಂಗಳೆ ದೇಹವಾಗಿ", 
        "9078": "ಸಂಯೋಗ ವಿಯೋಗವೆಂಬೆರಡಿಲ್ಲದ ನಿಜವನ್ನಾರಯ್ಯಾ ಬಲ್ಲವರು ?", 
        "9079": "ಸಂವಿದಾತ್ಮಂಗೆ ಕ್ರಿಯಾಪಾಶ ಘಟಿಸದಾಗಿ", 
        "9080": "ಸಕಲವ್ಯಾಪಾರಂಗಳುಡುಗಿ,", 
        "9081": "ಸಕಲೇಂದ್ರಿಯಂಗಳಲ್ಲಿ ವಿಕರಿಸುವ ಮನವ ಸೆಳೆದು ನಿಂದಾತ ಸುಖಿ,", 
        "9082": "ಸಗುಣನಾಗಬಲ್ಲ ನಿರ್ಗುಣನಾಗಬಲ್ಲ,", 
        "9083": "ಸತ್ಯವೆ ಜಲ, ಸಮತೆಯೆ ಗಂಧ, ಅರಿವೆ ಅಕ್ಷತೆ,", 
        "9084": "ಸತ್ಯಶುದ್ಧದಿಂ ಬಂದ ಪದಾರ್ಥವ", 
        "9085": "ಸತ್ಯಶುದ್ಧದಿಂ ಬಂದ ಪದಾರ್ಥವ ಶೀಲವ್ರತನೇಮಂಗಳಿಂ", 
        "9086": "ಸತ್ಯಸಹಜವುಳ್ಳ ಶಿವಶರಣರ ಕಂಡಲ್ಲಿ", 
        "9087": "ಸತ್ವರಜತಮೋಗುಣಗಳೆಂಬ ಒಂದೊಂದು", 
        "9088": "ಸಾಕಾರ ನಿರಾಕಾರದೊಳಗೆ ಚಿದ್ರೂಪವ ತಳೆದ", 
        "9089": "ಸಾನುರಾಗ ಸುಸಂಗವಾಗಿ", 
        "9090": "ಸುಜ್ಞಾನಾನುಭವದಿಂ ಭವಿ ಭವವಳಿದುಳಿದ ಶೇಷನು,", 
        "9091": "ಸುನಾದ ಮಹಾನಾದಂಗಳು ಸದ್ಭಾವದಲ್ಲಿ ಪ್ರಕಾಶಿಸಲು", 
        "9092": "ಸುರರೆಲ್ಲರೂ ಬೆಂದು ಹೋದರು.", 
        "9093": "ಸುಳಿದಡೆ ಒಡಲಿಲ್ಲ ನಿಂದಡೆ ನೆಳಲಿಲ್ಲ", 
        "9094": "ಸೂಕರಂಗೆ ಸುಗಂಧ ಸೊಗಸುವುದೆ ?", 
        "9095": "ಸೂಕ್ಷನಾದದಿಂದತ್ತಣ ಆದಿಬಿಂದುವಿನಲ್ಲಿ ಅರಿವು ರೂಪಾದ", 
        "9096": "ಸೋಂಕದ ಮುನ್ನವೇ ಸುಯಿಧಾನ ಪ್ರಸಾದಿ.", 
        "9097": "ಸ್ಫಟಿಕದ ಘಟದೊಳಗಣ ಜ್ಯೋತಿಯಂತೆ,", 
        "9098": "ಸ್ವಕಾಯದಂತರ್ಗತದಲ್ಲಿ, ಚಿದಾಕಾಶದಲ್ಲಿ", 
        "9099": "ಹಠಯೋಗ ಲಂಬಿಕಾಯೋಗ ಆತ್ಮಯೋಗ", 
        "9100": "ಹಣ ಬಂಗಾರ ವಸ್ತ್ರ ಕಪ್ಪಡ", 
        "9101": "ಹದಿನಾರಂಗುಲದುದ್ದ ಸರ್ಪನ ವಿಷ ಕೆಟ್ಟು", 
        "9102": "ಹರಣವಿರ್ದು ಹರಣವಿಲ್ಲ, ಹರಣ ಲಿಂಗದಲ್ಲಿ ನಿರ್ಯಾಣವಾಯಿತ್ತಾಗಿ.", 
        "9103": "ಹರಿಗೆ ಇಂದ್ರನೀಲಲಿಂಗ, ಬ್ರಹ್ಮಂಗೆ ಶೈಲಲಿಂಗ,", 
        "9104": "ಹರಿಹರಬ್ರಹ್ಮಾದಿಗಳ ನುಂಗಿತ್ತು ಈ ಮಾಯೆ.", 
        "9105": "ಹಲವು ಕರಣಂಗಳೆಂಬ ಹಲವು ಬಣ್ಣದ ಚಿನ್ನವ", 
        "9106": "ಹಾಲ ಹಿಡಿದು ಬೆಣ್ಣೆಯನರಸಲುಂಟೆ ?", 
        "9107": "ಹಿಂದಣ ಕಥೆಯ ಮುಂದೆ ಪೇಳುವ ಕಾವ್ಯವಲ್ಲ.", 
        "9108": "ಹಿಂದಣ ಭವವನೊರಸಿ, ಮುಂದಣ ಜನನಕರ್ಮವ ಸಂಹರಿಸಿ", 
        "9109": "ಹಿಡಿದ ವ್ರತವ ಬಿಡುವವನಲ್ಲ,", 
        "9110": "ಹಿಡಿಯದೆ, ಹಿಡಿದುದ ಬಿಡದೆ, ಬಿಟ್ಟುದನಾರೂ ಅರಿಯರಲ್ಲಾ !", 
        "9111": "ಹೀನಬುದ್ಧಿಯ ಕರ್ಮದ ಕ್ಷೀಣ ಬಣ್ಣವ ಬಿಟ್ಟು", 
        "9112": "ಹುಟ್ಟುವುದೇ ಬಂಧ, ಸಾವುದೇ ಮೋಕ್ಷ ಎಂಬ", 
        "9113": "ಹುಟ್ಟು ಹೊಂದುಗೆಟ್ಟುದೇ ನಿರ್ವಾಣ.", 
        "9114": "ಹುರಿದು ಬಿತ್ತಿದ ಬೀಜ ಮೊಳೆದೋರದಂತೆ", 
        "9115": "ಹುಸಿಯಿಂದ ಶಿವನ ಮುಕುಟವ ಕಂಡೆನೆಂದು", 
        "9116": "ಹೃದಯದೊಳಗಿಹನ್ನಕ್ಕ ಲಿಂಗಪ್ರಾಣಿ,", 
        "9117": "ಹೆಚ್ಚದು-ಕುಂದದು, ಮಚ್ಚದು-ಬೆಚ್ಚದು,", 
        "9118": "ಹೊಂಬಣ್ಣವ ಹಿಮ್ಮೆಟ್ಟದೆ ಕಂಡು,", 
        "9119": "ಹೊನ್ನು ಹೆಣ್ಣು ಮಣ್ಣು ನಿಮ್ಮ ಸೊಮ್ಮು."
    }
}