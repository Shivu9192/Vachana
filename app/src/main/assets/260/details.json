    "name": "ಮುಮ್ಮಡಿ ಕಾರ್ಯೇಂದ್ರ ಮುಮ್ಮಡಿ ಕಾರ್ಯ ಕ್ಷಿತೀಂದ್ರ", 
    "vachana": {
        "19134": "ನಿಷ್ಕಳಂಕಾತ್ಮನು ನಿಜಪ್ರಕೃತಿವಶದಿಂ", 
        "19135": "ಪ್ರಣವಸ್ವರೂಪಮಾದ ಪರಬ್ರಹ್ಮವೇ", 
        "19136": "ಅಂಗದೊಳಗಿರ್ಪ ಶಕ್ತಿಯಾತ್ಮಸಂಗದಿಂ ಪ್ರಕಾಶಮಾಗಿ,", 
        "19137": "ಮಾನಸಸರೋವರ ಚತುಸ್ಸೋಪಾನವೀಧಿಗಳಿಂ ತುಂಬಿರ್ಪ", 
        "19138": "ದೀಕ್ಷಿತನು ಯಜನಾರ್ಥಿಯಾಗಿ, ಭೂಶುದ್ಧಿಯಂ ಮಾಡಿ,", 
        "19139": "ಏಕಮಾಗಿರ್ಪ ಬೀಜಮಧ್ಯದಲ್ಲಿರಿಸಿ ಅಂಕುರಿಸೆ,", 
        "19140": "ಆದಿಯಲ್ಲಿ ಪ್ರಣವಸ್ವರೂಪಮಾದ ಪರಮಾತ್ಮನು", 
        "19141": "ಬೀಜದೊಳಗಿಹ ವೃಕ್ಷಫಲವ ಸವಿಯಬಹುದೆ ?", 
        "19142": "ಹಗಲಿದ್ದಲ್ಲಿ ಕತ್ತಲೆಯಿರ್ಪುದೆ ?", 
        "19143": "ಪರಮಾತ್ಮನು ಸ್ವಲೀಲಾನಿಮಿತ್ತ ಸಾಕಾರಸ್ವರೂಪನಾಗಿ,", 
        "19144": "ಲಿಂಗವನಾಚರಿಸುವ ಅಂಗವೇ ಸ್ಥೂಲದಲ್ಲಿ ಗುರುಲಿಂಗವಾಗಿ,", 
        "19145": "ಗುರುವು ಮನದಲ್ಲಿ ಮಂತ್ರವನ್ನೂ, ತನುವಿನಲ್ಲಿ ಲಿಂಗವನ್ನೂ ಸ್ಥಾಪಿಸಿ,", 
        "19147": "ಇಂದ್ರಿಯಸ್ವರೂಪಮಾದ ತನುವೇ ಜಾಗ್ರವು,", 
        "19149": "ರುಚಿಯಲ್ಲಿ ಸಂಹಾರ, ಸ್ಪರುಶನದಲ್ಲಿ ಸೃಷ್ಟಿ, ರೂಪಿನಲ್ಲಿ ಸ್ಥಿತಿ,", 
        "19153": "ಪ್ರಜೆಗಳ ಅಲೆಯ ದೊರೆಯು ಒರ್ಚುವನಲ್ಲದೆ,", 
        "19155": "ಪೃಥ್ವಿಯಲ್ಲಿ ಜನನವೂ, ಆಕಾಶದಲ್ಲಿ ಮರಣವೂ,", 
        "19158": "ಕಳಾಯುಕ್ತಮಾಗಿ ರೂಪುಳ್ಳ ಹೆಣ್ಣನು ಲಿಂಗಕ್ಕೆ ಕೊಟ್ಟು,", 
        "19162": "ಗುರುವೇ, ನಿನ್ನ ಕರದಲ್ಲೆನ್ನಂ ಪುಟ್ಟಿಸಿ,", 
        "19163": "ಗುರುವೇ, ನೀನು ನನ್ನ ಲಿಂಗಕ್ಕೆ ಮಂಗಳಸೂತ್ರವಂ ಕಟ್ಟಿ ಕೊಟ್ಟಂದಿಂದ", 
        "19164": "ಪೃಥ್ವಿಗೆ ಮೈಯೆಲ್ಲಾ ಯೋನಿ, ಜಲಕ್ಕೆ ಶರೀರವೆಲ್ಲಾ ಕಾಲು,", 
        "19365": "ಗಣನಾತೀತನಪ್ಪ ಪರಮೇಶ್ವರನು ಲೀಲಾನಿಮಿತ್ತ ಗಣನೆಗೆ ಬಂದಲ್ಲಿ,", 
        "19366": "ಪೃಥ್ವಿಯೇ ಬಹಿರಂಗ, ಆಕಾಶವೇ ಅಂತರಂಗ,", 
        "19367": "ಜಿಹ್ವೆಯಲ್ಲಿ ಮೃತ್ಯುವೂ, ನೇತ್ರದಲ್ಲಿ ಕಾಲನೂ ಇರಲು,", 
        "19368": "ನಿನ್ನ ಕಂಗಳಭ್ರಮೆಯೇ ಕಾಲರೂಪಮಾಗಿ", 
        "19369": "ವಿರಾಡ್ರೂಪನಾದ ಗುರುವೇ", 
        "19370": "ಸಪ್ತಧಾತುಗಳೆಂಬ ಸಪ್ತಪ್ರಕಾರಂಗಳಿಂದಲೂ", 
        "19371": "ನಿನ್ನ ಕಂಗಳಲ್ಲಿ ಕಾಲಮೃತ್ಯುಗಳುದಿಸಿದವಯ್ಯಾ.", 
        "19372": "ಕರ್ಣದಲ್ಲಿ ಲಯವಂ ಹೊಂದಿ, ಜಿಹ್ವೆಯಲ್ಲಿ ಸೃಷ್ಟಿಯಪ್ಪ ನಾದವೇ", 
        "19373": "ಕರಣಂಗಳೇ ಸ್ಥೂಲರೂಪದ ಶರೀರಮಾಗಿ,", 
        "19374": "ಸ್ಥೂಲಶರೀರವೇ ಬಹಿರಂಗವು, ಸೂಕ್ಷ್ಮಶರೀರವೇ ಅಂತರಂಗವು.", 
        "19375": "ಪಶ್ಚಿಮದಲ್ಲಿರ್ಪ ಪೃಥ್ವಿಯೇ ಶರೀರವು,", 
        "19376": "ವರ್ತಮಾನವೇ ಸ್ಥೂಲವು, ಭೂತಕಾಲವೇ ಸೂಕ್ಷ್ಮವು,", 
        "19377": "ಸೂರ್ಯನೇ ತೀರ್ಥವು, ಚಂದ್ರನೇ ಪ್ರಸಾದವು,", 
        "19378": "ಆಕಾಶವೇ ಸೂಕ್ಷ್ಮವು, ಪೃಥ್ವಿಯೇ ಸ್ಥೂಲವು,", 
        "19379": "ಶರೀರವೇ ಸ್ಥೂಲವು, ಮನಸ್ಸೇ ಸೂಕ್ಷ್ಮವು,", 
        "19380": "ಚಂದ್ರಮಂಡಲಮಧ್ಯಸ್ಥನೇ ಶಿವನು,", 
        "19381": "ಒಳಗೆ ಪಾಪವೂ ಹೊರಗೆ ಧರ್ಮವೂ ಮನುಷ್ಯರಿಗೆ,", 
        "19382": "ವಟಮೂಲವಾಸಿಯಪ್ಪ ಗುರುವು", 
        "19383": "ಶಿವಬಿಂದುವೇ ಚಂದ್ರಸ್ವರೂಪು, ಶಕ್ತಿಬಿಂದುವೇ ಸೂರ್ಯಸ್ವರೂಪು.", 
        "19384": "ಎಲ್ಲಾ ಮಹಿಮೆಯೂ ತನ್ನಲ್ಲುಳ್ಳ ಮಹಾಸತ್ತೇ ನಿಜವು.", 
        "19385": "ಬಾಲ್ಯವೇ ಬ್ರಹ್ಮಸ್ವರೂಪು, ಯೌವನವೇ ವಿಷ್ಣುಸ್ವರೂಪು,", 
        "19386": "e್ಞನವೇ ಮೂಲಮಾಗಿ, ಆನಂದವೇ ಪರಿಪಕ್ವಫಲಮಾಗಿಯೂ ಇರ್ಪ", 
        "19387": "ಭಕ್ತಿಯೆಂಬ ಪಾಲನ್ನು ಮನದಲ್ಲಿ ಹೆಪ್ಪುಗೊಟ್ಟು,", 
        "19388": "ಗೊಲ್ಲರಹಳ್ಳಿಯಲ್ಲಿ ಭೂವಲ್ಲಭನಿದ್ದರೆ,", 
        "19389": "ಮನಸ್ಸೇ ಅಂತಃಕರಣವಾಗಿಯೂ ಶರೀರವೇ ಕರಣವಾಗಿಯೂ", 
        "19390": "ಸತ್ವಸ್ವರೂಪನಾದ ಶಿವನೇ ವಿಷ್ಣುಗುಣವು.", 
        "19391": "ರಜೋಗುಣದಿಂದ ಶಕ್ತಿಲಯವು,", 
        "19392": "ವಸಂತಕಾಲವೇ ಸೂಕ್ಷ್ಮಶರೀರವು,", 
        "19393": "ಆತ್ಮಧರ್ಮವೇ ತಮಸ್ಸು, ಆತ್ಮನು ಜಡಮಾಗಿರ್ಪನು.", 
        "19394": "ಆಕಾಶವಿಷಯಕ್ಕೆ ವಾಯುವೇ ಪ್ರಕಾಶಕಾರಣವು,", 
        "19395": "ಜಾಗ್ರದೊಳಗೆ ನಾಲ್ಕರಲ್ಲೊಂದು ಪಾಲು ಸುಷುಪ್ತಿಯು,", 
        "19396": "ಸ್ಥಾವರದಲ್ಲಿ ಜೀವನು ಜಲರೂಪದಿಂ ಪಾದವನಾಶ್ರಯಿಸಿಹನು.", 
        "19397": "ಜೀವನು ತನಗೆ ರುದ್ರಸ್ವರೂಪಮಾದ", 
        "19398": "ಮನುಷ್ಯರಿಗೆ ಅಂತರಗ್ನಿಯು, ದೇವತೆಗಳಿಗೆ ಬಹಿರಗ್ನಿಯು.", 
        "19399": "ಶಿವನೇ ಪುರುಷನು, ವಿಷ್ಣುವೇ ಶಕ್ತಿಯು.", 
        "19400": "ಅಖಂಡಪರಿಪೂರ್ಣವಾಗಿ ಸಕಲವೂ ತಾನೇ ಆಗಿರ್ಪ ಸದಾಶಿವನು,", 
        "19401": "ಹ ಶಬ್ದಕ್ಕೆ ಹರಣಾರ್ಥವು, ಸ ಶಬ್ದಕ್ಕೆ ಯೋಗಾರ್ಥವು ಇರುವದು.", 
        "19402": "ಗುರುವೇ ಪರಬ್ರಹ್ಮವು.", 
        "19403": "ಹೃದಯದಲ್ಲಕ್ಷರಂಗಳಿರ್ಪಂತೆ ಆತ್ಮನಲ್ಲಿ ಪ್ರಪಂಚಮಿರ್ಪುದು,", 
        "19404": "ಪೂರ್ವಾದಿಯಾಗಿ ವಾಯವ್ಯಪರ್ಯಂತವೂ ಸ್ವರ್ಗವು,", 
        "19405": "ಆಧಾರಾದಿ ಷಟ್ಚಕ್ರಂಗಳ ವಿವರಮೆಂತೆಂದೊಡೆ :", 
        "19406": "ಆಧಾರದಲ್ಲಿ ಆಚಾರಲಿಂಗಕ್ಕೆ ಗೃಹಸ್ಥದಳವೇ ಸ್ಥಾನವು,", 
        "19407": "ಪೃಥ್ವೀಚಕ್ರಕ್ಕೆ ಶರೀರವೇ ಪೂರ್ವ,", 
        "19408": "ಹಕಾರವೇ ಶಿವನು, ಬಿಂದುವೇ ಪ್ರಕೃತಿಯು,", 
        "19409": "ಚಿದ್ರೂಪಮಾದ ಮಹವೆಂದರೆ ಮಹಾಜ್ಞಾನವೆಂದರ್ಥ.", 
        "19410": "ಭಾವಮೆಂಬುದು ಸತ್ವಪದಾರ್ಥವು.", 
        "19411": "ಸತ್ತೆಂದರೆ ಸತ್ತೇ, ಚಿತ್ತೆಂದರೆ ಚೇತನ,", 
        "19412": "ಸತ್ತೇ ಬಿಂದುರೂಪಮಾದ ಶರೀರವು,", 
        "19413": "ಚಿತ್ತೇ ಶಿವನು, ಸತ್ತೇ ವಿಷ್ಣುವು ಎಂಬುದರಲ್ಲಿ ಪ್ರಮಾಣವೇನೆಂದರೆ :", 
        "19414": "ಚಿತ್ತಿಗೆ ಸತ್ತೇ ತಮೋಗುಣವು,", 
        "19415": "ಪ್ರಪಂಚವೆಂಬ ಮಹಾಕಮಲವು", 
        "19416": "ನಾಸಿಕದಲ್ಲಿ ಸಂಚರಿಸುತಿರ್ಪ", 
        "19417": "ತತ್ತ್ವಮಸಿ ಎಂದರೆ ಅದೇ ನೀನಾಗುತ್ತಿ ಎಂಬರ್ಥವು.", 
        "19418": "ಆದಿಬಿಂದುವೇ ಶೂನ್ಯಮಾದುದರಿಂ ಸಂಹಾರರೂಪಮಾಯಿತ್ತು.", 
        "19419": "ಸೃಷ್ಟಿ ಕರ್ತುವಾದ ಸ್ತ್ರೀತ್ವಾಧಿದೇವತೆಯಾದಾತನೆ ಬ್ರಹ್ಮನು.", 
        "19420": "ಚತುರ್ಯುಗಂಗಳು ಪುಂಗವ ರೂಪಾಗಿರ್ಪ", 
        "19421": "e್ಞನಪಂಚೇಂದ್ರಿಯ, ಕರ್ಮಪಂಚೇಂದ್ರಿಯಂಗಳಲ್ಲಿ", 
        "19422": "ವಾಕ್ಪಾಣಿಪಾದ ಗುದ ಗುಹ್ಯಗಳೈದು", 
        "19423": "ಪೃಥ್ವಿಯನನುಸರಿಸಿರ್ಪ ಚಿತ್ತೇ ಪಶ್ಚಿಮ;", 
        "19424": "ಈಶಾನ್ಯ ಕರಕಮಲ ಕೈಲಾಸ ಸ್ಥಾಯಿಯಾಗಿರ್ಪ", 
        "19425": "ಪರಮಾತ್ಮನಿಗೆ ಆದಿಯಲ್ಲಿ", 
        "19426": "ಪರಬ್ರಹ್ಮ  ಸೃಷ್ಟಿರೂಪವಾಗಿ ಜನಿಸುವುದಲ್ಲ", 
        "19427": "ಸೃಷ್ಟಿ ಕರ್ತುವೇ ಸ್ತ್ರೀ, ಸ್ಥಿತಿ ಕರ್ತನೇ ಪುರುಷನು,", 
        "19428": "ಕಾಮವೆಂತು ಗುಹ್ಯದಲ್ಲಿ ಚಲನವೋ", 
        "19429": "ಗುರುವಿನ ಹಸ್ತವು ಜಂಗಮದ ಪಾದಾಂಗುಷ್ಠವು", 
        "19430": "ಕೇವಲ ವಿವಿಚ್ಚ ಜ್ಞಾನವೇ ಶಿವನು.", 
        "19431": "ಘಟ ಮುದಿಸುತಲೆ ಘಟಮುತ್ಪನ್ನವಾಗುವಂತೆ,", 
        "19432": "ಜಾಗ್ರ ಶರೀರದಲ್ಲಿ  ರಜಸ್ಸು", 
        "19433": "ಆತ್ಮಾಂಶವೆ ಚೈತನ್ಯ", 
        "19434": "ಶತಮಾನಃ ಪುರುಷಃ ಶತೇಂದ್ರಿಯ ಎಂಬ", 
        "21122": "ಬ್ರಹ್ಮಾಂಡವೇ ಸ್ಥಾವರವು, ಪಿಂಡಾಂಡವೇ ಜಂಗಮವು,", 
        "21123": "ಶಬ್ದಮಧ್ಯದಲ್ಲಿರ್ಪ ಅರ್ಥವೇ ಜೀವನು,", 
        "21124": "ತಿಷ್ಠತೀತಿ ಸ್ಥಾಣುಃ ಎಂಬ ಅರ್ಥದಿಂದ", 
        "21125": "ಕಾಯವೆಂಬ ಪುರನಾಳುವ ಜೀವನೆಂಬರಸು", 
        "21126": "ಅಗ್ನಿಯಲ್ಲಿ ಕಾಣಿಸುವ ಛಾಯೆಯು ನಿರಾಕಾರ ತಮೋರೂಪುಳ್ಳದ್ದು,", 
        "21127": "ಪೃಥ್ವಿಯೇ ಸ್ಥಾವರ, ಜಲವೇ ಜಂಗಮ,", 
        "21128": "ಜೀವಪಶುಗಳಂ ಕೊಲ್ವ ಕಾಲವ್ಯಘ್ರನಂ ಸಂಹರಿಸಿ,", 
        "21129": "ಸೂಕ್ಷ್ಮದಿಂದಲೇ ಸ್ಥೂಲಮಂದಿಸಿ,", 
        "21130": "ಕೇವಲ ಸದ್ರೂಪಮಾದ ನಿಷ್ಕಳಶಿವತತ್ವವು", 
        "21131": "ನೀರು ತನ್ನೊಳಗಿರುವ ಮಲಿನವಂ ಧರೆಗೆಹತ್ತಿಸಿ ತಾನಾರಿಪೋದಡೆ", 
        "21132": "ಭಂಡಾರದೊಳಗಿರ್ಪ ಸುವರ್ಣವೇ ಪ್ರಾಣಲಿಂಗವಾಗಿ,", 
        "21133": "ಪುರುಷನ ವೀರ್ಯವೇ ಸ್ತ್ರೀಗೆ ಶಕ್ತಿಯಾಗಿ,", 
        "21134": "ನಿಜಮಹಾತ್ಮ್ಯಪ್ರಕಟನ ಪ್ರಪಂಚ ಕ್ರೀಡಾಕಾರಣಮಾದ", 
        "21135": "ಜಂಗಮಫಲವೆಲ್ಲವೂ ದೇವತೆಗಳಾಹಾರವು;", 
        "21136": "ಘ್ರಾಣಜಿಹ್ವಾತ್ವಕ್ಫ್ರೋತ್ರಮಾನಸಾದಿ ವಿಷಯೇಂದ್ರಿಗಳಲ್ಲಿ", 
        "21137": "ಸತ್ಯವೇ ಪರಮನು, ಜ್ಞಾನನವೇ ಜೀವನು, ಆನಂದವೇ ಶರೀರವು.", 
        "21138": "ವಾಚಾತೀತವೂ ಮನೋತೀತವೂ ಭಾವಾತೀತವೂ ಆದ ಮಹಾಲಿಂಗವು", 
        "21139": "ಪೃಥ್ವಿಯಪ್ಪುಗಳಿಂದ ಸ್ಥೂಲ ಶರೀರವು,", 
        "21140": "ಪೂರ್ವ ದಕ್ಷಿಣೋತ್ತರದ ಪಶ್ಚಿಮದೇಶಗಳೊಳು", 
        "21141": "ಪೂರ್ವಪಶ್ಚಿಮ ದಕ್ಷಿಣೋತ್ತರಗಳಲ್ಲಿ ಕೋಟಲೆಗೊಳುತ್ತಿರ್ಪ", 
        "21142": "ಪೃಥಿವ್ಯಪ್ತೇಜೋವಾಯ್ವಾಕಾಶಂಗಳೈದು", 
        "21143": "ಪೃಥ್ವಿಯೇ ಶೂದ್ರನು, ಜಲವೇ ವೈಶ್ಯನು,", 
        "21144": "ಗೋಳಕಾಕಾರಮಾದ ಮಹಾಲಿಂಗವೇ ಬೀಜ,", 
        "21145": "ಅಖಂಡಕಾಲಾತೀತಮಾದ ಮಹಾಲಿಂಗವೇ", 
        "21146": "ಶರೀರವೇ ಹೇಯದಮೊಟ್ಟೆ ದುರ್ಗಂಧವೇ ನೈಜ,", 
        "21147": "ದಕ್ಷಿಣದಲ್ಲಿರ್ಪ ಯಮನೇ ವಿಷ್ಣುವು;", 
        "21148": "ಸೃಷ್ಟಿಹೇತುವಾದ ಸಂಸಾರವೇ ಪೃಥ್ವಿಯು,", 
        "21149": "ಪೃಥಿವ್ಯಪ್ತೇಜೋವಾಯ್ವಾಕಾಶಂಗಳಲ್ಲಿ", 
        "21150": "ಪಂಚಭೂತಂಗಳಂ ಮೀರಿರ್ಪಆತ್ಮರಾರೆಂದು ತಿಳಿವುದೆಂತೆಂದೊಡೆ:", 
        "21151": "ಜೀವನಿಗೆ ಮನಸ್ಸೇ ಪ್ರಥಮವೇಷ.", 
        "21152": "ಇಂದ್ರಿಯಗಳಲ್ಲಿ ಸುಷುಪ್ತಿಜ್ಞಾನವು,", 
        "21153": "ಸತ್ಯಸ್ವರೂಪಮಾದ ಬಸವೇಶ್ವರನೇ ಭಕ್ತನು,", 
        "21154": "ಪಶುಗಳು ಆಹಾರ ಮೈಥುನ ನಿದ್ರೆಗಳನ್ನು", 
        "21155": "ಪೃಥ್ವಿಯು ಜಲದಿಂದಲೇ ಪೂತಮಾಗಿ,", 
        "21156": "ಐಶ್ವರ್ಯದಿಂದ ಶರೀರವು ಪ್ರಕಾಶಿಸುತ್ತಿರ್ಪುದು,"
    }
}