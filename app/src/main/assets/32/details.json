        "8697": "ಕಾಳಮೇಘನೆಂಬ ಭೂಮಿಯಲ್ಲಿ ಕಾಳರಾತ್ರಿಯೆಂಬ ಏರಿ [ಕಟ್ಟೆ],", 
        "8698": "ಅರಿದೆನೆಂಬುದೆ ಅಜ್ಞಾನ, ಮರೆದೆನೆಂಬುದೆ ದಿವ್ಯಜ್ಞಾನ.", 
        "8699": "ಕೆಡದ ಒಡವೆಗೆ ಕೆಟ್ಟವ ನೋಡಿ ದೃಷ್ಟವ ಕಂಡ.", 
        "8700": "ಗುರುಭಕ್ತ ಗುರುವಿಲ್ಲದೆ ಶಿಷ್ಯನಾಗಿ,", 
        "8701": "ಕ್ರೀಯ ನುಡಿವಲ್ಲಿ ಸ್ಥಲ, ಸ್ಥಲವ ನುಡಿವಲ್ಲಿ ಆಚರಣೆ,", 
        "8702": "ಸ್ಥಲವಿವರಂಗಳ ವಿಚಾರಿಸುವಲ್ಲಿ", 
        "8703": "ಕುಂಭದ ಅಪ್ಪುವಿನ ಸಂಗದಲ್ಲಿ ತಂಡುಲವ ಹಾಕಿ", 
        "8704": "ರೂಪಿಲ್ಲದ ನಾಮವ ಕರೆದಡೆ", 
        "8705": "ಕಡೆಯಾಣಿಗೆ ಒರೆಗಲ್ಲ ಹಿಡಿದು", 
        "8706": "ಚಿತ್ರಜ್ಞ ಘಟಲಕ್ಷಣವ ಬರೆಯಬಲ್ಲನಲ್ಲದೆ", 
        "8707": "ದ್ವೈತವೆಂದಡೆ ತಾನಿದಿರು ಎಂಬುದು ಎರಡುಂಟಾದುದು.", 
        "8708": "ತನ್ನನರಿದು ಇದಿರನರಿಯಬೇಕೆಂಬುದು ಪ್ರಮಾಣು.", 
        "8709": "ಶಬ್ದ ರೂಪಿನೊಳಗೆ ಅಡಗುವುದ ಬಲ್ಲಡೆ ದ್ವೈತಿ,", 
        "8710": "ಬಂಧಿಸಿ ಘಟವ ಬಿಟ್ಟಿಹೆನೆಂದಡೆ ಅಭಿಸಂಧಿಯಲ್ಲಿ ನೋವುದು ಜೀವ.", 
        "8711": "ಕೈಲಾಸವೆಂಬುದು ಕ್ರಮಕೂಟ, ಮೋಕ್ಷವೆಂಬುದು ಭವದಾಗರ,", 
        "8712": "ಪ್ರಮೇಯ-ಅಪ್ರಮೇಯ, ಸುರಾಳ-ನಿರಾಳ,", 
        "8713": "ಅರಿದರಿದು ಕೊಡುವವನು ತಾನೆ ಲಿಂಗವೊ", 
        "8714": "ಇಷ್ಟಲಿಂಗಕ್ಕೆ ದೃಷ್ಟಾರ್ಪಿತ,", 
        "8715": "ಉಟ್ಟುಡಿಗೆ ಸಡಿಲಿದಡೆ ಕೆಟ್ಟಾಡಿ ಬಂದು", 
        "8716": "ದಾರಿಯಲ್ಲಿ ಬೀದಿಯಲ್ಲಿ ಮನೆಗಳಲ್ಲಿ"
    }
}